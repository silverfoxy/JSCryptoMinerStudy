     <script type="text/javascript">
     
     	var dataLayer = dataLayer || [];     	
     	var criteoObject = {};

     	var userPanels = document.getElementsByClassName('user-panel');
     	if (userPanels.length > 0 && userPanels[0].dataset.customeremail) {
     	
     		criteoObject['email'] = userPanels[0].dataset.customeremail.toLowerCase();
     	}
     	
     	if (window.pageContext && window.pageContext.type == 'storefront') {
     	
            criteoObject['PageType'] = 'HomePage';
            
        } else if (window.pageContext && window.pageContext.type == 'search') {
        
        	criteoObject['PageType'] = 'ListingPage';
        	
        	var listingProductIDs = [],
        		productTiles = document.getElementsByClassName('product-tile');

			for (var i = 0; i < productTiles.length; ++i) {
			    listingProductIDs.push(productTiles[i].dataset.itemid);
			} 
        	
        	criteoObject['ProductIDList'] = listingProductIDs;

        } else if (window.pageContext && window.pageContext.type == 'product') {
        
        	criteoObject['PageType'] = 'ProductPage';
        	
        	var productDetails = document.getElementsByClassName('product-detail');
	     	if (productDetails.length > 0 && productDetails[0].dataset.prodid) {
	     	
	     		criteoObject['ProductID'] = productDetails[0].dataset.prodid;
	     	}

        } else if (window.pageContext && window.pageContext.type == 'Cart') {
        
        	criteoObject['PageType'] = 'BasketPage';
        	
        	var basketProductsList = [];
        	var cartRows = document.getElementsByClassName('cart-row');  		
        	for (var i = 0; i < cartRows.length; ++i) {
        	
        		var cartRow = cartRows[i];
        		var itemId = cartRow.getElementsByClassName('sku')[0].getElementsByClassName('value')[0].innerText.trim();  		
        		var itemQty = parseInt( cartRow.getElementsByClassName('item-user-actions')[0].dataset.quantity.trim() );
        		var itemPrice = 0;        		
        		if (cartRow.getElementsByClassName('price-adjusted-total').length > 0) {
        		
        			itemPrice = (cartRow.getElementsByClassName('price-adjusted-total')[0].getElementsByTagName('span')[0]
        							.innerText.trim().replace('A$', '').replace('NZ$', '') / itemQty).toFixed(2);
        			
        		} else if (cartRow.getElementsByClassName('price-total').length > 0) {

        			itemPrice = (cartRow.getElementsByClassName('price-total')[0]
        							.innerText.trim().replace('A$', '').replace('NZ$', '') / itemQty).toFixed(2);
        		}
        		
        		basketProductsList.push(
                    {'id': itemId, 'price': itemPrice, 'quantity': itemQty}
                );
                
                criteoObject['ProductBasketProducts'] = basketProductsList;
        	}
        }

		var pageTypes = ['HomePage', 'ListingPage', 'ProductPage', 'BasketPage', 'TransactionPage'];        
        if (pageTypes.indexOf(criteoObject['PageType']) > -1) {
            dataLayer.push(criteoObject);
        }
     </script>
     
