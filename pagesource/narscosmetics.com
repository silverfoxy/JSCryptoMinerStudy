<script type="text/javascript">
  $(document).ready(function() {
    var headerHeight = $('.header-section').outerHeight();
    var slotHeight = $('#1a').outerHeight();
    var bottom1a = slotHeight - headerHeight * 3;
    var bottom1b = slotHeight * 2 - headerHeight * 3;
    var bottom2c = slotHeight * 5 - headerHeight * 3;
    var bottom3 = slotHeight * 6 - headerHeight * 3;
    $(window).on("resize", function() {
      headerHeight = $('.header-section').outerHeight();
      slotHeight = $('#1a').outerHeight();
      bottom1a = slotHeight - headerHeight * 3;
      bottom1b = slotHeight * 2 - headerHeight * 3;
      bottom2c = slotHeight * 5 - headerHeight * 3;
      bottom3 = slotHeight * 6 - headerHeight * 3;
      $(".slot").css('background-position-y', headerHeight + 'px');
    });
    $(window).scroll(function() {
      if ($(this).scrollTop() < bottom1a) {
        $(".v1").css('visibility', 'visible');
        $(".v2").css('visibility', 'hidden');
        $(".v3").css('visibility', 'hidden');
      } else if ($(this).scrollTop() > bottom1a && $(this).scrollTop() < bottom1b) {
        $(".v1").css('visibility', 'hidden');
        $(".v2").css('visibility', 'visible');
        $(".v3").css('visibility', 'hidden');
      } else if ($(this).scrollTop() > bottom2c && $(this).scrollTop() < bottom3) {
        $(".v1").css('visibility', 'hidden');
        $(".v2").css('visibility', 'hidden');
        $(".v3").css('visibility', 'visible');
      } else {
        $(".v1").css('visibility', 'hidden');
        $(".v2").css('visibility', 'hidden');
        $(".v3").css('visibility', 'hidden');
      }
      if ($(this).scrollTop() > 0) {
        slotTop = $('.mega-menu-logo-container').outerHeight() + $('.utility-nav').outerHeight();
        $(".slot").css('background-position-y', slotTop + 'px');
      } else {
        $(".slot").css('background-position-y', '131px');
      }
    });
    $('.slot-video img').fadeIn('slow');
  });

  $(".flyout-x").click(function() {
    $(".flyout").css("display", "none");
  });
</script>
