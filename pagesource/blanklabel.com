    <script type="text/javascript">
        function sendSubscriberEmail() {

            if (validateEmail($('#emailSubscriber').val())) {
                $('#k_id_email').val($('#emailSubscriber').val());
                var options = {
                    url: '//manage.kmail-lists.com/ajax/subscriptions/subscribe',
                    type: 'post',
                    success: function () {
                        $('.subscriber-message').text('Thanks. Check your inbox to confirm your email address.');
                        $('.subscriber-message').show();
                    }
                };

                $("#k_id_embedded_subscription_form").ajaxSubmit(options);
                // AJAXPost('k_id_embedded_subscription_form');
            }
            else {
                $('.subscriber-message').text('Please provide a valid email address.');
                $('.subscriber-message').show();
            }
        }

        function validateEmail(email) {
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(email);
        }
    </script>
