<script>
  String.prototype.toProperCase = function () {
    return this.replace(/[A-Za-z0-9\u00C0-\u00FF]*/g, function(txt){return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();});
  };
  function Logout() {
    document.cookie="LoggedIn=;Path=/;expires=;"
    //alert("loggedIn cookie deleted");
    //alert(GCRMUrlOut);
    window.location = GCRMUrlOut;
  }
  function LoginGetCookie(name) {
    var dc = document.cookie;
    var prefix = name + "=";
    var begin = dc.indexOf("; " + prefix);
    if (begin == -1) {
        begin = dc.indexOf(prefix);
        if (begin != 0) return null;
    }
    else
    {
        begin += 2;
        var end = document.cookie.indexOf(";", begin);
        if (end == -1) {
        end = dc.length;
        }
    }
    return unescape(dc.substring(begin + prefix.length, end));
  } 
  function LoginSetCookie(c_name,value,expiredays)
  {                         
      var LDate = new Date();
      LDate.setTime(LDate.getTime()+(expiredays*24*60*60*1000));
      var LExpires = "; expires="+LDate.toGMTString(); 
      document.cookie = c_name+"="+value+LExpires+"; path=/"; 
  }
  // Check Url For Specific String
  function CheckUrlForSpecificString(AString)
  {
    var LUrl = String(window.location);
    if (LUrl.search(AString) != -1) 
      return true;
    else
      return false;
  }
  if (CheckUrlForSpecificString('loggedin') || (LoginGetCookie("LoggedIn") != '' && LoginGetCookie("LoggedIn") != null))
  {  
    $.ajax(
    {
      url: '/misc/misc/sessioninfo.pbs&Nocache=1&mime=xml&d=' + (new Date()).getTime() ,
      success: function(data){
        try
        {
          var LxmlDoc = data || "no response text";
          var LDate = new Date();
          LDate.setTime(LDate.getTime() + (600 * 60 * 1000)); //timeout 10 hours
          var LReguser        = LxmlDoc.getElementsByTagName("reguser")[0].childNodes[0].nodeValue;
          var LUserfname      = LxmlDoc.getElementsByTagName("userfname")[0].childNodes[0].nodeValue;
          var LUserlname      = LxmlDoc.getElementsByTagName("userlname")[0].childNodes[0].nodeValue;
          var LAccessItem     = LxmlDoc.getElementsByTagName("accessitem")[0].childNodes[0].nodeValue;
          if(LReguser != 'null')
          {
            if(LUserfname != 'null' || LUserlname != 'null')
              $("#Subscribe_Name").html("Du er logget på som " + LUserfname.toProperCase() + " " + LUserlname.toProperCase()); 
              $("#Subscribe_Login").show();
              /*Disable Biblioteket and Eavis-arkivet buttons when user has no PLUSS access*/
              var hasPluss = LAccessItem.indexOf(",KKPLUSS,");
              var hasKombi = LAccessItem.indexOf(",KOMBIKLK,");
              if(hasPluss==-1 && hasKombi==-1){
                $('.bibliotek').attr('class','dimmed');
