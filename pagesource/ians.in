<script type="text/javascript">
function validate_form() 
{ 

   if((document.register.fname.value=="") || 
	(eval(document.register.fname).value.charAt()==' ')) 
	{ 
		window.alert("Please enter your name"); 
		document.register.fname.focus(); 
		return false; 
	} 
   else if((document.register.Email.value=="") || 
	   (document.register.Email.value.indexOf('@') == -1) || 
		(document.register.Email.value.indexOf('.') == -1) ) 
	   { 

			window.alert("Please enter a valid email Id"); 
			document.register.Email.focus(); 
			return false; 
		}

	else if((document.register.correct.value=="") || (document.register.rand_ans.value=="?"))
	{

			window.alert("Please enter the sum of the numbers"); 
			document.register.rand_ans.focus(); 
			return false;
	}

	else if(document.register.correct.value!=document.register.rand_ans.value)
	{
			window.alert("Entered sum does not match"); 
			document.register.rand_ans.focus(); 
			return false;
	}
	var random1 = document.register.rand_ans.value;
	var name = document.register.fname.value;
	var email = document.register.Email.value;

	if (window.XMLHttpRequest)
	{	
		// code for IE7+, Firefox, Chrome, Opera, Safari
		xmlhttp=new XMLHttpRequest();
	}
	else
	{	
		// code for IE6, IE5
		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}

	xmlhttp.onreadystatechange=function(){
		if (xmlhttp.readyState==4 && xmlhttp.status==200)
		{	
			document.getElementById("subDiv").style.display="block";
			var res=Array();
			var resarr = xmlhttp.responseText;
			res = resarr.split('[[]]');
			document.getElementById("subDiv").innerHTML=res[0]+res[3];
			jQuery("#subcriptionForm").slideUp();
			jQuery("#planetimg").slideDown();
			jQuery("#planetimg").css("display","block");
			document.register.fname.value = "";
			document.register.Email.value = "";
			document.getElementById("random").innerHTML= res[1];
			document.register.rand_ans.value="?";
			document.register.correct.value=res[2];
			document.getElementById("img").innerHTML="<INPUT  class='button' type='button' value='Subscribe'  name='btnreg'  onclick='javascript:return validate_form();'>";
		}
		else
			document.getElementById("img").innerHTML="<div><img src='images/icon_loading.gif'><span style='color: #ff0000'>Processing...</span></div>";
	}
		xmlhttp.open("POST","http://ians.in/register.php",true);
		xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
		xmlhttp.send("name="+name+"&email="+email+"&rand="+random1);
} 
</script>


