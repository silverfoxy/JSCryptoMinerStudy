		<script>
			$(function () {
				var ua = {};
				ua.name = window.navigator.userAgent.toLowerCase();
 
				ua.isIE = (ua.name.indexOf('msie') >= 0 || ua.name.indexOf('trident') >= 0);
				ua.isIE8 = (ua.isIE && ua.ver <= 8);
				ua.isiPhone = ua.name.indexOf('iphone') >= 0;
				ua.isiPod = ua.name.indexOf('ipod') >= 0;
				ua.isiPad = ua.name.indexOf('ipad') >= 0;
				ua.isiOS = (ua.isiPhone || ua.isiPod || ua.isiPad);
				ua.isAndroid = ua.name.indexOf('android') >= 0;
				ua.isTablet = (ua.isiPad || (ua.isAndroid && ua.name.indexOf('mobile') < 0));

				if(ua.isiOS || ua.isAndroid) {
					$(document).on({
						'touchstart': function(event){
							var $target =$(event.target);

							if($target.parent().hasClass('dropdown')){

								var $targetElement = $target.parent().find('.dropdown-tier2');

								if ($targetElement.is(":hidden")){

									event.preventDefault();
									$('.dropdown-tier2').fadeOut(100);

									$targetElement.breakColumn({
										height: 460
										});

									$targetElement.columnize({
										manualBreaks: true,
										height: 460 
										});

								} else {
									event.stopPropagation();
								}

							} else if($target.parent().hasClass('dropdown-link')) {
								event.stopPropagation();
							} else if($target.hasClass('column')) {
								return false;
							} else {
								$('.dropdown-tier2').fadeOut(100);
							}
						}
					});
				} else {
					$('.page-header .container ul .dropdown').on({
						'mouseenter': function () {
							
							$('.page-header .container ul li.focused').removeClass('focused');

							var $targetElement = $(this).find('.dropdown-tier2');

							sethover = setTimeout(function () {

								$targetElement.breakColumn({
									height: 460
									});

								$targetElement.columnize({
									manualBreaks: true,
									height: 460 
									});

							}, 400);
						},
						'mouseleave': function () {

							clearTimeout(sethover);

							var $targetElement = $(this).find('.dropdown-tier2');

							$targetElement.fadeOut(100);
						}
					});
					
					$('.page-header .container ul li > a').on({ 
						'focus': function () {
							var $tabFocused = $('.dropdown.focused');
							var $parent = $(this).parent();
							if ( $tabFocused ) {
								
								$tabFocused.trigger('mouseleave');
								
								$tabFocused.removeClass('focused');
							}
							
							if ( $parent.hasClass('dropdown') ) {
								
								$parent.trigger('mouseenter').addClass('focused');
							}
						}
					});
					// メガメニュー以外の要素にフォーカスがあたった場合にメニューを閉じる
					$(document).on( 'focus', '*',function(e){
						e.preventDefault();
						e.stopPropagation();
						console.log(!$(this).closest('.dropdown-megamenu')[0]);
						if ( !$(this).closest('.dropdown-megamenu')[0] ) {
							$('.dropdown.focused').trigger('mouseleave').removeClass('focused');
						}
					});

					
					var addFocusClassAttr = function(){
						$('.dropdown-megamenu .dropdown-link.focused').removeClass('focused');
						$(this).parent().addClass('focused');
					}
					$('.dropdown-megamenu .dropdown-link > a').on('focus', addFocusClassAttr);
					$('.dropdown-megamenu .dropdown-link > span').on('focus', addFocusClassAttr);
					
					$('.dropdown-megamenu .dropdown-link').on('mouseenter', function(){$(this).removeClass('focused');});
					$('.dropdown-megamenu .dropdown-link　> a').on('mouseenter', function(){$(this).removeClass('focused');});
					$('.dropdown-megamenu .dropdown-link　> span').on('mouseenter', function(){$(this).removeClass('focused');});
				}
			});
		</script>

