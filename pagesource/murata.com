<script>
	
	$(document).ready(function () {
		$('#carousel').carouFredSel({
			width: '100%',
			items: {
				visible: 3,
				start: -1
			},
			scroll: {
				items: 1,
				duration: 1000,
				timeoutDuration: 3000
			},
			prev: '.carousel-prev',
			next: '.carousel-next'
		});

		
		var $carousel = $('#carousel');
		var $pause = $('#pause');
		var $play = $('#play');
		$play.hide();

		$('#carousel_controll').on('click', function (e) {
			$this = $(this);
			e.preventDefault();
			e.stopPropagation();
			if ($this.hasClass('play')) {
				$carousel.trigger('configuration', { auto: false });
				$play.show();
				$pause.hide();
				$this.removeClass('play');
				$this.addClass('pause');
			} else {
				$carousel.trigger('configuration', { auto: true });
				$pause.show();
				$play.hide();
				$this.removeClass('pause');
				$this.addClass('play');
			}
		});
		
	});
</script>

