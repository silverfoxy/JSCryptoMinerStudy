    <script>
      // Used to prevent blank space in comments field on competition page
      $("#CompetitionQuestion").on("keydown", function (e) {
        var len = $("#CompetitionQuestion").val().length;
        if (len == 0) {
          return e.which !== 32;
        } else {
          var pos = $("#CompetitionQuestion").getCursorPosition();
          if (pos == 0) {
            return e.which !== 32;
          }
        }
      });
      $("#CompetitionQuestion").on("keyup", function (e) {
        var pos = $("#CompetitionQuestion").getCursorPosition();
        if (pos == 0) {
          var values = $("#CompetitionQuestion").val();
          if (values.charAt(0) == ' ') {
            values = values.trim();
            $("#CompetitionQuestion").val(values);
          }
          return e.which !== 32;
        }
      });
      (function ($, undefined) {
        $.fn.getCursorPosition = function () {
          var el = $(this).get(0);
          var pos = 0;
          if ('selectionStart' in el) {
            pos = el.selectionStart;
          } else if ('selection' in document) {
            el.focus();
            var Sel = document.selection.createRange();
            var SelLength = document.selection.createRange().text.length;
            Sel.moveStart('character', -el.value.length);
            pos = Sel.text.length - SelLength;
          }
          return pos;
        }
      })(jQuery);
    </script>
