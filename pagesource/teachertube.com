    function videoFav(video_id) {

        if ($("#checkSession").val()) {

            if ($("#fav_div_video_" + video_id + " a").hasClass("itemActive")) {
                $(".msgBox.error").css("display", "block");
                $(".msgBox.error p").html('This Video is already added to your favorites..');
                $('html, body').animate({scrollTop: $('.msgBox.error').position().top}, 'slow');
                return;
            }

            var data = {"video_id": video_id}

            $.post(baseUrl + "ajax-video/AjaxFavVideo", data, function (res) {
                $(".msgBox.success").css("display", "block");
                $("#fav_div_video_" + video_id + " a").addClass("itemActive");
                $('html, body').animate({scrollTop: $('.msgBox.success').position().top}, 'slow');
                $(".msgBox.success p").html(res);
            });
        } else {
            $(".msgBox.error").css("display", "block");
            $('html, body').animate({scrollTop: $('.msgBox.error').position().top}, 'fast');
            $(".msgBox.error p").html('Please Login to add into your favourite.');

            return;
        }
    }

