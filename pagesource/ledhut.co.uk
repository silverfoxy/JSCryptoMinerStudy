<script>

    var loginFormActive = false, loginFormShowing = 0;

    jQuery(document).ready(function (){
        centerIt(jQuery('#ajaxledhut-login-popup'));
        showForgotPassword();
        showLoginPopup();
    });

    function showAjaxLogin(){
        if(typeof(hideAjaxsignup) == "function"){
            hideAjaxsignup();
        }
        loginFormActive = true;
        loginFormShowing = 1;
        jQuery('#ajaxledhut-login-popup').fadeIn(500);
        jQuery('.ajaxledhut-login-bwrapper').fadeIn(500);
    }
    var hideAjaxLogin = function (){
        loginFormActive = false;
        loginFormShowing = 0;
        jQuery('#ajaxledhut-login-popup').fadeOut();
        jQuery('.ajaxledhut-login-bwrapper').fadeOut();
    };
    var centerIt = function (el /* (jQuery element) Element to center */) {
        if (!el) {
            return;
        }
        var moveIt = function () {
            var winWidth  = jQuery(window).width();
            var winHeight = jQuery(window).height();
            var scrollTopY = jQuery(window).scrollTop();
            el.css("position","absolute")
                .css("left", ((winWidth / 2) - (el.width() / 2)) + "px")
                .css("top", ((winHeight / 2) - (el.height() / 2) + scrollTopY) + "px")
                .css("z-index", 99999);
        };
        jQuery(window).resize(moveIt);
        jQuery(window).scroll(moveIt);
        moveIt();
    };

    jQuery(window).resize(centerIt(jQuery('#ajaxledhut-login-popup')));
    /*Close*/
    jQuery('#ajaxledhut-close-button, .ajaxledhut-login-bwrapper').on('click', function(e) {
        hideAjaxLogin();
    });
    // Forgotten
    jQuery('#ajaxledhut-forgot-password-link').on('click', function(e) {
        loginFormShowing = 2;
        jQuery('#ajaxledhut-login-popup-contents-login').hide();
        jQuery('#ajaxledhut-login-popup-contents-forgot').show();
    });
    // Back to login
    jQuery('#ajaxledhut-return-login-link').on('click', function(e) {
        loginFormShowing = 1;
        jQuery('#ajaxledhut-login-popup-contents-login').show();
        jQuery('#ajaxledhut-login-popup-contents-forgot').hide();
    });
    // Login Action
    jQuery('#ajaxledhut-login-button').on('click', function(e) {
        checkKeyAction();
    });
    jQuery(document).keypress(function(e) {
        if(e.which == 13) {
            if(loginFormShowing == 2) e.preventDefault();
            checkKeyAction();
        }
    });
    function checkKeyAction (){
        if(loginFormActive){
            if(loginFormShowing == 1){
                performLoginAction();
            }else{
                performForgotAction();
            }
        }
    }

    function showForgotPassword(){
        var reg = /[?&]forgot=/;
        if(reg.test(location.search)){
            showAjaxLogin();
            loginFormShowing = 2;
            jQuery('#ajaxledhut-login-popup-contents-login').hide();
            jQuery('#ajaxledhut-login-popup-contents-forgot').show();
        }
    }

    function showLoginPopup(){
        var reg = /[?&]login=/;
        if(reg.test(location.search)){
            showAjaxLogin();
        }
    }

    function performLoginAction(){
        /* Hide form and display loading */
        var table = jQuery('#ajaxledhut-login-table');
        var loading = jQuery('#ajaxledhut-login-loading');
        var error = jQuery('#ajaxledhut-login-error');

        table.hide();
        error.hide();
        loading.show();


        var form = jQuery('#ajaxledhut-login-form');
        var url = 'https://www.ledhut.co.uk/ajaxlogin/ajax/loginAjax/';
        new Ajax.Request(url, {
            parameters: form.serialize(true),
            method: 'POST',
            onComplete: function(transport) {
                loading.hide();
                if(transport.status == 200) {
                    var result = transport.responseText.evalJSON();
                    if(result.success) {
                        location.reload();
                    } else {
                        error.html(result.error);
                        table.show();
                        error.show();

                    }
                }
            }
        })
    }

    // Forgot Action
    jQuery('#ajaxledhut-forgot-button').on('click', function(e) {
        performForgotAction();
    });
    
    function performForgotAction(){
        var table = jQuery('#ajaxledhut-forgot-table');
        var loading = jQuery('#ajaxledhut-forgot-loading');
        var error = jQuery('#ajaxledhut-forgot-error');
        var success = jQuery('#ajaxledhut-forgot-success');
        var email = jQuery('#id_ajaxledhut_email').val();
        var timeout;

        if(email != '') {
            clearTimeout(timeout);
            table.hide();
            error.hide();
            loading.show();

            var url = 'https://www.ledhut.co.uk/ajaxlogin/ajax/forgotPasswordAjax/';
            var parameters = { email: email };

            new Ajax.Request(url, {
                method: 'POST',
                parameters: parameters,
                onSuccess: function(transport)  {
                    var result = transport.responseText.evalJSON();
                    loading.hide();
                    if(result.success)  {
                        success.html(result.success);
                        success.show();
                        table.show();

                        timeout = setTimeout(function (){
                            success.hide();
                        },10000)
                    }
                    else    {
                        error.html(result.error);
                        error.show();
                        table.show();
                        timeout = setTimeout(function (){
                            error.hide();
                        },10000)
                    }
                }
            });

        }
        else    {
            error.html('Please enter a valid email address');
            table.show();
            error.show();
        }
    }
