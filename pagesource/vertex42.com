.pv #ratingDiv{margin:1em 0;}a.video-link{display:inline-block;color:#3969ad;height:20px;line-height:1.429em;text-decoration:none;background:url(//cdn.vertex42.com/Images/play_20x30.png) no-repeat;padding-left:35px;}
a.video-link:hover{filter:alpha(opacity=60);opacity:.6;}
.versions a{display:block;margin-bottom:5px;text-decoration:none;line-height:1.143em;}
