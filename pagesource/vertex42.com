.db2 .preview img{max-width:100%;}
.db2 .preview img.border{border:1px solid #ccc;}
.db2 .preview.bg{position:relative;background:url(//cdn.vertex42.com/Images/bkg-download.png) no-repeat left top;}
.db2 .dl{margin-left:69%;padding-top:0.714em;color:#555;}
.btnDL{display:block;max-width:300px;background:#6d9f1d;color:#fff;padding:0.517em 0;font-size:1.286em;text-decoration:none;text-transform:uppercase;text-align:center;border-radius:4px;text-shadow:0 1px 2px rgba(0,0,0,0.2);}.btnDL:hover{color:#fff;background:#999;}.btnDL:active{background:#888;}
.btnDL.blue{background:#4a8eca;}.btnDL.red{background:#b71717;}.btnDL.blue:hover,.btnDL.red:hover{color:#fff;background:#999;}
.type{color:#6d9f1d;margin-top:0.5em;margin-bottom:1em;}.type.blue{color:#4a8eca;}
.pv .scr{font-weight:bold;color:#333;margin-bottom:1em;font-size:1em;}
.pv .scr a, .pv .scr span.current{display:inline-block;background:#ddd;width:1.714em;height:1.714em;line-height:1.714em;text-decoration:none;text-align:center;margin-right:0.5em;}
.pv .scr span.current{color:#aaa;background:#eee;}
.pv .scr a:hover{background:#aaa;}
.pv .dl h4{border-bottom:1px solid #ddd;padding:0.214em 0;text-transform:uppercase;margin-bottom:0.5em;margin-top:1.5em;font-weight:normal;margin-right:20px;}
