/* === DOWNLOAD BLOCK  === */
table.download{border:none;margin-top:1em;}
table.download td{font-size:1em;padding:10px;padding-top:2em;}
table.download td h3{margin-top:0;font-size:1.143em;}
table.download td.left{font-size:0.857em;background-color:#fff;vertical-align:top;text-align:center;}
.pv{margin-top:2em;margin-bottom:1em;}
.pv .title{font-size:0.857em;margin:0 0 0.5em 0;padding-top:1em;border-top:1px solid #eee;}
.pv .title h2{font-size:2em;line-height:1em;border:0;margin:0 0 0.214em 0;}
