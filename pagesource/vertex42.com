.more:hover {background:#ccc;}
/* === Template-Specific CSS === */
.calculators #testimonials{padding:3px;font-size:0.857em;color:#555;}
.calendars td h3{margin-top:0;}
.wordtemplates h2{color:#305992;margin-top:1.5em;}
