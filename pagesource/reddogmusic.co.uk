<script type="text/javascript">
    (function() {
        function injectServiceFooter(data) {
            document.getElementById('tp-service-footer').innerHTML = data;
            (function() {
                var box = document.querySelector('.car-o');
                var items = document.querySelectorAll('.car-o li');
                var width = box.dataset.columns;
                var amount = items.length;
                var unit = Math.round(100.0 / width);
                var position = 0;
                box.style.left = '0';
                box.style.transform = 'translate3d(0%, 0px, 0px)';

                function moveWindow(direction) {
                    position = position + direction;
                    if (position < 0) {
                        position = 0;
                    }
                    if (position > amount - width) {
                        position = amount - width;
                    }
                    box.style.transform = 'translate3d(-' + position * unit + '%, 0px, 0px)';
                }

                if (amount > 3) {
                    document.querySelector('#tp-left').addEventListener('click', function () {
                        moveWindow(-1);
                    });
                    document.querySelector('#tp-right').addEventListener('click', function () {
                        moveWindow(1);
                    });
                }
            })();
        }
        $.ajax({
            url: "/api/reviews/service_footer"
        }).done(function(data, textStatus, jqXHR){
            if (data) {
                injectServiceFooter(data);
            }
        });
    }());
</script>

