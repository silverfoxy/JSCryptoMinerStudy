<script type="text/javascript">
    function Response() {
        this.data = null;
        this.listeners = {};
    }
    Response.prototype = {
        get data(){
            return this._data
        },
        set data(data){
            this._data = data;
            this.changed({type: 'data', data: data});
        },
        changed: function(note) {
            try {
                this.listeners[note.type].forEach(function(listener){
                    listener.handler(note);
                });
            } catch(err) {/*silent*/}
        },

        addListener: function(message, handler) {
            this.listeners[message] = this.listeners[message] || [];
            this.listeners[message].push({ handler: handler });
        }
    };
    var search_box = (function() {
        var search_results_box = $('#sResults');
        var search_results_inner = $('#sResults').find('.cont');
        var products_box = $('#sProducts');
        var brands_box = $('#sBrands');
        var categories_box = $('#sCategories');
        var notification_box = $('#searchNote');
        var search_open = false;
        var base_url = '/api/instant_search/';
        var response = new Response();

        var show_results = function(data) {
            products_box.html(data.products);
            brands_box.html(data.brands);
            categories_box.html(data.categories);
            search_results_box.height(search_results_inner.outerHeight(true));
            notification_box.html(data.more_html);
            search_open = true;
        };

        var hide_results = function() {
            search_results_box.removeClass('open');
            search_results_box.height(0);
            search_open = false;
            notification_box.html('');
        };

        var response_changed = function(note) {
            if (note.data.results > 0) {
                show_results(note.data)
            } else {
                hide_results()
            }
        };

        response.addListener('data', response_changed);

        return {
            update: function(event) {
                var query = trim1(event.target.value);
                if (query !== '') {
                    this.search(query);
                } else {
                    hide_results();
                }
            },

            search: function(query) {
                query = encodeURI(query);

                $.ajax(base_url + query + '/', {
                    success: function(data, status, jqXHR) {
                        response.data = data;
                    }
                });
            }
        };
    }());
    var search_field_handler = function(event) {
        if (search_box.searchTimeoutID) {
            clearTimeout(search_box.searchTimeoutID);
        }
        search_box.searchTimeoutID = setTimeout(function(){ search_box.update(event) }, 500);
    };
    $(document).ready(function() {
        $('.search_container .search_field').keyup(search_field_handler);
    });
</script>

