			$(document).on({
				"ajaxStart.weather": function() {
					$weather.addClass("loading");
					$clock.hide();
					$location.hide();
					$suhu.hide();
				},
				"ajaxStop.weather": function() {
					$weather.removeClass("loading");
					$clock.show();
					$location.show();
					$suhu.show();
				}
			});

			var url = 'http://www.mcdonalds.co.id/api/geolocate';

			function updateDisplay(description, temperature, location, clock, iconUrl){
				$('.suhu > .status').text(description);
				$('.suhu > .celcius').text(temperature);
				$('.weather > .location > .inner').text(location);
				$('.waktu' ).text(clock);
				$('#iconweather').attr('src',iconUrl)

			}

			function getClock(){
				var date = new Date();
				var hours = (date.getHours()<10 ? '0':'') + date.getHours();
				var minutes = (date.getMinutes()<10 ? '0':'') + date.getMinutes();

				return hours + ':' + minutes;
			}
			function updateWeather(position) {
				$.ajax({
					url: 'http://api.openweathermap.org/data/2.5/weather',
					dataType: "jsonp",
					data: {
						lat: position.coords.latitude,
						lon: position.coords.longitude
					},
					success: function (response) {
						updateDisplay(response.weather[0].description, Math.round((response.main.temp - 273.15)*10)/10 + ' C', response.name, getClock(), '/img/'+ response.weather[0].icon +'.png');
					}
				});
			}

