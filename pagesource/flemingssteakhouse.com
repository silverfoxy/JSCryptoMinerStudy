<script>
    function GetUrlParameter(sParam) {
        if (window.location.search.length > 1) {
            var sPageURL = window.location.search.substring(1);
            var sURLVariables = sPageURL.split('&');
            for (var i = 0, iC = sURLVariables.length; i < iC; i++) {
                var sParameterName = sURLVariables[i].split('=');
                if (sParameterName[0] == sParam) {
                    return sParameterName[1];
                }
            }
        }
        return null;
    }
    function FOFModalIsSuppressed() {
        // Add awareness for omnivirt page/orientation overlay
        if (omnivirt_isVisible()) {
            return true;
        }
        // Add awareness for existing members
        if (IsFOF()) {
            return true;
        }
        
        var prefix = '[FOFModalSuppressed]=';
        var begin = document.cookie.indexOf('; ' + prefix);
        if (begin == -1)
        {
            begin = document.cookie.indexOf(prefix);
            if (begin != 0)
            {
                return false;
            }
        }
        else
        {
            begin += 2;
        }
        
        var end = document.cookie.indexOf(';', begin);
        if (end == -1)
        {
            end = document.cookie.length;
        }
        
        var val = unescape(document.cookie.substring(begin + prefix.length, end));
        
        return (val == 'true');
    }
    function FOFModalShow() {
        
        isVerified                  = false;
        $submitBtn                  = $('#btnSubmit');
        $flemingsForm               = $('#friends-of-flemings-form');
        $flemingsDescription        = $('.friend-of-flemings .cda-desc-wrap p');
        $flemingsTitle              = $('.friend-of-flemings .cda-desc-wrap h1');
        ServiceErrorText            = "We're sorry, but we are unable to register you at this time. Please try again later.";
        
        $submitBtn.bind('click', function (event) {
            event.preventDefault();
            Register();
        });

        jQuery.magnificPopup.open({
            callbacks: {
                close: function(){
                    FOFModalSuppress();
                }
            },
            items: {
                src: '#fof-modal'
            },
            type: 'inline'
        });
    }
    function FOFModalSuppress() {
        document.cookie = "[FOFModalSuppressed]=true; path=/";
    }
    function IsFOF() {
        if (GetUrlParameter('utm_source') == 'flemings' && GetUrlParameter('utm_medium') == 'email') {
            FOFModalSuppress();
            return true;
        }
        return false;
    }
    function ProcessServiceResponse(data) {
        var result = JSON.parse(data.ServiceResult);
        var errors = [];
        $(".contact-loader-container").addClass("disabled");
        if (result.IsSuccess == false) {
            //Model validation error
            if (result.BrokenRules.length != 0) {
                for (var i = 0; i < result.BrokenRules.length; i++) {    //build error array
                    errors.push({ ErrorMessage: result.BrokenRules[i].ErrorMessage, JQueryId: "" })
                }
                PrintValidationErrrors(errors); //display errors on the user interface
            } else {
                errors.push({ ErrorMessage: ServiceErrorText, JQueryId: "" });
                PrintValidationErrrors(errors);
            }
        } else {
            FOFModalSuppress();
            $('#fof-modal-greeting').html('<div style="text-align: center;"><img style="width: 157px; height: 40px; border: none; margin: 0 auto 30px auto;" src="/images/flemings-nav-brand.png" alt=""></div><h1>Thanks <em class="indent-alt">for Joining</em></h1><p>We appreciate you becoming a Friend of Fleming&#39;s. As our friend, you will be informed of all our limited-time seasonal menus, new wine offerings, exclusive tasting events and more.</p>');
            $('#friends-of-flemings-form').remove();
        }
    }
    jQuery(window).load(function(){
        if (!FOFModalIsSuppressed()) {
            setTimeout('FOFModalShow()', 1000);
        }
    });
</script>

