              function changeCont(m){
                   if(m == "web"){
                          var values = $("#search1").val();
                   } else {
                          var values = $("#search").val();
                   }
                   var seatchUrl = "http://forum.supersu.com/search?term="+  values +"&in=titlesposts";
                    $(".su_fromA").attr({ action: seatchUrl });
              }
              // click event
               var oWidth = $(window).width();
              function sendViaId(name){
                  if(oWidth > 768){
                    ga('send', 'event', name+'_pc', 'click', name+'_pc');
                    ga('ccmtana.send', 'event', name+'_pc', 'click', name+'_pc');
                  } else {
                    ga('send', 'event', name+'_web', 'click', name+'_web');
                    ga('ccmtana.send', 'event', name+'_web', 'click', name+'_web');
                  }
              }
              //search event
              var searchCont = "";
               if(oWidth > 768){
                  var form = document.getElementById('su_from1'); //pc
                  searchCont = "search_pc";
              } else {
                   var form = document.getElementById('su_from2'); //web
                   searchCont = "search_web";
              }
              form.addEventListener('submit', function(event) {
                event.preventDefault();
                ga('send', 'event', searchCont, 'submit', {
                  hitCallback: function() {
                    form.submit();
                  }
                });
                ga('ccmtana.send', 'event', searchCont, 'submit', {
                  hitCallback: function() {
                    form.submit();
