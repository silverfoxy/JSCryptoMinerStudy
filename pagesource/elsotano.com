                $("[name='comentarios'").focusout(function() {limpiarPlaceHolder($(this),'Comentarios');}).blur(function() {limpiarPlaceHolder($(this),'Comentarios');});


                $('.btnSubmitGray').click(function(event){
                    event.preventDefault();
                    submitForm = true;
                    url = $('#'+FormularioId).attr('action');
                    data = $("#"+FormularioId).serializeArray();
                    //validaci√≥n nivel 1
                    for($i=0;$i<data.length;$i++){
                        /*No vacios o textos Default*/
                        if(data[$i].value=='' || data[$i].value=='Default' || data[$i].value=='Comentarios'){
                            if(data[$i].name=='comentarios'){
                                $("[name='"+data[$i].name+"'").val('');
                            }
                            $("[name='"+data[$i].name+"'").focus();
                            submitForm =false;
                            break;
                        }
                        /*Validaci√≥n de longitudes m√≠nimas*/
                        minLong = parseInt($("[name='"+data[$i].name+"'").data('minlong'));
                        if($("[name='"+data[$i].name+"'").val().length < minLong){
                            $("[name='"+data[$i].name+"'").focus();
                            elemento=$("[name='"+data[$i].name+"'");
                            tipo=elemento.data('tipoinput');
                            validarCampo(elemento,tipo);
                            submitForm =false;
                            break;
                        }


                    }
                    if(submitForm){
                        $.ajax({
                            //beforeSend:,
                            type: "POST",  
                            url: url,
                            //dataType: "json",
                            data: data, 
                            success: function(data){
                                console.log(data);
                                if(data > 0){
                                    $('div.contactoForm').empty().html('<p class="txtContacto">Gracias, su mensaje <strong>ha sido enviado!</strong></p>');
                                }
                                
                            },
                            error: function(XMLHttpRequest, textStatus, errorThrown) { 
                                alert("Status: " + textStatus); alert("Error: " + errorThrown); 
                            }       
                        });//Fin ajax
                    }


                    
                });
                //Usabilidad Formularios JS



            });

               
                /*Funcionalidad y Usabilidad Formularios*/
