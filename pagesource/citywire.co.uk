<script type="text/javascript">
$(document).ready(function() {

  // for each article block
  $('.article').each(function() {
    var article = $(this);
    var index = 0;

    if ($(article).data('position')) {
      var position = $(article).data("position");
      index = position-1
    }

    // get the list code to use from data attr
    if ($(article).data('list')) {
    var list = $(article).data("list"); 
   
    // make api call to get content list stories
    var url = "//citywire.co.uk/data/api/contentlist/bycode/" + list + "-list.aspx";
    $.get(url, function( data ) {
     var item = data.listItems[index];
     
     // append headline to h4
     $(article).find("h4").text(item.headline);
     
     // if there is a base-url data attr present then append to front of url
     var url = item.url;
     if ($(article).data('base-url')) {
       url = $(article).data("base-url") + item.url;
     }
     
     // set the href of the link
     $(article).find(".article-link").attr("href", url);
    });
  }
 });
});
</script>

