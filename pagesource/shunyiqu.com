    this.code = code ? code : jQuery("#"+this.codeid).val();
    this.mobile = mobile ? mobile :  jQuery("#"+this.mobileid).val();
    this.verify = verify ? verify :  jQuery("#"+this.verifyid).val();

    this.count = count ? count : 60;
