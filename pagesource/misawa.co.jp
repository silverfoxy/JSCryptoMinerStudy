<!-- webフォント読み込み対策 -->
<script>
jQuery.fn.exists = function(){return Boolean(this.length > 0);}
$(function() {
	oncomplete = function(res) {

			if(location.hash){
				var hashId = location.hash;
				var scTop = $(hashId).offset().top;
				//PCのみgNaviの高さを考慮　170119
				if(window.parent.screen.width>=481){
					scTop -= $('#kodate_navi').height();
				}
				//console.log(scTop);
				$('html,body').animate({scrollTop:scTop});
				//$('html,body').css( 'scrollTop', $(hashId).offset().top );
			}

			else if (res.code == 0) {
				 $('html,body').animate({ scrollTop: 0 }, 0);
			}
			else {
				 $('html,body').animate({ scrollTop: 0 }, 0);
			}

		}
	FONTPLUS.attachCompleteEvent(oncomplete);
});
</script>


