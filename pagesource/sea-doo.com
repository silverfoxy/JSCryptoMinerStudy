      <ul id="continent-navigation" class="nav-tabs list">
        {% _.each(continents, function(i, index) { %} <!-- start loop in continents -->
          <li class="{{ (index == data.currentcontinent) ? 'active' : '' }}"><a class="continent element" href="{{'#' + data.cs}}-continent-{{index}}" data-toggle="tab" ><span>{{ i.name }}</span></a></li> <!-- continent name -->
        {% }); %} <!-- end loop in continents -->
      </ul>
    </div>

    <div class="span9 countries-list tab-content">
      {% _.each(continents, function(i, continentIndex) { %} <!-- start loop in continents -->
        <div id="{{data.cs}}-continent-{{continentIndex}}" class="tab-pane {{ (continentIndex == data.currentcontinent) ? 'active' : '' }}">
          <div class="span11">
            <div class="row-fluid countryList">
              <div class="span5 offset1">
          {% var countries = i.countries; %}
          {% var continentName = i.name; %}

          {%_.each(countries, function(i, countryIndex){ %}  <!-- start loop in countries -->
            {% var countryName = i.name; %}
			{% var isInternational = ((data.brand == 'ski-doo' || data.brand == 'lynx') && _.where(i.languages, {code: 'en_GB'}).length > 0) || (data.brand == 'corpo'); %}
            {% if (countryIndex == (countries.length/2) || countryIndex == (countries.length/2+0.5)) { %}
            </div><div class="span5 offset1"> <!-- generate two collumns from countries -->
            {% } %}
            {% var languages = i.languages,
                   countryCode = (languages[0].code).split("_").pop().toLowerCase();
            %}

            {% if (languages.length > 1) { %}
            <a href="#languageCollapse-{{data.cs}}-{{countryCode}}" data-toggle="collapse" class="accordion-toggle localeRedirect {{ (data.currentcountry == countryIndex && data.currentcontinent == continentIndex) ? 'active' : '' }}">{% if(!isInternational) { %}<span class="icon country country-{{countryCode.toLowerCase()}}"></span>{% } %}<span class="title">{{i.name}}</span></a> <!-- country name -->
              <div class="collapse {{(data.currentcountry == countryIndex) ? 'in' : ''}} languages" id="languageCollapse-{{data.cs}}-{{countryCode}}">
                {% _.each(languages, function(i) { %} <!-- start loop in languages -->
                  <a href="{{i.url}}" data-country="{{countryCode.toUpperCase()}}-{{countryName}}" data-continent="{{continentName}}" class="linkToLocale-{{i.code}} {{(data.currentlocale == i.code) ? 'activeLanguage' : ''}}" {{(i.openInNewTab) ? 'target="_blank"' : ''}}> {{i.name}} </a> <!-- language name -->
                {% }) %} <!-- end loop in languages -->
              </div>

            {% }else{ %}
            <a href="{{languages[0].url}}" data-country="{{countryCode.toUpperCase()}}-{{i.name}}" data-continent="{{continentName}}" class="linkToLocale-{{languages[0].code}} localeRedirect {{ (data.currentcountry == countryIndex && data.currentcontinent == continentIndex) ? 'active' : '' }}" {{(languages[0].openInNewTab) ? 'target="_blank"' : ''}}>{% if(!isInternational) { %}<span class="icon country country-{{countryCode.toLowerCase()}}"></span>{% } %}<span class="title">{{i.name}}</span></a> <!-- country name -->

            {% } %} <!-- end if multiple languages -->

          {% }) %} <!-- end loop in countries -->

              </div>
            </div>
          </div>
        </div>
      {% }); %} <!-- end loop in continents -->
    </div>
  </div>
</div>
</script>


