<!-- Build and Price Featured Builds modal -->

<script type="text/template" id="bap-featured-builds-modal-accessories-template">
  <ul class="list hide-object">
    {% var accessoriesPrice = 0.0; %}
    {% _.each( accessoriesCollection.models, function( CsticView ){ %}
        {% var accessory = new Array(); %}
        {% var accessoryStyle = ""; %}
        {% var accessoryCategoryPrice = 0.0; %}
        {% _.each( CsticView.Characteristics.models, function( Cstic ) { %}
            {% for (index in Cstic.CharacteristicValues) { %}
                {% if (Cstic.CharacteristicValues[index].abbreviation != "NONE" && Cstic.CharacteristicValues[index].assigned == true) { %}
                    {%
                      Cstic.CharacteristicValues[index].name = Cstic.attributes.name; //Set name in each accessory so we can access later
                    %}
                    {% accessory.push(Cstic.CharacteristicValues[index]); %}
                    {% accessoryStyle = "selected"; %}
                    {% if ($.inArray(Cstic.CharacteristicValues[index].abbreviation, product.includedAccessories) == -1) { %}
                      {% accessoriesPrice = accessoriesPrice + Cstic.CharacteristicValues[index].price.value; %}
                      {% accessoryCategoryPrice = accessoryCategoryPrice + Cstic.CharacteristicValues[index].price.value; %}
                    {% } %}
                {% } %}
            {% } %}
        {% }); %}
        {% if (accessoryStyle == 'selected') { %}
        <li class="{{ accessoryStyle }}" data-identifier="{{ CsticView.attributes.identifier }}">
          <div class="fb-preview-accesory">

            {%
              var accessoryHeading = CsticView.attributes.langName;
              if (!_.isUndefined(cqData.accessories[CsticView.attributes.id])) {
                accessoryHeading = cqData.accessories[CsticView.attributes.id].label;
              }
            %}

            <div class="accessory-name">{{ accessoryHeading }}</div>
          </div>
          <ul class="accessory-items">
            {% for (index in accessory) { %}
                {% if (!_.isFunction(accessory[index])) { %}
                <li>
                  {%
                    var accessoryName = accessory[index].abbreviation,
                        accessoryPrice = 0.0;

                    if ((!_.isUndefined(cqData.accessories[CsticView.attributes.id])) &&
                        (!_.isUndefined(cqData.accessories[CsticView.attributes.id].items[accessory[index].name])) &&
                        (!_.isUndefined(cqData.accessories[CsticView.attributes.id].items[accessory[index].name].options[accessory[index].abbreviation]))) {
                      accessoryName = cqData.accessories[CsticView.attributes.id].items[accessory[index].name].options[accessory[index].abbreviation].label;
                    }

                    //Check if accessory is included and set price to zero if it is
                    if ($.inArray(accessory[index].abbreviation, product.includedAccessories) == -1) {
                      accessoryPrice = accessory[index].price.value;
                    } else {
                      accessoryPrice = 0.0;
                    }

                  %}

                  <div class="accessory-name">{{ accessoryName }}</div>
                  {% if ((!_.isUndefined(accessory[index].price)) && (!_.isEmpty(accessory[index].price)) && (!_.isUndefined(accessory[index].price.value))) { %}
                  <div class="accessory-price">
                    
                      
                      
                        <sup class="currency-symbol">$</sup>
                        <span class="price">
                          {{ formatNumber(accessoryPrice, 'en_US') }}
                        </span>
                      
                    
                  </div>
                  {% } %}
                </li>
                {% } %}
            {% } %}
          </ul>
        </li>
        {% } %}
    {% }); %}
  </ul>

</script>

