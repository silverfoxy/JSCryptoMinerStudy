    <script language="javascript" type="text/javascript">
        $(function() {
 
            var tallest = 0;
            $("#hero").find(".heroSlide").each(function(i, slide) {
                var h = $(slide).height();
                tallest = (h > tallest) ? h : tallest;
            });


            // hero carousel
            $('#hero').slides({
                container: "heroSlides",
                preload: true,
                preloadImage: "/_assets/images/common/ajaxbar.gif",
                generateNextPrev: true,
                slideSpeed: 350,
                play: 8000
            });

            $("#hero").find(".slides_control").css("height", tallest + "px");

            // customer choice
            $('#cc').slides({
                container: "ccInfoSlides",
                preload: true,
                preloadImage: "/_assets/images/common/ajaxbar.gif",
                effect: 'fade'
            });

            $('#ccPage').append($('#cc .pagination'));

            $('#ccShowExplanation').colorbox({
                inline: true,
                href: "#ccExplanation",
                width: 450,
                height: 250,
                opacity: 0.5,
                transition: "none",
                close: "<span>Close</span>",
                onOpen: function() {
                    $('#ccExplanation').show();
                },
                onCleanup: function() {
                    $('#ccExplanation').hide();
                }
            });

            // our destinations
            $('#odInfo').slides({
                container: "odSlides",
                preload: true,
                preloadImage: "/_assets/images/common/ajaxbar.gif",
                generateNextPrev: true,
                generatePagination: false
            });

        });
    </script> 

    
