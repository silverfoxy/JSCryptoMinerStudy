        $('.content-page img').each(function() {
            var src = $(this).attr('src');
            if (typeof src == 'undefined')
                return;
            if ((src.indexOf('http') == 0))
                return;
            if ((src.indexOf('//') == 0))
                return;
            //src = src.replace('/userfiles/','/images/');
            if (src.indexOf('/protocols/') >= 0) {
                src = 'https://www.lef.org/' + src;
            }


            if ((src.indexOf('http') < 0) && (src.indexOf('/images/') < 0)) {
                src = '/images/' + src.split('/').pop();
            }

            $(this).attr('src', src);
        });

        $('.content-page a').each(function() {
            var href = $(this).attr('href');

            if (typeof href == 'undefined')
                return;

            if (href == '')
                return;
            if (href.toLowerCase().indexOf('javascript') >= 0)
                return;
            if ((href.toLowerCase().indexOf('http') >= 0) && ((href.toLowerCase().indexOf('drvitaminsolutions') < 0)))
                return;
            if ((href.toLowerCase().indexOf('https') >= 0))
                return;

            if (href.indexOf('mailto') >= 0) {
                href = href.toLowerCase();
                href = href.replace(/ /g, '_');
                href = href.replace(/%20/g, '_');
                $(this).attr('href', href);
                return;
            }

            if (href.indexOf('#') == 0) {
                return;
            }

            if (href.indexOf('page_view') >= 0) {
                href = href.toLowerCase();
                href = href.replace(/ /g, '_');
                href = href.replace(/%20/g, '_');
            }
            href = href.replace('..', '.');
            href = href.replace('http://www.drvitaminsolutions.com/', '/');
            href = href.replace('http://staging.drvitaminsolutions.com', '/');
            href = href.replace('//', '/');
            href = href.replace('http:/', 'http://');
            href = href.replace('https:/', 'https://');
            href = href.replace('/https:/', 'https:/');

            //
            // this is to try to fix le_protocols links to direct back to dr store
            if (href.indexOf('/newshop/cgi-shop/searchItems.cgi?keyword=') >= 0) {
                href = '/advanced_search/?match_mode=all&name=name&search_text=' + $(this).text().replace('/', ' ');
                $(this).attr('href', href);
                return;
                //href = href.replace('/newshop/cgi-shop/searchItems.cgi?keyword=','/search/');
            }
            if (href.indexOf('Search.aspx?keyword=') >= 0) {
                href = '/advanced_search/?match_mode=all&name=name&meta_k=meta_k&search_text=' + $(this).text().replace('/', ' ');
                $(this).attr('href', href);
                return;
                //href = href.replace('/newshop/cgi-shop/searchItems.cgi?keyword=','/search/');
            }

            if ((href.charAt(0) != '/'))
                href = '/' + href;

            if (href.indexOf('safety_appendix') >= 0) {
                $(this).parent().remove();

            }

            if (href.indexOf('/protocols/images/') >= 0) {
                href = 'https://www.lef.org' + href;
            }

            if (href.indexOf('/protocols/') >= 0) {
                href = '/content/' + href.split('/').pop();
                href = href.replace('.html', '');
                href = href.replace('.htm', '');
                href = href.replace('.shtml', '');
            }

            if (href.indexOf('#') >= 0) {
                //  href = href+'#'+href.split('#').pop();
            }



            // non-image redirects to lef.org should pop up in new window that is smaller tahn current window and 
            // has no toolbar, to encourage the user to return to DR site after reading link.
            if (href.indexOf('/magazine/') >= 0) {
                href = 'https://www.lef.org' + href;
                $(this).click(function() {
                    window.open(this.href, "", "toolbar=0,width=" + (window.outerWidth * 0.80) + ",height=" + (window.outerHeight * 0.80));
                    return false;
                });
            }
            //          

            $(this).attr('href', href);
        });
    });
</script>

