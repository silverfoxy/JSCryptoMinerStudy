<script>
	var miniBasketEnabled 	= 0;
	var checkoutInProgress 	= 0;
	
	function miniBasketViewBasket()
	{
		if(miniBasketEnabled)
			ibeViewCart();
	}
	
	function miniBasketCheckout()
	{
		if(miniBasketEnabled)
			ibeCheckout();
	}
	
	function setMiniBasketCheckoutInProgress()
	{
		checkoutInProgress = 1;
		$('#miniBasket').fadeTo(500,0);
	}

	function setMiniBasketCheckoutComplete()
	{
		checkoutInProgress = 0;
		$('#miniBasket').fadeTo(500,1);
	}
	
	function setMiniBasket(items,cost)
	{
		$('#miniBasket').fadeTo(500,1);
		miniBasketEnabled  = items != 0;
		
		if (miniBasketEnabled) 
		{
			$('#miniBasket').removeClass('miniBasketDisabled');	
		}
		else 
		{
			$('#miniBasket').addClass('miniBasketDisabled');
		}
		
		$('#mb_journeys').html(items);
		$('#mb_cost').html(CurrencyFormatted(cost));
	}
	
	
	function CurrencyFormatted(amount)
{
	var i = parseFloat(amount/100);
	if(isNaN(i)) { i = 0.00; }
	var minus = '';
	if(i < 0) { minus = '-'; }
	i = Math.abs(i);
	i = parseInt((i + .005) * 100);
	i = i / 100;
	s = new String(i);
	if(s.indexOf('.') < 0) { s += '.00'; }
	if(s.indexOf('.') == (s.length - 2)) { s += '0'; }
	s = minus + s;
	return s;
}

</script>

