<script type="text/javascript">
gusto.build_streaming_params = function() {
    function param_error(message) {
        if (console) {
            console.log('Streaming param error:', message);
        }
    }

    // internal function to process bits of the template and stiches the final value
    // eg stitch('|', 'a', 'b:', [1,2,3] ,';c:', 123) => "ab:|1|2|3|;c:123"
    function stitch() {
        var separator = Array.prototype.shift.apply(arguments) + "";
        var final_value = '';
        for (var i=0; i<arguments.length; i++) {
            var string_type = Object.prototype.toString.call(arguments[i]);
            switch (string_type) {
                case '[object Array]':
                    if (separator.length > 0) {
                        final_value = final_value + separator + arguments[i].join(separator) + separator;
                        break;
                    }
                case '[object Object]':
                case '[object Undefined]':
                    param_error(arguments[i]);
                    break;
                default:
                    final_value = final_value + arguments[i];
            }
        }
        return final_value;
    }

    var params = [];
    
        try {
            params.push('amsparams=' + stitch('', "playerid:UKRP;skey:1521311835;"));
        } catch(e) {
            param_error('amsparams');
        }
        
    
        try {
            params.push('awparams=' + stitch('|', "kxsegs:" , Krux.segments , ";"));
        } catch(e) {
            param_error('awparams');
        }
        
    
        try {
            params.push('kuid=' + stitch('', Krux('get', 'user')));
        } catch(e) {
            param_error('kuid');
        }
        
    
    return encodeURIComponent(params.join('&'));
};
</script>


