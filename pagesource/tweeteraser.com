                policyURL: '/privacy',
                autoEnable: true,
                acceptOnContinue: false,
                acceptOnScroll: false,
                acceptAnyClick: false,
                expireDays: 365,
                renewOnVisit: false,
                forceShow: false,
                effect: 'slide',
                element: 'body',
                append: true,
                bottom: true,
                zindex: '999',
                domain: 'www.example.com',
                referrer: 'www.example.com'
            });
        }

        var title = document.title;
        var pageTitle = title.replace("TweetEraser", "#TweetEraser");
        var description = document.head.querySelector("[name=description]").content;
        var pageDescription = description.replace("TweetEraser", "#TweetEraser");

        var image_url = document.head.querySelector('[property="og:image"]').content;

        var pageUrl = location.href; //Location of this page

        $('.share-btn-custom li').click(function (event) {
            var shareName = $(this).attr('class').split(' ')[0]; //get the first class name of clicked element
            switch (shareName) //switch to different links based on different social name
            {
                // https://css-tricks.com/essential-meta-tags-social-media/
                case 'facebook-custom':
                    url = 'http://www.facebook.com/sharer.php?s=100';
                    url += '&p[title]=' + encodeURIComponent(pageTitle);
                    url += '&p[summary]=' + encodeURIComponent(pageDescription);
                    url += '&p[url]=' + encodeURIComponent(pageUrl);
                    url += '&p[images][0]=' + encodeURIComponent(image_url);

                    OpenShareUrl('https://www.facebook.com/sharer/sharer.php?' + url);
                    break;
                case 'twitter-custom':
                    var message = pageTitle + ' ' + pageUrl;
                    OpenShareUrl('https://twitter.com/home?status=' + encodeURIComponent(message));
                    break;
                case 'linkedin-custom':
                    OpenShareUrl('https://www.linkedin.com/shareArticle?mini=true&url=' + encodeURIComponent(pageUrl) + '&title=' + encodeURIComponent(pageTitle) + '&summary=' + encodeURIComponent(pageDescription) + '&source=');
                    break;
                case 'google-custom':
                    OpenShareUrl('https://plus.google.com/share?url=' + encodeURIComponent(pageUrl));
                    break;
            }

        });

        function OpenShareUrl(openLink) {
            //Parameters for the Popup window
            var wWidth = 650;
            var wHeight = 450;
            var x = ($(window).width() / 2) - (wWidth / 2);
            var y = ($(window).height() / 2) - (wHeight / 2);
            window.open(openLink, 'Share TweetEraser', 'height=' + wHeight + ',width=' + wWidth + ',left=' + x + ',top=' + y);

            return false;
        }

        function getRandom(min, max) {
            return Math.random() * (max - min) + min;
        }

    });


</script>


