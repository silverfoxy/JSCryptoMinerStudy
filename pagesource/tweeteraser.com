        $(document).ready(function () {

            $('.buy').on('click', function (event) {
//                if(readCookie('cb-enabled') == 'accepted') {
                createCookie('login_attempt_location', 'plans', 0);
//                }
            });

            var setPrices = function () {
                // get selected currency
                var currency = $('#currency-selector').val();

                // set cookie
//                if(readCookie('cb-enabled') == 'accepted') {
                createCookie('selected_currency', currency, 0);
//                }

                // set for each price
                $('.price').each(function () {

                    var element = $(this);

                    // price data
                    var price_data = element.data();

                    // get currency rates
                    $.ajax({
                        url: '/product/get-currency-exchange-data',
                        type: 'POST',
                        data: {
                            selected_currency_code: currency
                        },
                        dataType: 'json',
                        success: function (data) {

//                            console.log(data);
                            if (typeof data.currency_rate != 'undefined') {

                                var new_price = parseFloat(data.currency_rate.Rate) * parseFloat(price_data.productPrice);

                                switch (currency) {
                                    case 'JPY':
                                        new_price = new_price.toFixed(0);
                                        break;
                                    default:
                                        new_price = new_price.toFixed(2);
                                }

                                element.html(currency + ' ' + new_price + ' <sup><i class="fa fa-asterisk fa-very-small red"></i></sup>');
                            }

                        },
                        error: function (data) {
//                            console.log('error', data);
                        }
                    });
                });
            };

            $('#currency-selector').on('change', function () {
                setPrices();
            });

            var selected_currency_cookie = readCookie('selected_currency');
            if (selected_currency_cookie != null) {
                var spe = $('#currency-selector');
                spe.val(selected_currency_cookie);
                spe.selectpicker('render');
                setPrices();
            }


        });
    </script>

