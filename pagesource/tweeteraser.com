<script>
    $(document).ready(function () {

        if (typeof $.cookieBar != 'undefined') {
            $.cookieBar({
                fixed: true,
                message: 'We use cookies to track usage and preferences',
                acceptButton: true,
                acceptText: 'I Understand',
                acceptFunction: null,
                declineButton: true,
