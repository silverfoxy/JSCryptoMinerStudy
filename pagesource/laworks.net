// Type the number of div containers to randomly display.
 
NumberOfDivsToRandomDisplay = 1;
 
// No other customizations required.

//NOTE...NOTE...NOTE...NOTE 2011-08-04 laiken
//	Took out the 'random' display and made it sequential forward of backward

////////////////////////////////////
var CookieName = 'DivRamdomValueCookie';
var CurrentDisplayDiv = 0;

function IntervalLoop()
{
	if(CurrentDisplayDiv == 0)
	{
		CurrentDisplayDiv = CurrentDisplayDiv + 1;
	}
	else
	{
		if(CurrentDisplayDiv >= NumberOfDivsToRandomDisplay)
		{
			CurrentDisplayDiv = 1;
		}
		else
		{
			CurrentDisplayDiv = CurrentDisplayDiv + 1;
		}
	}
	var r = CurrentDisplayDiv;
	if(IntervalValue==0)
	{
		r=1;
	}

	if(NumberOfDivsToRandomDisplay > 1) 
	{
		var ck = 0;
		var cookiebegin = document.cookie.indexOf(CookieName + "=");
		if(cookiebegin > -1) 
		{
			cookiebegin += 1 + CookieName.length;
			cookieend = document.cookie.indexOf(";",cookiebegin);
			if(cookieend < cookiebegin) { cookieend = document.cookie.length; }
			ck = parseInt(document.cookie.substring(cookiebegin,cookieend));
		}
		while(r == ck) 
		{ 
			r = Math.ceil(Math.random() * NumberOfDivsToRandomDisplay); 
		}
		//   document.cookie = CookieName + "=" + r; //Not used by LWC
	}
	for( var i=1; i<=NumberOfDivsToRandomDisplay; i++) 
	{
		document.getElementById("randomdiv"+i).style.display="none";
	}
	document.getElementById("randomdiv"+r).style.display="block";
	clearInterval ( IntervalValue );
	return r;
}

function IntervalLoopBack()
{
	if(CurrentDisplayDiv > 1)
	{
		CurrentDisplayDiv = CurrentDisplayDiv - 1;
	}
	else
	{
		CurrentDisplayDiv = NumberOfDivsToRandomDisplay; 
	}
	var r = CurrentDisplayDiv;
	if(IntervalValue==0)
	{
		r=1;
	}

	if(NumberOfDivsToRandomDisplay > 1) 
	{
		var ck = 0;
		var cookiebegin = document.cookie.indexOf(CookieName + "=");
		if(cookiebegin > -1) 
		{
			cookiebegin += 1 + CookieName.length;
			cookieend = document.cookie.indexOf(";",cookiebegin);
			if(cookieend < cookiebegin) { cookieend = document.cookie.length; }
			ck = parseInt(document.cookie.substring(cookiebegin,cookieend));
		}
		while(r == ck) 
		{ 
			r = Math.ceil(Math.random() * NumberOfDivsToRandomDisplay); 
		}
		//   document.cookie = CookieName + "=" + r; //Not used by LWC
	}
	for( var i=1; i<=NumberOfDivsToRandomDisplay; i++) 
	{
		document.getElementById("randomdiv"+i).style.display="none";
	}
	document.getElementById("randomdiv"+r).style.display="block";
	clearInterval ( IntervalValue );
	return r;
}

var IntervalValue = 0;

function IntervalCheck()
{
	if(document.getElementById("Left_Arrow").value=="0")
	{
		ee = IntervalLoop();
		IntervalValue = setInterval("IntervalCheck()" ,whatIsTime());
	}
}
function IntervalCheckBack()
{
	if(document.getElementById("Left_Arrow").value=="0")
	{
		ee = IntervalLoopBack();
		IntervalValue = setInterval("IntervalCheck()" ,whatIsTime());
	}
}
IntervalCheck();

function whatIsTime()
{
	theTimeIs = 500000
if(ee == 1) {theTimeIs = 10000;}

	return theTimeIs;
}



</script>