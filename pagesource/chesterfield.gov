function OpenContextHelp()
{ 
  var ContextHelpContentID ="0";
  var disp_setting = "toolbar=no,location=no,directories=no,menubar=yes,";
  var sitePath = '/';
  var fullURL = parent.location.href;
  var aPosition = fullURL.indexOf("efault.aspx");
  disp_setting += "scrollbars=yes,width=650, height=600, left=100, top=25";

  if (ContextHelpContentID == null | ContextHelpContentID == 0) {
      if (aPosition > 0) 
      {
          window.open(sitePath + "help.aspx?id=16169", "helpWindow", disp_setting);
      }
      else 
      { 
        parent.location.href(sitePath + "assistance.aspx");
      }
  }
  else { 
    window.open(sitePath + "help.aspx?id=" + ContextHelpContentID, "helpWindow", disp_setting);
  }      
} 

function fontSizer(size) {
    getBody = document.getElementById ("ctl00_PageBodyTag");
    getBody.style.fontSize = size;
    createCookie('textsize',size,7); 

}

function createCookie(name,value,days) {
	if (days) {
		var date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		var expires = "; expires="+date.toGMTString();
	}
	else var expires = "";
	document.cookie = name+"="+value+expires+"; path=/";
}

function TextBoxChanged(txtMessage)
{                
