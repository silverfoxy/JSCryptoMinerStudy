    <!-- ==== ADVANCED SCREEN ================================================================================== -->
    <section id="AdvancedDiv" style="display: none">

        <!-- actual AdvancedScreen content (the other is for filter structure-screen) -->
        <div id="AdvancedScreen_NormalContainer">

            <div class="accordion AccordionBtnStep1 AccordionOpen" onclick="gFilterBlade.advancedScreen.toggleStructureScreen()">
                <div class="ImageDiv AccordionIcon" style="background: url(https://pathofexile.gamepedia.com/media/pathofexile.gamepedia.com/b/bd/Hinekora%27s_Sight_inventory_icon.png?version=7dcf242877ca452fd38920f5c9d79fb8) no-repeat top center; background-size: contain;"></div>
                <label class="AccordionLabel">Filter Structure</label>
            </div>

            <!-- ==== ASSORTED FUNCTIONS (header) -->
            <div class="accordion AccordionBtnStep1" id="AccordionButtonB">
                <div class="ImageDiv AccordionIcon"
                     style="background: url(https://hydra-media.cursecdn.com/pathofexile.gamepedia.com/7/70/Malachai%27s_Artifice_inventory_icon.png?version=f4ec16f3aa1094429c9b485e1848f06a) no-repeat top center; background-size: contain;"></div>
                <label class="AccordionLabel">Quick Functions</label>
            </div>
            <!-- content -->
            <div class="ShowAccordion AccordionContent AccordionStep">
                <label class="VisualUtilityDescription">These functions provide a quick way to perform certain popular custom edits.</label>

                <div class="A_AF_Container">
                    <button id="removeFontSizesButton">Execute</button>
                    <label class="A_AF_Label"><b>Remove ALL Fontsizes.</b> Sets all sizes to their default value of
                        32.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="removeBackGroundColorsButton">Execute</button>
                    <label class="A_AF_Label"><b>Remove ALL Backgrounds.</b> Sets all backgrounds to their default value.
                    </label>
                </div>
                <div class="A_AF_Container">
                    <button id="removeBorderColorsButton">Execute</button>
                    <label class="A_AF_Label"><b>Remove ALL Bordercolors.</b> Sets all border colors to their default value.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="removeTextColorsButton">Execute</button>
                    <label class="A_AF_Label"><b>Remove ALL Textcolors.</b> Sets all textcolors to their default
                        value.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="removeSoundsButton">Execute</button>
                    <label class="A_AF_Label"><b>Remove ALL Alertsounds.</b> Nullifies all dropsounds set in the filter.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="minimizeHiddenButton">Execute</button>
                    <label class="A_AF_Label"><b>Minimize hidden items.</b> Sets the font size of all hidden items to the lowest possible size.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="maximizeShownButton">Execute</button>
                    <label class="A_AF_Label"><b>Maximize shown items.</b> Sets the font size of all shown items to the biggest possible size.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="showHiddenButton">Execute</button>
                    <label class="A_AF_Label"><b>Show all hidden items.</b> All items that are hidden will now be shown and their font size will be reduced.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="increaseFontSizes">Execute</button>
                    <input type="number" min="1" max="27" value="5" id="increaseFontSizesInput">
                    <label class="A_AF_Label"><b>Increase font sizes.</b> Increases all font sizes by a given value.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="decreaseFontSizes">Execute</button>
                    <input type="number" min="1" max="27" value="5" id="decreaseFontSizesInput">
                    <label class="A_AF_Label"><b>Decrease font sizes.</b> Decreases all font sizes by a given value.</label>
                </div>
                <div class="A_AF_Container">
                    <button id="scaleFontSizesButton">Execute</button>
                    <input type="number" min="18" max="45" value="18" id="scaleFontSizesMinInput">
                    <input type="number" min="18" max="45" value="45" id="scaleFontSizesMaxInput">
                    <label class="A_AF_Label"><b>Scale font sizes.</b> Reduced the range of the current font sizes. Changing it from 18-45 to e.g. 30-45, making the items overall bigger while keeping their ratios.</label>
                </div>
            </div>

            <!-- ==== GLOBAL COMMANDS (header) ==== -->
            <div class="accordion AccordionBtnStep1" id="AccordionButtonA">
                <div class="ImageDiv AccordionIcon" style="background: url(https://hydra-media.cursecdn.com/pathofexile.gamepedia.com/1/1d/The_Anvil_inventory_icon.png?version=e37bbc85e264f903f275fca39adfd8f1) no-repeat top center; background-size: contain;"></div>
                <label class="AccordionLabel">Command Builder</label>
            </div>
            <!-- content -->
            <div class="ShowAccordion AccordionContent AccordionStep">
                <label class="VisualUtilityDescription">Create custom commands!<br>
                    Add "Selectors" to select the parts of the filter that you want to edit.<br>
                    Add "Actors" to specify the changes you want to do.</label>
                <div id="Adv_SelectorActorContainer">
                </div>
            </div>

            <!-- ==== UPLOAD FILTER + OPTION (header) ==== -->
            <div class="accordion AccordionBtnStep1" id="AccordionButtonC">
                <div class="ImageDiv AccordionIcon" style="background: url(https://hydra-media.cursecdn.com/pathofexile.gamepedia.com/3/33/Regal_Orb_inventory_icon.png?version=41c893cd21ee40e555eedb271ae9af2b) no-repeat top center; background-size: contain;"></div>
                <label class="AccordionLabel">Upload Filter</label>
            </div>
            <!-- content -->
            <div class="ShowAccordion AccordionContent AccordionStep">
                <div class="CustomizeGroupBox">
                    <label class="VisualUtilityTitle">Step 1: Upload Filter File</label>
                    <label class="VisualWarningLabel">WARNING: Uploading ANY filter will:</label><br>
                    <ul class="VisualWarningLabel" style="margin: 4px 0">
                        <li>cause the Customizer to misbehave and cause unpredictable bugs</li>
                        <li>disable the Save & Load system</li>
                    </ul>
                    <label class="VisualWarningLabel">when uploaded without its fitting 'option File'.</label><br>
                    <label class="VisualWarningLabel" style="margin-top: 7px; display: inline-block;">Even if it's just an edited version of NeverSink.</label><br>
                    <label class="VisualWarningLabel" style="margin-bottom: 7px; display: inline-block;">Please use the <i onclick="gFilterBlade.screenSelector.selectDiv(5)" style="text-decoration: underline; cursor: pointer">Save & Load</i> system instead.</label><br>

                    <button onclick="document.getElementById('A_FU_uploadFilterButton').style.display = 'inline-block'; this.style.display = 'none'">I understand</button>
                    <button id="A_FU_uploadFilterButton" style="display: none">Upload Filter-File</button>

                    <br><br><br>

                    <!-- Input for custom filters -->
                    <input id="filterFileInput" type="file" accept=".filter" onclick="this.value = null;" onchange="gFilterBlade.fileSelectionHandler.uploadCustomFile(event, 'filter')" style="display: none">
                    <label class="VisualUtilityTitle">Step 2: Upload Customizer Option File</label>
                    <label class="VisualUtilityText">The Customizer needs a compatible 'Option File' for the selected Filter. An edited NeverSink filter will NOT work with the default NeverSink Option File!</label>
                    <button id="A_FC_uploadOptionFileButton">Upload Option-File</button>
                    <button id="downloadCustomizerFile">Download current</button>
                    <input id="optionFileInput" type="file" accept=".options" onclick="this.value = null;" onchange="gFilterBlade.fileSelectionHandler.uploadCustomFile(event, 'option')" style="display: none">
                </div>
            </div>

            <!-- ==== FOR CREATORS (header) ==== -->
            <div class="accordion AccordionBtnStep1" id="AccordionButtonD">
                <div class="ImageDiv AccordionIcon" style="background: url(https://hydra-media.cursecdn.com/pathofexile.gamepedia.com/e/e2/Shaper%27s_Orb_inventory_icon.png?version=e11ab54788967b77b380d7fd3f978d82) no-repeat top center; background-size: contain;"></div>
                <label class="AccordionLabel">For Creators</label>
            </div>
            <!-- content -->
            <div class="ShowAccordion AccordionContent AccordionStep">

                <div class="CustomizeGroupBox">
                    <label class="SubTitle">Style Summary (Overview)</label>
                    <label class="VisualUtilityDescription">The Option File determines the items and descriptions in the Style Summary on the Overview Screen.</label>
                    <button id="downLoadSSFileButton">Download Option File</button>
                    <button id="uploadSSFileButton">Upload Option File</button>
                    <input id="uploadSSFileInput" type="file" accept=".options" onclick="this.value = null;" onchange="gFilterBlade.fileSelectionHandler.uploadCustomFile(event, 'styleSummary')" style="display: none">
                </div>

                <div class="CustomizeGroupBox">
                    <label class="SubTitle">Loot Simulator</label>
                    <label class="VisualUtilityDescription">Drop chances in the Loot Simulator are determined by another Option File.</label>
                    <button id="downloadWeightingFileButton">Download Option File</button>
                    <button id="uploadWeightingFileButton">Upload Option-File</button>
                    <input id="weightingFileInput" type="file" accept=".options" onclick="this.value = null;" onchange="gFilterBlade.fileSelectionHandler.uploadCustomFile(event, 'weighting')" style="display: none">
                </div>

