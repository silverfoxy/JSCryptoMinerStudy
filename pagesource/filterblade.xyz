            <input type="checkbox" id="Loot_EnableHiddenItems" value="enableHiddenItems" onclick="gFilterBlade.lootSimulator.reloadLootSimulator();" checked>
            <label for="Loot_EnableHiddenItems" style="width: 188px">Show hidden items</label>
