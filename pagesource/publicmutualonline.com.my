        function btnClear_onclick() {
           frmLogin.txtUserName.value = '';
           frmLogin.txtPassword.value = '';
        }
        function Body_OnLoad() {
           frmLogin.txtUserName.focus();                 
        }
        function toUpper(e, e1){
            //if (e.keyCode<=31 || (e.keyCode >= 35 && e.keyCode <=40) || e.keyCode==46) return;            
            if (e1.keyCode==37 || e1.keyCode==39) return;
			var txtToUpper = document.getElementById(e.name);	
			var strUserKeyIn = txtToUpper.value;		
			if (strUserKeyIn != txtToUpper.value.toUpperCase())
			    txtToUpper.value = txtToUpper.value.toUpperCase();
		}
		function toFilter(e,filter){
		    var txtToFilter = document.getElementById(e.name);
		    if(txtToFilter.value.indexOf(filter) > -1)
			{
			    txtToFilter.value = txtToFilter.value.substring(0,txtToFilter.value.indexOf(filter));
			}
        }
        function showLoginProgress() {
            $('#spanDispUserId, #spanDispPassword, #txtUserName,#txtPassword,#LoginButton,#btnClear').hide();
            $('#divLoginProgress').show();
		}
    </script>
