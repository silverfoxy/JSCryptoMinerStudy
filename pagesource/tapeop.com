    <script type="text/javascript">
        
        // Init form fields behavior.
        var form_fields = jQuery('form input').not('input.button');
        
        form_fields.each(function() {
            var field = jQuery(this);
            if (field.val() != '') {
               field.addClass('valid');
            }
        })
        
        form_fields.blur(function(){
            var field = jQuery(this);
            if (field.val() != '') {
                field.addClass('valid');
            }
        });
        
        // Init mobile nav behavior.
        var mobile_nav = jQuery('#mobileNav');
        var mobile_nav_width = mobile_nav.width();
        var mobile_nav_toggle = jQuery('#navToggle');
        var site_wrapper = jQuery('#siteWrapper');
        var site_body = jQuery('main');
        
        mobile_nav_toggle.click(function() {
            if (mobile_nav_toggle.attr('state') == 'on') {
                mobile_nav.animate({'left': '-' + mobile_nav_width + 'px' }, { duration: 250, queue: false } );
                site_body.animate({'left': '0'}, { duration: 250, queue: false } );
                
                mobile_nav_toggle.attr('state', 'off');        
            }
            else { // Search is currently off.
                mobile_nav.animate({'left': '0%'}, { duration: 250, queue: false } );
                site_body.animate({'left': mobile_nav_width + 'px'}, { duration: 250, queue: false } );
                
                mobile_nav_toggle.attr('state', 'on'); 
            } 
            return false;
        });
        
        // Init primary search form behavior.
        var search_toggle = jQuery('#primarySearchToggle');
        var search_form   = jQuery('#primarySearch');
        var search_input  = jQuery('#primarySearchInput');
        
        search_toggle.click(function() {
            if (search_toggle.attr('state') == 'on') {
                search_input.blur();
                search_form.fadeOut();
                
                search_toggle.attr('state', 'off');        
            }
            else { // Search is currently off.
                search_form.css('display', 'block');
                search_form.fadeTo(250, 1);
                search_input.focus();
                
                search_toggle.attr('state', 'on'); 
            }
        });
        
        
        
    </script>
