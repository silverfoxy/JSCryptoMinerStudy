			<script>
jQuery( function( $ ) {
    
    var wmMarkers = $('#worldmap-wrap span.wm-marker');
    var rowOffices = $('.row_offices');
    var undef; // undefined
    
    function showInfo(country){
        country = country.toLowerCase();
        countryMarker = wmMarkers.filter("[data-country="+country+"]");
        if ( countryMarker.length !== 1 ) {
            return; // do nothing
        }
        wmMarkers.removeClass('active');
        countryMarker.addClass('active');
        rowOffices.removeClass('active');
        var maps = rowOffices.filter('.office_'+country).addClass('active').find('.google_map');
        var mapInstance =  maps.data('gmapsInstance');
        if( mapInstance !== undef ) {
            google.maps.event.trigger(mapInstance , "resize");
            var location = maps.data('gmapsMarker');
            var markerPosition = new google.maps.LatLng(location[0],location[1]);
            mapInstance.setCenter( markerPosition);
        }
    }
    

    wmMarkers.each(function( index, el ) {
        el = $(el);
        el.css({
            'top': el.data('posy'),
            'left': el.data('posx')
        }).on('click', function() {
            showInfo(el.data('country'));
        });
    });
    
    showInfo($('html').data('client-country'));
    
});
</script>
