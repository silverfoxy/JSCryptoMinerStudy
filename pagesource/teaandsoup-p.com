<script>
$(function(){
 var files = [
   'gahag-0112445699.jpg',
   '6f4fc5faa06c853030dc938ab93f92ab_l.jpg',
   'CK0I9A9050_TP_V.jpg',
   '807e546e7f62959b2251cfb7b82363b1_l.jpg',
   'SINGAPOL0I9A3009_TP_V2.jpg',
   //'waterfall-77676_1920.jpg'
 ];
 var img;
 var timer;
 var currentNum = 0;
 var nowPlaying = false;
 for (var i =0; i < files.length; i++) {
   img = $('<img>').attr('src' , files[i]).addClass('thumbnail');
   $('#thumbnails').append(img);
 }
   $('#main3').append(
     $('<img>').attr('src',files[0])
   );
   $('.thumbnail:first').addClass('current');
   $('.thumbnail').click(function() {
     $('#main3 img').attr('src',$(this).attr('src'));
     currentNum = $(this).index();
     $(this).addClass('current').siblings().removeClass('current');
   });
   $('#prev').click(function() {
     currentNum--;
     if (currentNum < 0) {
         currentNum = files.length - 1;
     }
     $('#main3 img').attr('src',files[currentNum]);
     $('.thumbnail').removeClass('current');
     $('.thumbnail').eq(currentNum).addClass('current');
   });
    $('#next').click(function() {
     currentNum++;
     if (currentNum > files.length - 1) {
         currentNum = 0;
     }
     $('#main3 img').attr('src',files[currentNum]);
     $('.thumbnail').removeClass('current');
     $('.thumbnail').eq(currentNum).addClass('current');
   });
   function autoPlay() {
     $('#next').click();
     timer = setTimeout(function() {
         autoPlay();
     }, 7000);
   }
   $('#play').click(function() {
     if (nowPlaying) return;
     nowPlaying = true;
     autoPlay();

   });

   $('#stop').click(function(){
       clearTimeout(timer);
       nowPlaying=false;
});
$(window).load(function(){
 $('.flexslider').flexslider({
   animaition:"slide"
 });
 $('#d1').contenthover({
     overlay_background:'#000',
     overlay_opacity:0.8
 });

$('#d2').contenthover({
   overlay_background:'#000',
   overlay_opacity:0.8
});
});
function modoru(){
 history.back();
}
function susumu(){
 history.forward();
}
 });


</script>

