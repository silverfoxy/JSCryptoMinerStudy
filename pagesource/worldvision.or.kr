<script type="text/javascript">

$(document).ready(function () {
		
	//	1. 브라우저 크기에 따른 main1 이미지 대응
	if ($(window).height() < 1080 && $(window).height() > 600)
	{
		resizeMain1($(window).height());
	}


	GetDB( 'child2', 0);

});


function resizeMain1(height)
{

}



	var xmlHttp;
	function GetDB(mode, value) {
		var f = document.wbForm;
		if ( mode == "child2" ) {
			url = "/campaign/2017/child_ajax.asp?mode="+mode+"&value="+value;
		}
		xmlHttp = GetXMLHttp();
		xmlHttp.open('POST', url, true);
		xmlHttp.onreadystatechange = callback;
		xmlHttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
		xmlHttp.send('');
	}

	function callback() {
		if(xmlHttp.readyState == 4) {
			if(xmlHttp.status == 200) {
				var data = xmlHttp.responseText;
				var info = data.split("][");
				if (info[0] == "child2" ) {
					if (info[1] <= 0) {
						alert("조건에 해당하는 아동이 없습니다.");
						return;
					}
					document.getElementById("child_img").src = unescape(info[6]);

					document.getElementById("child_name1").innerHTML = unescape(info[7]);
					document.getElementById("child_name2").innerHTML = unescape(info[7]);

