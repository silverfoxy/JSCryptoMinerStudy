      initXT3("uid");

  </script>

<script type="text/javascript">
var imgArr=["index_cmxt30/mainBg0.jpg","index_cmxt30/mainBg1.jpg","index_cmxt30/mainBg2.jpg","index_cmxt30/mainBg3.jpg","index_cmxt30/mainBg4.jpg","index_cmxt30/mainBg5.jpg","index_cmxt30/mainBg6.jpg"];
var index = GetRandomNum(0,6);
var currentImage=imgArr[index];
document.getElementById("MainBg").style.backgroundImage="url(" + currentImage + ")";

function GetRandomNum(Min,Max)
{   
var Range = Max - Min;   
var Rand = Math.random();   
return(Min + Math.round(Rand * Range));   
}
</script>
