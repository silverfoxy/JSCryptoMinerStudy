    color: #4285f4;
    border: 1px solid #ddd;
    background-color: transparent;
}
.post-pagination a:hover{
    text-decoration:underline;
}
.post-pagination a:hover span{
        background-color: #ddd;
}
/*eof:post-pagination*/
  
  
  
/*while unsynced from cdn*/
article.post img.wp-post-image{
  display:none;
}

/*bigger-size*/
article.post{
  font-size: 16px;
  line-height: 1.5em;
  color: #333;
}
/*gads*/
article.post .gad-inline-center.ads_widget_inline_square{
  float:left;
  margin-top: 22px!important;
  margin-right:10px;
}
article.post .gad-inline-center.ads_widget_inline_square.righty{
  float:right;
  margin-right:0;
  margin-left:10px;
}
#sidebar:first-child > .widget.widget_sp_image:first-child{
  margin-top:15px!important;
  margin-bottom:30px!important;
}
#sidebar:first-child > .widget.widget_sp_image:first-child .widget-title{
  display:none!important;
}


/*category, inline postdate*/
.post .category{
	margin-top: -6px!important;
}
  
/*top-social-notif*/
.top-social{
  margin-bottom: 0px;
  font-size: 14px;
  font-weight: normal;
  text-align: center;
  color: #373334;
  border: 4px solid #373334;
  padding: 2px 0;
  position: relative;
  top: -22px;
  background: #AFDEFD;
  border-radius: 3px;
  line-height: 22px;
}
.top-social a{
  color: #373334;
  font-weight: bold;  
}
.top-social span, .top-social a.ts-fblink{
  border-bottom: 1px solid #555;
}
.top-social .tsline{
  display:block;
}

@media (max-width: 500px) {
  .top-social .tsline{
     display:inline;
  }
}
    
/*badgequiz*/
#content .content-right-wrap .sticker{
    display: block;
}
#content .content-right-wrap .row.nobadge .sticker{
    display: none!important;
}
#sidebar .art-image .sticker.abs-left,
#sidebar_below  .art-image .sticker.abs-left,
.art-img-text-down .sticker.abs-left,
.art-img-text-right .sticker.abs-left,
.art-post .sticker.abs-left{
  top:0
}
.art-smallimg-text .art-post .sticker.abs-left{
  top:15px;
  left:5px;
}
.art-smallimg-text .art-post .sticker.abs-left img{
  width:30px;
}
  
/*badgevote*/
.art-smallimg-text .art-post .sticker.badgevote .badge_vote.badge_img{
  background-repeat: no-repeat !important;
  background-size: cover !important;
}
.art-smallimg-text .art-post .sticker.badgevote{
  opacity: .88;
}
.art-smallimg-text .art-post .sticker.badgevote .badge_vote.badge_img{
  width:20px;
  height:20px;
}
a[href*="w3-edge.com"]{
  display:none;
}
  
#comments{
  margin-bottom: 100px;
}
#comments .comment-list,
#comments .comment-pagination{
    padding-left:5px;
}
#toTop{
  bottom: 100px;
  z-index: 999999;
}
