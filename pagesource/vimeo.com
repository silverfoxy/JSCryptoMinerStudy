    window.handleJoinButton = function(e) {
        
        var shouldToggle = window.ABLincoln.getTest('Auth.ToggleModal').get('modal_toggle', 'off') === 'on';
        if (shouldToggle) {
            e.preventDefault();
            vimeo.Modal.create({
                size: 'xsmall',
                content: '/join?modal=new',
            }).then(function(response) {
                // This callback is defined in topnav/desktop.phtml.
                // I don't like having to do it this way.
                if (window.completeUserLogin) {
                    window.completeUserLogin(response);
                }
                else {
                    window.location.reload();
                }
            });
        }
    }

    (function() {
        var ctrlNodes = document.querySelectorAll('.ab_control');
        var testNodes = ctrlNodes;

        var initCopyTest = function () {
            if (window.ABLincoln) {
                var testVariant = window.ABLincoln.getTest('LOHP.Desktop.Copy');
                var variantNodes = document.querySelectorAll('.ab_variant');

                if (testVariant && ctrlNodes && variantNodes) {

                    var copyVariant = testVariant.get('lohp_copy', 'off') === 'on';
                    testNodes = copyVariant ? variantNodes : testNodes;
                }

                [].forEach.call(testNodes , function(ele) {
                    ele.classList.add('active');
                })
            }
            else {
                setTimeout(initCopyTest, 100);
            }

        }

        initCopyTest();
    }());
