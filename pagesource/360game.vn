<script type="text/javascript">
    // Distribution integration
    $(document).ready(function () {
        var data = {};
        data.action = 'get';


        var pageName = game360.CONFIG.pageName;
        if (pageName === "HOME" || pageName === "LIST_CODE_V3") {
//			data.pos = '64,65,66,74,112,113';
            data.pos = '1,2,3,9';
        } else {
            data.pos = '3';
        }

        $.ajax({
            url: 'https://360game.vn/dtbs/get',
            jsonp: 'callback',
            dataType: 'jsonp',
            data: data,
            success: function (resp) {
                if (resp.error >= 0) {
                    var fbA, fbImg;
                    var banner = resp.data;
                    if (data.pos.length === 1) {
                        fbA = $('#banner-flash-top-right a');
                        fbImg = $('#banner-flash-top-right a img');

                        fbA.attr('href', banner.link);
                        fbImg.attr('src', banner.img);
                        fbImg.attr('alt', banner.appname);

                        if (banner.open_new_tab) {
                            fbA.attr('target', '_blank');
                        }
                    } else {
                        for (var i in banner) {
                            switch (banner[i].pos) {
                                case 1:
                                    fbA = $('#background-left a');
                                    fbImg = $('#background-left a img');
                                    fbA.attr('href', banner[i].link);
                                    fbImg.attr('src', banner[i].img);
                                    fbImg.attr('alt', banner[i].appname);

                                    if (banner[i].open_new_tab) {
                                        fbA.attr('target', '_blank');
                                    }
                                    break;
                                case 2:
                                    fbA = $('#background-right a');
                                    fbImg = $('#background-right a img');
                                    fbA.attr('href', banner[i].link);
                                    fbImg.attr('src', banner[i].img);
                                    fbImg.attr('alt', banner[i].appname);

                                    if (banner[i].open_new_tab) {
                                        fbA.attr('target', '_blank');
                                    }
                                    break;
                                case 3:
                                    fbA = $('#banner-flash-top-right a');
                                    fbImg = $('#banner-flash-top-right a img');

                                    fbA.attr('href', banner[i].link);
                                    fbImg.attr('src', banner[i].img);
                                    fbImg.attr('alt', banner[i].appname);

                                    if (banner[i].open_new_tab) {
                                        fbA.attr('target', '_blank');
                                    }
                                    break;
                                case 9:
                                    $('#banner-bottom-left a').attr('href', banner[i].link);
                                    $('#banner-bottom-left a img').attr('src', banner[i].img);
                                    $('#banner-bottom-left a img').attr('alt', banner[i].appname);

                                    if (banner[i].open_new_tab) {
                                        $('#banner-bottom-left a').attr('target', '_blank');
                                    }
                                    break;
                            }
                        }
                    }
                }
            }
        });
    });

    function quickSearchTopMenuGame(event) {
        game360.CACHE = game360.CACHE || {};

        var term = $(event.target).val() + String.fromCharCode(event.keyCode);
        if (!term || term.trim().length === 0) {
            window.location.href = "/web-game/van-kiem-quy-tong_24798?_src=360-search&utm_source=360Game&utm_medium=search_sug-game";
            return;
        }

        if (event.keyCode === 13) {
            searchTopMenuGame(term);
            return;
        }

        if (game360.CACHE.quickSearchTimeOutId)
            clearInterval(game360.CACHE.quickSearchTimeOutId);

        game360.CACHE.quickSearchTimeOutId = setTimeout(doAjax, 800);

        function doAjax() {
            var url = '/ajax/box/game_autocomplete';
            ajaxCaller(url, {term: term}, function (data) {
                if (!data.html)
                    data.html = '<ul class="clearfix"><li><div align="center">Không tìm thấy dữ liệu "' + term + '"</div></li>';

                $('#suggestion-box-container').html(data.html);
                $('#suggestion-box-container').show();
                $(document.body).on('click', hideQuickSearch);
            });
        }

        function hideQuickSearch() {
            $('#suggestion-box-container').hide();
            $(document.body).unbind('click');
        }
    }

    function searchTopMenuGame(term) {
        if (!term) {
            window.location.href = "/web-game/van-kiem-quy-tong_24798?_src=360-search&utm_source=360Game&utm_medium=search_sug-game";
            return;
        }

        window.location = '/tim-kiem/search=' + term;

    }
    ;
