		}
		.bluet_hide_tooltip_button{
			    opacity: 0.7;
		        position: absolute;
			    font-family: 'Open sans';
			    right: 2px;
			    top: 2px;
			    display: block;
			    color: #dbdbdb  !important;
			    /*background-color: #36494e  !important;*/
			    height: 26px;
			    font-size: 31px;
			    border-radius: 50%;
			    font-weight: bold;
			    line-height: 0px;
			    padding: 11px 5px;
			    cursor: pointer;
		}
	}
	
	/*admin*/
	span.class_val{
		  margin-right: 5px;
	}
	span.elem_class {
		color: white;
