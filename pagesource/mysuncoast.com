        // Start code to adjust block region for mobile devices
        $(document).ready(function() {      
            var flag = 'true';             
            if (window.location.href.indexOf("admin-newyork1.bloxcms.com") == -1) {
                moveBlockRegion();
            } 
            
            $(window).resize(function(){        
                moveBlockRegion();
            });         
            
            function moveBlockRegion() { 
                if ($(window).width() <= 767 && flag == 'true') {
                    var weather_content = $('<span>').append($('div#tncms-region-global-side-primary').clone()[0]).html();
                    $('div#tncms-region-global-side-primary').remove();
                    var radar_content = $('<span>').append($('div#tncms-region-front-side-other div:eq(0)').clone()[0]).html();
                    $('div#tncms-region-front-side-other div:eq(0)').remove();
                    var sunset_content = $('<span>').append($('div#tncms-region-front-side-other div:eq(0)').clone()[0]).html();
                    $('div#tncms-region-front-side-other div:eq(0)').remove();
                    var signup_content = $('<span>').append($('div#tncms-region-front-side-other div:eq(0)').clone()[0]).html();
                    $('div#tncms-region-front-side-other div:eq(0)').remove();
                    var restaurant_content = $('<span>').append($('div#tncms-region-front-side-other div:eq(0)').clone()[0]).html();
                    $('div#tncms-region-front-side-other div:eq(0)').remove();
                    
                    $('<span id="new_mix_content">' + weather_content + radar_content + sunset_content + signup_content + restaurant_content + '</span>').insertBefore("div#tncms-region-front-secondary");                                                              
                    flag = 'false';                              
                }
                else if ($(window).width() > 767 && flag == 'false') {                 
                    var weather_content = $('<span>').append($('span#new_mix_content div:eq(0)').clone()[0]).html();
                    $('span#new_mix_content div:eq(0)').remove();
                    var radar_content = $('<span>').append($('span#new_mix_content div:eq(0)').clone()[0]).html();
                    $('span#new_mix_content div:eq(0)').remove();
                    var sunset_content = $('<span>').append($('span#new_mix_content div:eq(0)').clone()[0]).html();
                    $('span#new_mix_content div:eq(0)').remove();
                    var signup_content = $('<span>').append($('span#new_mix_content div:eq(0)').clone()[0]).html();
                    $('span#new_mix_content div:eq(0)').remove();
                    var restaurant_content = $('<span>').append($('span#new_mix_content div:eq(0)').clone()[0]).html();                
                    
                    $('div.main-sidebar').prepend(weather_content);
                    $('div#tncms-region-front-side-other').prepend(radar_content + sunset_content + signup_content + restaurant_content);              
                    $('span#new_mix_content').remove();
                    flag = 'true';                
                }
            }
        });
        // End code to adjust block region for mobile devices
        </script>
                
    
