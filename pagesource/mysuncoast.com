<script type="text/javascript">
function setNoOfSlider() {
    if ($(window).width() >= 320 && $(window).width() <= 375 ) {
        move_slide = 2;
    }
    else if ($(window).width() >= 376 && $(window).width() <= 500 ) {
        move_slide = 3;
    }
    else if ($(window).width() >= 501 && $(window).width() <= 660 ) {
        move_slide = 4;
    }
    else if ($(window).width() > 660 ) {
        move_slide = 5;
    }
     return move_slide; 
}

$(document).ready(function(){
    // Set no of slider on load
    var move_slide = setNoOfSlider();    
    
    $('#carousel').flexslider({
        animation: "slide",
        controlNav: true,
        animationLoop: true,
        slideshow: false,
        itemWidth: 100,
        itemMargin: 15,        
        asNavFor: '#slider'        
    });

    $('#slider').flexslider({
        animation: "fade",
        controlNav: true,
        animationLoop: true,
        slideshow: true,
        sync: "#carousel",
        directionNav: true,
        touch: true,
        slideshowSpeed: 8000,
        animationSpeed: 500,
        initDelay: 0,
        move: move_slide,
	pauseOnAction: true, 
        init: function(slider) { 
            
        },
        start: function(slider){
          //$('body').removeClass('loading');          
          var slide_count = slider.count - 1; 
           
          if((navigator.userAgent.indexOf("MSIE") != -1 ) || (!!document.documentMode == true )) { //IF IE > 10
             $('.flex-viewport ul li').hover(function(){                  
                  var index = $(this).parent().children().index(this);
                  $('#slider').flexslider(index);                   
              });
          } 
          else {
              $('.flex-viewport ul li').on('mouseover',function(){
                  $(this).trigger('click');
              });
          }       
          
          $('#carousel .slides li').css({'left':'0px', 'position':'realtive'});           

          // On next click
          $("li.flex-nav-next a.flex-next").click(function(){			  
                move_slide = setNoOfSlider();
                
                next_slide = slider.currentSlide + 1;                                                
                rem = next_slide % move_slide;
                next_slide = next_slide + (move_slide - rem);
                
                if (slider.count <= next_slide) {
                    next_slide = 0;
                }
                
                if (next_slide == 0) {
                    active_slide = 1;
                }
                else {
                    active_slide = next_slide + 1;
                }
                
                $('#slider').flexslider(next_slide);                
                
                $('.slides li').removeClass('flex-active-slide');
		$('.slides li:nth-child(' +  active_slide +')').addClass('flex-active-slide');
            }); 
                    
            // On previous click
            $("li.flex-nav-prev a.flex-prev").click(function(){ 
                move_slide = setNoOfSlider();
                
                prev_slide = slider.currentSlide + 1;
                
                if (prev_slide <= move_slide) {
                    prev_slide = slider.count - move_slide;					
                }
                else {
                    rem = prev_slide % move_slide;
                    prev_slide = prev_slide - rem - move_slide;	
                }
                
                if (prev_slide == 0) {
                    active_slide = 1;
                }
                else {
                    active_slide = prev_slide + 1;
                }
                
                $('#slider').flexslider(prev_slide);
                           
                $('.slides li').removeClass('flex-active-slide');
		$('.slides li:nth-child(' +  active_slide +')').addClass('flex-active-slide');	
            });
        },    
        before: function(slider) { 
            if (slider.currentSlide+1 == slider.count) {                
                $('#carousel').flexslider(0);  
            }
        },
        after: function(slider) {            
            if (!slider.playing) {
                slider.play();
            }
        }
    });
});
