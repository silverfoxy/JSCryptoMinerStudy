                    <script type="text/javascript">
                var loadDFPads = function (ignore_top_refresh) {
                    var dfp_adspots = '';

                    if (!ignore_top_refresh && $('#listings-adspot-top').length > 0) {
                        dfp_adspots += '#listings-adspot-top, ';
                    }

                    if ($('.adspot-leaderboard-general').length > 0) {
                        dfp_adspots += '.adspot-leaderboard-general, ';
                    }

                    if ($('#listings-adspot-bottom').length > 0) {
                        dfp_adspots += '#listings-adspot-bottom, ';
                    }

    
                    if (dfp_adspots !== '') {

                        dfp_adspots = dfp_adspots.substr(0, dfp_adspots.length - 2)

                        $(dfp_adspots).html('').dfp({
                            dfpID: '10607323',
                            enableSingleRequest: false
                        });
                    }

                    //console.log(dfp_adspots);
                }

                jQuery(document).ready(function ($) {
                    loadDFPads();
                });
            </script>
