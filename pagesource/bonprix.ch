    require(['domReady'],function(ready){
        ready(function(){
            window.documentHeight = WindowMeasurements.documentHeight();
            window.scrolledHeight = WindowMeasurements.scrollY() + WindowMeasurements.viewportHeight();
            window.scrollPercentage = WindowMeasurements.scrollYPercentage();

            if(!window.addEventListener){    // IE8 compatibility issue
                window.attachEvent("onscroll",function(){
                    var scrolledHeight = WindowMeasurements.scrollY() + WindowMeasurements.viewportHeight();
                    if(window.scrolledHeight < scrolledHeight) {
                        window.scrolledHeight = scrolledHeight;
                        window.scrollPercentage = WindowMeasurements.scrollYPercentage();
                        window.documentHeight = WindowMeasurements.documentHeight();
                    }
                });
                window.attachEvent("onunload",function(event){
                    var paramString = convertMapToParamString({
                        scrollPercentage:window.scrollPercentage,
                        scrolledHeight:window.scrolledHeight,
