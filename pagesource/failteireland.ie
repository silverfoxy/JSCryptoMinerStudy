            };
        });

        if($(".MenuOpacity").length != 0)
            setMenuTransparency();

        $('#HomeCarousel').trigger('play', true);
    };


    function setMenuTransparency(){
        $(".MenuTrans").each(function() {
            $(this).toggleClass("MenuOpacity");
        });
    }

</script>

 
