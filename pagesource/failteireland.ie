    function displayVmVdeo(vID) {
        //  stop carousel
        $('#HomeCarousel').trigger("stop");

        var vClass = $("div." + vID).attr("class");
        vClass = ('video-layer ' + vClass);

        //  Check hidden field for video info values
        var sVmInfo = "";
        sVmInfo= jQuery.trim($(".vm-info-" + vID).val());
        

        $("div." + vID).addClass(vClass);
        $("div." + vID).html("");
        $("div." + vID).html(sVmInfo + "<iframe src=\"http://player.vimeo.com/video/" + vID + "?autoplay=1\" width=\"640\" height=\"360\" frameborder=\"0\" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>");
        
        setMenuTransparency();
    };

    function hideVdeo() {
      
        //  YouTube videos
        $('.yt-video-wrapper').each(function () {
            $(this).removeClass('video-layer');
            var vID = $(this).attr("class").replace('yt-video-wrapper ', '');
            //vID = (vID.replace('video-layer','').trim());

            //$(this).removeClass('video-layer');

            //  Check hidden field for video info values
            var sYtInfo = "";
            sYtInfo= jQuery.trim($(".yt-info-" + vID).val());
            //alert(sYtInfo);
          
            //  Check hidden field for image values
            var sYtImg = "";
            sYtImg= jQuery.trim($(".yt-img-" + vID).val());

            if(sYtImg.length == 0){
                sYtImg = "http://img.youtube.com/vi/" + vID + "/0.jpg";
            }

            $("div." + vID).hide();
            $("div." + vID).html("");
            $("div." + vID).html(sYtInfo + "<a style=\"background:transparent url('" + sYtImg +"') no-repeat center center; float:left;\" title=\"play video\" href=\"javascript:;\" onclick=\"displayYtVdeo('" + vID + "')\"><img alt=\"play video\" src=\"/App_Themes/Failte_Ireland/images/home/carousel-play.png\" /></a>");
            $("div." + vID).show();
        });

        //  Vimeo videos
        $('.vimeo-video-wrapper').each(function () {
            $(this).removeClass('video-layer');
            var vmVideoID = $(this).attr("class").replace('vimeo-video-wrapper ', '');
            //vmVideoID = (vID.replace('video-layer','').trim());

            //  Check hidden field for video info values
            var sVmInfo = "";
            sVmInfo= jQuery.trim($(".vm-info-" + vID).val());

            //  Check hidden field for image values
            var sVmImg = "";
            sVmImg= jQuery.trim($(".vm-img-" + vmVideoID).val());

            if(sVmImg.length == 0){
                $.getJSON('http://www.vimeo.com/api/v2/video/' + vmVideoID + '.json?callback=?', { format: "json" },
