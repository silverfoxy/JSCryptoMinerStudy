window.$geoLocationDef = $.Deferred();
require(['jquery', 'jstorage'], function($) {
var GEO_LOC_KEY = setting('GEO_LOC_KEY'),
GEO_LOC_TTL = 3600000,
geolocData = $.jStorage.get(GEO_LOC_KEY);
var doGeoLocActions = function(data){
$(document).ready(function(){
// Push to GTM
if(typeof dataLayer !== 'undefined'){
var gaData ={};
gaData.event = 'sendGeoData';
gaData.countryShortCode = data.countryCode;
dataLayer.push(gaData);
}
if(typeof makeReevooCall != 'undefined')makeReevooCall(data.countryCode);
if(typeof makeCbpCall != 'undefined')makeCbpCall(data.countryCode);
})
};
if((typeof geolocData === 'undefined') || (geolocData === null)){
$.ajax({
url: '/coldboxproxy.cfc',
data: {
'method': 'getGeoLocation'
},
dataType: 'json',
context: this
}).done(function(data, textStatus, jqXHR){
//console.log('GeoLoc from web service',data);
if('statusCode' in data && data.statusCode == 'OK'){
$.jStorage.set(GEO_LOC_KEY, data, {TTL: GEO_LOC_TTL});
window.$geoLocationDef.resolve();
doGeoLocActions(data);
}
});
}else{
doGeoLocActions(geolocData);
window.$geoLocationDef.resolve();
}
});
</script>
