<script>
function strtr(str,list)
{
	for( var c in list )
	{
		str = String(str).replace( new RegExp( c ,"g"), list[c] )
	}
	return str;
}
function limpiar_cadena(cadena) {
	//cadena = strtrim(cadena);
	var chars = {'¿':'a','¡':'a','¬':'a','√':'a','ƒ':'a','≈':'a','‡':'a','·':'a','‚':'a','„':'a','‰':'a','Â':'a','“':'o','”':'o','‘':'o','’':'o','÷':'o','ÿ':'o','Ú':'o','Û':'o','Ù':'o','ı':'o','ˆ':'o','¯':'o','»':'e','…':'e',' ':'e','À':'e','Ë':'e','È':'e','Í':'e','Î':'e','«':'c','Á':'c','Ã':'i','Õ':'i','Œ':'i','œ':'i','Ï':'i','Ì':'i','Ó':'i','Ô':'i','Ÿ':'u','⁄':'u','€':'u','‹':'u','˘':'u','˙':'u','˚':'u','¸':'u','ˇ':'y','—':'n','Ò':'n','A':'a','B':'b','C':'c','D':'d','E':'e','F':'f','G':'g','H':'h','I':'i','J':'j','K':'k','L':'l','M':'m','N':'n','O':'o','P':'p','Q':'q','R':'r','S':'s','T':'t','U':'u','V':'v','W':'w','X':'x','Y':'y','Z':'z',' ':'_'};
	cadena = strtr(cadena,chars);
	cadena = cadena.replace('#([^.a-z0-9]+)#i', '_');
    cadena = cadena.replace('#-{2,}#','_');
    cadena = cadena.replace('#-$#','');
    cadena = cadena.replace('#^-#','');
	return cadena;
}
function buscar(id_menu,palabra)
{
	if(palabra=="" || palabra=="B˙squeda...")
	{
		alert("Debe ingresar la palabra clave");	
		document.getElementById('buscar').focus();
		return false;
	}	
	if(id_menu=="") id_menu=0;
	palabra=limpiar_cadena(palabra);
	location.href='/resultados/1/'+id_menu+'/'+palabra;
	return true;
}
</script>
	

