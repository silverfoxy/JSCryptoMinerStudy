  <script>
    /*
     * windowSize
     * call this function to get windowSize any time
     */
    function windowSize() {
      windowHeight = window.innerHeight ? window.innerHeight : $(window).height();
      windowWidth = window.innerWidth ? window.innerWidth : $(window).width();
    }
    function resizeWidth() {
      var existingWidth = $(document).data('resize-width');
      var newWidth = $(document).width();
      if (existingWidth != newWidth) {
        if (windowWidth >= 960) {
         	 controlPadding();
        }
        $(document).data('resize-width', newWidth);
      };
    };

    
    function controlPadding(){
      var gridHeight = $(".custom-grid-item").height();
      $(".grid-text-table").each(function(i){
        if(($(this).outerHeight()) > gridHeight){
          $(this).find(".grid-text-cell").css({"padding":"0"});
        }else {
          if(($(this).find(".grid-text-cell > div").height() + 90) < $(this).height() ){
            $(this).find(".grid-text-cell").removeAttr("style"); 
          }
        }
      }); 
    }
    
    $(function() {
      $(".custom-grid-item").matchHeight();
    });

    $(window).load(function(){
      windowSize();
      resizeWidth();
    });

    // For example, get window size on window resize
    $(window).resize(function() {
      windowSize();
      resizeWidth();
      if (windowWidth < 961) {
        if(typeof $(".custom-grid-item").matchHeight == "function"){
          $(".custom-grid-item").matchHeight({ remove: true });
        }
      }
    });
  </script>
  
  
