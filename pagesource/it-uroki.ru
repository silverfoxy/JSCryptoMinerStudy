    for (var i = 0; i < Sa.length; i++) {  // –ø–µ—Ä–µ—á–∏—Å–ª–∏—Ç—å —Å—Ç–∏–ª–∏ CSS, –∫–æ—Ç–æ—Ä—ã–µ –Ω—É–∂–Ω–æ —Å–∫–æ–ø–∏—Ä–æ–≤–∞—Ç—å —Å —Ä–æ–¥–∏—Ç–µ–ª—è
      if (Sa[i].indexOf('box-sizing') == 0 || Sa[i].indexOf('overflow') == 0 || Sa[i].indexOf('width') == 0 || Sa[i].indexOf('padding') == 0 || Sa[i].indexOf('border') == 0 || Sa[i].indexOf('outline') == 0 || Sa[i].indexOf('box-shadow') == 0 || Sa[i].indexOf('background') == 0) {
        s += Sa[i] + ': ' +Sa.getPropertyValue(Sa[i]) + '; '
      }
    }
    a.innerHTML = '<div style="'+s+'">'+a.innerHTML+'</div>';
    b = a.children[0];
    a.style.height = b.getBoundingClientRect().height + 'px';  // –µ—Å–ª–∏ –ø–æ–¥ —Å–∫–æ–ª—å–∑—è—â–∏–º —ç–ª–µ–º–µ–Ω—Ç–æ–º –µ—Å—Ç—å –¥—Ä—É–≥–∏–µ –±–ª–æ–∫–∏, –º–æ–∂–Ω–æ —Å–≤–æ—ë –∑–Ω–∞—á–µ–Ω–∏–µ
    a.style.padding = '0';
    a.style.border = '0';  // –µ—Å–ª–∏ —ç–ª–µ–º–µ–Ω—Ç—É –ø—Ä–∏—Å–≤–æ–µ–Ω padding –∏–ª–∏ border
  }
  if (a.getBoundingClientRect().top <= 0) { // elem.getBoundingClientRect() –≤–æ–∑–≤—Ä–∞—â–∞–µ—Ç –≤ px –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã —ç–ª–µ–º–µ–Ω—Ç–∞ –æ—Ç–Ω–æ—Å–∏—Ç–µ–ª—å–Ω–æ –≤–µ—Ä—Ö–Ω–µ–≥–æ –ª–µ–≤–æ–≥–æ —É–≥–ª–∞ –æ–±–ª–∞—Å—Ç–∏ –ø—Ä–æ—Å–º–æ—Ç—Ä–∞ –æ–∫–Ω–∞ –±—Ä–∞—É–∑–µ—Ä–∞
    b.className = 'sticky';
  } else {
    b.className = '';
  }
}
})()
</script>
<!-- //–ø–ª–∞–≤–∞—é—â–∏–π –±–ª–æ–∫ -->
