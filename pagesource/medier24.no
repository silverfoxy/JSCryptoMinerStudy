        <script type="text/javascript">
  
  /**
   * Tek class js
   */

  Tek = window.Tek || {};

  ;(function() {
      var requestAnimationFrame = window.requestAnimationFrame ||
          window.webkitRequestAnimationFrame ||
          window.mozRequestAnimationFrame ||
          window.msRequestAnimationFrame ||
          function(callback) { return setTimeout(callback, 1000 / 60); };

      var throttle = function(type, name, obj) {
          obj = obj || window;
          var running = false;
          var func = function() {
              if (running) { return; }
              running = true;
              requestAnimationFrame(function() {
                  if (typeof CustomEvent === 'function') {
                      var evt = new CustomEvent(name);
                  }
                  else {
                      var evt = document.createEvent("CustomEvent");
                      evt.initCustomEvent(name, false, false, {});
                  }
                  obj.dispatchEvent(evt);
                  running = false;
              });
          };
          obj.addEventListener(type, func, {
              passive: true
          });
      };

      throttle('scroll', 'optimizedscroll');
      throttle('resize', 'optimizedresize');
  })();

  Tek.autoRefresh = {
      idle: 0,
      enabled: true,
      id: null,
      timeout: 600,
      clear: function() {
          Tek.autoRefresh.idle = 0;
      },
      start: function(timeout) {
          var hour = new Date().getHours();

          if (typeof(timeout) != "undefined")
              Tek.autoRefresh.timeout = timeout;

          // slow down at night
          if (hour < 7)
              timeout = 1800;

          if (Tek.autoRefresh.id)
              clearInterval(Tek.autoRefresh.id);

          Tek.autoRefresh.id = setInterval(function () {
              if (Tek.autoRefresh && Tek.autoRefresh.enabled) {
                  Tek.autoRefresh.idle++;
                  if (Tek.autoRefresh.idle > Tek.autoRefresh.timeout) {
                      Tek.autoRefresh.idle = 0;
                      window.location.reload();
                  }
              }
          }, 1000);

          window.removeEventListener("keyup", Tek.autoRefresh.clear);
          window.removeEventListener("mousemove", Tek.autoRefresh.clear);
          window.addEventListener("keyup", Tek.autoRefresh.clear);
          window.addEventListener("mousemove", Tek.autoRefresh.clear);
      }
  };

  Tek.image = Tek.image || {};
  Tek.image.lazyLoadEnabled = false;

  Tek.image.selectImage = function (id, updateImages) {
      var selector = updateImages ? 'div.delay-image-load, .responsive-image' : 'div.delay-image-load';
      var els = null;
      if (typeof(id) == "undefined" || id === null)
          els = document.querySelectorAll(selector);
      else
          els = document.querySelectorAll('div.delay-image-load[data-id="' + id + '"], .responsive-image[data-id="' + id + '"]');
      if (els.length > 0) {
          (function(els) {
              for (var i in els) {
                  var el = els[i];
                  var data = getDataset(el);

                  if (data == undefined) {
                      return;
                  }
                  var widthAttribute  = null;
                  var heightAttribute = null;
                  var matchMedia = ((!!window.matchMedia && !window.matchMedia("(min-width: 750px)").matches) || screen.width <= 750);
                  if (data.widthNormal || data.widthSmall || data.width || data.heightNormal || data.heightSmall || data.height) {
                      widthAttribute   = el.clientWidth;
                      heightAttribute  = el.clientHeight;
                      var fileWidth    = matchMedia ?
                          (data.widthSmall || data.width) :
                          (data.widthNormal || data.width);
                      var fileHeight   = matchMedia ?
                          (data.heightSmall || data.height) :
                          (data.heightNormal || data.height);
                      fileWidth        = fileWidth > 0 ? fileWidth : widthAttribute;
                      fileHeight       = fileHeight > 0 ? fileHeight : heightAttribute;
                      if (fileWidth && fileHeight) {
                          var ratio    = (fileWidth / fileHeight);
                          //widthAttribute  = (widthAttribute > fileWidth) ? fileWidth : widthAttribute;
                          heightAttribute = Math.round(widthAttribute / ratio);
                      }
                      else if (fileHeight) {
                          heightAttribute = fileHeight;
                      }
                      else if (fileWidth) {
                          widthAttribute  = fileWidth;
                      }
                  }

                  var values = {
                      src: matchMedia ?
                          (data.srcSmall || data.src || el.src) :
                          (data.srcNormal || data.src || el.src),
                      width: widthAttribute || undefined,
                      height: heightAttribute || undefined,
                  };

                  if (el.nodeName == "IMG") {
                      for (var v in values) {
                          if (values[v]) {
                              el.setAttribute(v, values[v]);
                          }
                      }
                  }
                  else {
                      for (var v in values) {
                          if (values[v]) {
                              el.setAttribute('data-'+v, values[v]);
                          }
                      }
                      if (values.height) {
                          el.setAttribute('height', values.height);
                      }
                      if (values.width) {
                          el.setAttribute('width', values.width);
                      }
                  }
              }
          })(els);
      }
  };

  Tek.image.loadImage = function (id, el) {
      el = el || document;
      var divs = null;
      if (typeof(id) == "undefined" || id === null) {
          divs = el.querySelectorAll('div.delay-image-load');
      }
      else {
          divs = el.querySelectorAll('div.delay-image-load[data-id="' + id + '"]');
      }
      if (divs.length > 0) {
          var self = this;
          (function (divs) {
              for (var i in divs) {
                  var div = divs[i];
                  var data = getDataset(div);
                  if (data == undefined)
                      return;
                  if (!self.lazyLoadEnabled || id || (div.getAttribute("class").indexOf('load-asap') !== -1) || isOnScreen(div)) {
                      var img = new Image();
                      var attribs = ['src', 'height', 'width', 'alt', 'style', 'title'];
                      for (var a in attribs) {
                          var attr = attribs[a];
                          if (data[attr] && data[attr] != 0) {
                              img[attr] = data[attr];
                          }
                      }
                      img.className = (data['class'] || "") + " responsive-image";

                      // change resolution by device pixel ratio
                      if (window.devicePixelRatio != 1) {
                          img.src = img.src.replace(/\.(\d+)x(\d+)([!c]?\.\w+)$/, function(str, widthStr, heightStr, ending) {
                              return '.' + [widthStr, heightStr].map(function(str) { return Math.round(parseInt(str) * window.devicePixelRatio) || str; }).join('x') + ending;
                          });
                      }

                      var dataAttribs = ['src', 'srcSmall', 'srcNormal', 'widthSmall', 'widthNormal', 'heightSmall', 'heightNormal'];
                      for (var a in dataAttribs) {
                          var attr = dataAttribs[a];
                          if (data[attr] && data[attr] != 0) {
                              img.setAttribute("data-"+attr.replace(/([A-Z])/g, "-$1").toLowerCase(), data[attr]);
                          }
                      }
                      div.parentNode.replaceChild(img, div);
                  }
              }
          })(divs);
      }
  };

  Tek.image.selectImages = function(refresh) {
      Tek.image.selectImage(null,refresh);
  }

  Tek.image.loadImages = function (el) {
      Tek.image.loadImage(null,el);
  }

  Tek.image.lazyLoad = function() {
      this.lazyLoadEnabled = true;
      window.addEventListener('optimizedscroll', function() {
          Tek.image.loadImages();
      });
      var scrollbars = document.querySelectorAll('.tek-scrollbar');
      if (scrollbars.length) {
          for (i=0; i<scrollbars.length; i++) {
              var scrollbar = scrollbars[i];
              scrollbar.addEventListener('scroll', function() {
                  Tek.image.loadImages(scrollbar);
              });
          }
      }
  }

  function getDataset(el) {
      if (el.dataset != undefined) {
          var data = el.dataset;
      }
      else {
          var data,
              attributes = el.attributes;
          if (attributes != undefined) {
              for (var i = 0, n = attributes.length, data = {}; i < n; i++) {
                  var key = attributes[i].nodeName.replace('data-','');
                  // CamcelCase
                  key = key.replace(/(\-[a-z])/g, function($1){return $1.toUpperCase().replace('-','');});
                  data[key] = attributes[i].nodeValue;
              }
          }
      }
      return data;
  }

  // crappy viewports might trigger some resizing. Dont add event listener before page has fully loaded
  window.addEventListener('load', function() {
      window.addEventListener('optimizedresize', function() {
          Tek.image.selectImages(true);
      });
  });

  // Stats
  Tek.Stats = Tek.Stats || {};
  Tek.Stats.ish = {};

  Tek.Stats.ish.buildUrl = function(obj) {
      var projectId = obj.projectId || obj.pid || Tek.config.ishId,
          resourceId = obj.resourceId || obj.requestValue || obj.res || obj.req || obj.val;

      var params = [
          'pid=' + projectId
      ];

      if (resourceId !== undefined)
          params.push('val=' + encodeURIComponent(resourceId));
      if (obj.load !== undefined)
          params.push("load=" + obj.load);
      if (document.referrer.length)
          params.push("ref=" + encodeURIComponent(document.referrer));

      return "https://ish.tumedia.no/ish_tracker.gif?" + params.join('&');
  };

  // Track ish
  Tek.Stats.ish.trackView = function(obj) {
      var img = document.createElement('img');
      img.src = Tek.Stats.ish.buildUrl(obj);
      img.width = 1;
      img.height = 1;
      img.alt = 'ish';

      img.onload = function() {
          document.body.removeChild(img);
      };

      document.body.appendChild(img);
  };

  // Refresh all the stats things as if the page was reloaded
  Tek.Stats.pageView = function() {
