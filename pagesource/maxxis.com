        recaptchaScriptElement.setAttribute("async", "");
        recaptchaScriptElement.setAttribute("defer", "");
        $('.insert-recaptcha').append(recaptchaScriptElement);
    }
})();

$(function () {
    $('[id^=contour_form] form').on('submit', function (e) {
        var form = $(this);

        form.parsley().validate();

        if (!form.parsley().isValid() || !googleRecaptchaValidate()) {
            e.preventDefault();
            return;
        }

        $('[id^=contour_form] form button').attr('disabled', 'disabled');
    })
})

/* Tire Registration dropdown script start */

$(document).ready(function () {

    var categorySelect = $('.category select');
    var modelSelect = $('.model select');
    var sizeSelect = $('.size select');

    modelSelect.prop('disabled', true);
    //modelSelect.css("background-color", "lightgrey !important");
    sizeSelect.prop('disabled', true);
    //sizeSelect.css("background-color", "lightgrey !important");

    categorySelect.change(function () {
        updateSelect([categorySelect], modelSelect, 'GetTyreFinderPatterns', null)
    });

    modelSelect.change(function () {
        updateSelect([modelSelect], sizeSelect, 'GetTyreFinderPatternTyreSizeStrings', null)
    });

});

function updateSelect(previousSelects, nextSelect, method, params) {
    nextSelect.html('<option value="">Please select...</option>');
    if (previousSelects[previousSelects.length - 1].val() != '') {
        var urlParams = '';
        previousSelects.forEach(function (e) {
            urlParams += encodeForRest(e.children(':selected').attr('data-id').toString()) + '/';
        });
        $.getJSON('/base/api/{0}/{1}US'.f(method, urlParams), function (result) {
            if (result.length > 0) {
                $.each(result, function (index) {
                    var option = $('<option>', { value: result[index].Value }).text(result[index].Value);
                    option.attr('data-id', result[index].Key);
                    nextSelect.append(option);
                });
                nextSelect.find('option:eq({0})'.f(result.length == 1 ? 1 : 0)).prop('selected', true);
                nextSelect.change().prop('disabled', false);
            }
        });
    }
    else {
        nextSelect.change().prop('disabled', true);
        //nextSelect.css("background-color", "lightgrey !important");
    }
}

function encodeForRest(string) {
    return encodeURIComponent((string == null ? '' : string).replace("/", "~"));
}

/* Tire Registration dropdown script end */
