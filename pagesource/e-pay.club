<script>

function updateTeaser(){
	$("#teaser").empty();
	$("#teaser").prepend('<center id="loading-teaser"><img style="display:block;margin-bottom:25px;" src="/i/load.GIF"></center>');
	$.post( "/index.php?p1=/", { json: 'teaser', 'count':8}, function( data ) {
		 if(data.result.result != 1){
		 	alert('Ошибка!Обратитесь к администратору!');
		 }else{
		 	$("#teaser").empty();
		 	$.each(data.result.data, function( index, value ) {

                if(value.donate == 1){
                    text = '<div class="name"><span><!--noindex--><a href="'+value.url+'" target="_blank" style="z-index:9999 !important;"><u>'+value.name+'</u></a><!--/noindex--></span></div>'
                            +'<div class="price price_right">'
                            +'Отчисления:'
                            +'<span>'
                            +'<strong>'+value.persent+'</strong> %';
                }else{

                    prices  = '';

                    var dynamic2 = '';

		 			if(value.dynamic == 1){
		 				$.each(value.dynamics, function( indexd, valued ) {
		 					dynamic2 += '<p style="text-align:center;">'
                                +'от <strong>'+valued.count+'</strong> продаж:'
                                +' <strong>'+valued.persent+'</strong> %'
                              +'</p>';
		 				});

		 			}

                    if(value.prices)	{
                        $.each(value.prices, function( index2, value2 ) {
                            prices += '<p>Цена <strong>'+value2.price_val+' RUB</strong>  -  Отчисления <strong>'+value2.price_partner+' RUB</strong></p>';
                        });
                    }

                    text = '<div class="name"><span><!--noindex--><a href="'+value.url+'" target="_blank" style="z-index:9999 !important;"><u>'+value.name+'</u></a><!--/noindex--></span></div>'
                            +'<div class="price">'
                            +'Цена:'
                            +'<span>'
                            +'<strong>'+value.price_val+'</strong> RUB'
                            +'<ul '+((prices == '')?'style="display:none;"':'')+' class="tovar_icon tovar_icon_left">'
                            +'<li class="sale_icon"><a href="#">'
                            +'<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Capa_1" x="0px" y="0px" viewBox="0 0 100 100" style="enable-background:new 0 0 100 100;" xml:space="preserve" width="512px" height="512px">'
                            +'<g>'
                            +'<path d="M62.162,0c6.696,0,10.043,4.567,10.043,9.789c0,6.522-5.814,12.555-13.391,12.555    c-6.344,0-10.045-3.752-9.869-9.947C48.945,7.176,53.35,0,62.162,0z M41.543,100c-5.287,0-9.164-3.262-5.463-17.615l6.07-25.457    c1.057-4.077,1.23-5.707,0-5.707c-1.588,0-8.451,2.816-12.51,5.59L27,52.406C39.863,41.48,54.662,35.072,61.004,35.072    c5.285,0,6.168,6.361,3.525,16.148L57.58,77.98c-1.234,4.729-0.703,6.359,0.527,6.359c1.586,0,6.787-1.963,11.896-6.041L73,82.377    C60.488,95.1,46.83,100,41.543,100z" fill="#FFFFFF"/>'
                            +'</g>'
                            +'</svg>'
                            +'</a><span class="tooltip-content" style="width:236px; font-style:italic;"><div class="title_span">Дополнительные цены:</div>'+prices+'</span></li>'
                            +'</ul>'
                            +'</span>'
                            +'</div>'
                            +'<div class="price price_right">'
                            +'Отчисления:'
                            +'<span>'
                            +'<strong>'+value.partner_price+'</strong> руб.';
                }

                var dynamic = '<div class="tovar">'
            	+'<img src="/i/product/crop/'+value.path+'/'+value.id+'.jpg" alt="" />'
                +((value.donate == 0)?'<div class="percent_tovar">'+value.persent+'%</div>':'<div class="percent_tovar2">'+value.persent+'%</div>')
                +'<div class="tovar_hover">'
                	+text
		 		+'<ul class="tovar_icon">';

				if(value.dynamic == 1){
					dynamic += '<li>'
					+'<a href=""></a>'
					+'<span class="tooltip-content">'
                	+'<span style="text-align:center; margin-bottom:3px;">Динамические ставки:</span>'
                    $.each(value.dynamics, function( dindex, dvalue ) {
                    	dynamic += '<p style="text-align:center;">'
                                    +'от <strong>'+dvalue.count+'</strong> продаж:'
                                    +' <strong>'+dvalue.persent+'</strong> %.'
                                  	+'</p>';
                    });
                    dynamic
                    +'</span>'
                	+'</li>';
				}
				if(value.sound != 0){
            		dynamic += '<li class="sale_icon">'
            		+'<a href="">-'+value.sound+'%</a>'
            		+'<span class="tooltip-content" style="width:232px; font-style:italic;">'
            		+'<p><strong>Предлагаемая скидка </strong>покупателю, при уходе со страницы оплаты.</p>'
                    +'<p>Соответственно, отчисления уменьшаются на размер скидки. </p>'
                    +'</span>'
                    +'</li>';
            	}
				dynamic += '</ul>';
				dynamic += '</span>' 
                    +'</div>' 
                    +'<div class="clearfix"></div>'
			        +'<!--noindex--><a class="button" href="'+data.result.domain+'b/'+value.code+'" target="_blank">Купить</a> <!--/noindex-->' 
                    +'<a class="button" href="/marketplace/?id='+value.id+'" style="float:right;">Стать партнёром</a>'
                	+'</div>'
            	+'</div>';
            	//alert(dynamic);
            	$("#teaser").prepend(dynamic);

			});
			$("#teaser").append('<ul class="tovar_icon tovar_info_icon_new" style="float:none;">'
			+'<li class="sale_icon">'
			+'<a href="#">?</a>'
			+'<span class="tooltip-content" style="width:232px; font-style:italic;">'
			+'<p>Стоимость размещения одного сайта составляет 599 рублей сроком на 7 дней. <br>Перейдите по ссылке и добавьте свой сайт в рекламный блок.</p>'
			+'</span>'
			+'</li>'
			+'</ul>');
			$("#teaser").append('<a class="add" href="/teaser/">Добавить сайт</a>');



		 }
	}, "json");
}

