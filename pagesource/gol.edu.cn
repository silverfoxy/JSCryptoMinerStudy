<script>
	function _toCollegeList(){
		var name =document.getElementById("chname").value;
		name=$.trim(name);
		if(name=="请输入名称"){
			document.getElementById("chname").value="";
		}else{
			if($("#chname").val()){
				$("#chname").val('');
			}
			$("#chname").val(name);
		}
		if($("#chname").val() && ($('#cateid').val() == '1-2')){
			$("#country").val("");
			$("#search_type_").val("");
		}else{
			$("#country").val("US");
			$("#search_type_").val("us_col");
		}
		
		if(($('#cateid').val() == '3')){
			$("#country").val("US");
			$("#search_type_").val("us_middle");
		}
		
		if(($('#cateid').val() == '4')){
			$("#country").val("");
			$("#search_type_").val("list_guoji");
		}
		
		if(($('#cateid').val() == '5')){
			$("#country").val("");
			$("#search_type_").val("list_jigou");
		}
		
		
		document.getElementById("searchCollege1").submit();
	}


