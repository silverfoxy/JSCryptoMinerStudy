                        <div class="filterHeader">
                            <img src="/st/cross.png" @click="applyMobileFilter"> <span @click="applyMobileFilter">Filter</span>

                            <span class="pull-right clearbtn" @click="clearFilters">Clear</span>

                        </div>
                        <div class="filterType">
                            <ul>
                                <li :class="{ active: visibleFilterMobile===item.field }"
                                    @click="mobileFilterSelected(item.field)"
                                    v-for="item in filters">{{item.title}} <span
                                        v-if="giveFilterSelected(item.field) !== 0" style="
    float: right;
">({{giveFilterSelected(item.field)}})</span>
                                </li>
                            </ul>
                        </div>

                        <div class="filterValues" id="mobileFilterValues">

                            <div v-for="f in filters">

                                <!--Multi Select Checkbox-->
                                <ul v-if="f.type==='multiple'" v-show="visibleFilterMobile===f.field">
                                    <li v-for="item in f.selected">
                                        <div class="checkbox">
                                            <label @click="searchWithTimeout()">
                                                <input v-model="f.selected" :value="item" type="checkbox">
                                                <span class="cr">
<i class="cr-icon fa fa-check" style="right: 2px;"></i></span>
                                                {{item}}
                                            </label>
                                        </div>
                                    </li>
                                    <li v-for="item in getNotSelectedFilters(f.field)">
                                        <div class="checkbox">
                                            <label @click="searchWithTimeout()">
                                                <input v-model="f.selected" :value="item.name" type="checkbox">
                                                <span class="cr">
<i class="cr-icon fa fa-check" style="right: 2px;"></i></span>
                                                {{item.name}}
                                            </label>
                                        </div>
                                    </li>

                                </ul>

                                <ul v-if="f.type==='multiple_price'" v-show="visibleFilterMobile===f.field">
                                    <li v-for="item in f.values">
                                        <div class="checkbox">
                                            <label @click="searchWithTimeout()">
                                                <input v-model="f.selected" :value="item" type="checkbox">
                                                <span class="cr">
<i class="cr-icon fa fa-check" style="right: 2px;"></i></span>
                                                {{item}}
                                            </label>
                                        </div>
                                    </li>

                                </ul>

                            </div>
                        </div>
                        <div class="filterFooter" @click="applyMobileFilter">
                            <span>Apply</span>
                        </div>
                    </div>
                    <div class="apply-filter">
                        <img src="/st/tick.png" class="img-responsive center-block"/>
                    </div>
                </div>
            </div>

        </div>

    </transition>

        <div id="st-back-top" class="back-top "><a href="#" rel="tooltip" title="Top"></a></div>

    </div>
    <!-- template ends -->

    <script type="text/javascript" src="https://jamesallardice.github.io/Placeholders.js/assets/js/placeholders.min.js"></script>


</body>
</html>