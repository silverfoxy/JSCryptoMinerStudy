            $(document).ready(function(){

                var top = 999999,
                    scrolled = false;

                $(window).load(function(){
                    top = $('.alison-stats').offset().top;
                });

                var options = {
                    useEasing: true,
                    useGrouping: true,
                    separator: ',',
                    decimal: '.',
                };

                var learners = new CountUp('learners-amount', 0, parseInt($('#learners-amount').text()), 0, 2.5, options);
                var graduates = new CountUp('graduates-amount', 0, parseFloat($('#graduates-amount').text()), 1, 2.5, options);
                var countries = new CountUp('countries-amount', 0, parseInt($('#countries-amount').text()), 0, 2.5, options);
                var courses = new CountUp('courses-amount', 0, parseInt($('#courses-amount').text()), 0, 2.5, options);

                $(window).scroll(function () {
                    if( ( $(window).scrollTop() + $(window).height() ) > ( top + 300 ) && scrolled === false){
                        scrolled = true;

                        if (!learners.error) {
                            learners.start();
                        } else {
                            console.error(learners.error);
                        }
                        if (!graduates.error) {
                            graduates.start();
                        } else {
                            console.error(graduates.error);
                        }
                        if (!countries.error) {
                            countries.start();
                        } else {
                            console.error(countries.error);
                        }
                        if (!courses.error) {
                            courses.start();
                        } else {
                            console.error(courses.error);
                        }
                    }
                });
            });

        </script>
