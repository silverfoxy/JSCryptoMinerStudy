                $('#mailErr').css('display', 'block').css('padding', '10px 20px').css('margin-bottom', '15px');
                $('#s_button').css('display', 'none');
                $('#mailErr').html('Vr·ù sa sp‰ù a skontroluj si zadan˙ emailov˙ adresu.');
            }
        }

        function aToogleActive(idx) {
            var id = idx;
            $("#" + id).toggleClass('active');
