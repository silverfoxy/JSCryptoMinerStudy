    <script>
        $(document).ready(function () {
            //#region Search filters
            $(document).click(function (event) {
                hideFilterContainers(event, '.search-filters-list', '.search-filter-item-dropdown', '.search-filter-item-link');
                hideFilterContainers(event, '.search-filters-pictograph-list', '.search-filter-pictograph-list-dropdown', '.search-filter-pictograph-list-link');
            });

            //show/hide the dropdowns for Filters menus (First Level menu)
            $('.search-filters-list').on('click', '.search-filter-item-link', function (e) {
                e.preventDefault();
                // 'this' is '.search-filter-item-link'
                toggleFilters($(this), '.search-filter-item-link', '.search-filter-item', '.search-filter-item-dropdown');
            });

            //show/hide the dropdowns for Filters menus (Second Level menu)
            $('.search-filters-pictograph-list').on('click', '.search-filter-pictograph-list-link', function (e) {
                e.preventDefault();
                // 'this' is '.search-filter-pictograph-list-link'
                toggleFilters($(this), '.search-filter-pictograph-list-link', '.search-filter-pictograph-list-item', '.search-filter-pictograph-list-dropdown');
            });

            function toggleFilters(clickElem, filterItemSelector, parentSelector, containerSelector) {
                var wasActive = clickElem.hasClass('active');
                $(containerSelector).hide();
                $(filterItemSelector).removeClass('active');

                //  check previous state to know if the filter container needs to be shown or not
                if (!wasActive) {
                    clickElem.addClass('active');
                    clickElem.closest(parentSelector).find(containerSelector).show();
                }
            }

            function hideFilterContainers(event, filtersListSelector, containerSelector, filterItemSelector) {
                if (!$(event.target).closest(filtersListSelector).length) {
                    $(containerSelector).hide();
                    $(filterItemSelector).removeClass('active');
                }
            }
        });

        function filterDirectories() {
                return {
                    countryCode: $("#country-select-directory").val()
                };
            }

        function onDirectoryBound() {
            // get a reference to the dropdown list
            var dropdownlist = $("#directory-select").data("kendoDropDownList");
            var itemsCount = dropdownlist.dataSource.data().length;

            // disable the dropdown list
            if (itemsCount > 0)
                dropdownlist.enable();
            else
                dropdownlist.enable(false);
        };

        function onDirectoryChange() {
            var item = this.dataItem();

            location.href =
                encodeURI("/Landing/ExternalDirectory" +
                    "?ct={0}&co={1}&dn={2}&url={3}".format(item.ExtraFields.Continent, item.ExtraFields.Country, item.Text, item.ExtraFields.Url));
        }

    </script>

