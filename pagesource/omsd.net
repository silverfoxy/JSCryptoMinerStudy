	function photoGalleryPlaceholder() {
		var topOffSet = $("#sw-content-container10.ui-hp div.ui-widget.app.gallery.json").offset().top;
                var screenWidth = $(window).width();
                var screenHeight = $(window).height() - topOffSet;
                var screenRatio = screenHeight / screenWidth;
                var containerHeight = screenWidth * screenRatio;
                var imgWidth = screenWidth, imgHeight = 0;
                
                // GET IMAGE RATIO
                var initW = 1800, initH = 960;
                var ratio = initH / initW;
                var imgHeight = imgWidth * ratio;
                
                if(imgHeight < screenHeight){
                    imgHeight = screenHeight;
                    imgWidth = imgHeight / ratio;
                }
                
                var centerWidth = (imgWidth - screenWidth) / 2;
                var centerHeight = (imgHeight - screenHeight) / 2;
                
                // APPLY THE DIMENSIONS
                $("#hp-slideshow .rs-photo-gallery-container").css("height", containerHeight + "px");
                $("#hp-slideshow .rs-photo-gallery-container .rs-photo-gallery-images").css("height", containerHeight + "px");
                $("#hp-slideshow .rs-photo-gallery-container .rs-photo-gallery-images ul.rs-photo-gallery-images-list").css("height", containerHeight + "px");
                $("#hp-slideshow .rs-photo-gallery-container .rs-photo-gallery-images ul.rs-photo-gallery-images-list li").css("height", containerHeight + "px");
                $("#hp-slideshow .rs-photo-gallery-container .rs-photo-gallery-images ul.rs-photo-gallery-images-list li img").css({"width": imgWidth + "px", "height": imgHeight + "px", "margin": "-" + centerHeight + "px 0px 0px -" + centerWidth + "px"});
	}
	
