	<script type="text/javascript">
        jQuery(function($){
			width = $(window).width() * 0.65;
			$(".calculator_green").width(Math.ceil(width));
			width = width - 76; //Padding offset
			w1 = Math.ceil(width*.63);
			$('.graph').width(w1);
            $('.cost-calculator').width(Math.ceil(width*.35));
            $('#compare').click(function(e) {
				_gaq.push(['_trackEvent', 'Cost Calculator', 'click']);
                var $val = $('#select-college').val();
                var $fees = $('#select-college').find('option:selected').data('fees');
                var $college = $('#select-college').find('option:selected').text();
                //console.log($val + " " + $fees + " " + $college);
                if( $val == -1){
                    alert("Please select the college to compare");
                    $(this).focus();
                    return false;
                }

                var $compare_val = $('#select-college-compare').val();
                var $compare_fees = $('#select-college-compare').find('option:selected').data('fees');
                var $compare_college = $('#select-college-compare').find('option:selected').text();

                $('.compare_college_name').text($compare_college);
                $('.college_name').text($college);

                $('.compare_college_fees').text('$'+$compare_fees);
                $('.college_fees').text('$'+$fees);

                $('.difference').text('$'+(commaSeparateNumber((($compare_val - $val).toFixed(2)))));

                $('.graph .invisible').removeClass('invisible');
                //$('.cost-calculator').hide();
				/*$('.cost-calculator').animate({width:'33%'});
				$('.graph').show().animate({width:'66%'});*/
				$(window).resize();

                $bar2_height = ($val/$compare_val)*100;
                $('.bar-2').css('height',$bar2_height+'%');
                $('.bar-1').attr('title',$compare_college);
                $('.bar-2').attr('title',$college);
                $('.bar-1-text').text($compare_college);
                $('.bar-2-text').text($college);
            });

			$("div.single-college:hidden").each(function(){
				var $img = $(this).find("img");
				if($img.attr('src')=='#'){
					$img.attr('src',$img.data('gray_img'));
				}
				//console.log($img);
			})

            function commaSeparateNumber(val){
                while (/(\d+)(\d{3})/.test(val.toString())){
                    val = val.toString().replace(/(\d+)(\d{3})/, '$1'+','+'$2');
                }
                return val;
            }

            $('.graph .close').click(function(e) {
                $('#select-college').find('option:first').prop('selected',true);
                $('#select-college-compare').find('option:first').prop('selected',true);
                $('.graph').hide();
                $('.cost-calculator').show();
				$(window).resize();
            });

			$(window).resize(function(){
				var windWidth = $(window).width();
				$(".college-logo").width(Math.ceil(windWidth*0.65));
                var $parentContainer = $("div.college-container"), $ul = $parentContainer.find(" > ul"), $li = $ul.find("li:first");
                var contWidth = $parentContainer.width();
                var height = contWidth * 0.3072; //$li.height();
                $ul.height(height);
                $ul.find(' > li').each(function(){ $(this).width(contWidth); });
                $ul.width(contWidth * $ul.find(' > li').length);

                //transer-new-college-logo-info
                $ul.find(' > li').each(function(){
                    $(this).find(".transer-new-college-logo-info").height(($(this).height()*0.9425));
                });
            });
            $(window).trigger('resize');

            $(".leftArrow.scroll_arrow").click(function(){
                var $parentContainer = $("div.college-container"), $ul = $parentContainer.find(" > ul"), $li = $ul.find("li:first");
                if($parentContainer.data('page') == 1){return false;}
                var page = $parentContainer.data('page');
                $ul.animate({marginLeft:'+=100%'},100,'',function(){$(window).trigger('resize');});
                $parentContainer.data('page',--page)
                handleScroll();
            })

            $(".rightArrow.scroll_arrow").click(function(){
                var $parentContainer = $("div.college-container"), $ul = $parentContainer.find(" > ul"), $li = $ul.find("li:first");
                if($parentContainer.data('page') == $ul.find(' > li').length){return false;}
                var page = $parentContainer.data('page');
                $ul.animate({marginLeft:'-=100%'},100,'',function(){$(window).trigger('resize');});
                $parentContainer.data('page',++page)
                handleScroll();
                //$(window).trigger('resize');
            })

            handleScroll = function(){
                var $parentContainer = $("div.college-container");
                var page = $parentContainer.data('page'), $ul = $parentContainer.find(" > ul");
                if(page == 1){
                    $(".leftArrow.scroll_arrow").addClass("disable");
                }
                if(page == $ul.find(' > li').length){
                    $(".rightArrow.scroll_arrow").addClass("disable");
                }
                if(page > 1){
                    $(".leftArrow.scroll_arrow").removeClass("disable");
                }
                if(page < $ul.find(' > li').length){
                    $(".rightArrow.scroll_arrow").removeClass("disable");
                }
            }

			$("a.pjax").transferSlide();
        })
    </script>
