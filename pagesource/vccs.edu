		<script type="text/javascript">
				if(typeof $.cookie('geolocation') === "undefined"){
					if (navigator.geolocation) {
						navigator.geolocation.getCurrentPosition(successFunction, errorFunction,{timeout:10000});
						$.cookie('geolocation', (new Date()).getTime(), { expires: 1 , path: '/' });
					}
				}

				function successFunction(position) {
					var lat = position.coords.latitude;
					var lng = position.coords.longitude;
					codeLatLng(lat, lng)
				}

				function errorFunction(){ console.log("geolocation failed"); }

				function codeLatLng(lat,lng){
					$.post("http://www.vccs.edu/college/college-locator-ajax/?geoloc",{"lt":lat,"lg":lng},function(data){ $("#locator_pincode").val(data.zip); $("#locator_button").click(); },"json");
				}

			(function($){
				$("#locator_pincode").keyup(function(event){
					if(event.keyCode == 13){
						$("#locator_button").click();
					}
				});

				$("#locator_button").click(function(){
					var zipcode = $("#locator_pincode").val();
					if(zipcode==""){
						alert("Please provide zipcode to search");
						$("#locator_pincode").focus()
						return false;
					}
					if((zipcode.length < 5) || (zipcode.length > 5)){
						alert("Please provide a valid zipcode to search");
						$("#locator_pincode").focus()
						return false;
					}

					//$("#locator_pincode").val()
					$(this).addClass("open");
					var $collegeList = $(this).next("#college-list");
					$collegeList.addClass("loading").html("").show();
					$.post("http://www.vccs.edu/college/college-locator-ajax/",{"zipcode":$("#locator_pincode").val()},function(data){ $collegeList.removeClass('loading').html(data); if(data == ""){ /*alert("No colleges found");*/ $collegeList.removeClass('loading').html($("<p><strong>No colleges found close to your zip code</strong></p><a href='#' class='close'>&times;</a>")); } });
				})

				$(document).on('click', '#college-list a.close', function(event) {
					event.preventDefault();
					//$.pjax.click(event, "#college-popup #college-content")
					$("#college-list").hide();
				})
			})(jQuery);
        </script>
