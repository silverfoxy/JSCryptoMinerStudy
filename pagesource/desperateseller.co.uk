function getModels( makesel )
{
    var c, m, car, model;
    var make = makesel.options[makesel.selectedIndex].value;

    // always wipe out any prior model
    model = makesel.form.model;
    for ( m = model.options.length-1; m >= 0; --m )
    {
        model.options[m] = null;
    }
    model.options.length = 0;

    // now search for a match on given make
    for ( c = 0; c < cars.length-1; ++c )
    {
        car = cars[c];
        if ( car[0] == make )
        {
            model.options[0] = new Option( "Model","" );
            for ( m = 1; m < car.length; ++m )
            {
                  model.options[m] = new Option( car[m], car[m] );
            }
            return;
        }
    }
    // if we get here, we didn't find that make...why not???
    model.options[0] = new Option("Make","" );
}
</script>

