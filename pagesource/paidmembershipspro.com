<script type="text/javascript">
	
	jQuery(document).ready(function ($) { 	

		jQuery('#pmpro_mod_thread_notes_submit').on('click', function(event){ 

		// hide and show loading gif when user updates notes.
		$('#threads-success-notice').hide();
		$('#pmpro-loading-gif-threads').show();

		event.preventDefault();

		//ajax call
		var ajax_url = "https:\/\/www.paidmembershipspro.com\/wp-admin\/admin-ajax.php";
		var topic_id = 0
			$.ajax({
			url: ajax_url,
			type: "POST",
			data: {
				action : 'pmpro_update_thread_notes',
				pmpro_mod_thread_notes : $('#pmpro_mod_thread_notes').val(),
				topic_id : topic_id

			},
			success : function(response){
				//update text area here with data so page doesn't need to reload.
				$('#pmpro_mod_thread_notes').val(response);
				$('#pmpro-loading-gif-threads').hide();
				
				$('#threads-success-notice').show();
			},
			error : function(){
				console.log('error - update this.');
			}	  
		});	

	});

		
	});		//end of ready function
</script> 
