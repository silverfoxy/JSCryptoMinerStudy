		$button.removeClass("animated shake");
		setTimeout(
				function() {
					$button.addClass("animated shake");
				},
				25
		);
	}
	function flipInyButton($button) {
		console.log('flipIny button')
		$button.removeClass("animated flipInY");
		setTimeout(
				function() {
					$button.addClass("animated flipInY");
				},
				25
		);
	}
