<script type="text/cjs" data-cjssrc="https://dafitistatic-a.akamaihd.net/cms/teste/teste-cms/jquery.jcarousel.min.js"></script>
<script type="text/cjs" data-cjssrc="https://dafitistatic-a.akamaihd.net/js/bootstrap/jquery.cycle.all.js"></script>
<script type="text/cjs">
$(document).ready(function(){

MarcasCarousel = {
init: function(options){
$(options.xpath).css("max-width",parseInt(($(options.xpath).parent().width()-60)/155,10)*$(options.xpath+' ul li').width());
$(options.xpath+' ul').width($(options.xpath+' ul li').length*$(options.xpath+' ul li').width());
$(options.xpath+" ul").prepend($(options.xpath+" ul li:last"));
$(options.xpath+' ul').css({left:-$(options.xpath+' ul li').width()});

$(options.previousButton).unbind().click(function(){
$(options.xpath+" ul").prepend($(options.xpath+" ul li:last"));
$(options.xpath+' ul').css({left: "-="+$(options.xpath+' ul li').width()});
$(options.xpath+' ul').animate({left: "+="+$(options.xpath+' ul li').width()},400);
});

$(options.nextButton).unbind().click(function(){
$(options.xpath+" ul").append($(options.xpath+" ul li:first"));
$(options.xpath+' ul').css({left: "+="+$(options.xpath+' ul li').width()});
$(options.xpath+' ul').animate({left: "-="+$(options.xpath+' ul li').width()},400);
});
}
};

options = {
                xpath: ' #marcas-wrapper-home .icones',
                nextButton: '#marcas-wrapper-home .cright',
                previousButton: '#marcas-wrapper-home .cleft'
};

DmoCarousel = {
    init: function(selector,speed){
        $(selector).find('.dy_unit').hide();
        $(selector).find('.dy_unit').first().show();
        $(selector).find('#nextSlide').click(function(){
             $(selector).find('.dy_unit:visible').fadeOut(speed,function(){
                 if($(this).next().length == 0){
                     $(selector).find('.dy_unit').first().fadeIn(speed);
                 }else{
                    $(this).next().fadeIn(speed);
                 }
             });
        });
        $(selector).find('#prevSlide').click(function(){
             $(selector).find('.dy_unit:visible').fadeOut(speed,function(){
                 console.log();
                 if($(this).prev().length == 0){
                     $(selector).find('.dy_unit').last().fadeIn(speed);
                 }else{
                    $(this).prev().fadeIn(speed);
                 }
             });
        });
    }
};

$('#dy_Homepage_Carousel_Marcas').on('dy_loaded', function (e) {
options = {
                xpath: ' #marcas-wrapper-home .icones',
                nextButton: '#marcas-wrapper-home .cright',
                previousButton: '#marcas-wrapper-home .cleft'
};
            MarcasCarousel.init(options);
});
$('#dy_Carousel_Dafitimag_Home').on('dy_loaded', function (e) {
        DmoCarousel.init('#horizontalCarousel',300);
});
$('.dy_dynamicContent').trigger('dy_loaded');
});
</script>
