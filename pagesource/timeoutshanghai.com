<script type="text/javascript">    
    function combineBlogBarWithZone(zoneType_cssClass) {
        var zoneWithBlogBar = jQuery(".zone.zone_with_blogbar." + zoneType_cssClass);
        var blogBarContainer = zoneWithBlogBar.prev(".blogbar-container");
        if (blogBarContainer && blogBarContainer.length) {
            zoneWithBlogBar.wrap("<div class='topzone-container'/>");
            jQuery(".topzone-container").append(jQuery("<div class='mobile-blogbar-container'>" + blogBarContainer.html() + "</div>"));
            jQuery(".blogbar-container, .topzone-container").wrapAll("<div class='leader-zone'/>");
            blogBarContainer.show();
            jQuery(".tile_tiny.tile_blogs .blogs").on("scroll", checkIfLoadMoreBlogs);

           
            var compositeZone = zoneWithBlogBar.parent(".topzone-container").parent(".leader-zone").wrap("<div class='zone composite-zone'/>").parent();
            zoneWithBlogBar.children(".zone_title").prependTo(compositeZone);
            if (zoneWithBlogBar.find(".show_more_cta__button").length) {
                compositeZone.find(".tiles.blog-container > .tile.tile_tiny.tile_blogs").css("margin-bottom", 0);
            }
        }
    }


    function checkIfLoadMoreBlogs() {
        if (jQuery(this).scrollTop() + jQuery(this).innerHeight() >= jQuery(this)[0].scrollHeight) {
            ajaxLoadMoreBlogs();
        }
    }

    function ajaxLoadMoreBlogs() {
        var ajaxPlaceHolder = jQuery(".tile_tiny.tile_blogs .blogs .blog--step.ajax-placeholder");

        var json_config = ajaxPlaceHolder.parent(".blogs").attr("data-json-config");
        var first_pagination_size = ajaxPlaceHolder.parent(".blogs").attr("data-first-pagination-size");

        jQuery.ajax({
            type: "GET",
            cache: false,
            url: "/HttpHandlersNew/BlogVerticalBarNextPaginationHtmlHandler.ashx",
            data: { "JsonConfig": json_config, "PaginationOrdinal": ajaxPlaceHolder.attr("data-pagination-ordinal"), "FirstPaginationSize": first_pagination_size },
            success: function (htmlSegment) {
                if (htmlSegment == "")
                    ajaxPlaceHolder.hide();
                else {
                    ajaxPlaceHolder.replaceWith(htmlSegment);
                    jQuery(".tile_tiny.tile_blogs .blogs").on("scroll", checkIfLoadMoreBlogs);
                }
            },
            error: function () {
                ajaxPlaceHolder.hide();
            },
            beforeSend: function () {
                jQuery(".tile_tiny.tile_blogs .blogs").off("scroll");
                ajaxPlaceHolder.show();
            }

        });
    }
</script>

