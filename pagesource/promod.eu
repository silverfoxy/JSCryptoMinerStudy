		}

		var articlesvus = localStorage["articlesvus"];

		var _NbArticlesVus;
		if (articlesvus == null) {
			_NbArticlesVus = 0;
		} else {
			var tabArticlesVus = eval(dojo.fromJson(articlesvus));
			if (tabArticlesVus != null && tabArticlesVus != '') {
				_NbArticlesVus = tabArticlesVus.length;
			} else {
				_NbArticlesVus = 0;
			}
		}
	
		if(_NbArticlesVus == '0' || _NbArticlesVus == '') {
			if(document.getElementById('menu-aide-articlesvus')) document.getElementById('menu-aide-articlesvus').className="";
		}else{
			if(document.getElementById('menu-aide-articlesvus')) document.getElementById('menu-aide-articlesvus').className="viewarticles";
		}
	}else{
		if(document.getElementById('menu-aide-articlesvus')) document.getElementById('menu-aide-articlesvus').className="";
	}
}

