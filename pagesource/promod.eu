var OnMenuOpen = function () {

	if (jQuery(this).attr("id") == "it-l0-6"){ jQuery(this).addClass("it-l0-6-on");}  else{  jQuery(this).addClass("item-level0-on");}
	if(jQuery(this).children(".menu-level1")){
		ssmenuitem = jQuery(this).children(".menu-level1");
		timer = setTimeout( function () { OnMenuOpenTimer() }, 200);
	}
}


var ClearTimeOut = function () {
	clearTimeout(timer);
}


//Action quand on quite un sous menu
var OnMenuClose = function () {
	ClearTimeOut();
	if (jQuery(this).attr("id") == "it-l0-6"){ jQuery(this).removeClass("it-l0-6-on");}  else{  jQuery(this).removeClass("item-level0-on");}
	if(ssmenuitem)  ssmenuitem.removeClass("visibility-on");
	ssmenuitem =0;
}

var OnSousMenuClose = function () {
	if(jQuery(this).children(".menu-level1").length){
		jQuery(this).children(".menu-level1").removeClass("visibility-on");
	}
}

var OnTouchSwitch = function () {

	jQuery(this).click(function(e){if(jQuery(this).parent("li").children(".menu-level1").length){return false;}});

	jQuery('#menu-level0 > li').not(jQuery(this).parent("li")).each(OnMenuClose).each(OnSousMenuClose);

