	if(jQuery(this).parent("li").children(".menu-level1").length){
		jQuery(this).parent("li").children(".menu-level1").toggleClass("visibility-on");
	}
}

var OnAllClose = function () {
	jQuery("#menu-level0 > li").each(OnMenuClose).each(OnSousMenuClose);
}


function is_touch_device() {
	try{
		document.createEvent('TouchEvent');
		var touch = true;
	}catch(e){
		var touch = false;
	}
	
	return touch && ('ontouchstart' in window || 'onmsgesturechange' in window);
}

// Procédure d'initialisation
var ssmenuitem = 0;
var timer = 0;
var Init = function () {

