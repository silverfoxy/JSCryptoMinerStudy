        isCounting = false;
        $(document).scroll(function(){

            if($(this).scrollTop() >= $('#content-counter').offset().top-$(window).height()) {
                if(!isCounting){
                    $('#content-counter').addClass("counter")
                    doCounter()
                }
                isCounting = true;
     
            } else {
                isCounting = false;

            }
        });

         window.addEventListener('load', AOS.refresh);

        function commafy( num){
          var parts = (''+(num<0?-num:num)).split("."), s=parts[0], L, i=L= s.length, o='';
          while(i--){ o = (i===0?'':((L-i)%3?'':','))
                          +s.charAt(i) +o }
          return (num<0?'-':'') + o + (parts[1] ? '.' + parts[1] : '');
        }

        function doCounter(){
            $('.counter').each(function() {
              var $this = $(this),
                  countTo = $this.attr('data-count');

              $({ countNum: $this.text()}).animate({
                countNum: countTo
              },

              {

                duration: 12000,
                easing:'linear',
                step: function() {
                    if(isCounting){
                  $this.text(commafy(Math.floor(this.countNum)));
                  }

                },
                complete: function() {
                  $this.text(commafy(Math.floor(this.countNum)));

                }
              });
            });
        }

});

    $(document).ready(function(){
        setTimeout(initScrollAndSlide.bind(this), 4500)
    });

    function initScrollAndSlide(){
        initLogoScroll();
        initLogoSlide();
    }

    var logos;
    function initLogoScroll(){
        console.log("initLogoScroll")
        scrolling_height = $('.scrolling-logos-inner').height()/2;
        scrolling_height_xs = $('.scrolling-logos-inner-xs').height()/2;

        $('.scrolling-logos-inner-xs').css({top:0});
        $('.scrolling-logos-inner-xs').animate ({
            top: '-=' + scrolling_height_xs,
        }, 9000, 'linear', function() {
            //
            initLogoScroll();
        });


        $('.scrolling-logos-inner').css({top:0});
        $('.scrolling-logos-inner').animate ({
            top: '-=' + scrolling_height,
        }, 9000, 'linear', function() {
            initLogoScroll();
        });
    }

    function initLogoSlide(){
        $('.logo-slide-inner').width($('.logo-slide-inner img').width());

        $('.logo-slide-inner').css({left:0});
        $('.logo-slide-inner').animate ({
            left: '-=' + $('.logo-slide-inner').width()/2,
        }, 40000, 'linear', function() {
            initLogoSlide();
        });

    }

