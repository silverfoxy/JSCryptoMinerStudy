    function initLanguageDropdown(){
      enableDropdownClick();
      checkForClicksOutsideDrowdown();
    }

    function enableDropdownClick(){
      $('#language-selector').click(function(e){
        e.preventDefault();
        $(this).addClass('toggled');
        $('#language-selector').unbind('click');
      });
    }

    function checkForClicksOutsideDrowdown(){
      
      if(document.getElementById('language-selector') == null){
        return;
      }
      
      window.addEventListener('click', function(e){   
        if (document.getElementById('language-selector').contains(e.target)){
          // Clicked in box
          console.log('inside box');
        } else{
          // Clicked outside the box
          console.log('OUTSIIIIIIIIde box');
          $('#language-selector').removeClass('toggled');
          enableDropdownClick();
        }
      });
    }


    function getCountryCode(){
        $.ajax({
            type: 'GET',
            url: 'https://api.teletext.io/api/v1/geo-ip',
            async: false,
            beforeSend: function (xhr) {
                if (xhr && xhr.overrideMimeType) {
                    xhr.overrideMimeType('application/json;charset=utf-8');
                }
            },
            dataType: 'json',
            success: function (data) {
                var countryCode = data.alpha2.toLowerCase();
                if(countryCode == 'us'){
                    // Do nothing
                }else{
                    cookieCheck();
                }
            }
        });
    }

    function cookieCheck(){
        window.cookieconsent.initialise({
            palette:{
                popup: {background: "#000"},
                button: {background: "#f1d600"},
            },

            content:{
                message: "This site uses cookies to store information on your computer.<br>Some are essential to make our site work; others help us improve the site. By using this site, you consent to the placement of these cookies.",
                href: "/privacy-policy"
            },
        });
    }  

  </script>
