<script>
    // DOM Ready
    $(function() {

        var $el, $ps, $up, totalHeight;
        var open = [];

        $(".sidebar-box").each(function() {
           if($(this).outerHeight()<750){
               $(this).find(".read-more").hide();
           }
        });

        $(".sidebar-box .dropdown-button").click(function() {

            // IE 7 doesn't even get this far. I didn't feel like dicking with it.
            totalHeight = 30;
            $el = $(this);
            $p  = $el.parent();
            $up = $p.parent();
            $ps = $up.find('span.read-more-span:not(.read-more)');

            // measure how tall inside should be by adding together heights of all inside paragraphs (except read-more paragraph)
            var searchEles = document.getElementById($ps.attr("id")).children;
            for(var i = 0; i < searchEles.length; i++) {

                totalHeight+=searchEles[i].clientHeight;
            }
//            $ps.each(function() {
//                totalHeight += $(this).height();
//                // FAIL totalHeight += $(this).css("margin-bottom");
//            });

            var index = open.indexOf($(this).attr("id"));
            if(index > -1){
                $up.css({
                    // Set height to prevent instant jumpdown when max height is removed
                    "height": totalHeight,
                    "max-height": 9999
                })
                    .animate({
                        "height": 750
                    });
                $p.removeClass().addClass("read-more");
                open.splice(index, 1);
            }
            else{
                $up.css({
                    // Set height to prevent instant jumpdown when max height is removed
                    "height": $up.height(),
                    "max-height": 9999
                })
                    .animate({
                        "height": totalHeight
                    });
                $p.removeClass().addClass("read-more-closed");
                open.push($(this).attr("id"));
            }
            // prevent jump-down
            return false;
         });
      });
</script>









