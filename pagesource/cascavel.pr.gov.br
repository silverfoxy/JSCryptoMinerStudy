    var conteudo = $(this).attr('id').replace("aba", "conteudo");

    $('.conteudos').css('display','none');
    $('#'+conteudo).css('display','block');

  });


</script>

<script type="text/javascript" src="library/jquery.fancybox.pack.js?v=2.0.6"></script>
<script type="text/javascript" src="imagens/fancybox2/helpers/jquery.fancybox-buttons.js?v=1.0.2"></script>
<script type="text/javascript" src="imagens/fancybox2/helpers/jquery.fancybox-media.js?v=1.0.0"></script>
<script type="text/javascript" src="imagens/fancybox2/helpers/jquery.fancybox-thumbs.js?v=2.0.6"></script>

<script>
  $(document).ready(function(){


    if($('#aviso').length > 0){
       
       $('#aviso').fancybox().trigger('click');

       $('#aviso').fancybox({
         overlayShow : true,
         closeClick  : false,
         openEffect  : 'fade',
         closeEffect : 'fade',
      });   


    }  

  });

</script>


</body>
