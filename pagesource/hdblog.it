		// content type
		var contentType = undefined;
		
		// model ids for tracking
		var modelIds = [];
		
		
				
		
				
		$(window).on('load', function () {
			
			if ('serviceWorker' in navigator) {
				
				navigator.serviceWorker.register('/serviceworker.js?v=4');
			}
			
			appStateReady().then(function (appState) {
				
				console.log("app state ready", appState);
				
				// tracking de modelos en las secciones: review, compare y ficha tecnica
				if (['review', 'datasheet', 'compare'].indexOf(contentType) !== -1) {
					
					if (modelIds && $.isArray(modelIds) && modelIds.length > 0) {
						
						var modelIdsToTracking = [];
						
						for (var i = 0; i < modelIds.length; i++)
						(function (i){
							
							removeTrackingData(contentType, modelIds[i]).then(function (ok) {
								
								if (ok) {
									
									modelIdsToTracking.push(modelIds[i]);
								}
								
								// last model! send tracking data
								if (i === modelIds.length - 1) {
									
									if (modelIdsToTracking.length > 0) {
										
										submitTrackingData(contentType, modelIdsToTracking);
									}
								}
							});
						})(i);
					}
					
				} else {
					
					// update subscription
					if (appState.get('updated') !== SUBSCRIPTION_UPDATED_INDEX) {
						
						submitTrackingData();
					}
				}
				
			}).catch(function (reason) {
				
				console.log("app state wrong", reason);
			});
		});
		
	</script>

	

