<script>(function (url, siteUuid, payload) {
    var sessionUuid = localStorage.getItem('hadrian-session-uuid');

    function makeRequest(done) {
        var xhr = new XMLHttpRequest();

        xhr.open('post', url, true);

        xhr.setRequestHeader('content-type', 'application/json');
        siteUuid && xhr.setRequestHeader('x-site-uuid', siteUuid);
        sessionUuid && xhr.setRequestHeader('x-session-uuid', sessionUuid);

        xhr.onreadystatechange = function () {
            if (xhr.readyState === XMLHttpRequest.DONE && xhr.status === 200) {
                done(JSON.parse(xhr.responseText))
            }
        };

        xhr.send(JSON.stringify({
            payload: JSON.parse(payload)
        }));
    }

    makeRequest(function (response) {
        if (response.session.uuid !== sessionUuid) {
            localStorage.setItem('hadrian-session-uuid', response.session.uuid)
        }
    });
})(
