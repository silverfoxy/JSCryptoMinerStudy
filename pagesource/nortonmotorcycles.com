      $(this).children().children('div').addClass('show');
    },
    'mouseleave': function(){
      $('.bikes .bike').each(function(){ $(this).removeClass('show'); });
      $(this).children().children('div').removeClass('show');
    }
    
  });

$('.bike').on('touchend', '.close', function(e){
  e.preventDefault();
  e.stopPropagation();
  if($(this).parent('div').hasClass('show'))
  {
    $('.bike').children().children('div').removeClass('show');
  }
});

});
</script>

