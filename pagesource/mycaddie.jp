<script>
function fncHeaderInitialChange() {
	initial = document.headersearchform.initial.value;
	printHeaderMakerList(initial);
}

function printHeaderMakerList(initial) {
	target_url = 'getclub2m2.php?initial=' + initial;
	httpRequest(target_url, generateHeaderMakerList);
}

function generateHeaderMakerList(data) {
	var lines = data.split("\n");
	var element;

	document.headersearchform.KID.options.length = 0;
	document.headersearchform.KID.options[0] = new Option("", "");
	for (i = 0; i < lines.length; i++) {
		if (lines[i] == '') break;
		element = lines[i].split(",");
		document.headersearchform.KID.options[i+1] = new Option(element[1], element[0]);
	}
}

function fncHeaderMakerChange() {
	KID = document.headersearchform.KID.value;
	BID = document.headersearchform.BID.value;
	if (eval(KID) > 0) printHeaderBrandList(KID, BID);
}

function printHeaderBrandList(maker, brand) {
	target_url = 'getclub2b2.php?maker=' + maker + '&brand=' + brand + '&name=BID';
	httpRequest(target_url, generateHeaderBrandList);
}

function generateHeaderBrandList(data) {
	var lines = data.split("\n");
	var element;

	document.headersearchform.BID.options.length = 0;
	document.headersearchform.BID.options[0] = new Option("", "");
	for (i = 0; i < lines.length; i++) {
		if (lines[i] == '') break;
		element = lines[i].split(",");
		document.headersearchform.BID.options[i+1] = new Option(element[1], element[0]);
	}
}

function changeButton(type) {
	if (document.getElementById('hdrtype' + type).value == '0') {
		document.getElementById('hdr_button_type' + type).className = 'hdr_button_on';
		document.getElementById('hdrtype' + type).value = '1';
	} else {
		document.getElementById('hdr_button_type' + type).className = 'hdr_button_off';
		document.getElementById('hdrtype' + type).value = '0';
	}
}
</script>
