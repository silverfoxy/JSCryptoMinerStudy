<script type="text/javascript">
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
        }
        return "";
    }
    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }

    var cc = getParameterByName("druglabelid");
    var VerifiedSpecialty = getCookie("VerifiedSpecialty");
    var VerifiedSecondarySpecialty = getCookie("VerifiedSecondarySpecialty");
    var InferedSpecialty1 = getCookie("InferedSpecialty1");
    var InferedSpecialty2 = getCookie("InferedSpecialty2");
    var InferedSpecialty3 = getCookie("InferedSpecialty3");
    var isEhsAdBannerEnabled = true;
    var ehs_site="ehs.pro.pdr.pdrnetwork";
    var ehs_zone="";
    var ehs_inter=1;
    var ehs_kw="";
    var ehs_size="";
    var ehs_vpos="";
    var ehs_hpos="";
    var ehs_puid="";
    var ehs_kv=ehs_kv || [];
    ehs_kv.push("iprof=null;");
    if (InferedSpecialty1 != "" && cc !=="") {
        ehs_kv.push("ims1="+InferedSpecialty1+";");
    } else if (VerifiedSpecialty != "") {
        ehs_kv.push("ims1=null;");
    } else {
        ehs_kv.push("ims1=i19;");
    }

    if (InferedSpecialty2 != "" && cc !== "") {
        ehs_kv.push("ims2="+InferedSpecialty2+";");
    } else {
        ehs_kv.push("ims2=null;");
    }

    if (InferedSpecialty3 != "" && cc !== "") {
        ehs_kv.push("ims3="+InferedSpecialty3+";");
    } else {
        ehs_kv.push("ims3=null;");
    }

    ehs_kv.push("rprof=null;");

    if ("" != "") {
        ehs_kv.push("drg=;");
    } else {
        ehs_kv.push("drg=null;");
    }

    if (VerifiedSpecialty != "") {
        ehs_kv.push("pvms1=" + VerifiedSpecialty + ";");
        ehs_kv.push("rms1=" + VerifiedSpecialty + ";");
    } else {
        ehs_kv.push("pvms1=null;");
        ehs_kv.push("rms1=null;");
    }

    if (VerifiedSecondarySpecialty != "") {
        ehs_kv.push("rss1=" + VerifiedSecondarySpecialty + ";");
    } else {
        ehs_kv.push("rss1=null;");
    }

    ehs_kv.push("pvprof=null;");

    // DO NOT CHANGE ANYTHING BELOW
    var ehs_protocol = (document.location.protocol == "https:") ? "https://" : "http://";
    var ehs_tagsrc = ehs_protocol + 'ads.ehealthcaresolutions.com/uat/?' + ehs_site + ';' + ehs_puid + ';' + Math.random() * 1e16;
