<script type="text/javascript">
    //var searchField = document.getElementById("ctl00_MainContentPlaceHolder_DropZone2_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxWidgetHost_widget_Search1_searchText")
    $(document).on('keyup', "#ctl00_MainContentPlaceHolder_DropZone2_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxWidgetHost_widget_Search1_searchText", function (event) {
        if ($(this).val().length < parseInt("3")) {
            // Hide the suggestion box if text is empty.
            $('#suggestions').hide();
        } else {
            //get suggestions. 
            if (event.which != 38 && event.which != 40) {
                getSearchSuggestions("/Custom/AppCode/Utilities/SearchSuggestions.ashx",
                                    "searchTerm=" + encodeURIComponent($("#ctl00_MainContentPlaceHolder_DropZone2_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxWidgetHost_widget_Search1_searchText").val()),
                                    onRequestSuccess,
                                    onRequestFail);
                }

                if (event.which == 38 || event.which == 40) {  // up or down key
                    var selectedIndex = $(".suggestionList li.selected").index();
                    var suggestionCount = $(".suggestionList li").length;

                    if (selectedIndex < 0) {
                        selectedIndex = -1;
                    }

                    if (event.which == 40) {// down key
                        selectedIndex++;
                        if (selectedIndex == suggestionCount) {
                            selectedIndex = 0;
                        }
                    }

                    if (event.which == 38) {// up key
                        selectedIndex--;
                        if (selectedIndex < 0) {
                            selectedIndex = suggestionCount - 1;
                        }
                    }

                    $(".selected").removeClass("selected");
                    $(".suggestionList li").eq(selectedIndex).addClass("selected");
                    $("#ctl00_MainContentPlaceHolder_DropZone2_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxWidgetHost_widget_Search1_searchText").val(htmlDecode($(".selected").data("original-value")));
                }
            }
    });
        function htmlEncode(value) {
            //create a in-memory div, set it's inner text(which jQuery automatically encodes)
            //then grab the encoded contents back out.  The div never exists on the page.
            return $('<div/>').text(value).html();
        }

        function htmlDecode(value) {
            return $('<div/>').html(value).text();
        }
        function getSearchSuggestions(url, data, successEvent, failerEvent) {
            $.ajax({
                type: "GET",
                url: url,
                data: data,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: successEvent,
                failure: failerEvent
            });
        }
        function onRequestSuccess(response) {
            $('#autoSuggestionsList').empty();
            if (response.length > 0) {
                var searchSuggestionsLimit = parseInt("11");
            var searchSuggestionsMaxLength = parseInt("50");
            $.each(response, function (i) {
                var searchTearm = $("#ctl00_MainContentPlaceHolder_DropZone2_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxWidgetHost_widget_Search1_searchText").val().replace(/[\-\[\]\/\{\}\(\)\*\+\?\.\\\^\$\|]/g, "\\$&");
                if (i < searchSuggestionsLimit - 1) {
                    var text = htmlDecode(response[i]).trunc(searchSuggestionsMaxLength);

                    var re = new RegExp("(" + searchTearm + ")", "gi");
                    text = text.replace(re, "<span>$1</span>");
                    var el = $('<li/>').data("original-value", response[i]).html(text);

                    el.mouseover(function () {
                        $(".selected").removeClass("selected");
                        $(this).addClass("selected");
                    }).click(function () {
                        $("#ctl00_MainContentPlaceHolder_DropZone2_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxWidgetHost_widget_Search1_searchText").val(htmlDecode($(this).data("original-value")));
                        search();
                    });

                    $('#autoSuggestionsList').append(el);
                } else {
                    var allResultsLink = $("<div/>").addClass("viewAllSearchResult").html("View all search results...");
                    var el = $('<li/>').data("original-value", searchTearm).html(allResultsLink);
                    el.mouseover(function () {
                        $(".selected").removeClass("selected");
                        $(this).addClass("selected");
                    }).click(function () {
                        search();
                    });
                    $('#autoSuggestionsList').append(el);
                }
            });

            $('#suggestions').show();
        } else {
            $('#suggestions').hide();
        }

    }
    function onRequestFail(msg) {
        $('#suggestions').hide();
    }
</script>
