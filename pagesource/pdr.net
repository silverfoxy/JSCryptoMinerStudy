            $(this).width(totalSubmenuWidth);
            if ($(this).css("left") != "auto") {
                $(this).css("left","auto");
            }
            if ($(this).offset().left + totalSubmenuWidth > $(".wrapper").width()) {
                var left = $(".wrapper").width() - totalSubmenuWidth;
                if (left > 0 ){
                    $(this).offset({ left: left });
                 } else {
                    $(this).offset({ left: 0 });
                 }
            }
        });
    });

