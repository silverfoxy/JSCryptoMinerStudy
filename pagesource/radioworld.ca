<script type="text/template" id="compare_link_widget_template">
    <% if(sessionProductsCount > 0) { %>
        <div class="title">
            <a href="<%= compareLink %>">View Item Comparison</a>
        </div>
        <div class="compare_products_table">
            <table>
                <% _.each(sessionProducts, function(product) { %>
                    <tr>
                        <td class="remove_product" style="vertical-align:top;padding-right: 2px;">
                            <input class="remove_compare_product" type="image" src="https://radioworld.ca/application/modules/core/assets/images/icons/cross-small.png" data-part="<%= product.part_id_for_url %>" data-warehouse="<%= product.warehouse %>" title="Remove this product from your Compare List" />
                        </td>
                        <td>
                            <% if (product.customFields != "undefined") { %>
                                <% _.each(product.customFields, function (field) { %>
                                    <% if (field.name === "WebDescription") { %>
                                        <a href="<%= product.url %>" style="font-size: 10px;"><%= product.manufacturerName + " - " + field.value %></a>
                                   <% } %>
                                <% }); %>
                            <% } %>
                        </td>
                    </tr>
                <% }); %>
            </table>
        </div>
    <% } %>
</script>

