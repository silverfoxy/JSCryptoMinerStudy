                        var $this = $(this);
                        var newwhpnuom = {
                            wh : $this.data("wh"),
                            pn : whpnuom.pn,
                            uom : whpnuom.uom
                        };
                        var displayParams = self.resolveProductUOM(newwhpnuom);

                        if (!_.isNull(displayParams)) {
                            $this.find("span.stock-message").html(displayParams.display.message);

                            var $icon = $this.find("img.stock_indicator_icon");

