<script type="text/javascript">

    $(function () {
        var termTemplate = "<span class='ui-autocomplete-term'>%s</span>";
        var objectTemplate = "<span class='ui-autocomplete-objecttype'>%t</span>";
        var strItemTypes = [];

        $('#Recherche_box').autocomplete({
            //appendTo: "#Recherche_box",
            source: function (request, response) {
                var data_research = $("#Recherche_box").val();
                var data_temp = data_research;
                var data_final = "";
                var nb_Ing = 0;

                while (data_temp.indexOf(',') > 0) {
                    data_final += data_temp.substr(0, data_temp.indexOf(',') + 1);
                    data_temp = data_temp.substr(data_temp.indexOf(',') + 1);
                    nb_Ing++;
                }
                if (data_temp.trim().length < 2 || nb_Ing > 3)
                    return;

                $.ajax({
                    url: '/AutoCompleteResearch/AutoComplete',
                    dataType: "json",
                    contentType: 'application/json, charset=utf-8',
                    data: {
                        search: data_temp,
                        //type: $("#type_Search").val(),
                        
                        SectionId: 110451
                    },
                    success: function (data) {
                        var result = $.map(data, function (item) {
                            var strItemType = " - " + item.type;
                            strItemTypes.push(strItemType);
                            return {
                                label: item.mot + strItemType,
                                url: item.url
                            };
                        })
                        response(result);
                    },
                    error: function (xhr, status, error) {
                        alert(error);
                    }
                });
            },
            open: function(e,ui) {
                var
                    acData = $(this).data('uiAutocomplete'),
                    styledTerm = termTemplate.replace('%s', acData.term);
                    term = new RegExp(acData.term, "gi");
                    

                acData
                    .menu
                    .element
                    .find('li')
                    .each(function() {
                        var me = $(this);

                        var styledMe = me.text().replace(term, styledTerm).replace(strItemType, htmlItemType);

                        for (i = 0; i < strItemTypes.length; i++){
                            var strItemType = strItemTypes[i];
                            var htmlItemType = objectTemplate.replace('%t', strItemType);
                            styledMe = styledMe.replace(strItemType, htmlItemType);
                        }

                        me.html( styledMe );
                    });
            },
            minLength: 2,
            select: function (event, data)
            {
                window.location = data.item.url;
            }
        });

        function send(){
            if(characterCode == 13){
                $("#FormResearch").submit();
            }
        }
    });

</script>
