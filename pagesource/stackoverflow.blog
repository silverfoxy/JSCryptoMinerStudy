			#skeet-bot-interface {
  border: 2px solid #CCC;
  padding: 25px;
  margin: 0 -25px 25px;
  box-sizing: border-box;
  background-color: white;
}

#skeet-bot-interface label {
  display: block;
}

#skeet-bot-interface #question {
  margin: 10px 0;
  width: 100%;
}

#skeet-bot-interface #answer {
  margin-top: 10px;
  border: 2px solid lightblue;
  border-radius: 8px;
  min-height: 4em;
  padding: 25px;
  padding-left: 50px;
  position: relative;
}

#skeet-bot-interface #ask {
  margin: 10px 0;
  padding: 5px 20px;
  background-color: #FF7800;
	color: white;
	text-shadow: gray 3px;
}

#skeet-bot-interface #answer::before {
  position: absolute;
  content: '¶';
  font-size: 2em;
  left: 18px;
  top: 21px;
  color: #444;
}

/* Below: Fix for nested reply in comments not indenting properly */

.comment-list {
        list-style: none;
        margin: 0;
        padding: 0;
}

.comment-list .children {
        list-style: none;
        margin: 0;
        padding: 0;
        padding-left: 4em !important;
}

.children .comment-author .avatar {
	left: -65px !important;
}


/* Below: Fix Comment Box */

.comment {
  margin-top: 24px;
  margin-bottom: 24px;
}
.comments-wrapper .section-title--secondary {
	margin-bottom: 4.5rem;
}
.comment .comment-body p:last-of-type {
	margin-bottom:0.25em;
}
.comment .comment-body p {
	max-width: 728px;
}
.comment-author .avatar {
  float: left;
}
.comment-author .fn,
.comment-author .says {
  position: relative;
  top: -2px;
  line-height: 1.1667;
}
.comment-meta {
  font-size: 12px;
  line-height: 1.1667;
}
.comment-form > p {
  display: flex;
  flex-flow: column nowrap;
}
@media (min-width: 48em) {
	.comment-form > p {
		flex-flow: row nowrap;
	}
	.comment-form > p > label {
  	flex: 0 0 15%;
  	max-width: 200px;
	}
	.comment-form > .form-submit {
		padding-left: 15%;
	}
}
.comment-form > p.comment-notes,
.comment-form > p.logged-in-as {
	display: unset;
}

.comment-form > p.logged-in-as {
	font-size: 1.35rem
}
.comment-form > .form-submit > .submit {
	padding: 1rem;
	background-color: #FFF;
	border: 1px solid #F48024;
	font-size: 15px;
	color: #F48024;
	text-transform: uppercase;
	transition: all 100ms ease-in;
}
.comment-form > .form-submit > .submit:hover {
	background-color: #FDE3CE;
	border-color: #F2720C;
	color: #F2720C;
}
.comment-form > p > input[type="submit"] {
	max-width: 10em;
}

.comment-form > p > textarea,
.comment-form > p > input[type="text"] {
  width: 100%;
  max-width: 37.5em;
}

@media (max-width: 47.99em) {
	.comments-wrapper {
		padding: 48px 0 64px;
	}
}
@media (min-width: 48em) {
	.comments-wrapper {
		flex: 0;
		width: 66.666%;
		margin: 0 auto;
		padding: 0 0 112px;
	}
	.three-x-post-block {
		padding-bottom: 0;
		margin-bottom: 0;
	}
}
.comment-respond .comment-reply-title {
	margin-top: 3rem;
	margin-bottom: 4px;
	font-weight: 700;
	line-height: 1.16667;
}

.m-post {
	margin-bottom: 48px;
}

