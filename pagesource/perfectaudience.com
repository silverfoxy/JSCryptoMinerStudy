  <script>

  PANS = window.PANS || {};
  PANS.utmDetection = (function() {
    function init() {
      var params = document.location.search;
      var currentVal = getCookie();
      var result;
      
      if (params !== "") {
        var pairs = params.slice(1).split('&');
        var utm_params = {};

        var i;
        for(i = 0; i < pairs.length; i++) {
          pair = pairs[i].split('=');
          utm_params[pair[0]] = decodeURIComponent(pair[1] || '');
        }
        
        if (!$.isEmptyObject(currentVal)) {
          currentVal = JSON.parse(currentVal);
          result = $.extend({}, currentVal, utm_params);
        } else {
          result = utm_params;
        }

        if (!$.isEmptyObject(result)) {
          setCookie("signup_params", JSON.stringify(result), 30);
        }
      }
    }
    function getCookie() {
      return $.cookie('signup_params');
    }
    function setCookie(cname, cvalue, exdays) {
      $.cookie(cname, cvalue, { expires: exdays, domain: '.perfectaudience.com', path: '/' });
    }
    var exports = {
      init: init,
      getCookie: getCookie,
      setCookie: setCookie
    };
    return exports;
  })();

  $(document).ready(function() {
    var olarkVisible = true;
    PANS.utmDetection.init();
    function hideOlark() {
      olark('api.box.hide');
      olarkVisible = false;
    }
    function showOlark() {
      olark('api.box.show');
      olarkVisible = true;
    }
    if($(window).width() < 768) {
      hideOlark();
    } else {
      showOlark();
    }
    $(window).on("resize", function() {
      if(($(window).width() < 768) && olarkVisible) {
        hideOlark();
      } else if($(window).width() > 768 && !olarkVisible) {
        showOlark();
      }
    });
  });

  </script>

