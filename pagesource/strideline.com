<script async type="text/javascript">
  jQuery(function($){
    var $slider = $('#instagram')
    var slides = '' 
    var valencia = window.valencia.default
    var token = "198698720.e818a14.49a2b08e955c47cf8410faccd7ee2f59"
    var count = 4 || 5
    var template = '<div class="js-slide"><a class="instagram-img" target="_blank" href="{{link}}"><img src="{{image}}"/></a></div></div>'
    var cb = instaSlider

    if (!token){ return console.warn('Please provide an Instagram ACCESS_TOKEN in your theme settings.') }

    var feed = valencia({
      token: token,
      count: count
    }, function(data){
      if (!data.images){ return console.warn('Bad Instagram API request.') }

      data.images.forEach(function(img){
        slides += template.replace(/\{\{link\}\}/, img.link).replace(/\{\{image\}\}/, img.images.standard_resolution.url)
      })

      $slider.html(slides)

      instaSlider()
    })

    function instaSlider(){
      $slider.slick({
        dots: false,
        infinite: false,
        lazyLoad: 'ondemand',
        slide: '.js-slide',
        slidesToShow: 5,
        slidesToScroll: 1,
