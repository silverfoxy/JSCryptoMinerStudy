<script>
/* sidebar filler snippet */
(function($) {
    var $main = $(".mh-main");
    var $sidebar = $(".mh-sidebar-2");
    $sidebar.css("overflow", "hidden").css("position", "relative");
    $sidebar.append($("<div id='fade-bottom-sidebar'></div>"));

    function fixSidebar() {
        if(!$sidebar.is(':visible')) {
            return;
        }
        var heightContentLeft = $main.outerHeight(true);
        $sidebar.css("height", heightContentLeft);
    }
    
    $(document).ready(fixSidebar);
    $(window).scroll(fixSidebar);
    $(window).resize(fixSidebar);
})(jQuery);
</script>
