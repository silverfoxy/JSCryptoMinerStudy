		$(document).ready(function() {

			$(".apri").hover(
				function(){
					$('#overlay').fadeIn('fast');
					$('#box').fadeIn('slow');
				});
				
				$(".chiudi").click(
				function(){
				$('#overlay').fadeOut('fast');
				$('#box').hide();
				$('#banner_on').hide();
				$('#banner_off').show();
				});
				
				$("#box").mouseout(
				function(){
				$('#overlay').fadeOut('fast');
				$('#box').hide();
				$('#banner_on').hide();				
				$('#banner_off').show();
				});
								
				//chiusura emergenza 
				$("#overlay").click(
				function(){
				$(this).fadeOut('fast');
				$('#box').hide();
				$('#banner_on').hide();
				$('#banner_off').show();
				});
				
				$("#cookie").click(
				function(){
				$('#cookie').fadeOut('fast');
				$('#cookie_text').hide();
				});
				
				$("#cookie_text").click(
				function(){
				$('#cookie').fadeOut('fast');
				$('#cookie_text').hide();
				});
				
		   });     
				
		 </script>	
		 
		 

