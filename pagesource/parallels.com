      $(this).toggleClass('open');
      $(this).find('.glyphicon').toggleClass('glyphicon-chevron-down glyphicon-chevron-up');
    });
    
    $('.splitted-btn').click(function(){
        if(window.innerHeight - this.getBoundingClientRect().bottom < 0) {
            var scroll = window.innerHeight - this.getBoundingClientRect().bottom;
            $('html, body').animate({
                scrollTop: $(window).scrollTop() + scroll*-1
            }, 400);
        }
    });
    
    $('.splitted-btn .btn-lg').bind('blur', function(e){
      setTimeout((function() {
        $(this).closest('.splitted-btn.open').removeClass('open');
        $(this).find('.glyphicon.glyphicon-chevron-up').toggleClass('glyphicon-chevron-down glyphicon-chevron-up');
      }).bind(this), 300);     
    });
    
  });
</script>


