<script type="text/javascript">
$wn(document).bind('csWxCol4Ready', function() {
     CSWxManager.setOption('defaultHandler', 'wrcb');
     CSWxManager.setOption('iconSize', 60);

     var $CDEVcol4wx = $wn('#CDEV-cole4-weather');

     var wxZips  = [
          {station: '37401', name: 'Chattanooga'},     
          {station: '37311', name: 'Cleveland'},
          {station: '37347', name: 'Jasper'},
          {station: '37307', name: 'Benton'},
          {station: '37301', name: 'Altamont'},
          {station: '37327', name: 'Dunlap'},
          {station: '37367', name: 'Pikeville'},
          {station: '37321', name: 'Dayton'},
          {station: '37322', name: 'Decatur'},
          {station: '37303', name: 'Athens'},
          {station: '30752', name: 'Trenton'},
          {station: '30728', name: 'LaFayette'},
          {station: '30736', name: 'Ringgold'},
          {station: '30719', name: 'Dalton'},
          {station: '30705', name: 'Chatsworth'},
          {station: '28906', name: 'Murphy'}
     ];

     CSWxManager.loadConfig(wxZips);

     CSWxManager.ready(function(wx) {
     if (!wx.hasData()) { 
          $CDEVcol4wx.closest('.wnDVUtilityBlock').hide();
          return;
     }

     var cookieName = 'csWxCol4' + wng_pageInfo.affiliateName;
     var defaultStation = WNCookie.get(cookieName);

     // Populate the drop-down box
     $wn.each(wx.listNames(), function(i, name) {
          var $opt = $wn('<option />', {value: i, text: name});
          $CDEVcol4wx.find('.cdev-col4-weather-picker').append($opt);
     });

    // Define hi's and lo's via weather feed
    var hi = wx.fetch('high');
    var lo = wx.fetch('low');

     // Wire up the drop-down 'change' event
     $CDEVcol4wx.find('.cdev-col4-weather-picker').change(function() {
          wx.index($wn(this).val());
          $CDEVcol4wx.find('.cdev-col4-weather-location-name').html($wn(this).find('option:eq(' + $wn(this).val() + ')').text());
          $CDEVcol4wx.find('.cdev-col4-weather-current-temp').html(wx.fetch('temperature') + '&deg;');
          $CDEVcol4wx.find('.cdev-col4-weather-current-high').html('HI ' + hi + '&deg;');
          $CDEVcol4wx.find('.cdev-col4-weather-current-low').html('LO ' + lo + '&deg;');
          $CDEVcol4wx.find('.cdev-col4-weather-icon').css('background-image', wx.fetch('iconcss'));
          $CDEVcol4wx.find('.cdev-col4-weather-sky-condition').html(wx.fetch('sky'));
          $CDEVcol4wx.find('.cdev-col4-weather-picker')[0].checked = (wx.index() === parseInt(defaultStation, 10));
     });
    
    // Handle remembering and forgetting the default city
    $CDEVcol4wx.find('.cdev-col4-weather-saved-location').change(function() {
      if (this.checked) {
        // Expire one year from now
        var expDate = new Date();
        expDate.setFullYear(expDate.getFullYear() + 1);
        
        defaultStation = wx.index();
        WNCookie.set(cookieName, defaultStation, { expires: expDate });
      } else {
        defaultStation = null;
        WNCookie.set(cookieName, defaultStation, { expires: new Date(0) });
      }
    });
    
    // Load up the initial city
    if (defaultStation !== null) {
      $CDEVcol4wx.find('.cdev-col4-weather-picker')
          .val(defaultStation)
          .change()
          .css({
               'opacity':'0',
               'display':'block'
          });
    } else {
      $CDEVcol4wx.find('.cdev-col4-weather-picker')
          .change()
          .css({
               'opacity':'0',
               'display':'block'
          });
    }
    
  });
});

$wn(document).trigger('csWxCol4Ready');
</script></div><script>Worldnow.Helpers.debug('UB Redesign 2015 - Col. 4 Weather(412415)', 'UB_412415');</script></div>
