<script src="js/jquery.bxslider.js"></script>
<script src="js/jquery.parallax.js"></script>
<script type="text/javascript">
$('.bxslider').bxSlider({
	mode: "fade",
	controls: false
});

$(window).load(function() {
	$("#loader").addClass("carregado");
});
	var sWidth = window.innerWidth;
	if (sWidth >= 1024) {
	$(window).load(function() {
		var s = $(".btn-top");
		var pos = s.position();
		var allheight = $("html").height();	
		var windowheight = $(window).height();
		$(window).scroll(function() {
			var windowpos = $(window).scrollTop();
			if (windowpos > 200 ) {
				s.addClass("on");
				s.removeClass("fixed");
				if (windowpos > allheight - windowheight - 110) {
					s.addClass("fixed");
				} else {
					s.removeClass("fixed");
				}
			} else {
				s.removeClass("on");
			}
		});
	});
	$('#unidades .background').parallax({
		frictionY: 0
	});
	$('a[href*="#"]:not([href="#"])').click(function() {
		if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
			var target = $(this.hash);
			target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
			if (target.length) {
				$('html, body').animate({
					scrollTop: target.offset().top
				}, 900, "swing");
				return false;
			}
		}
	});
	} if (sWidth <= 768) {
		$('#unidades nav ul li:not(:nth-of-type(6))').click(function(){
			$(this).toggleClass('active');
			$('#unidades nav ul li').not($(this)).removeClass('active');
			$('html, body').animate({
					scrollTop: $('#unidades').offset().top
			}, 500, "swing");
		});
		$('a[href*="#"]:not([href="#"])').click(function() {
			$('.menu-mobile, .btn-menu-mobile').removeClass('menu-active');
			if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
				var target = $(this.hash);
				target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
				if (target.length) {
					$('html, body').animate({
						scrollTop: target.offset().top - 90
					}, 900, "swing");
					return false;
				}
			}
		});
		$('.btn-menu-mobile').on('click touchstart', function(e) {
			$('.menu-mobile, .btn-menu-mobile').toggleClass('menu-active');
			e.preventDefault();
		});
	}
</script>
