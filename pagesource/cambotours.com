// end 
});

$(window).scroll(function() {
	if(jQuery(window).width() > 768){
		
		var scroll = $(this).scrollTop();
		if(scroll >= 90){
			$('.header_wrapper').css("position","fixed");
			$('.header_wrapper').css("top","0px");
			$('.header_wrapper').css("z-index","999999");
			$('.total-record').css("visibility","hidden");
			$('.header_wrapper').addClass('animated fadeInDown');			
			
		}
		else if(scroll < 90){
			$('.header_wrapper').css("position","absolute");
			$('.total-record').css("visibility","visible");
			$('.header_wrapper').removeClass('animated fadeInDown');			
		}// end if
		
		
	}// end if 
});
