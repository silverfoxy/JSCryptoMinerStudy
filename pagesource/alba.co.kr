				$.ajax({
					url : "/person/notify/NotifyHeaderCntAjax.asp",
					dataType : "text",
					cache : false,
					success : function(data) {
						intPersonAlarmCnt = Number(data);
						if (intPersonAlarmCnt > 0) {
