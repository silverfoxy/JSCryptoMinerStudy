<script type="text/javascript">
var headerFix = function(){
	//상단 Header fixed
	if ($("#HeaderMenuWrap").length > 0 || $("#HeaderMenuBizWrap").length > 0) {
		var ScrHeader = $("#HeaderMenuWrap, #HeaderMenuBizWrap").offset().top;
		var ScrWindow = $(window);
		var ScrBody = $("body");
		var timer;

		function ScrollHeader() {
			var ScrTop = $(this).scrollTop();

			if (ScrTop > ScrHeader) {
				ScrBody.addClass("scroll");
			} else {
				ScrBody.removeClass("scroll");
			}
		}

		ScrWindow.scroll(function() {
			if (timer) {
				window.clearTimeout(timer);
			}

			timer = window.setTimeout(function() {
				ScrollHeader();
			}, 100);
		});

		ScrollHeader();
	}
}
$(function(){
