    var active = 1;
    function popup(count) {
        $('.story__element__image').hide();
        $('.' + count).show();
		$('.' + count).addClass('current');
        $('.slide-prev-count').html(count);
        $('.slideshow-container').fadeIn(300);
        active = count;
    }

	function totalImageCount(){

	    var allStoriesInDom = $('.span8 img');
        var count = 1;
        $.each(allStoriesInDom, function () {

		      count++;

	    });

		return count;
	}

    function addSlideShowEvent() {
        var allStoriesInDom = $('.span8 img');
        var count = 1;
        $.each(allStoriesInDom, function () {

            if($("#paywall").length <= 0)
            {
                var src = this.src;
                $(this).attr('id', count);
                $curr = $(this).prev('div.img-expand');
                if($curr.length <= 0){
                    $curr = $(this).parent('figure').prev('div.img-expand');
                }
                $curr.attr('onclick', 'popup(' + count + ');');
                $(this).attr('onclick', 'popup(' + count + ');');
                var html = '<div id="' + count + '" class="story__element story__element__image ' + count + ' " style="display: none;">' +
                        '<figure><img src="' + src + '"><figcaption></figcaption></figure>' +
                        '</div>';
                $('.slide-image-container').append(html);
                count++;
            }else{

			      $('.img-expanded').hide();
			}
        });
        $('.slide-next-count').html(count - 1);
    }

    function closepopup() {
        $('.slideshow-container').fadeOut(300);
    }

