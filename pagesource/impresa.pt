                $(portletSelector).find('.listContainer').find('[data-articleuri="' + $article.data('articleuri') + '"]').remove();

                var $cloned = $(portletSelector).find('.listContainer').find('.tfkn-playlist-item').first();
                var $container = $cloned.parent();
                var $clone = $cloned.clone().off('click');


                $clone.attr('id', $article.attr('id'));
                $clone.find('.titleContainer h3').text(         $article.attr('data-title'));
                $clone.find('.imageContainer').replaceWith(     $article.find('.imageContainer').clone().off('click'));
                $clone.find('.contentType').replaceWith(        $article.attr('data-contenttype'));
                $clone.find('.durationContainer').replaceWith(  $article.find('.durationContainer').clone().off('click'));

                $container.prepend($clone);

                $clone.attr('data-premium',$article             .attr('data-premium'));
                $clone.attr('data-src',$article                 .attr('data-src'));
                $clone.attr('data-type',$article                .attr('data-type'));
