            Liferay.detach(eventHandlers[i].event, eventHandlers[i].handler);
        }

        Liferay.fire(PortalCommon.Events.BEFORE_DESTROY_PORTLET, { portletId: 'listportlet_WAR_contentportlet_INSTANCE_McmNp7769i2d'});
        PortalCommon.loadAjax(portletUrl, portletSelector, function() {

            PortalCommon.initCustomScrollbar( 'p_p_id_listportlet_WAR_contentportlet_INSTANCE_McmNp7769i2d_' );
