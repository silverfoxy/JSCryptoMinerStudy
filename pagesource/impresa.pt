                sectionUniqueName = $article.data('sectionuniquename'),
                sectionName = $article.data('sectionname'),
                publicationName = $article.data('publicationname'),
                sicPlayListSelector = '.sidePlaylist .listContainer';

        if(event.sender !== 'playlist' && !$article.hasClass('ST-direto')) {

            var portletUrl = 'http://www.impresa.pt/home?p_p_id=listportlet_WAR_contentportlet_INSTANCE_McmNp7769i2d&p_p_lifecycle=0&p_p_state=exclusive&p_p_mode=view&p_p_col_id=column-1&p_p_col_count=7&_listportlet_WAR_contentportlet_INSTANCE_McmNp7769i2d_jspPage=%2Fhtml%2Fview.jsp' +
                    '&contentSource=Section' +
                    '&section=' + sectionUniqueName +
                    '&groupId=' + '' +
                    '&contentType=' + 'videoimpresa' +
                    '&playlistAutoplay=false';

            //clear playlist event handler
            for(i = 0; i < eventHandlers.length; i++) {
                Liferay.detach(eventHandlers[i].event, eventHandlers[i].handler);
            }

            Liferay.fire(PortalCommon.Events.BEFORE_DESTROY_PORTLET, { portletId: portletId});
            PortalCommon.loadAjax(portletUrl, portletSelector, function() {

                //sicnot martellini;

                $(portletSelector).find('.listContainer').find('[data-articleuri="' + $article.data('articleuri') + '"]').remove();

                var $cloned = $(portletSelector).find('.listContainer').find('.tfkn-playlist-item').first();
                var $container = $cloned.parent();
                var $clone = $cloned.clone().off('click');


                $clone.attr('id', $article.attr('id'));
                $clone.find('.titleContainer h3').text(         $article.attr('data-title'));
                $clone.find('.imageContainer').replaceWith(     $article.find('.imageContainer').clone().off('click'));
                $clone.find('.contentType').replaceWith(        $article.attr('data-contenttype'));
                $clone.find('.durationContainer').replaceWith(  $article.find('.durationContainer').clone().off('click'));

                $container.prepend($clone);

                $clone.attr('data-premium',$article             .attr('data-premium'));
                $clone.attr('data-src',$article                 .attr('data-src'));
                $clone.attr('data-type',$article                .attr('data-type'));
                $clone.attr('data-thumbnail',$article           .attr('data-thumbnail'));
                $clone.attr('data-sectionuniquename',$article   .attr('data-sectionuniquename'));
                $clone.attr('data-sectionname',$article         .attr('data-sectionname'));
                $clone.attr('data-articleuri',$article          .attr('data-articleuri'));
                $clone.attr('data-title',$article               .attr('data-title'));
                $clone.attr('data-href',$article                .attr('data-href'));
                $clone.attr('data-contenttype',$article         .attr('data-contenttype'));


                $(portletSelector).find('.listContainer .tfkn-playlist-item').each(function(index) {
                    $(this).find('.articleIndex span').text(index + 1);
                });



                //Add section title to the top of the article list
                if(publicationName == 'sic') {
                    $divTitle.find('.titleContainer').addClass('text');
                    $divTitle.find('.titleContainer').html("<h3>"+sectionName+"</h3>");
                    $sidePlaylistTabs.find('.selected').removeClass('selected');
                    $(sicPlayListSelector).before($divTitle.html());

                }

                PortalCommon.initCustomScrollbar( portletId );
                PortalCommon.init.initPlaylist(portletSelector,false);
                Liferay.fire(PortalCommon.Events.PLAYLIST_REFRESHED, portletSelector);

            });

            //give up atras de giveup
            if(publicationName == 'sicnot') {

                $divTitle.find('.titleContainer').text(sectionName);
                $sidePlaylistTabs.find('.divTitle').remove();
                $sidePlaylistTabs.find('.selected').removeClass('selected');
                $sidePlaylistTabs.find('.listContainer').append($divTitle);
                $divTitle.click(function(){
                    _onPrimaryRequest(event);
                });

            }

        }

