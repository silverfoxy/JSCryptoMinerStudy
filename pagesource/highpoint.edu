.sound {
  width: 40px;
    height: 40px;
    position: absolute;
    cursor: pointer;
    display: inline-block;
    top: 70px;
    right: 10px;
display: none;
}
.sound--icon {
  color: rgba(79, 44, 126, 0.41);
  width: 75%;
  height: 100%;
  line-height: 100%;
  font-size: 40px;
  display: block;
  margin: auto;
}
.sound--wave {
  position: absolute;
  border: 4px solid transparent;
  border-right: 4px solid rgba(79, 44, 126, 0.41);
  border-radius: 50%;
  -webkit-transition: all 200ms;
  transition: all 200ms;
  margin: auto;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.sound--wave_one {
  width: 50%;
  height: 50%;
}
.sound--wave_two {
  width: 75%;
  height: 75%;
}
.sound-mute .sound--wave {
  border-radius: 0;
  width: 50%;
  height: 50%;
  border-width: 0 4px 0 0;
}
.sound-mute .sound--wave_one {
  -webkit-transform: rotate(45deg) translate3d(0, -50%, 0);
          transform: rotate(45deg) translate3d(0, -50%, 0);
}
.sound-mute .sound--wave_two {
  -webkit-transform: rotate(-45deg) translate3d(0, 50%, 0);
          transform: rotate(-45deg) translate3d(0, 50%, 0);
}

