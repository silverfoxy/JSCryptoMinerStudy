text-transform: uppercase;
 }

/**
 * The "shine" element
 */

.fs-btn:after {
  content: "";
  position: absolute;
  top: -110%;
  left: -100%;
  width: 100%;
  height: 100%;
  opacity: 0;
  transform: rotate(130deg);
  
  background: rgba(255, 255, 255, 0.13);
  background: linear-gradient( to right, rgba(255, 255, 255, 0.13) 0%, rgba(255, 255, 255, 0.23) 77%, rgba(255, 255, 255, 0.4) 92%, rgba(255, 255, 255, 0) 100% );
}

/* Hover state - trigger effect */

.fs-btn:hover:after {
  opacity: 1;
  top: -90%;
  left: 130%;
  transition-property: left, top, opacity;
  transition-duration: 2.3s, 0.7s, 0.15s;
  transition-timing-function: ease;
}

/* Active state */

.fs-btn:active:after {
  opacity: 0;
}
