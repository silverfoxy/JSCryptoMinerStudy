<script>// Discover HPU 2017 SCRATCH OFF
(function($) {
if (document.querySelector('.page-template-template-discoverhpu2017') !== null) { 
//SCRATCH OFF
var scratchOff = (function () {
    // rewriting to work programatically
    // function that does all the magic
    function scratchOffBind(scratchElement) {
        // Construct our variables
        var scratch1Canvas = scratchElement.getContext('2d');
        var brushRadius = (scratchElement.width / 100) * 5;
        var img = new Image();

        //question image here
        img.src = $(scratchElement).data('scratchConceal');

        function getBrushPos(xRef, yRef) {
            var scratch1Rect = scratchElement.getBoundingClientRect();
            return {
                x: Math.floor((xRef - scratch1Rect.left) / (scratch1Rect.right - scratch1Rect.left) * $(scratchElement).attr('width')),
                y: Math.floor((yRef - scratch1Rect.top) / (scratch1Rect.bottom - scratch1Rect.top) * $(scratchElement).attr('height'))
            };
        }

        function drawDot(mouseX, mouseY) {
            scratch1Canvas.beginPath();
            scratch1Canvas.arc(mouseX, mouseY, brushRadius, 0, 2 * Math.PI, true);
            scratch1Canvas.fillStyle = '#000';
            scratch1Canvas.globalCompositeOperation = "destination-out";
            scratch1Canvas.fill();
        }

        // bind function to mousemove
        $(scratchElement).mousemove(function (e) {
            var brushPos = getBrushPos(e.clientX, e.clientY);
            if ('buttons' in e){
                if (e.buttons === 1){
                    drawDot(brushPos.x, brushPos.y);
                } else if (e.buttons == null && e.which === 1){
                    drawDot(brushPos.x, brushPos.y);
                }
            }
        });

        // bind function to touch move
        $(scratchElement).on("touchmove", function (e) {
            e.preventDefault();
            var touch = e.originalEvent.changedTouches[0];
            if (touch) {
                var brushPos = getBrushPos(touch.clientX, touch.clientY);
                drawDot(brushPos.x, brushPos.y);
            }
        });

        if (brushRadius < 150) {
            brushRadius = 150
        }

        // setup the canvas on load
        $(img).on('load', function () {
            scratch1Canvas.drawImage(img, 0, 0, scratchElement.width, scratchElement.height);

        });
    }

    function cleanup(scratchElement){
        var scratch1Canvas = scratchElement.getContext('2d');
        scratch1Canvas.globalCompositeOperation = "source-over";
        //scratch1Canvas.clearRect(0,0, scratchElement.width, scratchElement.height);
        scratchOffBind(scratchElement);
    }

    return {
        Bind: scratchOffBind,
        Cleanup: cleanup
    }
})();

function cleanupScratchOff(time){
    console.log('starting cleanup');
    // cleanup
    var scratchClasses = $('.scratch');
    for (var i = 0; i < scratchClasses.length; i++) {
            var element = scratchClasses[i];
            scratchOff.Cleanup(element);
    }
}

function kickoffCleanup(){
    /**
     * This kicks off a cleanup
     */
    setInterval(cleanupScratchOff,60000);
}

// On document load, fire off
$(document).ready(function () {
    var scratchClasses = $('.scratch');
    for (var i = 0; i < scratchClasses.length; i++) {
        // initialize and setup elements
        var element = scratchClasses[i];
        var concealImg = $(element).data('scratchReveal');
        $(element).css({'background-image':"url(" + concealImg + ")"});
        scratchOff.Bind(element);
    }
   kickoffCleanup();
});
  
  
}
})( jQuery );
</script>


