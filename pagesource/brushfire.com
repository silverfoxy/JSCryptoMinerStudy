	<script type="text/javascript">

		// This is the main JS file
		// index

		$(document).ready(function ()
		{
			var screenwidth = $(window).width();
			var $ipad = $(".image-half");
			var $fade2 = $("#fade2");
			var $fade3 = $("#fade3");
			var $trigger1 = $(".white");
			var $trigger2 = $(".js-trigger-2");
			var $trigger3 = $(".scew-right-up");
			var trigger1 = $trigger1.offset();
			var trigger1 = trigger1.top;
			trigger1 = trigger1 / 5;
			trigger1 = trigger1 - 200;
			var trigger2 = $trigger2.offset();
			var trigger2 = trigger2.top;
			var trigger2 = trigger2 - trigger1;
			var trigger3 = $trigger3.offset();
			var trigger3 = trigger3.top;
			var trigger3 = trigger3 - trigger1;
			console.log("sup " + trigger1);

			$(window).scroll(function ()
			{
				var delta = $(window).scrollTop();
				console.log("scrolled thus far = " + delta);
				if (delta >= trigger1)
				{
					if (screenwidth > 480)
					{
						$ipad.css("left", "-20px");
					}
				}
				if (delta >= trigger2)
				{
					if (screenwidth > 480)
					{
						$fade2.css({ "top": "0px", "opacity": "1", "color": "inherit" });
					}
				}
				if (delta >= trigger3)
				{
					if (screenwidth > 480)
					{
						$fade3.css({ "top": "0px", "opacity": "1", "color": "inherit" });
					}
				}
				if (delta < trigger1)
				{
					if (screenwidth > 480)
					{
						$ipad.css("left", "-2000px");
					}
				}
			});

		

		});

		$(".js-new-event").click(function ()
		{
			window.location.href = "https://brushfire.com/new";
		})

	</script>

