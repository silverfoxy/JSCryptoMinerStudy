function BillzMouseOutTimer() {
	return nbMouseOutTimer();
}

function nbMouseOutTimer() {
	if (TimerID) clearTimeout(TimerID);
	TimerID = setTimeout("nbLayerRestore()",5000);
	return true;
}

function RestoreAll() {
	nbMouseoverRestore();
	nbLayerRestore();
	return true;
