			counter2 = 0;
			counter1 = 0;		
			for (counter1=0;counter1 < noisearray.length;counter1++) {						
				if(noisearrayElement == noisearray[counter1]) {							
					search_phrase = array_pruned[counter];
					search_phrase_mid_position = " " + array_pruned[counter] + " ";					
