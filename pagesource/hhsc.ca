function nbMouseoverRestore() {
	var i = 0;
	var o = null;
	if (runMouseOvers) {
		for (i = 0; i < imageList.length; i++) {
			if (aryMoImages[imageList[i]].Moused && (o = aryMoImages[imageList[i]].ptrImage)) {
