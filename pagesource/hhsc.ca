function BillzMouseover() {
	var arg = null;
	if (runMouseOvers) {		
		arg = BillzMouseover.arguments; //args 0)ImageName 1)OriginalSrc 2)array index (caller imagename)		
		nbMouseover(arg[0], arg[1], arg[2]);
	}
	return true;
}
