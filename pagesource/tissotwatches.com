*/
function getCookie() {
return document.cookie.split(';').reduce(function(cookies, cookie) {
cookies[cookie.split('=')[0].trim()] = unescape(cookie.split('=')[1]);
return cookies;
}, {});
}
/**
* Placer le cookie dans le document
*/
function setCookie(name) {
var a = new Date();
a = new Date(a.getTime() +1000*60*60*24);
var name = 'tissot-'+name;
//var value = '8ef6bec3ce1fcdc7-'+name;
var value = 1;
var path = '/';
var domain = gDomain;
document.cookie = name+"="+value+
((path) ? ";path="+path:"")+
((domain)?";domain="+domain:"")+
";expires="+a.toGMTString();
}/**
* Effacer le cookie (passer en révolu)
*/
function deleteCookie (name) {
//console.log(name, 'DELETE');
var name = 'tissot-'+name;
var path = '/';
var domain = gDomain;
document.cookie = name+"="+
((path) ? ";path="+path:"")+
((domain)?";domain="+domain:"")+
";expires=Thu, 01 Jan 1970 00:00:01 GMT";
}
