<script>
	$j(document).on('click', '.configurable-swatch-list li', function(e){
		var superAttrId = $j(this).data('super-attr-id');
		var superAttrValue = $j(this).data('super-attr-val');
		var newImage = false;
		var currentImage = false;
		$j(this).closest('.configurable-swatch-list').find('.selected').removeClass('selected');
		$j(this).closest('.swatchers').find('.validation-advice').remove();
		$j(this).addClass('selected');
		$j(this).closest('.cart-promo-container').find('.cart-promo-image img').each(function(key, img){
			if( $j(img).is(":visible") ){
				currentImage = $j(img)
			}
			if( $j(img).data('super-attr') == superAttrId && $j(img).data('super-attr-value') == superAttrValue ){
				newImage = $j(img);
			}
		});
		if(currentImage && newImage){
			currentImage.hide();
			newImage.show();
		}
	});

	function addToCart(el, url){
		var swatcher = $j(el).closest('.cart-promo-container').find('.selected')
		if(swatcher.length > 0){
			var superAttrId = swatcher.data('super-attr-id');
			var superAttrValue = swatcher.data('super-attr-val');
			if( url.indexOf('___SID') != -1 ){
				url += '&';
			}else{
				url += '?';
			}
			if(superAttrValue && superAttrId){
				url += 'super_attribute['+ superAttrId +']=' + superAttrValue + '&';
				window.location = url;
			}
		}else{
			var swatchList = $j(el).closest('.cart-promo-container').find('.configurable-swatch-list');
			var swatchers =  $j(el).closest('.cart-promo-container').find('.swatchers');
			if(swatchers.find('.validation-advice').length > 0){
				swatchers.find('.validation-advice').hide().fadeIn();
			}else{
				swatchList.before('<div class="validation-advice">This is a required field.</div>');
			}
		}
	}
