    function updatedCheckboxes(){
        var currentIds = JSON.parse('[]');
        var currentList = jQuery('.compare-box');
        var fancyCompareClass = '';
        if(currentList.length > 0){
            currentList.each(function(key,el){
                var currentEl = jQuery(el);
                var currentElPid = currentEl.data('pid');
                if( currentIds.indexOf(currentElPid) >= 0 && ! currentEl.prop('checked')){
                    currentEl.prop('checked', true);
                }else{
                    if(fancyCompareClass){
                        currentEl.addClass(fancyCompareClass);
                        currentEl.addClass('disabled');
                        currentEl.next('.link-compare').addClass(fancyCompareClass);
                    }
                }
            });
        }
    }
    jQuery(document).ready(function(){
        updatedCheckboxes();
    });
