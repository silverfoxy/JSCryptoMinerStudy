	<!-- Facebook -->
	<script>
	
		// Init FB
		window.fbAsyncInit = function() {
			FB.init({
				appId      : '204916559529880',
				cookie     : true,
				xfbml      : true,
				version    : 'v2.8'
			});
		};
  
  		// Login FB
		function fb_login(val) {
			FB.login(function(response) {
				// Check response
				if (response.status === 'connected') {
					FB.api('/me', {fields: 'id'}, function(response) {
						if (response.id) {
							if (val) {
								val = val + '&login=1&facebookid=' + response.id;
							} else {
								val = '?reset=1&login=1&facebookid=' + response.id;
							}
							location = 'https://www.drogisterij.net' + val;
						} else {
							popError( 'Helaas kun je niet inloggen met Facebook.' );
						}
					});
				} else {
					popError( 'Log eerst in bij Facebook.' );
				}
			}, {scope: 'public_profile'});
			
			// Return
			return false;
		}
 
  		// Koppel FB
		function fb_koppel() {
			FB.login(function(response) {
				// Check response
				if (response.status === 'connected') {
					FB.api('/me', {fields: 'id'}, function(response) {
						if (response.id) {
							document.getElementById('fb-status').innerHTML = '<div id="fb-message">Je account wordt gekoppeld.</div>';
							document.getElementById('facebookid').value = response.id;
						} else {
							document.getElementById('fb-status').innerHTML = '<div id="fb-message">Helaas kun je niet koppelen met Facebook. ' + error + '</div>';
						}
					});
				} else {
					document.getElementById('fb-status').innerHTML = '<div id="fb-message">Log eerst in bij Facebook.</div>';
				}
			}, {scope: 'public_profile'});
			
			// Return
			return false;
		}
 
  		// Remove FB
		function fb_remove() {
			document.getElementById('fb-status').innerHTML = '<div id="fb-message">Je account wordt ontgekoppeld.</div>';
			document.getElementById('facebookid').value = '';
		}
 
		// Load the SDK asynchronously
		(function(d, s, id) {
			var js, fjs = d.getElementsByTagName(s)[0];
			if (d.getElementById(id)) return;
			js = d.createElement(s); js.id = id;
			js.src = "//connect.facebook.net/nl_NL/sdk.js";
			fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));

	</script>
