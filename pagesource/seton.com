<script type="application/javascript">

    function showRequestDemoForm(pname) {
        var url = BASE_URL + 'catalog/request/demo';
        Shadowbox.open({
            player: 'html',
            content: '',
            width: 625,
            height: 600,
            options: {
                onFinish: function () {
                    $('sb-loading').show();
                    new Ajax.Request(url, {
                        parameters: {pname: pname},
                        onSuccess: function (transport) {
                            try {
                                var response = transport.responseText;
                                response.evalScripts();

                                $('sb-loading').hide();
                                $('sb-player').update(response);
                            } catch (e) {
                                var message = e.error ? e.error : e.message;
                            }
                        }.bind(this)
                    });
                }.bind(this)
            }
        });
    }

    function saveRequestDemo(formDemo) {
        var dataForm = new VarienForm(formDemo.id, true);
        if(dataForm.validator && dataForm.validator.validate()) {
            $('loader-container').show();
            var url = BASE_URL + 'catalog/request/saveDemo';
            var data = jQuery('#requestdemo-form-validate').serialize();
            try {
                jQuery.ajax({
                    url: url,
                    dataType: 'json',
                    type: 'post',
                    data: data,
                    success: function (data) {
                        if (data.success) {
                            $('demo-success-message').show();
                            $('demo-error-message').hide();
                            jQuery('li.success-msg  ul li span').text(data.msg);
                        } else {
                            $('demo-error-message').show();
                            $('demo-success-message').hide();
                            jQuery('li.error-msg  ul li span').text(data.msg);
                        }
                    },
                    complete: function () {
                        $('loader-container').hide();
                    }
                });
            } catch (e) {
                var message = e.error ? e.error : e.message;
            }
        }

        return false;
    }
