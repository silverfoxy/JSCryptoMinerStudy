var iconClass = function (content_type) {
  content_type = content_type.toLowerCase();
  content_type = content_type.replace(/\s/, '_');
  switch(content_type) {
    case "press_releases":
      return 'rec_pr';
    case "news":
      return 'rec_news';
    case "events":
      return 'rec_event';
    case "insights":
      return 'rec_insight';
    case "alliances":
      return 'rec_alliance';
    case "offerings":
      return 'rec_offer';
    case "success_stories":
      return 'rec_success';
    case "blogs":
      return 'rec_blog';
    case "publications":
      return 'rec_pub';
    case "csc_world":
      return 'rec_pub';
    case "stories":
      return 'rec_pub';
    case "infographic":
      return 'rec_infogfx';
    case "global_homepage":
      return 'rec_hp';
    case "alternate_homepages":
      return 'rec_hp';
    case "zone_homepage":
      return 'rec_hp';
    case "alternate_zone_homepage":
      return 'rec_hp';
    case "ingenious_minds":
    case "ingenious_minds_individual_landing":
    case "ingenious_minds_profile":
      return 'rec_im';
    case "other":
      return 'rec_other';
    default: return 'rec_flexi';
  }
};
$(document).ready(function(){
  var typingTimer;
  var doneTypingInterval = 2000;
  $(".cscSearchPage input[name='q']").on('input propertychange paste', function (e) {
    var value = $(this).val(),
        dom = $(".cscSearchResult ul");
    clearTimeout(typingTimer);
    typingTimer = setTimeout(function(){
        value = value.replace("&","").replace("++","+");
        if(value.length > 2){
          $.ajax({
            url:"/utils/live_search?q="+value.replace("%26","").replace("++","+"),
            cache:!0,
            type:"GET",
            success:function(data){
              if(""!=data&&data.length>2){
                dom.html(data)
                // apply content type classes
                $.each( $('.cscSearchResult ul li div.contentIcon'), function( i, v ){
                  var ct = $(v).data('content-type')
                  if(ct !== undefined || ct !== ""){
                    $(v).addClass(iconClass(ct));
                  }
                } );
              }
            }
          })
        }
    }, doneTypingInterval);
  });
  $('.searchButton').on('click', function(e){
    $(this).parents('form').submit();
  })
  $('.cscCloseSearsh').on('click', function(e){
    $(".cscSearchPage input[name='q']").val('');
    $(".cscSearchResult ul").empty();
  });
  $('.contentIcon').tooltip();
})
</script>

