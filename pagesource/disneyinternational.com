  <script>
// $(document).foundation();
$(".accordion li").on("click", "a:eq(0)", function (event)
{
  var dd_parent = $(this).parent();

  if(dd_parent.hasClass('active'))
    $(".accordion li div.content:visible").slideToggle("normal");
  else
  {
    $(".accordion li div.content:visible").slideToggle("normal");
    $(this).parent().find(".content").slideToggle("normal");
  }
});
</script>
