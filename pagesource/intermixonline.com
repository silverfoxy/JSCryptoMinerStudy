<script>
<!-- GTM datalayer consolidator -->
var gtmConsolidator = GTMConsolidator();
function GTMConsolidator(){
gtmDataLayer = {};
function consolidate(objParam, dtLayer){
var dtLayer = dtLayer || gtmDataLayer;
var objParam = objParam || {};
var obj2keys = Object.keys(objParam);
objParam = JSON.parse(JSON.stringify(objParam));
for(var key in objParam){
if (key in dtLayer && key in objParam && typeof(dtLayer[key]) === 'object') {
if (Array.isArray(objParam[key])) {
Array.prototype.push.apply(dtLayer[key], objParam[key]);
if (key === 'impressions') {
dtLayer[key].splice(15); // Make sure we cap consolidated impressions products count at <=15
}
} else {
consolidate(objParam[key], dtLayer[key]);
}
} else {
dtLayer[key] = objParam[key];
if (key === 'impressions') {
dtLayer[key].splice(15); // Make sure cap consolidated impressions products count at <=15
}
}
};
gtmDataLayer = dtLayer;
}
function pushDataLayer(){
if (!isEmptyObject(gtmDataLayer)){
dataLayer.push(gtmDataLayer);
reset();
}
}
function reset() {
gtmDataLayer = {};
}
function isEmptyObject(obj) {
var name;
for (name in obj) {
if (obj.hasOwnProperty(name)) {
return false;
}
}
return true;
}
return {
consolidate: consolidate,
pushDataLayer: pushDataLayer,
reset: reset
}
}
<!-- End GTM datalayer consolidator -->
</script>
