function customizeOptions(data, externalLinks, keyword) {
    var opts = "<ul>";
    var keyword = keyword.toUpperCase();
    var shortNameIndex = 4;
    for (var i = 0; i < data.length; i++) {
        var choice = data[i];
        var symbol = highlightKeyword(choice.queryResult[6], keyword);
        var price = choice.queryResult[8] != null ? choice.queryResult[8] : "";
        var fullname = highlightKeyword(choice.label, keyword);
        var shortname = highlightKeyword(choice.queryResult[shortNameIndex], keyword);
        opts += "<li class='ui-helper-clearfix'>";
        opts += "<table class='tableHeaderSearch'>";
        opts += "<tr>";
        opts += "<td class='tdOneHeaderSearch'>";
        opts += "<span class='exchange'>&nbsp;<img src='http://lankabd.com/mubasherFileServer/File.Company_Logo/" + choice.queryResult[1] + "_company_logo.png' /></span>";
        opts += "</td>";
        opts += "<td class='tdTwoHeaderSearch'>";
        opts += "<span class='code ui-helper-clearfix'>";
        opts += "<span class='sympol'>" + symbol + "</span>";
        opts += "</span>";
        opts += "</td>";
        opts += "<td  class='tdThreeHeaderSearch'>";
        if (choice.queryResult[7] < 0) {
            opts += "<span class='pricedown'>" + price + "&nbsp;</span>";
        } else if (0 < choice.queryResult[7]) {
            opts += "<span class='priceup'>" + price + "&nbsp;</span>";
        } else {
            opts += "<span class='pricenochange'>" + price + "&nbsp;</span>";
        }
        if (choice.queryResult[7] < 0) {
            opts += "<span class='priceDownHeaderSearch'>&nbsp;</span>";
        } else if (0 < choice.queryResult[7]) {
            opts += "<span class='priceUpHeaderSearch'>&nbsp;</span>";
        } else {
            opts += "<span class='priceStaticHeaderSearch'>&nbsp;</span>";
        }
        opts += "</td>";
        opts += "</tr>";
        opts += "<tr>";
        opts += "<td colspan=3  class='tdThreeHeaderSearch'>";
        opts += "<span class='name'>";
        opts += "<span class='fullname'>" + fullname + "</span>";
        opts += "</span>";
        opts += "</td>";
        opts += "</tr>";
        opts += "</table>";
        opts += "</li>";
    }
    opts += externalLinks + "</ul>";
    return opts;
}
  
function highlightKeyword(text, keyword) {
    if (text != null) {
        return text.replace(keyword, "<span class='highlight'>" + keyword + "</span>");
    } else {
        return '';
    }
}

