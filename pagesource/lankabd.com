var notificationLock = false;
function notify(msg) {
    if (!notificationLock) {
        notificationLock = true;
        $jq('#notification-message').html(msg);
        $jq('#notification-box').fadeIn(500, function () {
            window.setTimeout(function () {
                hideNotification();
            }, 10000);
        });
    } else {
        window.setTimeout(function () {
            notify(msg);
        }, 1000);
    }
}

function notifyFancy(msg) {
//        $jq("#hidden_notification_popuplink").fancybox().trigger('click');
    $jq('#popup-message').html(msg);
    var html = $jq('.notification_popup');
    var ht = "<p>" + msg + "</p>";
    $jq.fancybox(
            ht,
            {
                'autoDimensions':false,
                'width':350,
                'height':'auto',
                'transitionIn':'none',
                'transitionOut':'none'
            }
    );
}


function hideNotification() {
    $jq('#notification-box').fadeOut(1000, function () {
        $jq('#notification-message').html('');
        notificationLock = false;
    });
}

function insertParam(key, value) {
    key = escape(key);
    value = escape(value);
    var kvp = document.location.search.substr(1).split('&');
    var i = kvp.length;
    var x;
    while (i--) {
        x = kvp[i].split('=');
        if (x[0] == key) {
            x[1] = value;
            kvp[i] = x.join('=');
            break;
        }
    }
    if (i < 0) {
        kvp[kvp.length] = [key, value].join('=');
    }
    document.location.search = kvp.join('&');
}

