var currentMarketTime = null;
function getMarketTime() {

    var marketTimeStr = '1521373435583'; // milliseconds';
    if (currentMarketTime == null) {
        var currentTime = new Date(parseFloat(marketTimeStr));
        currentMarketTime = new Date(currentTime.getTime() + currentTime.getTimezoneOffset() * 60 * 1000);
    } else {
        currentMarketTime.setMinutes(currentMarketTime.getMinutes() + 1);
    }
    return currentMarketTime;
}

function addCommas(nStr) {
    nStr += '';
    x = nStr.split('.');
    x1 = x[0];
    x2 = x.length > 1 ? '.' + x[1] : '';
    var rgx = /(\d+)(\d{3})/;
    while (rgx.test(x1)) {
        x1 = x1.replace(rgx, '$1' + ',' + '$2');
    }
    return x1 + x2;
}

