	<script type="text/javascript" src="http://www.normandie-univ.fr/jsp/scripts/jquery-1.8.0.min.js"></script>

<script type="text/javascript">
(function($jq){
	// element clique
	$jq('.menu_item').click(function() {
		// on ferme tous les menus ouverts (sauf celui de la rubrique en cours)
		$jq('.menu_item').next().slideUp();
		// si le sous menu de l'element clique est en display none, alors, on l'affiche
		var ouverturePossible = 'none'==$(this).next().style.display;
		if (ouverturePossible) {
			$(this).next().slideDown();
		}
	});
})(jQuery.noConflict());
</script>

