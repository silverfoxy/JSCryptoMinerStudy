<script>
jQuery('body').on('click', '.fancy-enabled a', function(e){
	e.preventDefault();
	var iframeUrl = jQuery(this).attr('href'),
		commentCount = jQuery(this).children('.commentCount').text(),
		linkToArticle = jQuery(this).data('link-to-post');

	jQuery.fancybox({
		type : 'iframe',
		href : iframeUrl,
		onComplete:function(){
			jQuery('#fancybox-content').append('<div class="post-info"><div class="comments">'+commentCount+'</div><div class="linkToPost"><a href="'+linkToArticle+'">Start the discussion</a></div></div>');
			setTimeout(function(){
				fbContentHeight = jQuery('#fancybox-content').height();
				jQuery('#fancybox-content').height(fbContentHeight + 30);
				jQuery('#fancybox-content iframe').height(340);
				jQuery.fancybox.resize();
			},500);
		}
	});
});

jQuery('.pick-topic').change(function(){
	$thisParent = jQuery(this).parent();
	thisValue = jQuery(this).val();
	jQuery.ajax({
		type: 'GET',
		dataType: 'JSON',
		url: '/api/get_recent_posts/?post_type=video&dev=1&category_name='+thisValue+'&count=3',
		success:function(data){
			var scrollItems = "";
			//console.log('ajax complete',data);
			jQuery(data.posts).each(function(){
				scrollItems += "<div class='fancy-enabled'>";
				scrollItems += "<a href='"+this.custom_fields['wpcf-youtube-url'][0]+"'><img src='"+this.custom_fields['wpcf-thumbnail-url'][0]+"' alt='"+this.title+"' />";
				scrollItems += "<span>"+this.title+"</span>";
				scrollItems += "</a>";
				scrollItems += "</div>";
			});

			$thisParent.find('.items').html(scrollItems);
			$thisParent.find('.topic-video-container').show();
		}
	});
});

