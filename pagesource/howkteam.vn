    <script>
        var timer;
        $(".profile-hover-card").mouseenter(function () {
            var userId = $(this).data("hovercard");
            var pos = $(this).offset();
            var height = $(this).height();
            var width = $(this).width();
            timer = setTimeout(function () {
                $.ajax({
                    method: "GET",
                    url: "/Manage/GetInformationUserCard",
                    data: { id: userId },
                    success: function (res) {
                        $("#tip-profile").css("display", "block");
                        $("#tip-profile").css("top", pos.top + height + 18);
                        $("#tip-profile").css("left", pos.left - width - 10);
                        $("#tip-profile").html(res);
                    }
                });
            }, 300);
        }).mouseleave(function () {
            clearTimeout(timer);
            $("#tip-profile").css("display", "none");
        });

    </script>
	

    
