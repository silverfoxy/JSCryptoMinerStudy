    <script type="text/javascript">
        // click link
        function linkscourse(item) {
            //window.open(item);
            location.href = item;
        }

        //load video
        var hightvideo = parseInt(screen.width * 0.5625);
        var hightposition = parseInt(hightvideo * 0.833333)
        $("#ytplayer").width(screen.width);
        $("#ytplayer").height(hightvideo);
        $(".positionrelative").height(hightposition);

        // onload video
        window.onload = function () {
            var linkyt = $("#ytplayer").attr("data-src");
            $("#ytplayer").attr("src", linkyt);
            $(".positionrelative >img").css("display", "none");
        }

        //load video when resize
        $(window).resize(function () {
            var hightvideo = parseInt(screen.width * 0.5625);
            var hightposition = parseInt(hightvideo * 0.833333)
            $("#ytplayer").width(screen.width);
            $("#ytplayer").height(hightvideo);
            $(".positionrelative").height(hightposition);


            var width = $(window).width();
            if (width <= 800) {
                $('#searchControlBanner').removeClass('input-group-lg');
            }
            else {
                $('#searchControlBanner').addClass('input-group-lg');
            }
        });

    </script>

