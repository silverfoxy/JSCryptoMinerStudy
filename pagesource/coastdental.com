<script type="text/javascript">
$(document).ready(function(){
  // hide #back-top first
  $("#back-top").hide();

  // fade in #back-top
  $(function(){
    $(window).scroll(function(){
      if ($(this).scrollTop() > 100){
		$('#back-top').fadeIn();
      }else{
		$('#back-top').fadeOut();
      }
    });

    // scroll body to 0px on click
    $('#back-top a').click(function(){
      $('body,html').animate({
        scrollTop: 0
      }, 800);
      
      return false;
    });
  });
});
</script>
<script type="text/javascript">
  $('.menu-btn').on('click', function(){
    var elem = $(this),
    item = $('.menu__item'),
    active = 'is-active',
    play = 'menu__item--play';
    
    if(elem.hasClass(active)){
      elem.removeClass(active);
      $(item.get().reverse()).each(function(i){
        var row = $(this);
        setTimeout(function(){
          row.removeClass(play);
        }, 50*i);
        $('.menu').css('z-index','0');
      });
    }else{
      elem.addClass(active);
        item.each(function(i){
        var row = $(this);
        setTimeout(function(){
          row.addClass(play);
        }, 50*i);
        $('.menu').css('z-index','91');
      });
    }
    $('#bottomBoxes').css('opacity', '0').fadeTo(1500, 0.4);

  });
</script>
