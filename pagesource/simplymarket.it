   function ShareFB(url)
   {
	   
		FB.ui({
		  method: 'share',
		  href: url,
		}, function(response){});
		
	}
    
    function TagManagerFireSocial(evento,social,azione,url,object)
    {
		if(social == 'Facebook')
		{
			dataLayer.push({'event':evento,'socialNetwork':social,'socialAction':azione,'socialTarget':url}); 
		}
		else
		{
			event.preventDefault();
			dataLayer.push({'event':evento,'socialNetwork':social,'socialAction':azione,'socialTarget':url}); 
			var link = document.createElement('a');
			link.href = object.href;
			link.target = object.target;
			document.body.appendChild(link);
			var allowDefaultAction = true;
		  
		   if (link.click)
		   {
			  link.click();
			  return;
		   }
		   else if (document.createEvent)
		   {
			  var e = document.createEvent('MouseEvents');
			  e.initEvent(
				 'click'    
				 ,true      
				 ,true      
			  );
			  allowDefaultAction = link.dispatchEvent(e);           
		   }     
		   if (allowDefaultAction)       
		   {
			  var f = document.createElement('form');
			  f.action = object.href;
			  f.target = object.target;
			  document.body.appendChild(f);
			  f.submit();
		   }
		   
		   if(f)
		   {
			f.remove();
		   }
		   if(link)
		   {
			link.remove();
		   }
		}
    }
</script>
