.widget-bottom.right h2.title:after {font-family: fontawesome;content: "\f121";float: right;margin-right: 5px}
.widget-bottom .label_with_thumbs {margin: 0;padding: 0}
.widget-bottom .label_with_thumbs li {width: 100%;display: inline-block;margin: 0 0 0.7em 0;padding: 0 0 0.7em 0;border-bottom: 1px solid #e3e3e3;font-size: 10px;color: #999;list-style: none}
.widget-bottom .label_with_thumbs li:last-child {margin: 0;border:0px}
.widget-bottom .label_with_thumbs .label_thumb {float: left;margin-right: 10px;width: 62px}
.widget-bottom .label_with_thumbs a {font-size: 14px;color: #444;line-height: 1.4em}
.widget-bottom .label_with_thumbs a:hover {color: #008efa}
.widget-bottom .more-box {text-align:right}
.widget-bottom .labelmore {color: #999 !important;clear: both;padding: 2px 8px;text-transform: uppercase;font-size: 50% !important;border: 1px solid #e3e3e3}

/* Widget Bawah Post
   ======================================= */
.widget-bottom.right {float: right;}
.widget-bottom.left {float: left;}
.widget-bottom.left, .widget-bottom.right {width: 49%;}

/* blog-pager
   ======================================= */
#blog-pager{float:left;clear:both;display:block;overflow:visible;width:auto;background-color:#fff;margin:10px auto;border:1px solid #DDD;padding:0;text-align:left;line-height:normal;font-size:13px;font-weight:400}
#blog-pager a{display:inline-block;vertical-align:top;line-height:20px;padding:8px 13px;margin:0;font-size:13px;text-align:center;border-left:1px solid #DDD;font-weight:700;color:#666;transition:color .3s}
#blog-pager a:hover{text-decoration:none;background-color:#fafafa;color:#454545;box-shadow:inset 0 1px 1px rgba(0,0,0,0.1);transition:color .3s}
.current{background-color:#fafafa;display:inline-block;vertical-align:top;line-height:20px;padding:8px 13px;margin:0;font-size:13px;text-align:center;border-left:1px solid #DDD;font-weight:700;color:#666;transition:color .3s}
.current:hover{color:#454545;box-shadow:inset 0 1px 1px rgba(0,0,0,0.1)}
#blog-pager .pages{display:inline-block;font-size:90%;padding:8px 13px;vertical-align:top;line-height:20px;font-weight:700;color:#666}

