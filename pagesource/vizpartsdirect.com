<script type="text/javascript">
function submitMainSearchForm()
{
	if(checkSearchField())
	{
		document.forms['searchForm'].submit();
	}
	else
	{
		//inactiveSearch();
	}
}

function checkSearchField()
{
	if($('#searchField').val()=="Search" || $('#searchField').val()=="")
	{
		alert("Warning: without search key word!");
		//inactiveSearch();
		return false;
	}
	
	return true;
}


//-----------------------------------------
var isSearchActive=0;
var isSearchClick=0;

function activeSearch()
{
	if(isSearchActive==0 && isSearchClick==1)
	{
		if($('#searchField').val()=='Search')
		{
			$('#searchField').val("");
		}
		
		$('#rightMenu').animate({
			 opacity: 0
		}, 200, function()
		{
			$('#rightMenu').hide();
		});
		
		$('#searchField').animate({
			width:'+=100'
		}, 1000);
		
		
		
		isSearchActive=1;
		
	}
}

function inactiveSearch()
{
	if(isSearchActive==1)
	{	
		if($('#searchField').val()=='')
		{
			$('#searchField').val("Search");
		}
	
		$('#searchField').animate({
			width:'-=100'
		}, 1000, function()
		{
			$('#rightMenu').animate({
			 opacity: 1
			}, 200, function()
			{
				$('#rightMenu').show();
			});
		});
		
		isSearchActive=0;
		
	}
}

