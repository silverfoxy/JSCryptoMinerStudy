            $("#choix_size option").each(function () {
                if (!inArray(authorval, $(this).text())) {
                    $(this).hide();                    
                }
                else {
                    $(this).show();
                    if ($("#choix_size option:selected").length == 0)
                        $(this).attr('selected', 'selected');
                }
            });
            //$("#choix_size").val($("#choix_size option:visible:first").val());
