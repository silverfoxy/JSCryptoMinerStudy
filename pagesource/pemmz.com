<script type="text/javascript">
//<![CDATA[
// Bypass default autocomplete
Ajax.Autocompleter.prototype.getUpdatedChoices = function() {
    return false;
};

jQuery('#search').removeClass('required-entry');

// init some useful variables
var priceFormat = {"pattern":"Rp. %s","precision":2,"requiredPrecision":2,"decimalSymbol":",","groupSymbol":".","groupLength":3,"integerRequired":1};
var baseUrl = 'http://www.pemmz.com/';
var baseUrlMedia = 'http://www.pemmz.com/media/catalog/product';
var imgPlaceholder = 'http://www.pemmz.com/media/catalog/product/cache/5/small_image/100x100/9df78eab33525d08d6e5fb8d27136e95/placeholder/default/default_product_img_1.png';

// constructs the suggestion engine
var engine = new Bloodhound({
    datumTokenizer: Bloodhound.tokenizers.obj.whitespace('name'),
    queryTokenizer: Bloodhound.tokenizers.whitespace,
        prefetch: { url: 'http://www.pemmz.com/autocomplete/product/json/', ttl: 120000 },
        limit: 5,
    ttl: 43200000 // milliseconds
	//ttl: 120000
});
var curval = "";
// kicks off the loading/processing of `local` and `prefetch`
engine.initialize();
jQuery('#search').typeahead({
    hint: true,
    highlight: true,
    minLength: 1}, {
    name: 'products',
    displayKey: 'name',
    // `ttAdapter` wraps the suggestion engine in an adapter that
    // is compatible with the typeahead jQuery plugin
    source: engine.ttAdapter(),
    templates: {
        empty: '<div class="empty">' +
                   'No Result' +
               '</div>',
	//footer: '<a id="seemore" href="#">See More</a>',
	footer: function(query) {
		return '<a href="'+baseUrl+ 'catalogsearch/result/?q='+ query.query+'">See More</a>';
		//console.log(query);
	},
        suggestion: function(product) {
            var img = imgPlaceholder;
            if (product.small_image && product.small_image != 'no_selection') {
                img = baseUrlMedia + product.small_image;
            }
            return '<a href="' + baseUrl + product.url_path + '">' +
                        '<div class="image">' +
                            '<img src="' + img + '">' +
                        '</div>' +
                        '<div class="info">' +
                            '<span class="name">' + product.name +  '</span>' +
                            '<div class="price-box">' + formatSuggestionPrice(product, priceFormat) + '</div>' +
                        '</div>' +
                        '<div class="clearer"><!-- --></div>' +
                    '</a>';
        }
    }
}).on('keyup', this, function(e){
	if(e.keyCode == 13) {
		window.location.href = baseUrl + 'catalogsearch/result/?q='+jQuery(this).val();
	}
	curval = jQuery(this).val();
});


jQuery('#search').bind('typeahead:selected', function(obj, product, name) {
    window.location.href = baseUrl + product.url_path;
});

function formatSuggestionPrice(product, priceFormat) {
    var price = '';
    switch (product.type_id) {
        case 'grouped':
        case 'configurable':
        case 'bundle':
            price = '<p class="minimal-price">' +
                        '<span class="price-label">Starting at:&nbsp;</span>' +
                        '<span class="price">' + formatCurrency(product.min_price, priceFormat, false) + '</span>' +
                    '</p>';
            break;
        default:
            //if (parseFloat(product.final_price) < parseFloat(product.price)) {
		if (product.special_price != null) {

				if (product.special_from_date == null || product.special_to_date == null) {
					 price += '<p class="old-price" style="font-size:9px">' +
                             //'<span class="price-label">Regular Price:&nbsp;</span>' +
                             '<span class="price">' + formatCurrency(product.price, priceFormat, false) + '</span>' +
                         '</p>' +
                         '<p class="special-price">' +
                             '<span class="price">' +
                                 //formatCurrency(product.final_price, priceFormat, false) +
                                formatCurrency(product.special_price, priceFormat, false) +
                             '</span>' +
                         '</p>';
				}else {
				var date_from = product.special_from_date;
				var date_to = product.special_to_date;
				var ts_from = new Date(Date.parse(date_from.replace('-','/','g'))).getTime();
				var ts_to = new Date(Date.parse(date_to.replace('-','/','g'))).getTime();
				var ts_now = new Date().getTime();
				if ( ts_now >= ts_from && ts_now <= ts_to ) {
						price += '<p class="old-price" style="font-size:9px">' +
                             //'<span class="price-label">Regular Price:&nbsp;</span>' +
                             '<span class="price">' + formatCurrency(product.price, priceFormat, false) + '</span>' +
                         '</p>' +
                         '<p class="special-price">' +
                             '<span class="price">' +
                                 //formatCurrency(product.final_price, priceFormat, false) +
                                formatCurrency(product.special_price, priceFormat, false) +
                             '</span>' +
                         '</p>';
						 }else {
							price += '<span class="price">' +
                             formatCurrency(product.price, priceFormat, false) +
                         '</span>';
						 }
				}

            } else {
                price += '<span class="price">' +
                             formatCurrency(product.price, priceFormat, false) +
                         '</span>';
            }
    }

    return price;
}
//]]>
</script>
