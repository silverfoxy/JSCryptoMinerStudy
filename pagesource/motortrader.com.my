<script>
function loadSubCategory() {
	var category = jQuery("#autoparts-popup-category").val();
	if (category != '') {
		jQuery.ajax({
			type: 'POST',
			dataType: 'json',
			url: 'http://www.motortrader.com.my/ajax/autopartsSubCategory',
			data: {
				'category':category
			},
			success: function(data){
				if (data.result == 1) {
					var display_html = '';

					display_html += '<option value="">All Sub Category</option>';
					if (data.result == '1') {
						jQuery.each(data.subcategory, function(i, item) {
							display_html += '<option value="' + item.id + '">' + item.name + '</option>';
						});
					}

					jQuery("#autoparts-popup-subcategory").html(display_html).attr('disabled', false);

				} else {
					alert(data.message);
				}
			}	  
		});
	} else {
		jQuery("#autoparts-popup-subcategory").html('<option value="">Sub Category</option>').attr('disabled', true);
	}
}

