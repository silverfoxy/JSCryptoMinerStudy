<script>
  jQuery(window).ready(function() {
    $('.typeahead').on('focus', function() {
        $(this).parent().siblings().addClass('active');
    }).on('blur', function() {
        if (!$(this).val()) {
            $(this).parent().siblings().removeClass('active');
        }
    });

    carkeyword = new Bloodhound({
      identify: function(o) { return o.id_str; },
      queryTokenizer: Bloodhound.tokenizers.whitespace,
      datumTokenizer: Bloodhound.tokenizers.obj.whitespace('name', 'screen_name'),
      dupDetector: function(a, b) { return a.id_str === b.id_str; },
      prefetch: 'http://www.motortrader.com.my/ajax/carkeyword',
      remote: {
        url: 'http://www.motortrader.com.my/ajax/carkeyword?q=%QUERY',
        wildcard: '%QUERY'
      }
    });

    function carkeyworddefault(q, sync, async) {
      if (q === '') {
        async([]);
      } else {
        carkeyword.search(q, sync, async);
      }
    }
    
    jQuery('.typeahead').typeahead({
        highlight: true,
        hint: true,
        minLength: 3
    }, {
        name: 'keyword',
        source: carkeyworddefault,
        limit: 10
    });
  });
  </script>
