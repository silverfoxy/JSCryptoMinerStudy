function nuevoAjax(){
	 var xmlhttp = false;
		if (window.XMLHttpRequest)
		{
			// Para es Mozilla
			xmlhttp = new XMLHttpRequest ();
		} else if (window.ActiveXObject)
		{
			// PARA es IE
			try 
			{
				xmlhttp = new ActiveXObject ("Msxml2.XMLHTTP");
			}
			catch (e)
			{
				// version antigua de IE
				try
				{
					xmlhttp = new ActiveXObject ("Microsoft.XMLHTTP");
				}
				catch (e)
				{
				}
			}
		} 	

	if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
	  xmlhttp = new XMLHttpRequest();
	}

	return xmlhttp;
	}	

	function ir_votar(cant, id_encuesta, contenedor, nomb_opc, contenedor_b)
	{	
		//alert(cant);
		var seleccionado=false;
	 	for(var ctr = 1 ; ctr <= cant; ctr++)
		{ 
			var opc = document.getElementById(nomb_opc+ctr);

			//alert(opc.checked);

			

			if(opc.checked)

			{ 

		    	seleccionado=true;

				var valor=opc.value

		    } 

		}

		

		//alert(seleccionado);

		//si ha seleccionado una opcion

		if (seleccionado)

		{

			

			var cont    = document.getElementById(contenedor);

			cont.style.visibility    = 'hidden';

			cont.style.display       = 'none';

			

			

			var cont_enc = document.getElementById(contenedor_b);

			cont_enc.style.visibility    = 'visible';

			cont_enc.style.display       = '';

	

	//alert(contenedor+"**"+contenedor_b);

			ajax=nuevoAjax();

			ajax.open("GET", "gcw_includes/votar.php?opc="+valor+"&id_encuesta="+id_encuesta+"&contenedor="+contenedor+"&contenedor_b="+contenedor_b,true);

			ajax.onreadystatechange=function() 

			{

				//contenedor.innerHTML="Cargando... <img src='prueba.gif' >";

				cont_enc.innerHTML="<div align='center' class='texto_blanco_12'> Cargando... <img src='gcw_images/loading.gif' ><div>";

				if (ajax.readyState==4) 

				{

					cont_enc.innerHTML = ajax.responseText

				}

			}

			ajax.send(null)

		}

	}	

	

	

