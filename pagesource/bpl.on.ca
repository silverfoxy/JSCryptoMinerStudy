        var Mobilebase64_encoding_map = {"=":"PQ==", "/": "Lw==", "\\":"XA==", "?":"Pw=="};

        var MobileescapeRegExp = function(Mobilestring) {
            return Mobilestring.replace(/([.*+?^=!:${}()|\[\]\/\\])/g, "\\$1");
