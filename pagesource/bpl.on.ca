<script type="text/javascript">
   function MobilesearchEncore(){
        var MobileMobileencoreBaseURLInput,MobileencoreBaseURL, MobilesearchInput, MobilescopeInput, MobilesearchString, MobilescopeString, MobilelocationHref, MobilecharRegExString, Mobilebase64Regex;
       /*Mobilebase64_encoding_map includes special characters that need to be
