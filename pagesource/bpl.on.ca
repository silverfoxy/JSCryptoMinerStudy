            MobilesearchString = MobilesearchInput.value;
            for(var MobilespecialChar in Mobilebase64_encoding_map) {
                MobilecharRegExString = MobileescapeRegExp(MobilespecialChar);
                Mobilebase64Regex = new RegExp(MobilecharRegExString, "g");
                MobilesearchString = MobilesearchString.replace(Mobilebase64Regex, Mobilebase64_encoding_map[MobilespecialChar])
            }
            MobilesearchString = encodeURIComponent(MobilesearchString);

            MobilescopeInput = document.getElementById('encoreSearchLocation');

            if (MobilescopeInput) {
                MobilescopeString = MobilescopeInput.value;
            }

            if (MobilescopeString) {
                MobilescopeString = encodeURIComponent(MobilescopeString);
                MobilelocationHref = MobileencoreBaseURL + "C__S" + MobilesearchString + MobilescopeString +  "__Orightresult__U";
