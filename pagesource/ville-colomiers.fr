function ajoutPageRSSCompte(idpage){
	$('.rssajouter').hide();
	$('.rsssupprimer').hide();

	$.ajax({
		type: "POST",
		url: urlSite+"index.php/moncompte/ajoutPageRSSCompte",
		data: "idpage="+idpage,
		success: function(msg){
				if (msg=='1')
					$('.rssajouter').show('bounce',{}, 500);
				else
					$('.rsssupprimer').show('bounce',{}, 500);

			}
	});
}
