			aEvents = msg.split('|');
			for (var i=0; i<aEvents.length; i++) {
 				aElements = aEvents[i].split(',');
				events[i] = new Array();
				for (var j=0; j<aElements.length; j++) {
					events[i][j]=aElements[j];
				}
			}
		}
	});
	
	
		changedate('return', events);
	
	$("#calendar").show('blind');
});
//--><!]]>
</script>
