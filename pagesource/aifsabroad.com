<script type="text/javascript">
(function (newClass) {
    "use strict";

    var classToFind = "LPMcontainer";
    var settings = {
        attributes : true, 
        childList  : true
    };
    var livePerson = new MutationObserver(function (mutations) {
        for (var i = 0; i < mutations.length; i++) {
            var record = mutations[i].addedNodes;
          
            if (record.length) {
                var node = mutations[i].addedNodes[0];
               
                if (node.nodeName.toLowerCase() === "div" && 
                    node.hasAttribute("class") && 
                    node.classList.contains(classToFind) ) 
                {
                    node.classList.add(newClass);
                    livePerson.disconnect();
                    
                    return;
                }
            }
        }
    });

    document.addEventListener("DOMContentLoaded", function () {
        if ( !document.querySelector("." + classToFind) ) {
            livePerson.observe(document.body, settings);
        }
        else {
            document.querySelector("." + classToFind).classList.add(newClass);
        }        
    }, false);    
}("lpChatTextLink"));
</script>
    
    
    
