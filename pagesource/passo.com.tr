<script type="text/javascript">
    function addGlobalSettings() {
        
        // set the organisation
        G4.Model.Organisation('test');
        
        // set culture
        G4.Model.UpdateCulture('tr-TR', '1055');
        // set the timezone
        //G4.Model.DefaultTimeZone('');
        
        G4.Model.Ticketing.Currency.PositivePattern(3);
        G4.Model.Ticketing.Currency.NegativePattern(8);
        G4.Model.Ticketing.Currency.NegativeSign('-');
        G4.Model.Ticketing.Currency.Symbol('₺');
        G4.Model.Ticketing.Currency.DecimalDigits(2);
        G4.Model.Ticketing.Currency.DecimalSeparator(',');
        G4.Model.Ticketing.Currency.GroupSeparator('.');
        G4.Model.Ticketing.Currency.GroupSizes('3');
        
    }

    (function ($) {
        $(document).ready(function () {
            G4.Model.ApplyPartialBindings.unshift(addGlobalSettings);
        });
    })(jQuery);
</script>

