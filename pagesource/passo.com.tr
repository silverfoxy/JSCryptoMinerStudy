        <script type="text/javascript">
            function formatDate(date) {
                var jsonDate = date;
                var value = new Date(parseInt(jsonDate.substr(6)));
                var ret = value.getDate() + "." + (value.getMonth() + 1) + "." + value.getFullYear();
                return ret;
            }

            function applyBindings() {
                // apply the bindings for the user
                G4.Model.ApplyBindings();

                var loginUrl = '/tr/common/test/login.aspx';

                G4.Model.Facebook.SetAppId('194962910702696');
                G4.Model.Login.LogoutRedirect(loginUrl);

                // apply partial bindings
                if (G4.Model.ApplyPartialBindings() != null && G4.Model.ApplyPartialBindings().length > 0) {
                    loadPartialBinding(0, G4.Model.ApplyPartialBindings().length);
                }
            }

            function loadPartialBinding(index, total) {
                // call the partial binding
                var partialBinding = G4.Model.ApplyPartialBindings()[index];
                partialBinding();
                // check if we can load the next binding
                index++;
                if (index < total) {
                    loadPartialBinding(index, total);
                }
            }

            (function ($, window) {
                'use strict';

                $(document).ready(function () {
                    // validate that the browser is not IE8 or below
                    if ($('html').hasClass('lt-ie9')) {
                        G4.Redirect('/tr/common/test/invalid-ie-browser.aspx');
                    }

                    

                    G4.Model.Login.CurrentUser(applyBindings);
