		function showModels(models) 
		{
			_models			=	models;
			
			var o_makes 	=	'<option value=\'%\'>Select All Makes</option>';
						
			for (var model in models)
			{
				o_makes	=	o_makes += '<option value=\'' + (model.toLowerCase().replace(/\s/g,'_')) + '\'>'+ model  +'</option>';
			}
			
			o_makes			=	'<select name=\'hsmake\' id=\'s-make\'>' + o_makes + '</select>';
			
			$('.megamenu2').find('[name=hsmake]').replaceWith(o_makes);
			$('#mnav_search').find('[name=hsmake]').replaceWith(o_makes);
			
			$('.megamenu2 #s-make, #mnav_search #s-make').off('change').on('change', function(){
				var selected 	= $(this).children(':selected');
				var value		= selected.val();
				
				var o_models	=   '<option value=\'%\' rel=\'start\'>Select All Models</option>';
				
				if (value === '%')
				{
					for (var model in _models)
					{
						for( var m in _models[model])
						{
							o_models	=	o_models += '<option rel=\'' + (model.toLowerCase().replace(/\s/g,'_')) + '\' value=\'' + (m.toLowerCase().replace(/\s/g,'_')) + '\'>' + m + '</option>';
						}
					}	
				}
				else
				{
					for (var model in _models)
					{
						for( var m in _models[model])
						{
							if ((model.toLowerCase().replace(/\s/g,'_')) == value)
							{
								o_models	=	o_models += '<option rel=\'' + (model.toLowerCase().replace(/\s/g,'_')) + '\' value=\'' + (m.toLowerCase().replace(/\s/g,'_')) + '\'>' + m + '</option>';
							}
						}
					}
				}
				o_models	=	'<select name=\'hsmodel\' id=\'s-model\'>' + o_models + '</select>';
				$('.megamenu2').find('[name=hsmodel]').replaceWith(o_models);
				$('#mnav_search').find('[name=hsmodel]').replaceWith(o_models);
			})
		}
