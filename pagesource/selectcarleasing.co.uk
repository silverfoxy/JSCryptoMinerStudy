	$(document).ready(function(){
		$('.headerarea').css({'z-index': 999});
		
		
		$('.ved-header').show()
		$('.ved-header').find('.accept').on('click', function(){
			$.cookie('ved', 'open', { expires: 60, path: '/' });
			$('.ved-header').hide();
			$('.headerarea').css({'z-index': 99999});
			$('html, body').animate({
				scrollTop: 0
			}, 250);
			
		});
		$('.ved-header').find('.close').on('click', function(){
			$('.ved-header').hide();
			$('.headerarea').css({'z-index': 99999});
			$('html, body').animate({
				scrollTop: 0
			}, 250);
		});
		$('.ved-header').find('.mobiletrigger').on('click', function(e){
			e.preventDefault();
			$('.ved-header').addClass('ved-fullscreen').css({'z-index': 9008});
			if ($(window).width() < 600)
			{
				var $height = $(window).height();
				var $vedh	= ($('.ved-header').height() / 2);
				$('.ved-fullscreen').css({'height': $(window).height() + 120});
				$('.ved-banner').css({'margin-top': $vedh + 'px'});
			}
			$('.ved-header').find('.ved-banner').removeClass('mobilenoshow');
			$('.ved-header').find('.ved-splash').removeClass('mobilenoshow');
			$('.ved-header').find('strong').addClass('mobilenoshow');
			$('.ved-header').find('ol').removeClass('mobilenoshow');
			$('.ved-header').find('.ved-buttons').removeClass('mobilenoshow');
			$('.ved-header').find('.close').removeClass('mobilenoshow');
			$('.ved-header').find('a.read-more').removeClass('mobilenoshow');
		});
		
		$(window).scroll(function(){
		
			if ( $(document).scrollTop() > 0 ) {
				if ($(window).width() < 600 && (!$('.ved-header').hasClass('ved-fullscreen')))
				{
					$('.ved-header').hide();
				}
			} 
			else 
			{
				if ($(window).width() < 600 && (!$('.ved-header').hasClass('ved-fullscreen')))
				{
					$('.ved-header').show();
				}
			}

		});
		
	});
	
