<script type="text/javascript">
	var ad_count = total_ad_count = $(".featured_listing", "#featured_adlist").length;
	var current_slide_width = 0;
	var slider;
	var stop = 0, start = 0;

	var resize_featured_list = function(use_slider, enable_controls) {
		var window_width = $("#main").width() || $("#container").width();
		var old_slide_width = current_slide_width;
		ad_count = total_ad_count;
		if (total_ad_count) {
			if (window_width >= 1531 && ad_count > 6) {
				ad_count = 6;
			} else if (window_width >= 1226 && window_width < 1531 && ad_count >= 5) {
				ad_count = 5;
			} else if (window_width >= 931 && window_width < 1226 && ad_count > 4) {
				ad_count = 4;
			} else if (window_width >= 616 && window_width < 931 && ad_count > 3) {
				ad_count = 3;
			} else if (window_width >= 480 && window_width < 616 && ad_count > 2) {
				ad_count = 2;
			} else if (window_width < 480) {
				ad_count = 1;
			}
			current_slide_width = Math.floor(((window_width)/ad_count)-(use_slider ? 6 : 14));
			if (!BrowserDetect.show_mobile_mode() && current_slide_width > 300) {
				current_slide_width = 300;
			}
			if (use_slider) {
				if (typeof slider == 'undefined') {
					load_slider(enable_controls);
				} else if (old_slide_width != current_slide_width) {
					slider.reloadSlider(get_slider_option(enable_controls));
					reset_slider_wrapper_width();
				}
				if (!BrowserDetect.show_mobile_mode()) {
					var wrapper_width = ((300*ad_count)+((ad_count+1)*5));
					$("#featured_adlist").css('max-width', wrapper_width+"px");
				}
				$("a", ".bx-controls-direction, .bx-pager-item").on('click', function(e) {
					slider.stopAuto();
					stop++;
					setTimeout(function() {
						start++;
						if (start == stop) {
							slider.startAuto();
						}
					}, 4000)
				});
			} else {
				$('li', '#featured_adlist ul').show();
				$(".featured_listing").css('min-width', current_slide_width+'px');
			}
			$(".caption, .price", ".featured_listing").removeClass('hidden');
		}
	};

	load_slider = function(enable_controls) {
		slider = $('.slider').bxSlider(get_slider_option(enable_controls));
		reset_slider_wrapper_width();
	};

	get_slider_option = function(enable_controls) {
		var auto = (total_ad_count > ad_count);
		var infiniteloop = (total_ad_count > 1);
		var touch_enabled = (total_ad_count > 1);
		var pause = 6000;
		if (ad_count == 5) {
			pause = 5500;
		} else if (ad_count == 4) {
			pause = 5000;
		} else if (ad_count == 3) {
			pause = 4500;
		} else if (ad_count == 2) {
			pause = 4000;
		} else if (ad_count == 1) {
			pause = 3000;
		}
		return {
			slideMargin: 5,
			pager: (!BrowserDetect.show_mobile_mode() && auto),
			controls: (!BrowserDetect.show_mobile_mode() && auto),
			auto: auto,
			autoHover: true,
			minSlides: 1,
			maxSlides: 6,
			moveSlides: 0,
			slideWidth: current_slide_width,
			responsive: (!BrowserDetect.show_mobile_mode()),
			speed: 1000,
			pause: pause,
			infiniteLoop: infiniteloop,
			touchEnabled: touch_enabled,
			autoControls: (enable_controls),
			autoStart: (!enable_controls)
		};
	};

	reset_slider_wrapper_width = function() {
		if (ad_count > 1) {
			$(".bx-wrapper", "#featured_adlist").css('max-width', $("#featured_adlist").width());
		}
	};

	var chosenVariation = 2;
	if (BrowserDetect.show_mobile_mode() || typeof chosenVariation == 'undefined' ) {
		chosenVariation = 0;
	}
	// Define JavaScript for each page variation of this experiment.
	var pageVariations = [
		function() {
			resize_featured_list(true, false);
		},
		function() {
			resize_featured_list(false, false);
			$('li:gt('+((ad_count*2)-1)+')', '#featured_adlist ul').hide();
			$('#featured_adlist').css({ height:'auto', position:'inherit' });
			$('ul', '#featured_adlist').css({ padding:0, height:'auto', position:'inherit', "white-space":'normal' });
		},
		function() {
			resize_featured_list(false, false);
			$('li:gt('+((ad_count*3)-1)+')', '#featured_adlist ul').hide();
			$('#featured_adlist').css({ height:'auto', position:'inherit' });
			$('ul', '#featured_adlist').css({ padding:0, height:'auto', position:'inherit', "white-space":'normal' });
			if ($('li:hidden', '#featured_adlist ul').length) {
				$('#toggle_featured_ads').parent('div').removeClass('hide');
			}
		}
	];

	$(function() {
		pageVariations[chosenVariation]();
		$(window).resize(function() { pageVariations[chosenVariation](); });

		$(".featured_listing").on('mouseover', function(e) {
			$('.price', $(this)).css('background-color', 'rgba(255, 255, 204, 0.9)').css('color', '#000');
			$('.caption', $(this)).css('background-color', 'rgba(255, 255, 204, 0.7)').css('color', '#000').css('font-weight', 'bolder');
		}).on('mouseout', function(e) {
			$('.price', $(this)).css('background-color', '').css('color', '');
			$('.caption', $(this)).css('background-color', '').css('color', '').css('font-weight', 'normal');
		});
				$(document).on('click', '#toggle_featured_ads', function(e) {
			e.preventDefault();
			var shown = $(this).data('toggle');
			if (!shown) {
				$('li', '#featured_adlist ul').show();
				$(this).html('<i class="icon-collapse-alt icon-fw"></i>Show Less').data('toggle', 1);
			} else {
				$('li:gt('+((ad_count*3)-1)+')', '#featured_adlist ul').hide();
				$(this).html('<i class="icon-expand-alt icon-fw"></i>Show All').data('toggle', 0);
			}
		});
			});
</script>



