  background-size: cover;
  width: 27px;
  height: 24px;
  display: block;
}

.breadcrumb-chevron {
  background: url('data:image/svg+xml;base64,') center  center no-repeat;
  height: 10px;
  transform: rotate(270deg);
  width: 16px;
  color: black;
}

[data-x-icon], [class*="x-icon-"] {
  font-family: "FontAwesome" !impoortant;
}
.x-icon-angle-right {
  font-size: 12px !important;
  font-weight: 400 !important;
  font-family: "FontAwesome" !important;
  content: "\f054" !important;
  padding-left: 2px !important;
  padding-right: 2px !important;
}

/** SINGLE NEWs PAGE **/
body.single.single-post .x-breadcrumbs-wrap {
  text-align: left;
}
body.single.single-post .x-breadcrumbs-wrap * {
  margin-bottom: 23px;
  font-size: 13px;
  font-weight: 600 !important;
  letter-spacing: 1px;
}
body.single.single-post .x-breadcrumbs a {
  color: black !important;
}
body.single.single-post .x-breadcrumbs a:hover,
body.single.single-post .x-breadcrumbs a:focus, 
body.single.single-post .x-breadcrumbs a:active {
  color: #666666 !important;
}
body.single.single-post .x-icon-angle-right {
  font-size: 18px !important;
  font-weight: 600 !important;
  font-family: "FontAwesome" !important;
  content: "\f054" !important;
  padding-left: 0px !important;
  padding-right: 0px !important;
}
body.single.single-post .x-header-landmark {
  border: none;
  border: 0;
}
body.single.single-post .entry-title:before {
  content: "";
}
body.single.single-post .flex-direction-nav a, 
body.single.single-post .flex-control-nav a:hover, 
body.single.single-post .flex-control-nav a.flex-active, 
body.single.single-post .x-dropcap, 
body.single.single-post .x-skill-bar .bar, 
body.single.single-post .x-pricing-column.featured h2, 
body.single.single-post .h-comments-title small, 
body.single.single-post .x-pagination a:hover, 
body.single.single-post .x-entry-share .x-share:hover, 
body.single.single-post .entry-thumb, 
body.single.single-post .widget_tag_cloud .tagcloud a:hover, 
body.single.single-post .widget_product_tag_cloud .tagcloud a:hover, 
body.single.single-post .x-highlight, 
body.single.single-post .x-recent-posts .x-recent-posts-img::after, 
body.single.single-post .x-portfolio-filters {
  background: white;
}

body.single.single-post .entry-featured {
  background-color: #ffffff;
  border: medium none;
  margin-top: 25px;
  padding: 0;
  position: relative;
}
body.single.single-post .x-entry-share {
  border: none;
  border: 0;
  padding-top: 60px
}
body.single.single-post .x-share,
body.single.single-post .x-share:hover {
  color: black;
}
body.single.single-post .x-share:hover {
  background: black !important;
  color: #ffffff;
}
/** SINGLE NEWs PAGE END **/

