<script type="text/template" id="search-results-products-template">
	<div class="search-item" id="Product-<%= Product.id %>">
		<a href="<%= Product.url %>">
			<img src="<%= Product.image %>" alt="<%- Product.final_name %> Image" class="pull-left" />
			<img src="/theme/RVCA/img/sale-triangle.png" class="<%= Product.sale_status %> sale-triangle" style="display: none;" />
			<div class="item-text">
				<span class="title"><%= Product.tease %></span>
				<span class="price"><%= Product.promo_price.formatMoney(2, '.', ',') %> <span class="regular-price <%= Product.sale_status %>" style="display: none;"><%= Product.price.formatMoney(2, '.', ',') %></span></span>
			</div>
		</a>
	</div>
</script>

