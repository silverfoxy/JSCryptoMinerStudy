<script type="text/template" id="shipping-suggestions-template">
	<div class="shipping-suggestions">
		<h4>Please select the best address format</h4>
			<ul>
                                <li>
                                    <span class="title">Original</span><br>
                                    <input type="radio" class="shipping-address-suggestion" name="address_item" value="0" ><div id="user_original_address" class="address_data"></div>
                                </li>
                                <li><span class="title">Recommended</span></li>
                        	<%
					var names = [];
					_.each(suggestions, function(suggestion){
						if (_.indexOf(names, suggestion.name) < 0) {
						%>
							<li><input class="shipping-address-suggestion" type="radio" name="address_item" value="0" data-name="<%= suggestion.name %>"><div class="address_data"><%= suggestion.adr_address %></div></li>
						<%
						}
						names.push(suggestion.name);
					});
				%>
                                
				<li><input type="button" value="continue"  id="address_selection"></li>
			</ul>
	</div>
</script>		
		
				

		
