                $wads         = Array.prototype.filter.call(document.querySelectorAll("script"),function(e){return !e.classList.contains("analytics") && e.textContent.indexOf("createAdPosition('")>-1}),
                $adsense      = document.querySelectorAll(".adsbygoogle"),
                $outbrain     = document.querySelectorAll(".OUTBRAIN");

            Array.prototype.forEach.call($wads,function(e){
                var pos = /'(.*?)'/.exec(e.textContent)[1];
                if (typeof(wads.ads) != "undefined" && typeof(wads.ads[pos]) != "undefined" && typeof(wads.ads[pos][0].empty) == "undefined") {
                    wadsFilled["filled"] += '{'+pos+'}';
                } else {
                    wadsFilled["emptied"] += '{'+pos+'}';
                }
            });
            Array.prototype.forEach.call($adsense,function(e,i){
                otherFilled[e.innerHTML != "" ? "filled" : "emptied"] += '{adsense-'+(i?'bottom':'top')+'-'+e.getAttribute("data-ad-slot")+'}';
            });
            Array.prototype.forEach.call($outbrain,function(e,i){
                otherFilled[!wadsBlocking ? "filled" : "emptied"] += '{outbrain-'+e.getAttribute("data-widget-id")+'}';
            });

                        ga('send', 'event', 'ads', 'inventory', {
              'eventLabel': location.href,'dimension34':wadsFilled.filled + wadsFilled.emptied,'dimension37':otherFilled.filled + otherFilled.emptied,
              'nonInteraction': true
            });

            ga('send', 'event', 'ads', 'filled', {
              'eventLabel': location.href,'dimension34':wadsFilled.filled,'dimension37':otherFilled.filled,
              'nonInteraction': true
            });

            ga('send', 'event', 'ads', 'emptied', {
              'eventLabel': location.href,'dimension34':wadsFilled.emptied,'dimension37':otherFilled.emptied,
              'nonInteraction': true
            });
        }, 2000);
    </script>
    
    <script src="/js/9f1aaa4-9a2fca3.js?1"></script>

    
