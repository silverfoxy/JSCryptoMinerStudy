                    $('.ui-dialog-titlebar').hide();
                    $('.ui-dialog .ui-dialog-content').css('padding', '0');
                    $('#videoLightbox').css('padding', '20px 0 1px').css('overflow', 'hidden');
                    dialogSize();
                    $('#modalClose').click(function(){
                        $('#videoLightbox').dialog('close');
                    })
                }
            });
            $("#videoLightbox").bind("dialogclose", function (event, ui) {
                $('#theVideo').attr('src', '');
            });

        });

        function dialogSize(){
            var wWidth = Math.min(1000, $(window).width());
            var wHeight = wWidth * 0.8 * 0.5625;
            $('#theVideo').attr('width', wWidth * 0.80);
            $('#theVideo').attr('height', wHeight);
            $('#videoLightbox').dialog('option', 'position', { my: "center", at: "center", of: window });
        }

        function SetVideo(source, title) {
            $('#theVideo').attr('src', source);
            $('#videoLightbox').dialog("option", "title", title);
            $('#videoLightbox').dialog('open');
            $('.ui-widget-overlay').click(function () { $('#videoLightbox').dialog('close'); });
        };

    </script>
