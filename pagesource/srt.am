<script>
   var copy = 0;
   $("#short").click(function(){
    var myUrl = $("#myUrl").val();
    if (copy == 0) {
      $.ajax({
          type: "POST",
          url: "short_home.php",
          data: { myUrl: myUrl},
          cache: false,
          success: function(html){
            if(html.indexOf("http://short.am/") != -1){
               $("#myUrl").val(html);
               $("#short").html("<strong><i class='fa fa-copy'></i></strong>");
               $('.target').toggle('slow');
               copyURL(html);
               copy = 1;
            } else {
              $("#myUrl").val(html);
            }
                       
          }
      });
    } else {
      $("#short").html("<strong><i class='fa fa-chain-broken'></i></strong>");
      $('.target').toggle('slow');
      $('.copiedtext').toggle('slow');
      $("#myUrl").val("");
      copy = 0;
    }
   });
</script>

<script type="text/javascript">
   function copyURL(myUrl) {
     var client = new ZeroClipboard($('#short'));
   
     client.on( 'ready', function(event) {
   
       client.on( 'copy', function(event) {
         event.clipboardData.setData('text/plain', myUrl);
       } );
   
       client.on( 'aftercopy', function(event) {
         ZeroClipboard.destroy();
       } );
     } );
   
     client.on( 'error', function(event) {
       ZeroClipboard.destroy();
     } );
     }
</script>
