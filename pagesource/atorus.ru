	$(document).ready(function() {
		var foo = new Date;
		var unixtime = parseInt(foo.getTime()/1000) - (foo.getTimezoneOffset()*60) - (3*60*60);
		$.cookie('uto', unixtime, {expires: 7, path: '/', domain: 'www.atorus.ru'});
