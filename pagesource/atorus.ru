		$("a.fancy").fancybox();
		$(".reg_email").validate();
					$("#inlinef").fancybox({'hideOnContentClick': false, 'autoScale': false, 'frameWidth': 300, 'frameHeight': 150});
		
		var loc = window.location.hash.replace("#","");
		if (loc == "fdata") {
			$('#inlinef').click();
		}

		$("body").on("click", "#form_ebtn", function(){
			var link = $("#inlinef").attr('lang')+'email/'+$(this).parent().find('#form_email_in').val()+'.html';
						if (email_filter($(this).parent().find('#form_email_in').val()))
			{
				$('.emeil_in').removeClass('eerr');
				document.location.href=link;
			} else $('.emeil_in').addClass('eerr');
			
		});
	});

	function send_mail(event, value){
        var code = (event.keyCode ? event.keyCode : event.which);
	    if(code == 13) { // Код клавиши ENTER
	        var link = $("#inlinef").attr('lang')+'email/'+value+'.html';
						if (email_filter(value))
			{
				$('.emeil_in').removeClass('eerr');
				document.location.href=link;
			} else $('.emeil_in').addClass('eerr');
	    }
    };

	function email_filter(mail) {
        if(mail != '') {
            var pattern = /^([a-z0-9_\.-])+@[a-z0-9-]+\.([a-z]{2,4}\.)?[a-z]{2,4}$/i;
            if(pattern.test(mail)){
                return true;
            } else {
                return false;
            }
        } else {
            return false;
        }
    };

