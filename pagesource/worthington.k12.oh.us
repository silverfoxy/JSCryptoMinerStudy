		        hasIcons = 1;
		}
		if(youtubeCheck == "true"){
		        var youtubeColor = '#bb0000';
		        $(".icn.youtube").removeClass("hidden").css("color", youtubeColor);
		        hasIcons = 1;
		}
		if(pinterestCheck == "true"){
