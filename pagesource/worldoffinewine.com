          if ($("#hidCurrentPage") != null && $("#hidCurrentPage").val() != null) {
              var year = $('#hid_yearofWOFW').val();
              $.getJSON('/Home/GetWineListAwardsSearch/' + year, { 'tblName': 'Table2' }, function (data) {
                  var datas = $.parseJSON(data);
                  var arrVenueName = [];
                  var arrCountry = []; var arrZipCode = []; var arrCityName = [];
                  var arrRegion = [];
                  for (var x in datas) {
                      //debugger;-
                      if (datas[x].VenueName != null && datas[x].VenueName != undefined)
                          arrVenueName.push(datas[x].VenueName);
                      else if (datas[x].Country != null && datas[x].Country != undefined)
                          arrCountry.push(datas[x].Country);
                      else if (datas[x].ZipCode != null && datas[x].ZipCode != undefined)
                          arrZipCode.push(datas[x].ZipCode.toString());
                      else if (datas[x].CityName != null && datas[x].CityName != undefined)
                          arrCityName.push(datas[x].CityName.toString());
                      else if (datas[x].RegionName != null && datas[x].RegionName != undefined)
                          arrRegion.push(datas[x].RegionName.toString());
                  }


                  jq13("#Country").autocomplete({ source: function (request, response) {
                      //                      alert('code in country');
                      var matcher = new RegExp(jq13.ui.autocomplete.escapeRegex(request.term), "i");
                      response(jq13.grep(arrCountry, function (item) {
                          return matcher.test(item);
                      }));
                  }, minLength: 3
                  });

                  jq13("#City").autocomplete({ source: function (request, response) {
                      var matcher = new RegExp(jq13.ui.autocomplete.escapeRegex(request.term), "i");
                      response(jq13.grep(arrCityName, function (item) {
                          return matcher.test(item);
                      }));
                  }
                             , minLength: 2
                  });

                  jq13("#Zip").autocomplete({ source: function (request, response) {
                      var matcher = new RegExp(jq13.ui.autocomplete.escapeRegex(request.term), "i");
                      response(jq13.grep(arrZipCode, function (item) {
                          return matcher.test(item);
                      }));
                  }, minLength: 2
                  });
                  jq13("#Restaurant").autocomplete({ source: function (request, response) {
                      var matcher = new RegExp(jq13.ui.autocomplete.escapeRegex(request.term), "i");
                      response(jq13.grep(arrVenueName, function (item) {
                          return matcher.test(item);
                      }));
                  }, minLength: 2
                  });
                  jq13("#Region").autocomplete({ source: function (request, response) {
                      var matcher = new RegExp(jq13.ui.autocomplete.escapeRegex(request.term), "i");
                      response(jq13.grep(arrRegion, function (item) {
