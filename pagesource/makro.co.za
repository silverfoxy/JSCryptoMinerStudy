    <script>

        function pad(str, max) {
            str = str.toString();
            return str.length < max ? pad("0" + str, max) : str;
        }

        //localStorage.Timed_ProductKey = '103529EA';
        //localStorage.Timed_ProductName = 'DEFY 490mm Ceran Hob Stainless steel DHD356/381';

        var InitialCartGuid = "";
        if ($.cookie("MakroCart") != null)
        {
            var InitialCartGuid = $.cookie("MakroCart").replace("CartId=", "");
        }
        
            
        var initialData = { CartId: InitialCartGuid }; 

        $.ajax({
            'dataType': 'json',
            'contentType': 'application/json; charset=utf-8',
            'type': 'POST',
            'url': '/AJAX.aspx/GetCartReservationTimeRemaining',
            'data': JSON.stringify(initialData),
            'success': function (msg) {
                var obj = $.parseJSON(msg.d);
                var Expired = true;
                if (obj.HasExpired) {
                    countDownDate = new Date("Jan 1, 1900 00:00:00").getTime();
                }
                else {
                    //Update countdown date                    
                    localStorage.Timed_ProductName = obj.CountdownProductName;
                    localStorage.Timed_ProductKey = obj.CountdownProductKey;
                    countDownDate = new Date(parseInt(obj.CountdownEndDate.substr(6))).getTime() + (120 * 60 * 1000);
                }
            }
        });


        setInterval(function () {

            var CartGuid = "";
            if ($.cookie("MakroCart") != null) {
                var CartGuid = $.cookie("MakroCart").replace("CartId=", "");
            }

            var data = { CartId: CartGuid };            

            if (localStorage.Timed_ProductName != undefined)
            {
                $.ajax({
                    'dataType': 'json',
                    'contentType': 'application/json; charset=utf-8',
                    'type': 'POST',
                    'url': '/AJAX.aspx/GetCartReservationTimeRemaining',
                    'data': JSON.stringify(data),
                    'success': function (msg) {
                        var obj = $.parseJSON(msg.d);
                        if (obj.HasExpired) {

                            $.ajax({
                                'dataType': 'json',
                                'contentType': 'application/json; charset=utf-8',
                                'type': 'POST',
                                'url': '/AJAX.aspx/SyncCookiesWithCart' 
       
                            });

                            countDownDate = new Date("Jan 1, 1900 00:00:00").getTime();
                            //Change Cart Total
                            localStorage.UserData_NumItems = obj.CartTotalItemQty;
                            $('#trolley-item-number').html(obj.CartTotalItemQty);

                            if (window.location.href.indexOf("payment-success") < 0)
                            {
                                // fire up modal
                                $('#removedItemProductName').html(localStorage.Timed_ProductName);
                                $('#timedProductRemovalModal').modal('show');
                            }
                            
                            //delete local stoage
                            localStorage.removeItem('Timed_ProductName');
                            localStorage.removeItem('Timed_ProductKey');

                            if (window.location.href.indexOf("trolley") > -1)
                            {
                                setTimeout(function () {
                                window.location = window.location;
                                }, 5000);                                                           
                            }
                        }
                        else {
                            //Check if product changed
                            if (localStorage.Timed_ProductKey == obj.CountdownProductKey)
                            {
                                countDownDate = new Date(parseInt(obj.CountdownEndDate.substr(6))).getTime() + (120 * 60 * 1000);
                            }
                            else
                            {
                                // fire up modal
                                $('#removedItemProductName').html(localStorage.Timed_ProductName);
                                $('#timedProductRemovalModal').modal('show');
                                localStorage.Timed_ProductName = obj.CountdownProductName;
                                localStorage.Timed_ProductKey = obj.CountdownProductKey;
                                countDownDate = new Date(parseInt(obj.CountdownEndDate.substr(6))).getTime() + (120 * 60 * 1000);
                                //setTimeout(function () {
                                //    window.location = window.location;
                                //}, 5000);  
                            }   
                            
                        }
                    }
                });
            }
        }, 5000);

        // Set the date we're counting down to
        var x = new Date();
        x.setTime(x.getTime());

        var countDownDate = x.getTime();

        // Update the count down every 1 second
        var x = setInterval(function () {

            // Get todays date and time
            var now = new Date().getTime();

            // Find the distance between now an the count down date
            var distance = countDownDate - now;

            if (distance > 0)
            {
                // Time calculations for days, hours, minutes and seconds
                //var days = Math.floor(distance / (1000 * 60 * 60 * 24));
                //var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
                var seconds = Math.floor((distance % (1000 * 60)) / 1000);

                // Display the result in the element with id="demo"
                document.getElementById("timedProductCountDown").innerHTML = "(" + minutes + ":" + pad(seconds, 2) + ")";
                if (document.getElementById("timedProductCountDownCart") != null)
                {
                    document.getElementById("timedProductCountDownCart").innerHTML = minutes + ":" + pad(seconds, 2);                
                }

            }
            else
            {
                document.getElementById("timedProductCountDown").innerHTML = "";

                if (document.getElementById("timedProductCountDownCart") != null)
                {
                    document.getElementById("timedProductCountDownCart").innerHTML = "";
                }
            }

        }, 1000);
        
    </script>
    <script>
        $(document).ready(function () {

            $(document).on('click', '.addOrderToCart', function (e) {
                e.preventDefault();

                var orderId = $(this).data('customerorderid');

                var data = { CustomerOrderId: orderId };

                $.ajax({
                    'dataType': 'json',
                    'contentType': 'application/json; charset=utf-8',
                    'type': 'POST',
                    'url': '/AJAX.aspx/AddOrderToTrolley',
                    'data': JSON.stringify(data),
                    'success': function (msg) {
                        var obj = $.parseJSON(msg.d);

                        if (obj.CustomerLimitExceeded) {
                            $('#BulkAddError_CustomerLimitProductBlock').css('display', 'block');
                            $('#BulkAddError_CustomerLimitProductList').html(obj.CustomerLimitHTML);
                        }
                        if (obj.GlobalLimitExceeded) {
                            $('#BulkAddError_LimitedStockProductBlock').css('display', 'block');
                            $('#BulkAddError_LimitedStockProductList').html(obj.GlobalLimitHTML);
                        }
                        if (obj.InvalidProduct) {
                            $('#BulkAddError_ProblemProductBlock').css('display', 'block');
                            $('#BulkAddError_ProblemProductList').html(obj.InvalidProductHTML);
                        }

                        if (obj.CustomerLimitExceeded || obj.GlobalLimitExceeded || obj.InvalidProduct) {
                            $('#bulkProductAddError').modal('show');
                        }
                        else {
                            window.location = window.location;
                        }
                    }
                });

                $(document).on('click', '#bulkAddDismiss', function (e) {
                    e.preventDefault();
                    $('#bulkProductAddError').modal('hide');
                    window.location = window.location;
                });
            });
        });
    </script>
    
    <script src="/d/assets/js/countdown/flipclock.js" type="text/javascript"></script>
    <script>
        var clock;

        $(document).ready(function () {
            var clock;

            clock = $('.clock').FlipClock({
                clockFace: 'DailyCounter',
                autoStart: false,
                callbacks: {
                    stop: function () {
                        $('.message').html('')
                    }
                }
            });

            clock.setTime(0);
            clock.setCountdown(true);
            clock.start();
        });
    </script>


