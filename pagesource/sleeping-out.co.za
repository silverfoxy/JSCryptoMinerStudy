        {
            CreateElements();
            init();
            MakePrice();
            MakeAccommodations();
            MakeCurrency();
            MakeFacility();           
            MakeAuction();
        }
        else
        {
            cet = window.setTimeout("checkLoadingFunctions()", 100);
        }
