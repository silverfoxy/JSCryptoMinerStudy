    function CreateElements() {
        CreateDrop("BPrice", "MinDrop", "height: 32px; width: 80px; margin-top:5px;", "", "if(document.InputForm.BPrice.selectedIndex > document.InputForm.TPrice.selectedIndex){document.InputForm.TPrice.selectedIndex=document.InputForm.TPrice.length - 1}");
        CreateDrop("TPrice", "MaxDrop", "height: 32px; width: 80px; margin-top:5px;", "", "if(document.InputForm.BPrice.selectedIndex > document.InputForm.TPrice.selectedIndex){document.InputForm.BPrice.selectedIndex=0}");
        CreateDrop("AccommodationID", "AccomDrop", "height: 32px;", "", "");
        CreateDrop("Currency", "CurrDrop", "height: 32px; width: 80px; margin-top:5px;", "", "PopMinMaxValues(this.selectedIndex);");
    }
    
    function MakePrice()
    {
        if ((typeof (PR) != "undefined") && (typeof(document.getElementById("BPrice"))!="undefined") && (typeof(document.getElementById("TPrice"))!="undefined")) {
            for (var n = 0;n < PR.length;n++){PR[n][1]= Math.round(PR[n][0]*1).toString();}
            PopulateDrop("BPrice", PR, "0", "0", "0", "B");
            PopulateDrop("TPrice", PR, "900000", sAll, "900000", "T");
            document.InputForm.BPrice.disabled = false;
            document.InputForm.TPrice.disabled = false;
        }
        else {
            mp = window.setTimeout("MakePrice()", 100);
        }
    }

    function MakeCurrency()
    {
        if ((typeof (CURR) != "undefined") && (typeof(document.getElementById("Currency"))!="undefined")) {
            PopulateDrop("Currency", CURR, "ZAR", "", "", "");
            document.InputForm.Currency.disabled = false;
        }
        else {
            mct = window.setTimeout("MakeCurrency()", 100);
        }
    }

    function MakeAccommodations()
    {
        if ((typeof (ACC) != "undefined") && (typeof(document.getElementById("AccommodationID"))!="undefined")) {
            PopulateDrop("AccommodationID", ACC, "0", sAll, "0", "B");
            document.InputForm.AccommodationID.disabled = false;
        }
        else {
            mct = window.setTimeout("MakeAccommodations()", 100);
        }
    }

    function MakeAuction()
    {
        if ((typeof (BaseDataHome) != "undefined")) 
        {
            document.getElementById("search_result_featured_data_Home").innerHTML = BaseDataHome;
        }
        else 
        {
            mp = window.setTimeout("MakeAuction()", 100);
        }
    }

    function MakeFacility()
    {
