            CreateFacilities("CategoryID", "search_facilities", FAC, "", "", "", "Tick the facilities you require,in room,on site,near by");
        }
        else
        {
            mft = window.setTimeout("MakeFacility()", 100);
        }
    }

    function ChangeDateByNights(nightValue)
    {
        if (nightValue > 0 && (isNaN(nightValue) == false) && isFinite(nightValue))
        {
            nightValue = parseInt(nightValue);

            var newDate = new Date(document.InputForm.ArrDate.value);
            newDate.setDate(newDate.getDate() + nightValue); 
            document.InputForm.DepDate.value = newDate.getDate() + " " + (months[newDate.getMonth()]) + " " + newDate.getFullYear();
        }
        else
        {
            if(nightValue != ""){
                alert("Please only enter numbers for the nights value");
            }
        }
    }
