<script type="text/javascript">
(function(window,$,undefined) {
$(function(){
	var delay = (function(){
	  var timer = 0;
	  return function(callback, ms, clear){
		if(typeof(clear)==='undefined') clear = false;
		clearTimeout (timer);
		if(!clear) {
			timer = setTimeout(callback, ms);
		}
	  };
	})();


	var $photoTour = $('#photo_tour');
	var $trame = $('#trame');
	var $bandeauDroite = $('#bandeau_droite');
	var minWidth = 50;
	var margeBas = 40; // marge pour la signature
	function adaptImageToWindow() {
		var winWidth = $(window).width();
		
		var siteWidth = $('#menu').outerWidth()+$('#conteneur').width();
		
		var imageWidth = winWidth - siteWidth;
		
		if(imageWidth>0) {
			if(imageWidth<minWidth) imageWidth = minWidth;
			$photoTour.width(imageWidth).height($(window).height()-margeBas);
			$bandeauDroite.width(imageWidth);
		}
		$trame.height($(window).height()-margeBas);
		//$('#menu').height(($('#conteneur_interieur').height()-369));
	}
	adaptImageToWindow();
	
	$(window).on('resize',function(){
		delay(adaptImageToWindow,300);
	});
	var $sousMenu = $('<div id="conteneur_sous_menu" />');
	$sousMenu.css({
		"width":$('#conteneur_interieur').width()
		,"height":($(window).height()-$('#onglets').outerHeight())
		,"display":"none"
		,"position":"fixed"
		,"left":($('#menu').outerWidth()-$('#conteneur_interieur').width())
		,"top":$('#onglets').outerHeight()
		,"z-index":"38"
	}).append('<div id="conteneur_sous_menu_contenu"></div>')
		.appendTo('body');
	
	$('ul li a','#menu').on('click',function(e){
		var w = $(window).width();
		if($(this).parent().hasClass('ref_77')) {
			// comportement normal, page contact
		}
		else {
			if (w > 750) {
				e.preventDefault();
				if(!$sousMenu.hasClass('sous_menu_visible')) {
					$('#conteneur_sous_menu_contenu').html($(this).parent().find('ul.sous_menu').clone());
					$('#conteneur_sous_menu_contenu').prepend("<h1>"+$(this).find('span').html()+'<a href="Javascript:;" alt="Fermer" id="conteneur_sous_menu_fermer"><span>Fermer</span></a>'+"</h1>");
					afficherSousMenu();
				}
				else {
					// refermer, puis réouvrir
					var s = this;
					masquerSousMenu(function() {
						$('#conteneur_sous_menu_contenu').html($(s).parent().find('ul.sous_menu').clone());
						$('#conteneur_sous_menu_contenu').prepend("<h1>"+$(s).find('span').html()+'<a href="Javascript:;" alt="Fermer" id="conteneur_sous_menu_fermer"><span>Fermer</span></a>'+"</h1>");
						afficherSousMenu();
					});
				}
			}
		}
	});
	$(document).on('scroll',function(){
		if($(document).scrollTop()>$('#onglets').outerHeight()) {
			$sousMenu.css('top',0);
			$sousMenu.css('position','fixed');
		}
		else {
			$sousMenu.css('top',$('#onglets').outerHeight());
			$sousMenu.css('position','absolute');
		}
	});
	$('#conteneur_sous_menu').on('click','#conteneur_sous_menu_fermer',function(){
		masquerSousMenu();
	});
	function afficherSousMenu() {
		$sousMenu.show().stop().animate({
			left:"+="+$('#conteneur_interieur').width()
		},700,"swing").addClass('sous_menu_visible');
		
	}
	function masquerSousMenu(callback) {
		$sousMenu.stop().animate({
			left:"-="+$('#conteneur_interieur').width()
		},{
			duration: 500
			,easing:"swing"
			,complete:function(){
				$sousMenu.hide();
				if(typeof(callback)==="function") {
					callback.call();
				}
			}
		}).removeClass('sous_menu_visible');
	}
	//$sousMenu.show();
});
})(window,jQuery);
</script>
