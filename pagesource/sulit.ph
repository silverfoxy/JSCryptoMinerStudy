	margin:0px 20px 5px 20px;
	transition:all 200ms ease;
}

.navigation .visible-menu:last-child{margin-bottom:30px;}

.active-item {position: relative;}
.active-item .fa-angle-right{display:none;}
.active-item:before {
    color: #FFFFFF;
