  <!-- ADDED SEARCH AUTO-COMPLETE -->
  






<script>
$(function() {
  // Current Ajax request.
  var currentAjaxRequest = null;
  // Grabbing all search forms on the page, and adding a .search-results list to each.
  var searchForms = $('form[id="search_products"]').css('position','relative').each(function() {
 
    //For the searching of products
    var input = $(this).find('input[name="q"]'),
    input_width = input.width() + 3,
   	offSet = input.position().top + input.innerHeight();
   
    $('<ul class="search-results search-results-list"></ul>').css( { 'position': 'absolute', 'left': '0px', 'top': offSet } ).appendTo($(this)).hide();    
    
    input.attr('autocomplete', 'off').bind('keyup change', function() {
      input_width = input.width() + 3;
      var term = $(this).val(),
      form = $(this).closest('form'),
      searchURL = '/search?type=product&q=' + term,
      resultsList = form.find('.search-results');     
      
      if (term.length > 3 && term != $(this).attr('data-old-term')) {
        $(this).attr('data-old-term', term);       
        if (currentAjaxRequest != null) currentAjaxRequest.abort();
        currentAjaxRequest = $.getJSON(searchURL + '&view=json', function(data) {        
          resultsList.empty();
          if(data.results_count == 0) {       
            resultsList.hide();
          } else {          
            $.each(data.results, function(index, item) {
              var link = $('<a></a>').attr('href', item.url);
              link.append('<span class="thumbnail"><img src="' + item.thumbnail + '" /></span>');
              link.append('<span class="title">' + item.title + '</span>');           
              link.wrap('<li></li>');           
              resultsList.append(link.parent());
            });           
            if(data.results_count > 10) {
              resultsList.append('<li><span class="title"><a href="' + searchURL + '">See all results (' + data.results_count + ')</a></span></li>');
            }
            resultsList.css({"width":input_width+"px"});
            resultsList.fadeIn(200);
          }        
        });
      }
    }); //end input autocomplete
     
    
  }); //end /search autocomplete
  
  
  var blogSearchForms = $('form[id="search_articles"]').css('position','relative').each(function() {
    
    var blog_input = $(this).find('input[name="bq"]'),
    blog_input_width = blog_input.width() + 3,
    blog_offset = blog_input.position().top + blog_input.innerHeight();
    $('<ul class="blog-search-results search-results-list"></ul>').css( { 'position': 'absolute', 'left': '0px', 'top': blog_offset } ).appendTo($(this)).hide();
   
    blog_input.attr('autocomplete', 'off').bind('keyup change', function() {
    	
      blog_input_width = blog_input.width() + 3;
      
      var blog_term = $(this).val(),
      blog_form = $(this).closest('form'),
      blog_search_url = '/search?type=article&q=' + blog_term,
      blog_search_results = blog_form.find('.blog-search-results');
      
      if( blog_term.length > 3 && blog_term != $(this).data('data-old-term')) {
        
        $(this).attr('data-old-term', blog_term);
        if(currentAjaxRequest != null) currentAjaxRequest.abort();
        currentAjaxRequest = $.getJSON(blog_search_url + '&view=json', function(data) {
        	blog_search_results.empty();
          if(data.results_count == 0) {
            
          	blog_search_results.hide();
            
          } else  {
          
            $.each(data.results, function(index, item) {
            	var link = $('<a></a>').attr('href', item.url);
              	link.append('<span class="thumbnail"><img src="' + item.thumbnail + '" /></span>');
           		link.append('<span class="title">' + item.title + '</span>');           
            	link.wrap('<li></li>');
              	blog_search_results.append(link.parent());
              
            }); //end each
            if(data.results_count > 10) {
              blog_search_results.append('<li><span class="title"><a href="' + blog_search_url + '">See all results (' + data.results_count + ')</a></span></li>');
            }
            blog_search_results.css({"width":blog_input_width+"px"});
            blog_search_results.fadeIn(200);
            
          }
        }); //end getjson
      
      } //end if
      
    });
    
    
  });
  
  $('.btn-search-blog').bind('click', function(e) {
    e.preventDefault();
    var search_redirect = '/search/?type=article&q='+ $(this).parent().find('input[name="bq"]').val();
    window.location = search_redirect;
    return true;
  });
  
  // Clicking outside makes the results disappear.
  $('body').bind('click', function(){
    $('.search-results-list').hide();
  });
});
</script>

