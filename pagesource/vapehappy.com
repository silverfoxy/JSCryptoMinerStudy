        style_height = t.find('.prod-image').css('height'),
        cur_height =  t.find('.product-info').height(),
        style_h = parseInt(style_height.replace('px',""));
        if(style_h > max_image_h) max_image_h = style_h;
        if(cur_height > max_desc_h) max_desc_h = cur_height;
  
      });
      normalize_max_h = max_image_h + max_desc_h;
      el.css({"height": normalize_max_h+"px","max-height":normalize_max_h+"px","margin-bottom":"10px"});
