function openModal_c2a(what) {
    $('#form_c2a').addClass("modal fade");
    $('#form_c2a_dialog').addClass("modal-dialog");
    $('#form_c2a_content').addClass("modal-content");
    $('#form_c2a_content').removeClass("no-modal-content");

    switch (what){
        case 'prezzi':
            $("#opt_prezzi").prop( "checked", true );
            $("#opt_rivend").prop( "checked", false );
            $("#opt_catalog").prop( "checked", false );
            $("#opt_altro").prop( "checked", false );
            $('.textarea_c2a').prop('placeholder', 'Scrivi la tua richiesta');
            $('.textarea_c2a').addClass("bianca");
            $('#MOTIVAZIONE').val(""); //reset valore testuale
            break;
        case 'catalog':
            $("#opt_prezzi").prop( "checked", false );
            $("#opt_rivend").prop( "checked", false );
            $("#opt_catalog").prop( "checked", true );
            $("#opt_altro").prop( "checked", false );
            $('.textarea_c2a').prop('placeholder', 'Scrivi la tua richiesta');
            $('.textarea_c2a').addClass("bianca");
            $('#MOTIVAZIONE').val(""); //reset valore testuale
            break;
        case 'rivend':
            $("#opt_prezzi").prop( "checked", false );
            $("#opt_rivend").prop( "checked", true );
            $("#opt_catalog").prop( "checked", false );
            $("#opt_altro").prop( "checked", false );
            $('.textarea_c2a').prop('placeholder', 'Scrivi la tua richiesta');
            $('.textarea_c2a').addClass("bianca");
            $('#MOTIVAZIONE').val(""); //reset valore testuale
            break;
        case 'altro':
            $("#opt_prezzi").prop( "checked", false );
            $("#opt_rivend").prop( "checked", false );
            $("#opt_catalog").prop( "checked", false );
            $("#opt_altro").prop( "checked", true );
            $('.textarea_c2a').prop('placeholder', 'Scrivi la tua richiesta*');
            $('.textarea_c2a').removeClass("bianca");
            break;
    }
}
function CallPageSync(url) {
        var response;
        $.ajax({
            async: false,
            url: url,
            timeout: 4000,
            success: function(result) {
                response = result;
            },
            error: function(XMLHttpRequest, textStatus, errorThrown) {
                response = "err--" + XMLHttpRequest.status + " -- " + XMLHttpRequest.statusText;
            }
        });
        return response;
    }

