<script type="text/javascript">
function setMFCookie(cname, cvalue, exdays) {
	var d = new Date();
	d.setTime(d.getTime() + (exdays*24*60*60*1000));
	var expires = "expires="+d.toGMTString();
	document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
}

function getMFCookie(cname) {
	var name = cname + "=",
		ca = document.cookie.split(';');
	for(var i=0; i<ca.length; i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1);
		if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
	}
	return "";
}

function mfServiceNu() {
	var storedServiceNu = getMFCookie('mf_service_number');
	if (storedServiceNu == '') {
		//if a stored service number does not exist, create a random string and store it in a session cookie
		var n = 7;
		storedServiceNu = new Array(n+1).join((Math.random().toString(36)+'00000000000000000').slice(2, 18)).slice(0, n);
		document.cookie = 'mf_service_number' + '=' + storedServiceNu + ';domain=.princess.com;path=/'; //store service_number for current session
	}
	return storedServiceNu;
}

var location_code = getMFCookie('_fipc_');
var sccn = getMFCookie('sccn');
// add this above the "var _mfq" line in the mouseflow code
if (sccn) {
	mouseflowPath = document.location.pathname + '-' + location_code + '-loggedin';
} else {
	mouseflowPath = document.location.pathname + '-' + location_code;
}

// mouseflow tracking code - vars need to be in the global scope
var mouseflowDisableKeyLogging = true,
	mouseflowHtmlDelay = 2000,
	_mfq = _mfq || [];

function loadMouseflow(country) {
	//orginally, we only set the mf_record_user cookie for all traffic that entered into the FNC & PB:
	//(window.location.href.indexOf('/reserve/') > -1 || window.location.href.indexOf('/find/')
	//now, we set the mf_record_user cookie for all US traffic, regardless of where they visit on the site.
	if ((country === 'US' || country === 'CA') && (getMFCookie('mf_record_user') === '' || getMFCookie('mf_record_user') === null)) {
		setMFCookie('mf_record_user', 1, 1);
	}

	if (getMFCookie('mf_record_user')) {
		(function() {
		   var mf = document.createElement("script"); mf.type = "text/javascript"; mf.async = true;
		   mf.src = "//cdn.mouseflow.com/projects/f3a02463-b43f-48da-9dcb-90e7d2f103b1.js";
		   document.getElementsByTagName("head")[0].appendChild(mf);
		})();
		//display the mouseflow service number in the footer, by using _mfq.push() to display mouseflow service number for active recordings.
		if (typeof mouseflow == 'object' && mouseflow.isRecording == true) {
			displayMouseflowNu();
		}
	}
}

function displayMouseflowNu() {
	//if the #mouseflow-number element is present in the footer, populate the service number and make it visible
	var mouseflowElemCount = 0;
	var mouseflowElemCheck = setInterval(function() {
		var mouseflowElem = document.getElementById("mouseflow-number");
		if (mouseflowElem !== null && mouseflowElem.innerHTML === '') {
			clearInterval(mouseflowElemCheck);
			var serviceNumber = mfServiceNu();
			//push the service number to mouseflow
			_mfq.push(["setVariable", "service_number", serviceNumber]);
			mouseflowElem.innerHTML = 'Service #' + serviceNumber;
			mouseflowElem.style.display = 'block';
		} else {
			//stop checking if the mouseflowElem is not available after 5 seconds.
			if (mouseflowElemCount > 100) {
				clearInterval(mouseflowElemCheck);
			}
		}
		mouseflowElemCount++;
	}, 50);
}

var mfLocaleCheckCount = 0;
var mfLocaleCheck = setInterval(function() {
	var mfLocale = getMFCookie('getLocale');
	//stop the setInterval once the getLocale cookie is set
