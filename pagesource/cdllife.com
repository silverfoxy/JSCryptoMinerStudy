    border: 1px solid #FFF;
    border-radius: 1.1em 0 0 1.1em;
    margin-right: 0;
    width: 235px;
    padding: 0 .8em;
    color: #404041;
    transition: border-color .2s;
    font-weight: 400;
    line-height: 30px;
    height: 30px;
}

