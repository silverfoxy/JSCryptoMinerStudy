	  <script type="text/javascript">

		//<![CDATA[
			//var searchForm = new Varien.searchForm('search_mini_form', 'searchInput', '');
		//]]>

		var $j = jQuery.noConflict();

		$j(document).ready(function() {

			// Global vars
			var JSON   		   = null;
			var ready  		   = false;
			var customSearches = null;

			// Page elements
			var $searchInput 	  = $j('#searchInput');
			var $resultsContainer = $j('#resultsContainer');
			var $resultsTable	  = $j('#resultsTable');

			$j('.header__search-go-btn').on('click', function() {
				var searchValue = $searchInput.val();
				if (searchValue && searchValue.trim() != '') {
					window.location.href = "https://www.narescue.com/catalogsearch/result/?q=" + $searchInput.val(); 
				} else {
					$searchInput.val('');
					$searchInput.focus();
				}
			});

			// Set up conveniently hiding the search results on body click
			$j('body').on('click', function(e) {
				if ( ! $j(e.target).hasClass('tt-dataset-a1') &&
					 ! $j(e.target).hasClass('tt-dropdown-menu') &&
					 ! $j(e.target).hasClass('header__search-field') &&
					 ! $j(e.target).hasClass('predictive-search-result')) {
					$resultsContainer.hide();
					$searchInput.val('');
				}
			});

			// Removing duplicates from an object by the name
			function removeDuplicates(array) {
				var result = [];
				var holder = [];
				$j.each(array, function(i, e) {
					if ($j.inArray(e.name.toLowerCase().trim(), holder) == -1) {
						holder.push(e.name.toLowerCase().trim());
						result.push(e);
					}
				});
				return result;
			}

			// Function sorts the JSON object by a specified attribute
			function sortJSON(prop, asc, JSON) {
				JSON = removeDuplicates(JSON);
				JSON = JSON.sort(function(a, b) {
					if ( ! a[prop]) { a[prop] = 1000000000; }
					if ( ! b[prop]) { b[prop] = 1000000000; }
					if (asc) return (a[prop] > b[prop]) ? 1 : ((a[prop] < b[prop]) ? -1 : 0);
					else return (b[prop] > a[prop]) ? 1 : ((b[prop] < a[prop]) ? -1 : 0);
				});
				return JSON;
			}

			// Run AJAX to get the custom searches
			$j.ajax({
                                url: location.protocol+'//'+location.host+'/search-configuration/custom-searches.txt',
				cache: false,
				success: function(data) {
					data = data.replace(/[\n\r]/g, '');
					customSearches = data.split(',');
					for (var i=0; i<customSearches.length; i++) {
						customSearches[i] = customSearches[i].toLowerCase().trim();
					}
				}
			});

			// Run AJAX to get the initial JSON file
			$j.ajax({
                                url: location.protocol+'//'+location.host+'/searchinfo.json',
				cache: false,
				success: function(data) {
					JSON  = data;
					ready = true;
				}
			});

			// Interval to determine if page is ready or not
			var interval = setInterval(function() {
				if (ready == true) {
					// We're ready, clear the interval out
					clearInterval(interval);
					// Set up search change event
					$searchInput.on('keyup', function(event) { searchChange(event); });

					$searchInput.on('keydown', function(event) {
						if (event.keyCode == 38 || event.keyCode == 40) {
							event.preventDefault();
							return false;
						}
					});
				}
			}, 50);

			// Handles search changing
			function searchChange(event) {
				var searchValue = $searchInput.val().toLowerCase();
				// Up arrow key
				if (event.keyCode == 38) {
					$active   = $j('.predictive-search-result.active');
					$previous = $active.prev();
					if ($previous.length == 0) {
						return false;
					}
					$previous.css({ 'background-color' : '#dedede' });
					$active.css({ 'background-color' : 'white' });
					$active.removeClass('active');
					$previous.addClass('active');
					return false;
				}
				// Down arrow key
				if (event.keyCode == 40) {
					$active = $j('.predictive-search-result.active');
					$next 	= $active.next();
					if ($next.length == 0 && $active.length == 0) {
						$j('.predictive-search-result').first().addClass('active');
						$j('.predictive-search-result').first().css({ 'background-color' : '#dedede' });
					} else {
						if ($next.length != 0) {
							$next.css({ 'background-color' : '#dedede' });
							$active.css({ 'background-color' : 'white' });
							$active.removeClass('active');
							$next.addClass('active');
						}
					}
					return false;
				}
				if (event.keyCode != 13) {
					if (searchValue && searchValue.trim() != '') {
						if ($j.inArray(searchValue.toLowerCase().trim(), customSearches) == -1) {
							var searchResults = findInJSON(searchValue);
							handleResults(searchResults, searchValue);
						} else {
							searchCustomResults(searchValue.trim());
						}
					} else {
						$resultsContainer.hide();
					}
				} else {
					if (searchValue && searchValue.trim() != '') {
						$active = $j('.predictive-search-result.active');
						if ($active.length != 0) {
							var href = $active.attr('href');
							window.location.href = href;
						} else {
							window.location.href = "https://www.narescue.com/catalogsearch/result/?q=" + $searchInput.val(); 
						}
					}
				}
			}

			// Searching the custom results by SKU
			function searchCustomResults(searchValue) {
				$j.ajax({
                                        url: location.protocol+'//'+location.host+'/search-configuration/searches/' + searchValue + '.txt',
					cache: false,
					success: function(data) {
						var searchResults = null;
						data 	 = data.replace(/[\n\r]/g, '');
						data 	 = data.toLowerCase();
						var skus = data.split(',');
						if (skus.length >= 1) {
							searchResults 		  = findBySkusInJSON(skus);
							var additionalResults = findInJSON(searchValue);
							additionalResults 	  = sortJSON('importance', true, additionalResults);
							searchResults 		  = searchResults.concat(additionalResults);
							searchResults 		  = removeDuplicates(searchResults);
						}
						if (skus.length < 1) {
							searchResults = findInJSON(searchValue);
						}
						handleResults(searchResults, searchValue);
					}
				});
			}

			function findBySkusInJSON(skus) {
				searchResults = [];
				for (var i=0; i<skus.length; i++) {
					var sku = skus[i];
					sku 	= sku.toLowerCase().trim();
					for (var key in JSON) {
						var jsonObject = JSON[key];
						if (jsonObject.sku && jsonObject.name) {
							var jsonObjectSku = jsonObject.sku;
							jsonObjectSku = jsonObjectSku.toLowerCase().trim();
							if (jsonObjectSku == sku) {
								searchResults.push({ name : jsonObject.name,
													 link : jsonObject.url_path });
							}
						}
					}
				}
				return searchResults;
			}

			// Returns array of found values
			function findInJSON(searchValue) {

				var sv = searchValue.replace(/\W/g, "");
				var newSearchValue = sv[0];
				if (sv.length > 1) {
					for (var i=1; i<sv.length; i++) {
						newSearchValue = newSearchValue + "\\W*" + sv[i];
					}
				}

				var re = new RegExp("(?:\\b(" + newSearchValue + ")\\w*\\b|\\b\\w*(" + newSearchValue + ")\\b)", "i");

				var searchResults = [];

				for (var key in JSON) {
					if (searchResults.length <= 15) {
						var jsonObject = JSON[key];
						var visibility = jsonObject.visibility;
						var link 	   = jsonObject.url_path;
						var name 	   = jsonObject.name.toLowerCase().trim();
						var importance = jsonObject.importance;
						var addedToResults = false;

						if (visibility != 1) {

							// Handling searching name
							if (re.test(name)) {
								searchResults.push({ name 		: name,
													 link 		: link,
													 importance : importance });
								continue;
							}

							// Handling searching SKUs
							if (jsonObject.sku) {
								jsonObject.sku = jsonObject.sku.toLowerCase().trim();
								if (jsonObject.sku.indexOf(searchValue) > -1) {
									searchResults.push({ name 		: name,
														 link 		: link,
														 importance : importance });
									continue;
								}
							}

							// Handling searching of meta description
							// if (jsonObject.meta_description) {
							// 	if (re.test(jsonObject.meta_description)) {
							// 		searchResults.push({ name 		: name,
							// 							 link 		: link,
							// 							 importance : importance });
							// 		continue;	
							// 	}
							// }

							// Handling searching keywords
							if (jsonObject.search_keyword) {
								var searchKeywords  = jsonObject.search_keyword;
								var searchKeywords1 = searchKeywords.split(' ');
								var searchKeywords2 = searchKeywords.split(',');
								searchKeywords = searchKeywords1.concat(searchKeywords2);
								for (var i=0; i<searchKeywords.length; i++) {
									var lowerTrimmed = searchKeywords[i];
									lowerTrimmed = lowerTrimmed.toLowerCase().trim();
									if (lowerTrimmed == searchValue || lowerTrimmed.indexOf(searchValue) > -1) {
										searchResults.push({ name 		: name,
															 link 		: link,
															 importance : importance });
										addedToResults = true;
										break;
									}
								}
								if (addedToResults == true) { continue; }
							}

							// Handling searching meta keywords
							if (jsonObject.meta_keyword) {
								var metaKeywords  = jsonObject.meta_keyword;
								var metaKeywords1 = metaKeywords.split(' ');
								var metaKeywords2 = metaKeywords.split(',');
								metaKeywords = metaKeywords1.concat(metaKeywords2);
								for (var j=0; j<metaKeywords.length; j++) {
									var lowerTrimmed = metaKeywords[j];
									lowerTrimmed = lowerTrimmed.toLowerCase().trim();
									if (lowerTrimmed == searchValue || lowerTrimmed.indexOf(searchValue) > -1) {
										searchResults.push({ name 		: name,
															 link 		: link,
															 importance : importance });
										addedToResults = true;
										break;
									}
								}
								if (addedToResults == true) { continue; }
							}
						} else {
							if (jsonObject.parent_data && jsonObject.parent_data.length) {
								for (var k in jsonObject.parent_data) {
									
									// Handling searching name
									if (re.test(name)) {
										searchResults.push({ name 		: jsonObject.parent_data[k].name,
															 link 		: jsonObject.parent_data[k].url_path,
															 importance : jsonObject.parent_data[k].importance });
										continue;
									}

									// Handling searching SKUs
                                                                        // We have to manually lowercase because this one is a string test, not a regex test
									var jsonObjectSku = jsonObject.sku.toLowerCase().trim();
									if (jsonObjectSku.indexOf(searchValue.toLowerCase().trim()) > -1) {
										searchResults.push({ name 		: jsonObject.parent_data[k].name,
															 link 		: jsonObject.parent_data[k].url_path,
															 importance : jsonObject.parent_data[k].importance });
										continue;
									}

									// Handling searching of meta description
									// if (jsonObject.meta_description) {
									// 	if (re.test(jsonObject.meta_description)) {
									// 		searchResults.push({ name 		: jsonObject.parent_data[k].name,
									// 							 link 		: jsonObject.parent_data[k].url_path,
									// 							 importance : jsonObject.parent_data[k].importance });
									// 		continue;
									// 	}
									// }

									// Handling searching keywords
									if (jsonObject.search_keyword) {
										var searchKeywords  = jsonObject.search_keyword;
										var searchKeywords1 = searchKeywords.split(' ');
										var searchKeywords2 = searchKeywords.split(',');
										searchKeywords = searchKeywords1.concat(searchKeywords2);
										for (var i=0; i<searchKeywords.length; i++) {
											var lowerTrimmed = searchKeywords[i];
											lowerTrimmed = lowerTrimmed.toLowerCase().trim();
											if (lowerTrimmed == searchValue || lowerTrimmed.indexOf(searchValue) > -1) {
												searchResults.push({ name 		: jsonObject.parent_data[k].name,
																	 link 		: jsonObject.parent_data[k].url_path,
																	 importance : jsonObject.parent_data[k].importance });
												addedToResults = true;
												break;
											}
										}
										if (addedToResults == true) { continue; }
									}

									// Handling searching meta keywords
									if (jsonObject.meta_keyword) {
										var metaKeywords  = jsonObject.meta_keyword;
										var metaKeywords1 = metaKeywords.split(' ');
										var metaKeywords2 = metaKeywords.split(',');
										metaKeywords = metaKeywords1.concat(metaKeywords2);
										for (var j=0; j<metaKeywords.length; j++) {
											var lowerTrimmed = metaKeywords[j];
											lowerTrimmed = lowerTrimmed.toLowerCase().trim();
											if (lowerTrimmed == searchValue || lowerTrimmed.indexOf(searchValue) > -1) {
												searchResults.push({ name 		: jsonObject.parent_data[k].name,
																	 link 		: jsonObject.parent_data[k].url_path,
																	 importance : jsonObject.parent_data[k].importance });
												addedToResults = true;
												break;
											}
										}
										if (addedToResults == true) { continue; }
									}
								}
							}
						}
					} else {
						return sortJSON('importance', true, searchResults);
					}
				}
				return sortJSON('importance', true, searchResults);
			}

			// Handles the display of search results
			function handleResults(searchResults, searchValue) {
				$resultsTable.empty();
				if ( ! searchResults.length) {
					$resultsContainer.hide();
					return;
				}

				var sv = searchValue.replace(/\W/g, "");
				var newSearchValue = sv[0];
				for (var j=1; j<sv.length; j++) {
					if (sv.length > 1) {
						newSearchValue = newSearchValue + "\\W*" + sv[j];
					}
				}
				for (var i=0; i<searchResults.length; i++) {
					var name  = searchResults[i].name;
					var link  = searchResults[i].link;
					if (typeof name !== 'undefined' && typeof link !== 'undefined') {

						var re = new RegExp("(?:\\b(" + newSearchValue + ")\\w*\\b|\\b\\w*(" + newSearchValue + ")\\b)", "i");
						var matches = name.match(re);

						if (matches && matches.length == 3) {
							if (typeof matches[1] !== 'undefined') {
								name = name.replace(matches[1], "<font color='red'>" + matches[1] + "</font>");
							} else {
								if (typeof matches[2] !== 'undefined') {
									name = name.replace(matches[2], "<font color='red'>" + matches[2] + "</font>");
								}
							}
						}

						$resultsTable.append('<a class="predictive-search-result" href="'+window.location.protocol+'//'+location.host+'/' + link + '">' + name.toUpperCase() + '</a>');
					}
				}
				$resultsContainer.show();
			}

		});
	  </script>

