			var cURL=window.parent.location.href;
			var array=cURL.split("/")[2].split(".");
			var sld=array[array.length-2]+"."+array[array.length-1]
			return sld;

		}

		function getUrlParameter(sParam)
		{
			var sPageURL = window.location.search.substring(1);
			var sURLVariables = sPageURL.split("&");
			for (var i = 0; i < sURLVariables.length; i++)
			{
				var sParameterName = sURLVariables[i].split("=");
				if (sParameterName[0] == sParam)
				{
					return sParameterName[1];
				}
			}
		}

		function cookie_append (cookie_name) {
			cookie_url_value = decodeURIComponent(getUrlParameter(cookie_name));

			if (cookie_url_value != "" && cookie_url_value != undefined && cookie_url_value != "undefined") {

				if (jQuery.cookie(cookie_name) != "" && jQuery.cookie(cookie_name) != undefined && jQuery.cookie(cookie_name) != "undefined") {
					var array = jQuery.cookie(cookie_name).split(",");

					set_cookie = true;

					jQuery.each(array,function(i){
						if (array[i] == cookie_url_value) {
							set_cookie = false;
						}
					});

					if (set_cookie) {
						jQuery.cookie(cookie_name, [cookie_url_value,jQuery.cookie(cookie_name)], {expires: 3650, path: "/", domain: getsld()});
					}
				}
				else if (cookie_url_value != undefined && cookie_url_value != "undefined") {
					jQuery.cookie(cookie_name, [cookie_url_value], {expires: 3650, path: "/", domain: getsld()});
				}
			}
		}

		param_array = ["utm_source","utm_medium","utm_term","utm_content","utm_campaign","mrm_id"];

		jQuery.each( param_array, function( key, value ) {
