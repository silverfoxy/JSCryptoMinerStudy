	<script>
		/**
		 * Determine the mobile operating system.
		 * This function either returns 'iOS', 'Android' or 'unknown'
		 *
		 * @returns {String}
		 */
		function getMobileOperatingSystem() {
			var userAgent = navigator.userAgent || navigator.vendor || window.opera;

			if (userAgent.match(/iPad/i) || userAgent.match(/iPhone/i) || userAgent.match(/iPod/i)) {
				return 'iOS';
			}
			else if (userAgent.match(/Android/i)) {
				return 'Android';
			}
			else {
				return 'unknown';
			}
		}

		/**
		 * Object that holds the last menu item hovered over (.topNavFlyoutMenuItem)
		 * and the associated timeout its mouseout event
		 */
		var menuObject = {
			lastHovered: null,

			timeout: window.setTimeout(function () {
//				console.log('init')
			}, 1),

			clearTimeout: function () {
				clearTimeout(this.timeout);
			}
		};

		// set up the menus:
		jQuery(document).ready(function ($) {

			var hoverFlag = false;

			//Hide others and add active class for iOS due to the order it fires touch events
			if (getMobileOperatingSystem() === 'iOS') {
				$('.flyout-menu').on('mouseover', function (e) {
					$('.flyout-menu').not($(this)).each(function () {
						$(this).removeClass('active');
					});
					$(this).addClass('active');
				});
			}

			/** CLICK / MOBILE FUNCTIONALITY **/
			$('.topNavFlyoutMenuItem').click(function (e) {
				var $topNavFlyoutMenuItem = $(this);
				var target = jQuery(this).data('target');

				e.preventDefault();
				e.stopImmediatePropagation();

				menuObject.timeout = window.setTimeout(function () {
					if (target && !hoverFlag) {

					} else {
						window.location = $topNavFlyoutMenuItem.find('a').attr('href');
					}
				}, 100);


				if ($(this).hasClass('active')) {
					// hide menu
					$(target).addClass('js-is-hidden');

				} else {
					//show menu
					$(target).removeClass('js-is-hidden').addClass('flyout-open');

					$('.flyoutPanelTarget').each(function () {
						$(this).not($(target)).addClass('js-is-hidden');
					});
				}

				$('.topNavFlyoutMenuItem').not($(this)).each(function () {
					$(this).removeClass('active');
				});

				$(this).toggleClass('active');

			});

			/** set flag on mobile devices */
			$(".topNavFlyoutMenuItem").on('touchstart', function () {
				window.setTimeout(function () {
					hoverFlag = false;
				}, 150);
			});

			/**
			 * When you enter another element,
			 * and within the delay you hover back to an element's target
			 */

			$('.flyoutPanelTarget').on('mouseenter', function () {
				var id = $(this).attr('id');
				var callee = $('[data-target="#' + id + '"]');

				if(menuObject.lastHovered === $(callee).data('target')){
					menuObject.clearTimeout();
				}
			});

			/**
			 * DESKTOP/ HOVER FUNCTIONALITY
			 */
			$(".topNavFlyoutMenuItem").hover(

				function (e) {
					//mouseover

					//target is who we're going to show
					var target = $(this).data('target');
					//this sets a reference back to $(this) so we have a sort of binding
					var calleeTarget = $(target).data('calleeTarget', $(this));

					//Show the target
					$(target).removeClass('js-is-hidden');

					//Globally tell that we're hovering
					hoverFlag = true;

					//Hide the others
					$('.flyoutPanelTarget').each(function () {
						$(this).not($(target)).addClass('js-is-hidden');
					});

					//If someone mouseentered another element, then clear the timeout that closes the panel
					if (menuObject.lastHovered === target) {
						menuObject.clearTimeout();
					}

					//If we don't have touch events
					if (typeof document.ontouchstart === 'undefined') {
						$(".topNavFlyoutMenuItem").removeClass('active');
						$(this).addClass('active');
					}

				},

				//mouseleave
				function (e) {
					var target = $(this).data('target');
					$(target).data('calleeTarget', $(this));

					if (!$(e.toElement).hasClass('flyoutPanelTarget') && !$(e.relatedTarget).hasClass('flyoutPanelTarget')) {
						if ($(e.toElement).closest('.flyoutPanelTarget').length > 0) {

						} else {
							//if we're not hovering over an element that is, or is contained by a flyoutPanelTarget
							menuObject.lastHovered = target;
							menuObject.timeout = window.setTimeout(function () {
								$(target).addClass('js-is-hidden');
								$(".topNavFlyoutMenuItem").removeClass('active');
							}, 300);

						}
					}

				}
			);

			/** HANDLING SPECIFIC DESKTOP HOVER EVENTS **/
				//Just mouse leave
			$('.flyoutPanelTarget').mouseleave(function (e) {
				var $calleeTarget = $(this).data('calleeTarget');

				if (!$(e.toElement).hasClass('topNavFlyoutMenuItem') && !$(e.relatedTarget).hasClass('topNavFlyoutMenuItem')) {
					if ($(e.toElement).closest('.topNavFlyoutMenuItem').length > 0 || $(e.relatedTarget).closest('.topNavFlyoutMenuItem').length > 0) {
						//a parent of mine is the class we want
					} else {
						$(this).addClass('js-is-hidden');
						$(".topNavFlyoutMenuItem").removeClass('active');
					}

				} else {
					//if the element we mouseout back into is the topnav menu item that calls this menu panel
					if ($(e.toElement).data('target') === $calleeTarget.data('target') || $(e.relatedTarget).data('target') === $calleeTarget.data('target')) {
						//do nothing
					} else {
						//otherwise hide this panel and we'll show the new one
						$(this).addClass('js-is-hidden');
						$(".topNavFlyoutMenuItem").removeClass('active');
					}
				}
			});
		})

	</script>
