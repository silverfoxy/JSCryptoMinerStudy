var isIE = (navigator.userAgent.indexOf('MSIE') != -1 && !window.opera);

window.pagehead = document.getElementsByTagName('head')[0];

function G(id){
	return document.getElementById(id);
}

function addEvent(elem, type, handler){
	if(isIE){
		elem.attachEvent('on' + type, (function(elm) {
			return function() {
				handler.call(elm);
			}
		})(elem));
	}
	else{
		elem.addEventListener(type, handler, true);
	}
}

function initTabs(tabArray,cssName,tabLoadCallback) {
	var len = tabArray.length;

	for (var i=0; i<len; i++) {

		var tabItem = G(tabArray[i]);

		tabItem.loadFlag = false;

		var self = this;

		addEvent(tabItem, "click",(function(tab) {
			return function() {
				
				if (tab.loadFlag == false) {
					
					if (tabLoadCallback) tabLoadCallback.call(self,tab);
					tab.loadFlag = true;
				}
					
				for (var j=0; j<len; j++) {
					G(tabArray[j]).className = cssName;
				}
				tab.className = cssName+"2";

				return false;
			}
		})(tabItem));
	}
}


function attachTabsContent(tabArray,contentArray) {

	var len = tabArray.length;
	var clen = contentArray.length;
	
	//the count of tab MUSK EQUAL TO the count of content
	if (len != clen) return false;

	for (var i=0; i<len; i++) {
		var tabItem = G(tabArray[i]);
		addEvent(tabItem, "click",(function(index) {
			return function() {
				for (var j=0; j<len; j++) {
					G(contentArray[j]).style.display= "none";
				}
				G(contentArray[index]).style.display= "block";
				return false;
			}
		})(i));
	}
}

