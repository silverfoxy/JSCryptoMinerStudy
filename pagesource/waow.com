<script type="text/javascript">
function CDEVweatherPage(url, curl){

  var $container = $wn('#CDEV-col4-weather'),
    wxCl_url = curl,
    zipcode = ['54401','54521','54451','54501','54481','54494'],
    totalZips = zipcode.length,
    h = 0,
    timer;

  function makeRequest(url){

    $container.find('.wx-location').text('');
    $container.find('.temperature').html('');
    $container.find('.wx-icon')
      .text('')
      .css({
        'background-image' : 'none'
        });
    $container.find('.forecast').html('');

      WNHttpRequestManager.makeRequest(url, {
        onSuccess: function() {

        // Load API data
            var $data = $wn(this.response.responseXML),
              $city = $data.find('City'),
              $cO = $data.find('CurrentObservation'),
              $tF = $data.find('DailyForecast').find('Day:eq(0)'),
              cityName = $city.attr('Name'),
              stateAbbr = $city.attr('StateAbbr');

            if($data.children().length < 1){
              this.onError();
            }

          $container.find('.wx-location').text(cityName + ', ' + stateAbbr);
          $container.find('.temperature').html($cO.attr('TempF') + '&deg;');
          $container.find('.wx-icon')
            .text($cO.attr('Sky'))
            .css({
              'background-image' : 'url(http://ftpcontent.worldnow.com/wncustom/wx_icons/wsi55/' + $cO.attr('IconCode') + '.png'
              });
          $container.find('.hi').html($tF.attr('HiTempF') + '&deg;');
          $container.find('.lo').html($tF.attr('LoTempF') + '&deg;');
          
          if((typeof(redAlert) !== 'undefined') && (redAlert === true)){
            $container.addClass('alert');
            $wn('#CDEV-col4-weather-radar').addClass('alert');
          }
          
            return;
        },
        onError: function(e){
          console.log('error');
          return;
        }
    });
    }
  $wn('.wx-submit').click(function(e){
    e.preventDefault();
    toSubmit();

    return;
  });

  $wn('.wx-search').keypress(function (e) {
        if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
            $wn('.wx-submit').click();
            return false;
        } else {
            return true;
        }
    }); 

  function toSubmit(){
    var regex = /^(\d{5})?$/,
      zip = $wn('.wx-search').val();

    if(zip.length < 1){
      alert('Please enter a 5-digit zip code.');
      return;
    }

    if(!regex.test(zip)){
      alert('Please enter a 5-digit zip code.');
      return;
    }

    clearInterval(timer);
    makeRequest('http://data-services.wsi.com/200904-01/710249140/Weather/Report/' + zip);
    return;   
  }

  function wxClInit(){
    wxClosings(this);
  }
   
  function wxClosings(xml){
    var fullFeed = xml.response.responseXML;
    var num_closings = $wn(fullFeed).find("NUM_CLOSINGS").text();
    $wn("#CDEV-col4-weather .button.closings").html(num_closings + " Closings");
    
    if (num_closings != '0'){
      $wn("#CDEV-col4-weather .button.closings").addClass('hasClosings');
    }
    
  }
   
  function wxClError(){}
   
  if(document.location.toString().indexOf("http:") != -1){  
    $wn(document).ready(function(){
      WNHttpRequestManager.makeRequest(wxCl_url, { onSuccess: wxClInit, onError: wxClError});
    });
  }

  function rotate(){

    if(h < totalZips-1){
      h++;
    } else {
      h = 0;
    }

    makeRequest('http://data-services.wsi.com/200904-01/710249140/Weather/Report/' + zipcode[h]);
  }

  makeRequest(url);

  timer = setInterval(function(){rotate()},5000);
}

CDEVweatherPage('http://data-services.wsi.com/200904-01/710249140/Weather/Report/54401', 'http://ftpcontent6.worldnow.com/waow/newsticker/closings.xml'); 
</script></div><script>Worldnow.Helpers.debug('UB Redesign 2015 - CDEV - Col. 4 Weather(433871)', 'UB_433871');</script></div>
