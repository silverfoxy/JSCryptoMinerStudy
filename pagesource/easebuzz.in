<script>



    $(document).ready(function(){

        $('#enquirebtn').on('click',function(){
        var formdata= $('#enquiryform').serialize();
        //$(this).prop('disabled',true)
        $('#enquirebtn').addClass('disabled');
        $.ajax({
            method: 'POST',
            url: "/enquireForm",
            data: formdata
            })
                .done(function (data) {
                window.google_trackConversion({
                    google_conversion_id : 932546429,
                    google_remarketing_only :false,
                    google_conversion_label: "e-VRCPeThGsQ_Y7WvAM",
                    google_conversion_color:"ffffff",
                    google_conversion_format:"3",
                    google_conversion_language:"en"
                });
                    $('#enquirebtn').removeClass('disabled');

                    $('.error-messages-rfp').html('');
                    if(data.status=="formerrors")
                    {
                        $.each(data.errors,function(key,value){
                        $('.error-messages-rfp[data-for=' + key +']').html(value);

                        });

                    }
                    else if(data.status) {

                        $('.modalformdiv').hide();
                        $('.modalformsuccess').show();
                        $('#enquiryform')[0].reset();
                        Messenger().post({ message : 'Enquiry submitted' , type : 'success' });

                    }
                    else{
                        Messenger().post({ message : data.reason , type : 'error' });
                    }
            });
        });

    });

</script>


            
            
    
                
