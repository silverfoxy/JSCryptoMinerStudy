<div class="cappopupoverlay"></div>

<div class="cappopup"><div class="cappopupwrapper">
<a class="closepop" href="#"></a>
<div class="popcontent"></div>
</div></div>

<script>

jQuery(document).ready(function($){


	$('#spin_navigation li a').click(function(){

		var popc=$(this).attr('href');
		var popconent =$(popc).html();
		$('.popcontent').html(popconent);
		$('.cappopup').show().css('margin-top',$(window).scrollTop()+50);
		$('.cappopupoverlay').height($(document).height()).show();
		return false;

	});   

	
	$('.closepop').click(function(){
		$('.cappopup').hide();
		$('.cappopupoverlay').fadeOut();
		return false;
	});
	


$('.sub-menu').hide();	

$('.menu-item-has-children').click(function(){

$('.menu-item-has-children .sub-menu').toggle();		
});	
$('.sidemenu').each(function(){
$(this).find('.widget_nav_menu').before().append('<div class="menu_button"><img src="http://www.vidyanikethan.edu/wp-content/uploads/2017/11/menu_btn.png"/></div>');

})	




$('.menu_button').toggle( function() {
$(this).parents().find('.sidemenu').css({"right":"0px"});
}, function () {
 $(this).parents().find('.sidemenu').css({"right":"-280px"});
});




$('.sidemenu ul li a').click(function(){
$('.sidemenu ul li').removeClass('active');
$(this).parent().addClass('active');
});	

});	
</script>
</body>
</html>