<script type='text/javascript'>
    var opta = {
        feed: {},
        interval: {},
        getFeed: function (partido) {
            $.get('http://cms.elgrafico.com.ar/desarrollo2014/opta/feed.php', {feed: opta.feed[partido]}, function (data) {
                $('.mam-' + partido + ' .comentarioMam').text(data.comment);
                if (data.isInPlay) {
                    $('.mam-' + partido + ' .vivoMam').html('<div class="circuloVivoMam"></div> Vivo');
                    if (data.isHalfTime) {
                        opta.timers[partido].stop();
                        $('.mam-' + partido + ' .tiempoMam').text(data.period);
                    } else {
                        opta.timers[partido].start('.mam-' + partido + ' .tiempoMam', data.time, data.period);
                    }
                    $('.mam-' + partido + ' .resultado1Mam').text(data.score.team1);
                    $('.mam-' + partido + ' .resultado2Mam').text(data.score.team2);
                }
                if (data.isPreMatch) {
                    $('.mam-' + partido + ' .resultado1Mam').text('-');
                    $('.mam-' + partido + ' .resultado2Mam').text('-');
                    $('.mam-' + partido + ' .tiempoMam').text(data.schedule);
                } else {
                    if (data.matchEnded) {
                        opta.timers[partido].stop();
                        $('.mam-' + partido + ' .vivoMam').html('Finalizado');
                        clearInterval(opta.interval[partido]);
                        $('.mam-' + partido + ' .resultado1Mam').text(data.score.team1);
                        $('.mam-' + partido + ' .resultado2Mam').text(data.score.team2);
                    } else if (!data.score) {
                        $('.mam-' + partido + ' .resultado1Mam').text('-');
                        $('.mam-' + partido + ' .resultado2Mam').text('-');
                    }

                }
            }, 'json');
        },
        timers: {}
    };
</script>

