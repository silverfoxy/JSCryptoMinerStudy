<script>

var posicionAlternativo = 0;
var noAlternativos = 0;

function anteriorAlternativo () {

    if (posicionAlternativo < 1) {
        return false;
    }

    posicionAlternativo--;
    var ocultar = posicionAlternativo+1;
    document.getElementById('infoDirecto'+ocultar).style.display='none';
    document.getElementById('infoDirecto'+posicionAlternativo).style.display='block';
    document.getElementById('botonSiguiente').className='boton siguiente';

    if (posicionAlternativo < 1) {
        document.getElementById('botonAnterior').className='boton anterior inactivo';
    }
}

function siguienteAlternativo () {

    if (posicionAlternativo >= -1) {
        return false;
    }

    posicionAlternativo++;
    var ocultar = posicionAlternativo-1;
    document.getElementById('infoDirecto'+ocultar).style.display='none';
    document.getElementById('infoDirecto'+posicionAlternativo).style.display='block';
    document.getElementById('botonAnterior').className='boton anterior';

    if (posicionAlternativo >= -1) {
        document.getElementById('botonSiguiente').className='boton siguiente inactivo';
    }
}

function gestionarBotonera () {

    if (noAlternativos > 1) {

        if (posicionAlternativo < 1) {
            document.getElementById('botonAnterior').className='boton anterior inactivo';
        }

        if (posicionAlternativo >= -1) {
            document.getElementById('botonSiguiente').className='boton siguiente inactivo';
        }
    }
}
gestionarBotonera();
</script>
