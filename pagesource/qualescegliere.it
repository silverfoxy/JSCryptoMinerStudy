function validateFooterform() {
		
	var checked = jQuery( "fieldset.footer-fieldset > input" ).is(':checked');
		
	if (checked) {
		jQuery( "fieldset.footer-fieldset > span" ).css('display', 'none');
		return true;
	} else {
		jQuery( "fieldset.footer-fieldset > span" ).css('display', 'block');
		return false;
	}
	
}

function validateExitform() {
		
	var checked = jQuery( "fieldset.exit-fieldset > input" ).is(':checked');
		
