        var calVol = Math.floor(((volume - 0) / (100 - 0)) * 100);
        $slider.find(".video-control-slider-main").css("width", volume);

        calVol = calVol > 0 ? calVol : 0;
        if (!notrecord) {
            currentVolume = calVol;
