        $slider.find(".video-control-slider-main>i").attr("title", 'éŸ³é‡ï¼š' + calVol + '%');
        if (calVol > 0) {
            $(".video-control-volume").removeClass("mute");

        }
        if (window.ChangeVolume) {
            ChangeVolume(calVol);
        }
    }
    $(".video-control-volume>.icon").click(function () {
        var $this = $(this).parents(".video-control-volume");
        $this.toggleClass("mute");
        var myVolume = 0;
        if (!$this.hasClass("mute")) {
            myVolume = currentVolume;

