                $scheduleCont.show();

                $rankMenu.removeClass('active');
                $scheduleMenu.addClass('active');
            }
            else {
                $scheduleCont.hide();
                $rankCont.show();

                $scheduleMenu.removeClass('active');
                $rankMenu.addClass('active');
            }

            return false;
        });
        /*************** Sidebar end **************/
    </script>

<script type="text/javascript">
    function transferClick(el) {
        //console.log($(el).attr("data-link"));
