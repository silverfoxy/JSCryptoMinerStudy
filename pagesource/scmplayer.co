<script type="text/javascript" >
require([
	'knockout','jquery','underscore',
	'util/parseparams',

	'text!view/player.html',
	'text!view/playlist.html',
	'text!view/wizard.html',

	'scm','scm.ui','scm.wizard',

	'jquery.scrollto',
	'jquery.colorbox',

	'binding/slider',
	'binding/scm'
	],function(ko,$,_,parse,playerView,playlistView,wizardView,SCM){
    SCM.gallery = ko.observableArray();
    SCM.isBlog = false;

		//Views
		$("#playerW").html(playerView);
		$("#playlistW").html(playlistView);
		$("#wizard").html(wizardView);
    var $bsaHtml = $("#bsap_1306386").parent().clone();
		// $("#bsap_1306386").parent().remove();

		//hash params
		var params = parse(location.hash.substr(1)); 
		
		//restore localStorage config
		if(localStorage && localStorage['script']){
			SCM.setScript(localStorage['script']);
			$('#restoreTip').show()
				.on('click','a',function(){
					localStorage.removeItem('script');
					$('#restoreTip').hide();
				});
		}

		//hash config
		SCM.config(params);
		//if has params scroll to wizard
		_(params).each(_.once(function(){
			$('#containerW').scrollTo($('#setup'),{duration:0,axis:'Y'});
		}));

		//track self hosted SCM 
		if(SCM.baseURL()) ga('send','event', 'Self Hosted', SCM.baseURL());

		//Knockout bindings
		$('#skin,body').each(function(){
			ko.applyBindings(SCM,this);
		});

		SCM.step.subscribe(function(step){
      if (!window.onbeforeunload) {
        window.onbeforeunload = function () { 
          return 'Are you sure of leaving Setup Wizard?'; 
        }
        // $.getScript('//go.oclasrv.com/apu.php?zoneid=1019804');
        // $.getScript('//pl13436211.puserving.com/12/06/70/12067055eda005e387f74dd41d8ebf4e.js');
      }
			//smooth scrolling
			$('#containerW').scrollTo('#setup',{duration:200,axis:'Y'});
			//export script
			if(step==4){
				//Done
				var script = SCM.getScript();
				$('#scriptDone').val(script);
				if(localStorage) localStorage['script'] = script;

				//track custom skins
				if(SCM.skin()==params.skin) 
					ga('send','event','Custom Skins',document.referrer, params.skin);
			}
		});
		$('#importButton').click(function(){
			//import script
			SCM.setScript($('#importField').val())
			SCM.step(1);
		});
		$('#scriptDone').click(function(){
			//auto select all
			this.focus();
			this.select();
		});
		$('#container').on('click','a',function(e){
			var href = $(this).attr('href');
			//smooth scrolling
			if(href.match(/#/)){
				$('#containerW').scrollTo($(href),{duration:200,axis:'Y'});
				e.preventDefault()
			}
		});

		//autolist links
		$('#autolist a').click(function(e){
			SCM.autoPlaylist($(this).text());
			e.preventDefault();
		});

		//plus one, twitter, google analytics
		require([
      'https://platform.twitter.com/widgets.js',
      '//s3.buysellads.com/ac/bsa.js'
		]);
    var shown = false;
    function checkAd(){
      if(SCM.showPlaylist() && !shown){
        // $.getScript('https://s3.buysellads.com/ac/bsa.js');
        $('#bsap_1306386').html('<iframe src="/ads/300_250.html" scrolling="no" width="300" height="250" frameborder="0"></iframe>');
        shown = true;
      }
    }
    SCM.showPlaylist.subscribe(checkAd);
    checkAd();

		$('.gallery').on('click','a',function(e){
      $.colorbox({
        href:$(this).attr('href'),
        iframe: true, 
        width: "940px", 
        height:"670px",
        opacity: 0.8
      });
      // window.open('https://go.ad2upapp.com/afu.php?id=1019815','_blank');
			e.preventDefault();
		});
	});
</script>