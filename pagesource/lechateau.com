<p><script>
        // read 
        var closePromo = $.cookie("closePromo");
        if (closePromo == "true") {
            $("#banner-top-promo").hide();
        } else {
            $("#banner-top-promo").show();
            var shippingPromoSlider = new MasterSlider();
            shippingPromoSlider.setup('masterslidertop', {
                width: 815, // slider standard width
                height: 38, // slider standard height
                heightLimit: true,
                space: 20,
                view: 'basic',
                fullwidth: true,
                loop: true,
                layout: 'partialview',
                instantStartLayers: true,
                autoplay: 'true',
                startOnAppear: true,
                fillMode: 'center',
                // more slider options goes here...
                // check slider options section in documentation for more options.
            });
            // adds Arrows navigation control to the slider.
            shippingPromoSlider.control('arrows', {
                autohide: false,
                hideUnder: true
            });
        }

        // hide the promo bar
        $(document).ready(function() {
            $(".site-wide .promo #close-promo").click(function() {
                $("#banner-top-promo").hide();
                // create 8 hours cookie
                var CookieEndDate = new Date();

                CookieEndDate.setTime(CookieEndDate.getTime() + (8 * 60 * 60 * 1000));
                $.cookie("closePromo", "true", {
                    path: '/',
                    expires: CookieEndDate
                });
            });
        });
    </script></p>
