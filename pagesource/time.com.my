<script type="text/javascript">

jQuery(document).ready(function() {


	var availableTags = [];
	var availableTagsFB = [];
	var availableTagsFBiz = [];

	var availableProducts = [];

	var markersContent = [];

    var searchContent = [];

	jQuery.ajax({
		type: 'GET',
		url: "https://apps.time.com.my/dealer-api/index.php/redblock/coverage/search/",
		dataType: "jsonp",
		crossDomain: true,
		success: function(response) {

            for (var i=0; i<response.length;i++)
            {

                markersContent.push([[response[i].blk_name],[response[i].blk_address1+" "+response[i].blk_address2],[response[i].blk_city],[response[i].blk_postcode]]);

                var searchTag = response[i].blk_name;

                //To add street and city
                if (response[i].blk_address2 == "" || response[i].blk_address2 == "-")
                    var searchTag = response[i].blk_name+", "+response[i].blk_address1+", "+response[i].blk_city;
                else
                    var searchTag = response[i].blk_name+", "+response[i].blk_address1+", "+response[i].blk_address2+", "+response[i].blk_city;

            searchContent.push(searchTag); // the content to be searched

            availableTags.push(searchTag); // add the tag for autocomplete search 

            if (response[i].product == "FIBRE BROADBAND") //add the tag only for FIBRE BROADBAND
            	availableTagsFB.push(searchTag);

            else if (response[i].product == "FIBRE BUSINESS BROADBAND") //add the tag only for FIBRE BUSINESS BROADBAND
            	availableTagsFBiz.push(searchTag);

            }//end of for loop

        }//end of success

    });//end of ajax call


jQuery(document).on('click touchstart', '.icon-plan-personal', function() { //only show the business tags
    jQuery( "#edit-search" ).autocomplete({
     source: availableTagsFB
 });
});

jQuery(document).on('click touchstart', '.icon-plan-business', function() { //only show the personal tags
    jQuery( "#edit-search" ).autocomplete({
     source: availableTagsFBiz
 });
});

var currentURL = window.location.pathname;

if (currentURL.indexOf('small-business') >= 0 || currentURL.indexOf('755') >= 0) { //only show the business tags
    jQuery( "#edit-search" ).autocomplete({
     source: availableTagsFBiz
});
}


else if (currentURL.indexOf('personal') >= 0 || currentURL.indexOf('751') >= 0) { //only show the personal tags
    jQuery( "#edit-search" ).autocomplete({ 
     source: availableTagsFB
});
}




// SEARCH FUNCTION START
jQuery("#timeform-check-coverage-form").submit(function (e) {
    e.preventDefault();
    
    var searchValue = jQuery("#edit-search").val(); //look for the value
    var found = false;

    for (var i=0; i<markersContent.length;i++)
    {
        // var content = markersContent[i].toString();
        // content = content.replace(","," ");
        // console.log(content);
        // if (searchContent[i].includes(searchValue))
        if (searchContent[i].indexOf(searchValue) > -1)
        {
            jQuery("#coverage-result").fadeOut();
            jQuery("#result-display").fadeIn();
            jQuery("#building-holder").text(markersContent[i][0]);
            jQuery("#address-holder").text(markersContent[i][1]);
            jQuery("#city-holder").text(markersContent[i][2]);
            jQuery("#postcode-holder").text(markersContent[i][3]);
            found = true;
        }
    }

    if (!found)
    {
        jQuery("#result-display").fadeOut();
        jQuery("#coverage-result").fadeIn();
    }

});
//SEARCH FUNCTION END 



});//end of doc ready


function getUrlParameter(sParam) {
	var sPageURL = decodeURIComponent(window.location.search.substring(1)),
	sURLVariables = sPageURL.split('&'),
	sParameterName,
	i;

	for (i = 0; i < sURLVariables.length; i++) {
		sParameterName = sURLVariables[i].split('=');

		if (sParameterName[0] === sParam) {
			return sParameterName[1] === undefined ? true : sParameterName[1];
		}
	}
}

