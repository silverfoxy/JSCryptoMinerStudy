			var $tsw = $wn("#WNTopStoryWrap");

			// Create healdine list box
			$tsw.find("#WNDS-20").after("<div id='tsHeadlines'><div class='wnDSHeader'><h3>Headlines</h3></div></div>");

			// Fix Top Story 1 timestamp and convert to time ago
			if($tsw.find("#WNDS-20 .wnDVSummary .wnDate").length) {
				var od = $tsw.find("#WNDS-20 .wnDVSummary .wnDate");
				var sd = fixDate(od);
				sd = sd.slice(sd.indexOf("Updated: ") + 9, sd.length);
				sd = new Date(sd);
				var iTime = timeDiff(currentTime.getTime(), sd.getTime());
				var timeAgo = tsr_timeAgo(iTime, currentTime, sd, "", $tsw.find("#WNDS-20 .wnDVSummary .wnDate"));
				$tsw.find("#WNDS-20 .wnDVSummary .wnDate").text(timeAgo);
			}

			// Fix all Top Story Col2 timestamps and convert to time ago
			for(var ii = 0; ii < hlNum; ii++) {
				var $t = $wn("#WNCol2 #WNDS-20 .wnDVSummary").eq(ii);
				if($t.find(".wnDate").length) {
					var od = $t.find(".wnDate");
					var sd = fixDate(od);
					sd = sd.slice(sd.indexOf("Updated: ") + 9, sd.length);
					sd = new Date(sd);
					var iTime = timeDiff(currentTime.getTime(), sd.getTime());
					var timeAgo = tsr_timeAgo(iTime, currentTime, sd, "", $t);
					$t.find(".wnDate").text(timeAgo);
				}
			}
			$wn("#WNCol2 #WNDS-20 .wnDVSummary").eq(0).hide().clone().show().addClass("secondary").show().appendTo($tsw.find("#WNDS-20"));
			var $tshl = $tsw.find("#tsHeadlines");
			for(var ii = 1; ii < hlNum; ii++) {
				$wn("#WNCol2 #WNDS-20 .wnDVSummary").eq(ii).hide().clone().show().appendTo($tshl);
				$tshl.find(".wnDVSummary").last().find(".wnDate").appendTo($tshl.find(".wnDVSummary").last().find("h4"));
			}
			$wn("#WNCol2 #WNDS-20 .wnDVSummary:gt(" + (hlNum + 14) + ")").hide();
			$tsw.find(".markNew").each(function() {
				if($wn(this).find("h4").length) {
					$wn(this).find("h4").prepend("<span class='new'></span>");
				} else {
					$wn(this).parents(".wnDVSummary").find("h4").prepend("<span class='new'></span>");
				}
			});
			$wn("#WNCol2 #WNDS-20 .wnDSHeader h3 span").text("More News");
			$tsw.find(".wnDVSummary").each(function(){
				var $st = $wn(this).find(".wnSectionTitle");
				if($st.length){
					$wn(this).find(".new").hide();
					var $stt = $st.text();
					if($stt.toLowerCase().indexOf("breaking") != -1){
						$wn(this).find("h4").prepend("<span class='badge breaking'><span class='left'></span>"+ $stt +"</span>");
					}
					else if ($stt.toLowerCase().indexOf("developing") != -1){
						$wn(this).find("h4").prepend("<span class='badge developing'><span class='left'></span>"+ $stt +"</span>");
					}
					else if ($stt.toLowerCase().indexOf("update") != -1){
						$wn(this).find("h4").prepend("<span class='badge update'><span class='left'></span>"+ $stt +"</span>");
					}
					else if ($stt.toLowerCase().indexOf("stream") != -1){
						$wn(this).find("h4").prepend("<span class='badge livestream'><span class='left'></span>"+ $stt +"</span>");
					}
					else {
						$wn(this).find("h4").prepend("<span class='badge'><span class='left'></span>"+ $stt +"</span>");
					}
				}
			});
		}
	});
