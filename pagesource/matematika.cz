<script type="text/javascript">
function toggle_chapters () {
    $('#article-chapters,').toggle();
    el = $('#article-chapters-button');
    content = el.html();
    if (content.indexOf("Skrýt") != -1) {
        el.html(content.replace("Skrýt", "Zobrazit"));
    } else {
        el.html(content.replace("Zobrazit", "Skrýt"));
    }
    return false;
}
</script>
