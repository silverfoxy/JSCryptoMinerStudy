<script>
var Slider = {
	c: 0,
	Slides: null,
	t: null,
	Container: null,
	Times: {
		Pause: 10000,
		Fade: 800
	},
	Playing: true,
	Init: function(){
		Slider.Container = $('#homebanner');
		Slider.Slides = $(Slider.Container).find('.slide');
		$('#homebanner-controls-start').hide();
		$(Slider.Container).find('.slide:gt(0)').hide();
		$("#homebanner-slidetext").html($(Slider.Slides[0]).html());
		Slider.CreateTicks();
		Slider.BindEvents();
		Slider.Start();
	},
	Start: function(){
		Slider.t = setInterval(function(){Slider.Run();},Slider.Times.Pause);
	},
	Stop: function(){
		window.clearInterval(Slider.t);
		Slider.t = null;
	},
	Run: function(){
		Slider.Next();
	},
	Next: function(){
		Slider.NextFade();
	},
	Previous: function(){
		Slider.PreviousFade();
	},
	NextFade: function(){
		$(Slider.Slides[Slider.c]).fadeOut(Slider.Times.Fade);
		$('.homebanner-controls-tick:eq('+Slider.c+')').removeClass('homebanner-controls-tick-active').addClass('homebanner-controls-tick-inactive');
		$('#homebanner-controls-container').removeClass('slide-active-'+(Slider.c+1));
		Slider.c++;
		if(Slider.c == Slider.Slides.length){
			Slider.c = 0;
		}
		$("#homebanner-slidetext").html($(Slider.Slides[Slider.c]).html());
		$('.homebanner-controls-tick:eq('+Slider.c+')').removeClass('homebanner-controls-tick-inactive').addClass('homebanner-controls-tick-active');
		$('#homebanner-controls-container').addClass('slide-active-'+(Slider.c+1));
		$(Slider.Slides[Slider.c]).fadeIn(Slider.Times.Fade);
		Shadowbox.setup();
	},
	PreviousFade: function(){
		$(Slider.Slides[Slider.c]).fadeOut(Slider.Times.Fade);
		$('.homebanner-controls-tick:eq('+Slider.c+')').removeClass('homebanner-controls-tick-active').addClass('homebanner-controls-tick-inactive');
		$('#homebanner-controls-container').removeClass('slide-active-'+(Slider.c+1));
		Slider.c--;
		if(Slider.c == -1){
			Slider.c = Slider.Slides.length-1;
		}
		$("#homebanner-slidetext").html($(Slider.Slides[Slider.c]).html());
		$('.homebanner-controls-tick:eq('+Slider.c+')').removeClass('homebanner-controls-tick-inactive').addClass('homebanner-controls-tick-active');
		$('#homebanner-controls-container').addClass('slide-active-'+(Slider.c+1));
		$(Slider.Slides[Slider.c]).fadeIn(Slider.Times.Fade);
		Shadowbox.setup();
	},
	CreateTicks: function(){
		for(i=0;i<Slider.Slides.length;i++){
			var theclass = "homebanner-controls-tick-inactive";
			if(i==0){
				theclass = "homebanner-controls-tick-active";
			}
			var tick = $("<a class=\"homebanner-controls-tick "+theclass+"\" href=\"#\">TICK</a>");
			tick.data( 'tick-number', i );
			tick.click(function(){
				var ticknumber = $(this).data('tick-number');
				if(ticknumber != Slider.c){
					$(Slider.Slides[Slider.c]).fadeOut(Slider.Times.Fade);
					$('.homebanner-controls-tick:eq('+Slider.c+')').removeClass('homebanner-controls-tick-active').addClass('homebanner-controls-tick-inactive');
					$('#homebanner-controls-container').removeClass('slide-active-'+(Slider.c+1));
					Slider.c = ticknumber;
					$("#homebanner-slidetext").html($(Slider.Slides[Slider.c]).html());
					$('.homebanner-controls-tick:eq('+Slider.c+')').removeClass('homebanner-controls-tick-inactive').addClass('homebanner-controls-tick-active');
					$('#homebanner-controls-container').addClass('slide-active-'+(Slider.c+1));
					$(Slider.Slides[Slider.c]).fadeIn(Slider.Times.Fade);
					Shadowbox.setup();
				}
				return false;
			});
			$("#homebanner-controls-ticks").append(tick);
		}
	},
	BindEvents: function(){
		$('#homebanner-controls-next').click(function(){
			Slider.Stop();
			Slider.Next();
			if(Slider.Playing){
				Slider.Start();
			}
			return false;
		});
		
		$('#homebanner-controls-previous').click(function(){
			Slider.Stop();
			Slider.Previous();
			if(Slider.Playing){
				Slider.Start();
			}
			return false;
		});
		
		$('#homebanner-controls-start').click(function(){
			Slider.Playing = true;
			Slider.Start();
			$(this).hide();
			$('#homebanner-controls-stop').show();
			return false;
		});
		
		
		$('#homebanner-controls-stop').click(function(){
			Slider.Playing = false;
			Slider.Stop();
			$(this).hide();
			$('#homebanner-controls-start').show();
			return false;
		});
	}
}

$(function(){
	Slider.Init();
	Shadowbox.init();
});
</script>


