                      $('#main_prod_img a:first-child').attr('href', href);
                      $('#m_img').fadeOut(200, function() {
                          $('#main_prod_img a:first-child').attr('href', href);
                          $('#m_img').attr('src', ($PRIMG_ROOT+$IM));
                          $('#m_img').fadeIn(200);
                      });
                  });
              }
          }
      });
});
</script>

