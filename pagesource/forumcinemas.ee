        return (uName != '' && uName != 'Kasutajanimi');
    }

    function showLoginBox(isShow) {
    }

    function hideLoginSubPanel(sender) {
        if (sender && $(sender).parents('#topLoginContainer').length == 0) {
            if ($(document.activeElement).attr('id') != 'topLoginUsername') {
                $('#loginSubPanel').stop(true, true);
                $('#loginSubPanel').slideUp(function () {
                    $('#loginShadow').hide();
                });
            }
        }
    }

    $(window).load(function () {

        if ($('#topLoginUsername').val().length == 0) {
            $('#topLoginUsername').val('Kasutajanimi');
        }
        var isPassword = ($('#topLoginPassword').val().length > 0);
        if (isPassword) {
            $('#topLoginPassword').show();
            $('#topLoginPasswordNull').hide();
        } else {
            $('#topLoginPassword').hide();
            $('#topLoginPasswordNull').show();
        }
    });

    $(document).ready(function () {
        $('#loginShadow').hide();
        $('*').focus(function (e) {
            //alert('focus');
            if (e.target) {
                hideLoginSubPanel(e.target);
            }
        });
        $(document).click(function (e) {
            if (e.target) {
                hideLoginSubPanel(e.target);
            }
        });

        $("#topLoginContainer").hover(function () {
            $("#topLoginContainer").addClass('hover');
        }, function () {
            $("#topLoginContainer").removeClass('hover');
        }).click(function (e) {

        });

        $("#topLoginContainer").find('input,label').focus(function () {
            $(this).addClass('focused');
        }).blur(function () {
            $(this).removeClass('focused');
        });


        $('#topLoginUsername').focus(function () {
            if ($(this).val() == 'Kasutajanimi') {
                $(this).val('');
            }
            $('html, body').animate({ scrollTop: $('#topLoginContainer').offset().top }, 500);
            $('#loginShadow:hidden').show();
            $('#loginSubPanel:hidden').slideDown();
        }).blur(function () {
            if (jQuery.trim($(this).val()) == '') {
                $(this).val('Kasutajanimi');
            }
        }).focus(function () {
            $(this).select();
        });

        $('#topLoginPassword').after('<input type="text" id="topLoginPasswordNull" tabindex="2" style="width: 120px;" name="password_null" class="inputTXT" />');
        $('#topLoginPasswordNull').val('Parool');
        $('#topLoginPasswordNull').hide();

        $('#topLoginPasswordNull').focus(function () {
            $('#topLoginPasswordNull').hide();
            $('#topLoginPassword').show();
            $('#topLoginPassword').focus();
        });

        $('#topLoginPassword').focus(function () {
            $('#loginShadow:hidden').show();
            $('#loginSubPanel:hidden').slideDown();
        }).blur(function () {
            if ($('#topLoginPassword').val().length == 0) {
                $('#topLoginPassword').hide();
                $('#topLoginPasswordNull').show();
            }
        }).focus(function () {
            $(this).select();
        });

    });
    //]]>
