<!-- SCAPE POPUP JS -->
<script>
var timer;
function closeScapePopup() {
    var scapePopup = document.getElementById('scape-popup');
    var scapePopupOverlay = document.getElementById('scape-popup--overlay');
    scapePopup.parentElement.removeChild(scapePopup);
    scapePopupOverlay.parentElement.removeChild(scapePopupOverlay);
    clearInterval(timer);
}
function GoToPopupURL(){
    closeScapePopup();
    window.location.href = 'http://www.forumcinemas.ee/CocaColaPlaza15';
}
CountDownTimer('March 13, 2016 23:59:59', 'scape-popup--countdown');
function CountDownTimer(date, id) {
    var scapePopupLoaded = new Date();
    var end = new Date(date);
    if (end > scapePopupLoaded) {
        if (sessionStorage.getItem('advertOnce') !== 'true') {
            document.getElementById('scape-popup').style.display = 'block';
            document.getElementById('scape-popup--overlay').style.display = 'block';
            sessionStorage.setItem('advertOnce', 'true');
        } else { 
            return;
        }         
    } else {
        return;
    };
    var _second = 1000;
    var _minute = _second * 60;
    var _hour = _minute * 60;
    var _day = _hour * 24;
    var showRemaining = function() {
        var now = new Date();
        if (now - scapePopupLoaded > 5000) {
            closeScapePopup();
            return;
        };
        var distance = end - now;
        if (distance < 0) {
            clearInterval(timer);
            document.getElementById(id).innerHTML = 'Aeg on täis!';
            return;
        }
        var hours = Math.floor(distance / _hour);
        var minutes = Math.floor((distance % _hour) / _minute);
        var seconds = Math.floor((distance % _minute) / _second);
        document.querySelector('.scape-hour').innerHTML = hours;
        document.querySelector('.scape-minute').innerHTML = minutes;
        document.querySelector('.scape-second').innerHTML = seconds;
    };
    timer = setInterval(showRemaining, 0);
}


</script>
