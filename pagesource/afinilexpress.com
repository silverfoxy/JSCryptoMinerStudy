                $('.orderboxdesc').slideDown('slow');
                $('.comordttl').hide();
                $('.orederboxmsg').hide();
                $('.orderboxdesclist').hide();
                $('.closechitm').hide();
                $('.openchitm').show();
                $('.boxcheckout').show();
            }

            function clickproduct(cssname) {
                $(cssname).next('.fpselquablock').slideToggle('slow');
                $('.orderboxdesc').slideDown('slow');
                $('.comordttl').hide();
                $('.orederboxmsg').hide();
                $('.orderboxdesclist').hide();
                $('.closechitm').hide();
                $('.openchitm').show();
                $('.boxcheckout').show();
            }


        });

    </script>
</div>

<script>
	$(document).ready(function () {
		$('a[href^="#"]').on('click', function (e) {
			e.preventDefault();

			var target = this.hash;
			var $target = $(target);

			$('html, body').stop().animate({
				'scrollTop': $target.offset().top
			}, 1000, 'swing', function () {
				window.location.hash = target;
			});
		});
		
	});
</script>
</body>
</html>