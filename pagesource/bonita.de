<div class="m-cart o-header__function js-cart-position" data-component="cart">
    <a href="https://www.bonita.de/cart" class="m-iconlink">
        

    <svg class="a-icon" ><use xlink:href="//www.bonita.de/assets/bonita/svg/sprite.svg?v=20180319083752#shopping-bag"></use></svg>        <span class="m-iconlink__text">Warenkorb&nbsp;</span><span class="m-cart__cart-amount js-icon-text">0</span>
    </a>

    <a href="https://www.bonita.de/cart" class="m-cart-notification js-cart"></a>

    <script type="text/template">
        
            {% if status == 'error' %}
        
            <p class="m-cart-notification__msg m-cart-notification__msg--error js-notification-error">
                

    <svg class="a-icon a-icon--medium u-color-error" ><use xlink:href="//www.bonita.de/assets/bonita/svg/sprite.svg?v=20180319083752#error"></use></svg>                
                    <span>{{ message }}</span>
                
            </p>
        
            {% else %}
        
            <p class="m-cart-notification__msg">
                

    <svg class="a-icon a-icon--medium u-color-success" ><use xlink:href="//www.bonita.de/assets/bonita/svg/sprite.svg?v=20180319083752#check"></use></svg>                <span>Zum Warenkorb hinzugefügt</span>
            </p>
        
            <img class="m-cart-notification__img lazyload" data-src="{{ img }}" alt="{{ name }}" />
            <div class="m-cart-notification__desc">
                <h3 class="small m-cart-notification__title">{{ name }}</h3>
                <div class="m-cart-notification__price">
                    <span class="a-price">
                    {% if price and price != originalPrice and originalPrice > 0 %}
                        <del>{{(originalPrice / 100) | number_format(2,',','.')}} EUR</del>
                        <ins>{{(price / 100) | number_format(2,',','.')}} EUR</ins>
                    {% else -%}
                        {{( price / 100) | number_format(2,',','.')}} EUR
                    {% endif -%}
                    </span>
                </div>
            </div>
            {% endif %}
        
    </script>
