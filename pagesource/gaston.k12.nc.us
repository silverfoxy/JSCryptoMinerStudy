                	setTimeout(function(){
						$("#our-schools-button").focus();
					}, 100);
				break;
			}
		});

		//CLOSE SCHOOLS BUTTON
		$("#close-school-list").keydown(function(e) {
            
            // CAPTURE KEY CODE
			switch(e.keyCode) {
				// CONSUME LEFT AND UP ARROWS
				case csKeyCodes.left:
				case csKeyCodes.up:
					e.preventDefault();
					
					$(this).prev().focus();

				break;
				
				// CONSUME RIGHT AND DOWN ARROWS
				case csKeyCodes.right:
				case csKeyCodes.down:
					e.preventDefault();
					
					$(".school-tab:first-child").focus();

				break;

				// CONSUME TAB KEY
				case csKeyCodes.tab:
					if(e.shiftKey){
						e.preventDefault();
						
						$("#our-schools-button").focus();

					} else {
						e.preventDefault();
					
						$(".school-tab:first-child").focus();

					}
				break;
				
				// CONSUME HOME KEY
				case csKeyCodes.home:
					e.preventDefault();
					
					// FOCUS FIRST ITEM
					$(".school-tab:first-child").focus();
				break;
				
				// CONSUME END KEY
				case csKeyCodes.end:
					e.preventDefault();
					
					// FOCUS LAST ITEM
