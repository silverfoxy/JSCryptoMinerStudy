				case csKeyCodes.left:
				case csKeyCodes.up:
					e.preventDefault();
					
					// IS FIRST ITEM
					if($(this).is(":first-child")) {
						// FOCUS DROPDOWN BUTTON
						$("#close-school-list").focus();
					} else {
						// FOCUS PREVIOUS ITEM
						$(this).prev().focus();
					}
				break;
				
				// CONSUME RIGHT AND DOWN ARROWS
				case csKeyCodes.right:
				case csKeyCodes.down:
					e.preventDefault();
					
					// IS LAST ITEM
					if($(this).is(":last-child")) {
						// FOCUS FIRST ITEM
						$(".school-tab:first-child").focus();
					} else {
						// FOCUS NEXT ITEM
						$(this).next().focus();
					}
				break;

				// CONSUME TAB KEY
				case csKeyCodes.tab:
					if(e.shiftKey){
						e.preventDefault();
						// IS FIRST ITEM
						$("#our-schools-button").focus();
					} else {
						e.preventDefault();
					
						$(".school-listing-container.active li:first-child a").focus();
					}
				break;
				
				// CONSUME HOME KEY
				case csKeyCodes.home:
					e.preventDefault();
					
					// FOCUS FIRST ITEM
					$(".school-tab:first-child").focus();
				break;
				
				// CONSUME END KEY
				case csKeyCodes.end:
					e.preventDefault();
					
					// FOCUS LAST ITEM
					$("#close-school-list").focus();
				break;

				case csKeyCodes.esc:
					$("#school-list-container").slideUp(300, "swing").attr("aria-hidden","true");
                	$(this).attr("aria-expanded","false");
