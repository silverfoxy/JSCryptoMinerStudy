.gb-social-icons {
	display: flex;
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
}

.gb-social-icons[data="true"] {
	display:none;
}

.social-icon {
	text-decoration: none;
	color: #fff;
	margin: 0px 4px;
	-ms-transition: all .3s ease 0s;
	-moz-transition: all .3s ease 0s;
	-webkit-transition: all .3s ease 0s;
	transition: all .3s ease 0s;
	font-size: 20px;
	display: block;
	width: 20px;
	height: 20px;
	text-align: center;
	box-sizing: border-box;
}

.social-icon[data="false"] {
	display:none;
}

.gb-social-icons.hover .social-icon {
	opacity: .5;
}

.gb-social-icons.hover .social-icon.hover {
	opacity: 1;
}

.social-icon::before {
	display: block;
}

.social-icon.facebook::before {
	content: "\e904";
}

.social-icon.twitter {
	margin-left: 1px;
}

.social-icon.twitter::before {
	content: "\e908";
	font-size: 16px;
	padding-top: 3px;
}

.social-icon.youtube {
	margin-left: 7px;
}

.social-icon.youtube::before {
	content: "\e909";
	font-size: 13px;
	padding-top: 4px;
}

.social-icon.instagram {
	margin-left: 5px;
}

.social-icon.instagram::before {
	content: "\e906";
}

#our-schools-button {
	color: #054C55;
	background: #fff;
	font-family: 'Roboto Slab', serif;
	font-size: 16px;
	font-weight: 700;
	padding: 13px 18px 13px 20px;
	align-self: flex-end;
	border-radius: 1px;
	margin-left: 20px;
	box-shadow: 2px 2px 1px rgba(0, 0, 0, 0.2);
    border: 1px solid;
	border-color: #fff;
    -ms-transition: all .3s ease 0s;
	-moz-transition: all .3s ease 0s;
	-webkit-transition: all .3s ease 0s;
	transition: all .3s ease 0s;
}

#our-schools-button span {
	position: relative;
	padding-right: 24px;
	letter-spacing: .56px;
	font-size: 14px;
}

#our-schools-button span::after {
	content: "\e90a";
	font-size: 10px;
	position: absolute;
	right: 2px;
	top: 6px;
	-ms-transition: all .3s ease 0s;
	-moz-transition: all .3s ease 0s;
	-webkit-transition: all .3s ease 0s;
	transition: all .3s ease 0s;
	display: block;
}

#our-schools-button.open {
	border-color: #fff;
}

#our-schools-button.open span::after {
	-webkit-transform: rotate(-90deg);
	-moz-transform: rotate(-90deg);
	-ms-transform: rotate(-90deg);
	-o-transform: rotate(-90deg);
	transform: rotate(-90deg);
}

