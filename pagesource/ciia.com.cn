var img=document.getElementById('pcimg');

var l=0,t=0;
var flagx=0,flagy=0;
var time=null;
time=setInterval(function  () {
	if (flagx==0) {
		l++;
		if (l>=box.offsetWidth-img.offsetWidth) {
			flagx=1
		};
	} else{
		l--;
		if (l<=0) {
			flagx=0;
		};
	};
	img.style.left=l+"px";
	if (flagy==0) {
		t++;
		if (t>=box.offsetHeight-img.offsetHeight) {
			flagy=1;
		};
	} else{
		t--;
		if (t<=0) {
			flagy=0;
		};
	};
	img.style.top=t+"px";
},10)
