 <script>
    function whereAmI() {
      latty = '';
      longy = '';
      var startPos;
      var geoOptions = {
         timeout: 10 * 1000
      }
      var geoSuccess = function(position) {
        startPos = position;
        latty = startPos.coords.latitude;
        longy = startPos.coords.longitude;
        // Now that we have the coordinates, let's find the zip code
        var xmlhttp = new XMLHttpRequest();
        var url = "https://maps.googleapis.com/maps/api/geocode/json?latlng="+latty+","+longy+"&components=postal_code&key=AIzaSyBVRTN3j0yjEHxDoWboYorzJzEyjaRml8M";

        xmlhttp.onreadystatechange = function() {
          if (this.readyState == 4 && this.status == 200) {
            var myArr = JSON.parse(this.responseText);
            var isFound = "no";
            for (i = 0; i < myArr.results.length; i++) {
              myAddress = myArr.results[i];
              for (x = 0; x < myAddress.address_components.length; x++) {
                myVar2 = myAddress.address_components[x];
                if(myVar2.types[0] == "postal_code" && isFound == "no") {
                  isFound = "yes";
                  document.getElementById('edit-submitted-zip-code').value = myVar2.short_name;
                };
              } // for loop
            } // for loop
          } // if readystate
        }; //onreadystatechange function
        xmlhttp.open("GET", url, true);
        xmlhttp.send();
      };
      var geoError = function(error) {
        console.log('Error occurred. Error code: ' + error.code);
        // error.code can be:
        //   0: unknown error
        //   1: permission denied
        //   2: position unavailable (error response from location provider)
        //   3: timed out
      };

      navigator.geolocation.getCurrentPosition(geoSuccess, geoError, geoOptions);


    };
    </script>
