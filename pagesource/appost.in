     <script>
         var searchFlag = false;
         $(document).ready(function () {
    $("#SearchPanel").toggle("slow");
   

    $("#flip").click(function () {
        searchFlag = true;
        $("#SearchPanel").toggle("slow");
        $(".gsc-clear-button").click(function () {
            if (searchFlag == true) {
                $("#SearchPanel").toggle("slow");
                searchFlag = false;
            }
        });
        });
    

 

    $('#idmenu').mnmenu();
    $('.Searchpopupdiv').hide();
    $(".Searchpopupdiv ul li").click(function () {       
        $("#txtSearch").val($(this).text());
        $(".Searchpopupdiv").hide();
    });  
    $("#txtSearch").on("keyup click input", function () {
        $('.Searchpopupdiv').hide();
        var i = 0;
        if (this.value.length > 0) {
            var list = $('.Searchpopupdiv ul li');
            $(list.get()).each(function () {
                if ($(this).text().toLowerCase().indexOf($("#txtSearch").val().toLowerCase()) != -1) {
                    $(this).show();
                    i = i + 1;
                }
                else {
                    $(this).hide();
                }

            });
        }
        else {
            $('.Searchpopupdiv').hide();
        }
        if (i != 0) {
            $('.Searchpopupdiv').show();
        }
    });
   
    $(".headersearchbutton").click(function () {

        var searchText = $("#txtSearch").val();
        if (searchText != "") {
            window.location.href = "http://localhost:2393/AP_Pos_Cir/" + searchText + ".aspx";
        }
        else {
            alert("Please enter Text to Search.");
        }
    });
});</script>
    
