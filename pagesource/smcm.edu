            jQuery(document).ready(function($) {
                // Send user to URL on <option> value
                  $('.action-menu').on('change', function () {
                      var url = $(this).val(); // get selected value
                      if (url) { // require a URL
                          window.location = url; // redirect
                      }
                      return false;
                  });

                // show/hide select options
                $('#identify').on('change.state', function() {
                $("#default").toggle($(this).val() == 'default');
              }).trigger('change.state');


                $('#identify').on('change.state', function() {
                $("#p-student").toggle($(this).val() == 'p-student');
              }).trigger('change.state');


                $('#identify').on('change.state', function() {
                $("#student").toggle($(this).val() == 'student');
              }).trigger('change.state');

                $('#identify').on('change.state', function() {
                $("#new-student").toggle($(this).val() == 'new-student');
              }).trigger('change.state');

                $('#identify').on('change.state', function() {
                $("#parent").toggle($(this).val() == 'parent');
              }).trigger('change.state');


                $('#identify').on('change.state', function() {
                $("#alum").toggle($(this).val() == 'alum');
              }).trigger('change.state');

                $('#identify').on('change.state', function() {
                $("#faculty").toggle($(this).val() == 'faculty');
              }).trigger('change.state');

                $('#identify').on('change.state', function() {
                $("#staff").toggle($(this).val() == 'staff');
              }).trigger('change.state');
            
            // Begin Flippy Cards in Education Section
            $('.flip').mouseover(function(){
                $(this).find('.card').addClass('flipped').mouseleave(function(){
                    $(this).removeClass('flipped');
                });
                return false;
            });
            // Remove Card Flip small viewport
            var alterClass = function() {
                var ww = document.body.clientWidth;
                        if (ww < 700) {
                            $(".card-core").removeClass("flip");
                    }
                        else if (ww >= 700) {
                            $(".card-core").addClass("flip");
                }
              };
              $(window).resize(function(){
                alterClass();
              });
          });
        </script>
     
