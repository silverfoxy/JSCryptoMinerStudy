<script type="text/javascript">
jQuery(function(){
    jQuery('.login-link').click(function(){
	var href = jQuery(this).attr('href');
	var height, width;
	if (jQuery(this).hasClass('login-google')) {
	    width = 450;
	    height = 500;
	} else if (jQuery(this).hasClass('login-yahoo')) {
	    width = 500;
	    height = 500;
	} else if (jQuery(this).hasClass('login-live')) {
	    width = 900;
	    height = 550;
	} else if (jQuery(this).hasClass('login-pixnet')) {
	    width = 450;
	    height = 500;
	} else {
	    width = 580;
	    height = 850;
	}

	var args = 'popup=1';
	if (href.match(/\?/)) {
	    window.open(jQuery(this).attr('href') + '&' + args, 'login', 'height=' + height + ',width=' + width + ',scrollbars=yes');
	} else {
	    window.open(jQuery(this).attr('href') + '?' + args, 'login', 'height=' + height + ',width=' + width + ',scrollbars=yes');
	}
	return false;
    });
});
</script>
