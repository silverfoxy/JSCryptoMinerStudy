	function appArrowsInit(){
		if($(".hp").length){
			var canvasArrow = $(".region .app-triangle");
			$(".region .app-triangle").each(function(i) {
				var cv = canvasArrow[i];
				
				//IF IE
				if (typeof(G_vmlCanvasManager) != "undefined"){G_vmlCanvasManager.initElement(cv);}
				var ctx = cv.getContext('2d');
				drawAppArrows(ctx);
			});
		}		
