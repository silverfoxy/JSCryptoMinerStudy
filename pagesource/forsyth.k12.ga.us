	function drawAppArrows(ctx){
		ctx.save();
		ctx.beginPath();
		ctx.moveTo(0.0, 90.0);
		ctx.lineTo(157.0, 0.0);
		ctx.lineTo(315.0, 90.0);
		ctx.lineTo(0.0, 90.0);
		ctx.closePath();
		ctx.fillStyle = "#ECEAE6";
		ctx.fill();
		ctx.restore();
   	 }
   	 
