			var ctx = canvas.getContext("2d");
			drawSchoolListArrow(ctx);
		}
	}
	
	function drawSchoolListArrow(ctx){
		ctx.save();
		ctx.beginPath();
		ctx.moveTo(0.0, 90.0);
		ctx.lineTo(157.0, 0.0);
		ctx.lineTo(315.0, 90.0);
		ctx.lineTo(0.0, 90.0);
