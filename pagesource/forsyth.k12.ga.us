	}
	
	function drawSpArrow(ctx){
		ctx.save();
		ctx.beginPath();
		ctx.moveTo(0.0, 55.0);
		ctx.lineTo(144.0, 0.0);
		ctx.lineTo(288.0, 55.0);
		ctx.lineTo(0.0, 55.0);
		ctx.closePath();
		ctx.fillStyle = '#C99600';
		ctx.fill();
		ctx.restore();
	}
	
