          <li @click="fetchNews(currentLeague, '')">Select Team</li>
          <li v-for="team in teams" :class="{ selected: currentTeam === team.code }" @click="fetchNews(currentLeague, team)">{{ team.name }}</li>
        </ul>
      </div>
    </div>
  </div>

  <div class="player-news__list">
    <player-news-item v-for="(entry, index) in entries" :key="entry.id" :entry="entry" :index="index" :loading="loading"></player-news-item>
  </div>
  <footer class="player-news__footer">
    <a class="link link_type-block" :href="footerLink" target="_blank">More Player News</a>
  </footer>
</section>

<script type="x-template" id="player-news-item">
  <div class="player-news__item" :class="{ 'player-news-item-enter': !loading, 'player-news-item-leave': loading }" :style="{ animationDelay: 30 * index + 'ms' }">
    <a class="player-news__link-wrapper" :href="entry.rotoworldLink" target="_blank">
      <header class="player-profile player-profile_type-small player-news__player-profile" :style="{ borderTopColor: '#' + entry.rowbgcolor }">
        <img class="image player-profile__image" :src="entry.playerImageUrl" :alt="entry.playerName" />
        <span class="team player-profile__team">
          <img v-if="entry.blockLogoImgPath" class="image player-profile__team-logo" :src="entry.blockLogoImgPath" :alt="entry.teamName" @error="defaultImgSrc" />
          {{ entry.playerPosition + ( entry.teamName !== '' ? ' - ' + entry.teamName : '' ) }}
        </span>
        <strong class="player-profile__name"><span>{{ entry.playerName }}</span></strong>
      </header>
    </a>
    <div class="story player-news__story">
      <a class="player-news__link-wrapper" :href="entry.rotoworldLink" target="_blank"><strong class="story__title">{{ entry.title }}</strong></a>
      <p class="story__text"><span v-html="entry.description"></span>
        <a class="player-news__link-wrapper" :href="entry.rotoworldLink" target="_blank">more</a>
      </p>
    </div>
    <footer class="player-news__meta">
      <span class="player-news__source" v-html="entry.newsSource"></span>
      <span class="player-news__date">{{ entry.newsDate }}</span>
    </footer>
  </div>
</script>
