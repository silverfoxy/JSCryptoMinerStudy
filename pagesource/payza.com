                [{ b: -1, d: 1, o: -1 }, { b: 2500, d: 500, x: -76, y: -25, o: 1 }],
                [{ b: -1, d: 1, o: -1 }, { b: 2500, d: 500, y: -80, o: 1 }],
                [{ b: -1, d: 1, c: { x: 120.0, t: -120.0 } }, { b: 6100, d: 400, c: { x: -120.0, t: 120.0 } }, { b: 10000, d: 500, y: -120 }],
                [{ b: 6500, d: 500, x: 220 }],
                [{ b: -1, d: 1, o: -1, r: 180, sX: -0.5, sY: -0.5 }, { b: 7000, d: 500, o: 1, r: 180, sX: 0.5, sY: 0.5 }, { b: 8000, d: 500, o: -1, r: 180, sX: 9, sY: 9 }],
                [{ b: -1, d: 1, o: -1, r: 180, sX: -0.5, sY: -0.5 }, { b: 8000, d: 500, o: 1, r: 180, sX: 0.5, sY: 0.5 }, { b: 9000, d: 500, o: -1, r: 180, sX: 9, sY: 9 }],
                [{ b: -1, d: 1, o: -1, r: 180, sX: -0.5, sY: -0.5 }, { b: 9000, d: 500, o: 1, r: 180, sX: 0.5, sY: 0.5 }, { b: 9500, d: 500, o: -1, r: 180, sX: 9, sY: 9 }]
            ];

            var jssor_1_options = {
                $AutoPlay: true,
                $SlideEasing: $Jease$.$InOutSine,
                $CaptionSliderOptions: {
                    $Class: $JssorCaptionSlideo$,
                    $Transitions: jssor_1_SlideoTransitions,
                    $Breaks: [
                        [{ d: 3000, b: 8000, t: 2 }],
                        [{ d: 2000, b: 5000 }],
                        [{ d: 3000, b: 9500, t: 2 }]
                    ]
                },
                $ArrowNavigatorOptions: {
                    $Class: $JssorArrowNavigator$
                },
                $BulletNavigatorOptions: {
                    $Class: $JssorBulletNavigator$
                }
            };

            var sliders = [];
            sliders.push(new $JssorSlider$('main-carousel', jssor_1_options));
            



            

            
            function scaleSlider(slider) {
                var refSize = slider.$Elmt.parentNode.clientWidth;
                if (refSize) {
                    refSize = Math.min(refSize, 3000);
                    slider.$ScaleWidth(refSize);
                }
                else {
                    window.setTimeout(function () { scaleSlider(slider); }, 30);
                }
            }

            for (var i = 0; i < sliders.length; i++) {
                var oneSlider = sliders[i];

                scaleSlider(oneSlider);

                $Jssor$.$AddEvent(window, "load", function () { scaleSlider(oneSlider); });
                $Jssor$.$AddEvent(window, "resize", function () { scaleSlider(oneSlider); });
                $Jssor$.$AddEvent(window, "orientationchange", function () { scaleSlider(oneSlider); });
            }

            
        };



        jssor_1_slider_init();

    </script>






    

    <script type="text/javascript">

        $(document).ready(function () {

            function setCarouselImagesForWindowWidth(idOfCarouselRoot) {

                var windowWidth = $(window).width();

                var $carouselRoot = $('#' + idOfCarouselRoot);

                var elementsToSetHeight = [];

                function addToElementsToSetHeight($elementsToAdd) {
                    $elementsToAdd.each(function () { elementsToSetHeight.push($(this)); })
                }

                
                var $containerWithinCarouselRoot = $carouselRoot.children('div');
                addToElementsToSetHeight($containerWithinCarouselRoot);

                
                var $loadingIndicatorContainers = $carouselRoot.find('.js-carousel-loading-indicator-container');
                addToElementsToSetHeight($loadingIndicatorContainers);

                
                var $containerOfSlides = $($carouselRoot.find('.js-carousel-container-of-slides')[1]);
                
                var $firstChildOfContainerOfSlides = $containerOfSlides.children(':first');
                addToElementsToSetHeight($firstChildOfContainerOfSlides);

                
                var $carouselSlides = $carouselRoot.find('.js-carousel-slide-unit');
                addToElementsToSetHeight($carouselSlides);

                
                var $carouselSlideImages = $carouselRoot.find('.js-carousel-slide-image');
                addToElementsToSetHeight($carouselSlideImages);

                var $carouselSlideImage1 = $carouselRoot.find('.js-carousel-slide-image-1');
                var $carouselSlideImage2 = $carouselRoot.find('.js-carousel-slide-image-2');
                var $carouselSlideImage3 = $carouselRoot.find('.js-carousel-slide-image-3');

                function setHeightForEachInElementsToSetHeight(heightToSet) {
                    for (var i = 0; i < elementsToSetHeight.length; i++) {
                        var $element = elementsToSetHeight[i];
                        $element.height(heightToSet);
                    }
                }

                if (windowWidth > 800) {
                    setHeightForEachInElementsToSetHeight('640px');

                    $carouselSlideImage1.attr('src', '/static/img/carousel-slide-1-desktop.jpg');
                    $carouselSlideImage2.attr('src', '/static/img/carousel-slide-2-desktop.jpg');
                    $carouselSlideImage3.attr('src', '/static/img/carousel-slide-3-desktop.jpg');
                }
                else {
                    setHeightForEachInElementsToSetHeight('1280px');

                    $carouselSlideImage1.attr('src', '/static/img/carousel-slide-1-mobile.jpg');
                    $carouselSlideImage2.attr('src', '/static/img/carousel-slide-2-mobile.jpg');
                    $carouselSlideImage3.attr('src', '/static/img/carousel-slide-3-mobile.jpg');
                }

            }

            var idOfCarouselRoot = 'main-carousel';

            $(window).resize(function () { setCarouselImagesForWindowWidth(idOfCarouselRoot); });

            setCarouselImagesForWindowWidth(idOfCarouselRoot);

        });

    </script>





    



