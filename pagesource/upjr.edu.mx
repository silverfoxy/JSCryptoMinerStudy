<script type="text/javascript" src="resources/js/lib/angular-sanitize/angular-sanitize.min.js"></script>
<script type="text/javascript" src="resources/js/lib/angular-animate/angular-animate.min.js"></script>
<script type="text/javascript" src="resources/js/lib/angular-touch/angular-touch.min.js"></script>
