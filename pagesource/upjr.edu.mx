</script>    <!-- Libs for index.php -->
    <script type="text/javascript" src="resources/js/lib/underscore/underscore-min.js"></script>
    <script type="text/javascript" src="resources/js/lib/clndr/clndr.min.js"></script>

    <!-- Custom scripts for index.php -->
    <script type="text/javascript" src="resources/js/custom/index/calendar.js"></script>
    <script type="text/javascript" src="resources/js/custom/index/index.js"></script>
    <script type="text/javascript" src="resources/js/custom/app.js"></script>
    <script type="text/javascript" src="resources/js/custom/events/UpcomingEventsController.js"></script>
    <script type="text/javascript" src="resources/js/custom/news/NewsController.js"></script>
    <script type="text/javascript" src="resources/js/custom/videos/VideosController.js"></script>
    <script type="text/javascript" src="resources/js/custom/general/SideImagesController.js"></script>
    <script type="text/javascript" src="resources/js/custom/general/TopMenuController.js"></script>
    <script type="text/javascript" src="resources/js/custom/general/SideMenuController.js"></script>
    <script type="text/javascript" src="resources/js/custom/general/BottomMenuController.js"></script>
    <script type="text/javascript" src="resources/js/custom/general/MainSliderController.js"></script>

</head>

    <body>
        <!-- WRAPPER -->
        <div id="wrapper">
            <!-- HEADER  -->
            <header>
                <!-- TOP BAR -->
                <section class="topbar">
                    <div class="container" ng-controller="TopMenuController">

                        <!-- Topbar buttons -->
                        <div class="topbar__buttons">
                            <a href="">Inicio</a><span></span>
                            <a ng-repeat-start="(keys, menu) in topMenuData" href="{{menu.url}}">{{menu.title}}</a><span ng-repeat-end></span>
                            <a href="#"><div class="icon search"></div></a>
                        </div>

                    </div>
                </section>



                <!-- BANNER -->
                <section class="slide slide--index" ng-controller="MainSliderController">

                     <!-- MAIN MENU -->
                    <div class="container">

                        <nav class="main-menu main-menu--index">
                           <div class="main-menu__container">
                                <!-- LOGO -->
                                <div class="logo">
                                    <a href="{{home_index}}"><img src="resources/img/logo_upjr.png" alt="Logo UPJR"></a>
                                </div>
                                
                                <ul class="menu menu--index" ng-controller="SideMenuController">
                                    <li ng-repeat="(keym, menu) in sideMenuData" class="menu__item">
                                        <a href="{{menuHref(menu)}}">{{menu.title}}<div class="{{iconMenu(keym)}}"></div></a>
                                        <ul ng-show="menu.children.length > 0" class="submenu">
                                            <li ng-repeat="(keys, submenu) in menu.children" class="submenu__item">
                                                <a href="{{submenu.url}}">{{submenu.title}}</a>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </nav>


                        <!-- Float bar -->
                        <div class="float-bar">
                            <div class="datetime">
                                <b>{{time}}</b> | {{date}}
                            </div>
                        </div>

                        <!-- Slide controls -->
                    </div>

                    <div slide-source="slide in imagesSliderData track by $index" class="slide-viewer" auto-advance="5000" slide-transition="fade" background-source="image_url" next-arrow-class="arrow-next" previous-arrow-class="arrow-prev" pin-container-class="pin-container" control-pin-class="ctrl-pin">
                        <div class="slide__caption"><p ng-bind-html='getCaption($index);'></p></div>
                    </div>

                </section>


                <!-- Second menu-->
                <section class="second-menu">
                    <div class="container">
                        <div class="row">

                            <div class="col large--12 medium--12 small--12">
                                <h3>Solicitar informes</h3>
                            </div>

                            <div class="col large--3 medium--3 small--3">
                                <input type="text" class="form__item" placeholder="Nombre">
                            </div>

                            <div class="col large--3 medium--3 small--3">
                                <input type="text" class="form__item" placeholder="Email">
                            </div>

                            <div class="col large--3 medium--3 small--3">
                                <input type="text" class="form__item" placeholder="Teléfono">
                            </div>

                            <div class="col large--3 medium--3 small--3">
                                <button class="button button--full button--orange">Solicitar folleto general</button>
                            </div>

                        </div>
                    </div>
                </section>
            </header>



            <section class="section-main">
                <div class="container">
                    <div class="row">
                        <main class="col large--9 medium--12 small--12">
                            <!-- EVENTOS -->
                            <article class="events">
                                <div class="row">
                                    <div class="col large--4 medium--6 small--12">
                                        <h3>Calendario UPJR</h3>
                                        <div class="widget-clndr" id="eventsCalendar">
                                            <!--img src="resources/img/calendar.jpg"-->
                                        </div>
                                    </div>

                                    <div class="col large--8 medium--6 small--12" ng-controller="UpcomingEventsController">
                                        <h3 ng-show="(eventData.length <= 0)">No se encontraron eventos próximos</h3>
                                        <h3><a href="eventos" ng-show="(eventData.length > 0)" >Ver todos los eventos</a></h3>
                                        <div ng-repeat="(key, event) in eventData">
                                            <a href="{{event.permalink}}">
                                                <div class="event">
                                                    <div class="event__title">{{event.title}}</div>
                                                    <div class="event__datetime"><div class="icon angle-right"></div><b>{{event.datetime}}</b><span ng-show="event.venue!==''"> | {{event.venue}}</span></div>
                                                </div>
                                            </a>
                                        </div>
                                    </div>
                                </div>

                            </article>


                            <article>
                                <div class="row" ng-controller="NewsController">
                                    <div class="col large--12 medium--12 small--12">
                                        <h3>Noticias UPJR | <a href="noticias"> Ver todas las noticias </a></h3>
                                    </div>
                                    <h3 ng-show="(articleData.length <= 0)" >No se encontraron noticias recientes</h3>
                                    <div class="col large--4 medium--4 small--12" ng-repeat="(key, article) in articleData">
                                        <div class="news">
                                            <div class="news__image">
                                                <img src="{{(article.firstImageURL !== '' ? article.firstImageURL : 'resources/img/news_default.jpg')}}">
                                            </div>
                                            <div class="news__title">{{article.title}}</div>
                                            <div ng-bind-html="getDateFormatted(article.date);"></div>
                                            <div class="news__description">{{article.shortDescription.substr(0, 50) + '...'}}</div>
                                            <a href="noticias/{{article.permalink}}"><button class="button button--small"><div class="icon search"></div>LEER MÁS</button></a>
                                        </div>
                                    </div>
                                </div>


                                <div class="line"></div>


                                <div class="row" ng-controller="VideosController">
                                    <div class="col large--12 medium--12 small--12">
                                        <h3>Videos UPJR | <a href="videos"> Ver todos los videos </a></h3>
                                    </div>
                                    <h3 ng-show="(articleData.length <= 0)" >No se encontraron videos</h3>
                                    <div class="col large--6 medium--6 small--12" ng-repeat="(key, article) in articleData">
                                        <div class="news">
                                            <div ng-bind-html='videoContent(key);'></div>
                                            <div class="news__title">{{article.title}}</div>
                                            <a href="videos/{{article.permalink}}"><button class="button button--small"><div class="icon search"></div>LEER MÁS</button></a>
                                        </div>
                                    </div>
                                </div>

                            </article>
                        </main>
                        <!-- ASIDE -->
                        <aside class="col large--3 medium--12 small--12">
                            <article>
                                <div class="follow-us">
                                    <b>SÍGUENOS: </b>
                                    <a href="https://www.facebook.com/universidadpolitecnicajuventinorosas" target="_blank"><div class="icon facebook-square size--24"></div></a>
                                    <a href="https://twitter.com/UPJR1" target="_blank"><div class="icon twitter-square size--24"></div></a>
                                    <a href="https://www.youtube.com/channel/UC0VgcWfycIldwDC-ljkhBsQ" target="_blank"><div class="icon youtube-square size--24"></div></a>
                                </div>

                            </article>


                            <article>
                                <div class="aside__buttons" ng-controller="SideImagesController">
                                    <div class="row">
                                          <div ng-repeat="image in imagesData" class="col large--12 medium--3 small--12">
                                             <a class="aside__button" href="{{image.destination_url}}"><img src="{{image.image_url}}" /></a>
                                          </div>
                                    </div>
                                </div>
                            </article>

                        </aside>
                    </div>
                </div>
            </section>







            <!-- FOOTER -->
            <footer>
                <section class="footer__logos">
                    <div class="container">
                        <img src="resources/img/logos.jpg">
                        <!-- <img src="resources/img/logos/logo1.png"> -->
                        <!-- <img src="resources/img/logos/logo1.png"> -->
                        <!-- <img src="resources/img/logos/logo1.png"> -->
                        <!-- <img src="resources/img/logos/logo1.png"> -->
                        <!-- <img src="resources/img/logos/logo1.png"> -->
                        <!-- <img src="resources/img/logos/logo1.png"> -->
                    </div>
                </section>

                <section class="footer__menu">
                    <div class="container" ng-controller="BottomMenuController">
                        <div class="row">

                            <div class="col large--3 medium--6 small--12">
                                <img src="resources/img/logo_upjr_footer.png" alt="Logo UPJR">
                            </div>

                            <div ng-repeat="(keym, menu) in bottomMenuData" class="col large--3 medium--3 small--3">
                                <h3>{{menu.title}}</h3>
                                <ul ng-show="menu.children.length > 0" class="list">
                                    <li ng-repeat="(keys, submenu) in menu.children" class="list__item">
                                        <a href="{{submenu.url}}">{{submenu.title}}</a>
                                    </li>
                                </ul>
                            </div>

                            <div class="col large--3 medium--6 small--12">
                                <h3>Contacto</h3>
                                <p>
                                    Hidalgo 102, Comunidad de Valencia
                                    <br>
                                    Santa Cruz de Juventino Rosas, Gto.
                                </p>
                                <p>
                                    C.P 38253
                                    <br>
                                    Tel.: 01 (412) 157 8000
                                </p>
                            </div>
                        </div>
                    </div>
                </section>

                <section class="footer__bottom">
                    <div class="container">
                        <p>© 2015 <b>UPJR</b> | <a href="#">Aviso de Privacidad</a> | <a href="#">Contactar al web admin</a></p>
                    </div>
                </section>
            </footer>
        </div>
    </body>
</html>