	function GetShortCutItem(sFH){
		var url;
		if (sFH.value.indexOf("/") > -1) {
		    // send the user to a non-product page (e.g. an ad landing page)
		    url = sFH.value;
		} else {
		    // send the user to a product page
		    url = "/product/product.aspx?id=" + sFH.value;
		}
        $("#ctl00_idSidebarUC_idProdShortCuts_idShortCutsEntry").val(sFH.options[sFH.selectedIndex].text);
        parent.window.location.href = AddClickSource(url, clicksource);
	}

	function AddClickSource(url,s) {
		//Strip off the hash to add it back later
		var hash = "";
		var hashindex;
		var paramsymbol;
		hashindex = url.indexOf("#");
		if (hashindex > -1) {
		    hash = url.substring(hashindex);
		    url = url.substring(0, hashindex);
		}
		//Determine whether to use ? or &
		if (url.indexOf("?") > -1) {
		    paramsymbol = "&";
		} else {
		    paramsymbol = "?";
		}
		return url + paramsymbol + "s=" + s + hash;
	}	    
</script>
