    $(document).ready(function () {

        $.ajaxSetup ({
            cache: false
        });

        bgViewportWidth = $("#idPanelView").width(); //The width of the panel view
        
        //Load the thumbnails
        $("#idThumbnailContainer").load('/slideshow/content/homepage/usa/english/layout.htm', filterThumbnails);

        // Set hover actions for navigation arrows.
        $("#idPrevSlide").hover(function() {
            $(this).attr("src","/slideshow/img/arrow_L_hover.png");
        }, function () {
            $(this).attr("src","/slideshow/img/arrow_L.png");
        });

        $("#idNextSlide").hover(function() {
            $(this).attr("src","/slideshow/img/arrow_R_hover.png");
        }, function () {
            $(this).attr("src","/slideshow/img/arrow_R.png");
        });

        //on first load we delay the thumbnail animations
        if (showThumbnails) {
            $("#idThumbnails").delay(loadDelay).show("slide", { direction: "up" },700);
        } else {
            $("#idSlideshowContainer").height($("#idPanelView").height());
        }
    });

    function bindThumbnails() {
        $(".thumbnail").hover(function() {
            $(this).find("img").animate({top:"6px"},250);
        }, function() {
            $(this).find("img").animate({top:"10px"},150);
        });

        totalThumbnails = $("#idThumbnails").find(".thumbnail").length;
        bgSegmentWidth = (bgWidth - bgViewportWidth) / (totalThumbnails-1);
        
        if (totalThumbnails > 1) {
            // Only show next/prev buttons if more than 1 thumbnail
            $(".nextprev").delay(loadDelay).show("slide", { direction: "down" },700);
        }

        $("#idPanelContainer").width(bgViewportWidth*totalThumbnails);
        //Create the empty panelcontent containers. These will be loaded with panel data on demand.
        for (i=0;i<totalThumbnails;i++) {
            $("<div class='panelcontent'></div>").appendTo($("#idPanelContainer"));
        }

        $(".thumbnail").click(function(){showPanel($(".thumbnail").index(this))});
        //if required, generate random number between 0 to the length of available thumbnails "minus" 1 because the index starts at 0.        
        if(requireRandomization == true) currIndex = Math.ceil(Math.random() * totalThumbnails - 1);

        //Show first slide in [loadDelay * 2] seconds and start the slideshow timer.
        window.setTimeout(function(){showPanel(currIndex);startSlideshow();},loadDelay*2);

        //disable slide advance when mouse is over or enters slideshow
        $("#idSlideshowContainer").bind('mouseenter mouseover',function(){
            mouseOver = true;
            pauseForced = true;
        });

        //re-enable slide advance when mouse leaves slideshow
        $("#idSlideshowContainer").bind('mouseleave mouseout',function(){
            mouseOver = false;
        });

    }

    function filterThumbnails() {
        //Filter thumbnails that need hiding, then bind thumbnail click events.
        $.getJSON('/login/currlogintype.ashx', function(data) {
            //Get current user's login type
            userlogintype = data;

            $('[data-login_specific]').each(function(index) {
                var accessList = $(this).attr('data-login_specific').replace(/ /g,"").split(",");
                // login_specific value of "ALL" means all logged in users. "NONE" means not logged in.
                if (($.inArray("ALL", accessList) > -1 && userlogintype == "NONE") ||
                    (($.inArray("ALL", accessList) == -1) && ($.inArray(userlogintype, accessList) == -1))) {
                    // If we specify ALL, but user is not logged in - OR - if we don't specify ALL, but the user is not one of the accesslist entries...
                    // Then current user's login type shouldn't see this, so remove it.
                    $(this).remove();
                }
            });

            $('[data-domain_specific]').each(function(index) {
                var domainList = $(this).attr('data-domain_specific').replace(/ /g,"").split(",");
                // Only show elements depending on the current website domain
                if ($.inArray(document.domain, domainList) == -1) {
                    // Current domain is not in the list of allowed domains, remove this element.
                    $(this).remove();
                }
            });

            bindThumbnails();
        });
    }

    function showPanel(index) {
        // don't switch to another panel until we finish showing the current panel
        if (panelChanging == true) {return false};
        panelChanging = true;

        // index wraparound check. if too low, reset to end. too high, reset to beginning
        if (index < 0) {index = totalThumbnails -1}
        if (index == totalThumbnails) {index = 0}
        var thumbnail = $(".thumbnail").eq(index);

        //if we came from the end to the beginning, make the selector reappear from the very left
        if (currIndex == totalThumbnails-1 & index == 0) {
            $("#idThumbSelector").css("left","-150px");
        };
        //if we came from the beginning to the end, make the selector reappear from the very right
        if (currIndex == 0 & index == totalThumbnails-1) {
            $("#idThumbSelector").css("left",bgViewportWidth);
        };

        //Slide the thumbnail selector to the current thumbnail
        if (thumbnail.position() != null) {
            $("#idThumbSelector").animate({left: thumbnail.position().left-7},500); //Need the -7 since the selector is wider than the thumbnails
        }

        currIndex = index;
        
        //Panels are added to the collection of .panelcontent divs inside idPanelContainer.
        //Only load a panel once; Load it if the current index isn't already listed in loadedIndexes.
        if ($.inArray(index,loadedIndexes) == -1) {
            $.get($(thumbnail).attr("src"),function(data){
                $(".panelcontent").eq(index).replaceWith(data);
                loadedIndexes.push(index); //append current index to the array loaded indexes so we don't load it again.
                fadeAndChange();
            },'html');
        } else {
            fadeAndChange();
        }

        function fadeAndChange() {
            //Private function so we can call it from different places within this showPanel() function.
            //Fade in the overlay to hide the panel, then fade it back out to reveal the slide once the slide has changed.
            //Had to do it this way to get around .png fading issues transparency in IE
            $("#idPanelWhiteOverlay").fadeIn(fadeSpeed,function() {
                $("#idPanelView").css("background-position",'-'+ index*bgSegmentWidth + 'px 0px');
                $("#idPanelContainer").css("left",-parseInt(index*bgViewportWidth));
                $("#idPanelWhiteOverlay").fadeOut(fadeSpeed);
                panelChanging = false;
            });
        }

    }

    function startSlideshow(){
        // Only start a slideshow if we have more than one thumbnail
        if (totalThumbnails > 1) {
            slideshowPlayCount = 0; //Reset the playcount
            slideshowTimer = setInterval(function() {
                if (slideshowPlayCount > (slideshowMaxCycles*totalThumbnails)) {pauseForced=true;clearInterval(slideshowTimer);};
                if (pauseForced == true) {
                    if (mouseOver == false) {
                        //mouse is no longer over slideshow, unpause for next slide change
                        pauseForced = false;
                    }
                    return false;
                }
                slideshowPlayCount += 1;
                showPanel(currIndex+1);
            },slideDuration);
        }
    }


</script>
