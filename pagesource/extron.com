<script type="text/javascript">
    var deckManagerCardHref = ""; // store link for selected card
    var deckManagerCardHrefStartRecord = ""; // store link for selected card only if user chooses Yes to start recording
    $(document).ready(function () {        
        initControlBar();

        // click event for record button
        $("#idRecord").click(function () {           
            var record = getCookie("recording");
            if (record == "") { // no recording in progress
                if ((window.location.pathname.indexOf("/product/deckmanager/deck.aspx") === 0 || window.location.pathname.indexOf("/product/deckmanager/thankyou.aspx") === 0) && $("#idRecord > i").hasClass("pause") == false) { // start recording
                    // add cookies and add record in database                    
                    $.ajax({
                        url: "/product/deckmanager/record.ashx?deckid=" + getDeckID(),
                        cache: false,
                        success: function (response) {
                            if (response == "true") {
                                $("#idRecord > i").addClass("pause"); // display pause button
                                $("#idReportIcon").parents("li").hide();
                                if (deckManagerCardHrefStartRecord != undefined && deckManagerCardHrefStartRecord != "") {
                                    window.location.assign(deckManagerCardHrefStartRecord); // go to selected page/card (user chose Yes in confirmation dialog)
                                }
                            }
                        }
                    });
                } else { // recording was stopped in another window/tab
                    stopRecording();                    
                }
            } else { // recording in progress - stop recording
                $("#idRecord > i").removeClass("pause"); // display record button
                $("#idPreviousCard").parents("li").hide();
                $("#idNextCard").parents("li").hide();
                // delete cookies and update record in database
                $.ajax({
                    url: "/product/deckmanager/record.ashx?stop=1",
                    cache: false,
                    success: function (response) {
                        // load the Thank You page
                        window.location.assign("/product/deckmanager/thankyou.aspx?deckid=" + response);
                    }
                });
            }
        });

        if (window.location.pathname === "/product/deckmanager/index.aspx") {
            $("#idReportIcon").addClass("active");
            $("#idHomeIcon").removeClass("active");
        }
    });


    // get cookie value by name
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    // extract deckid from query string
    function getDeckID() {        
        var regex = new RegExp("[\\?&]deckid=[0-9]{1,3}");
        var qs = regex.exec(window.location.href);
        if (qs != null) {
            return parseInt(qs[0].substr(1, qs[0].length - 1).replace("deckid=", ""));
        } else {
            return 0;
        }
    }

    function initControlBar() {
        var record = getCookie("recording");
        var deckID = getCookie("deckid");
        var homeLink = $("#idHomeIcon").attr("href") + "?deckid=";
        $("#idPreviousCard").parents("li").hide();
        $("#idNextCard").parents("li").hide();
        if (window.location.pathname.indexOf("/product/deckmanager/deck.aspx") === 0 || window.location.pathname.indexOf("/product/deckmanager/thankyou.aspx") === 0) {
            $("#idDeckManagerControlBar").removeClass("hide"); // display control bar
            if (record != "") { // recording in progress
                $("#idRecord > i").addClass("pause"); // display pause button
                $("#idDeckManagerControlBar").removeClass("animated"); // no animation
                $("#idDeckManagerControlBar").removeClass("slideInRight"); // no slide in
                $("#idReportIcon").parents("li").hide();
                $("#idHomeIcon").attr("href", homeLink + deckID);
                initNavigationButtons();
            } else { // no recording in progress
                if (window.location.pathname.indexOf("/product/deckmanager/thankyou.aspx") === 0) {
                    $("#idRecord > i").parents("li").hide();
                }
                $("#idRecord > i").removeClass("pause"); // display record button
                $("#idReportIcon").parents("li").show();
                $("#idHomeIcon").attr("href", homeLink + getDeckID());
            }
        } else {
            if (record != "") { // recording in progress
                $("#idRecord > i").addClass("pause"); // display pause button
                $("#idDeckManagerControlBar").removeClass("hide"); // display control bar
                $("#idReportIcon").parents("li").hide();
                $("#idHomeIcon").attr("href", homeLink + deckID);
                if (window.location.pathname.indexOf("/technology/landing/") === 0) {
                    $("#idDeckManagerControlBar").css("visibility", "visible"); // .wf-loading for Typekit causes control bar to be hidden in IE so add inline style to fix that
                }
                initNavigationButtons();
            } else { // no recording in progress
                $("#idRecord > i").removeClass("pause"); // display record button
                $("#idDeckManagerControlBar").addClass("hide"); // hide control bar
                $("#idReportIcon").parents("li").show();
            }
        }
    }

    function initNavigationButtons() {
        var array = decodeURIComponent(getCookie("deckcards")).replace(/[\n\r]+/g, "").split(",");
        var currentUrl = decodeURIComponent(window.location.href.replace(window.location.origin, ""));
        var previous = "";
        var next = ""
        var index = jQuery.inArray(currentUrl, array);
        if (index != -1) { //current page is on the deck
            previous = (index > 0) ? array[index - 1] : "";
            next = (index < array.length - 1) ? array[index + 1] : "";
            document.cookie = "prevdeckcard=" + currentUrl + "; path=/";
        } else {
            var prevCardCookie = getCookie("prevdeckcard");
            if (prevCardCookie != "") {
                index = jQuery.inArray(prevCardCookie, array);
                previous = (index > 0) ? array[index - 1] : "";
                next = (index < array.length - 1) ? array[index + 1] : "";
            }
        }
        if (previous != "") {
            $("#idPreviousCard").attr("href", previous).parents("li").show();
        }
        if (next != "") {
            $("#idNextCard").attr("href", next).parents("li").show();
        }
    }

    function stopRecording() {
        $("#idRecord > i").removeClass("pause"); // display record button
        if (window.location.pathname.indexOf("/product/deckmanager/deck.aspx") === 0) {
            $("#idDeckManagerControlBar").removeClass("hide"); // display control bar
        } else {
            $("#idDeckManagerControlBar").addClass("hide"); // hide control bar
        }
    }
</script>

