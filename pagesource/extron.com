    <script type="text/javascript">
        $(document).ready(function () {
            //*** START: track file downloads in Adobe Analytics (this handles hyperlinks only)
            // tracking of file downloads going through dltrack are handled in dltrack.aspx.vb)
            $(document).on("click", "a[href*='/download/files/'], a[href*='/dealer/files/'], a[href*='files/']", function () {

                var href = $(this).attr("href");
                if (href != undefined && href != "") {
                    var fileName = href;

                    // extract file path (without domain)
                    var idx = href.indexOf("/download/files/");
                    if (idx > -1) { fileName = href.substring(idx); }
                    idx = href.indexOf("/dealer/files/");
                    if (idx > -1) { fileName = href.substring(idx); }
                    idx = href.indexOf("files/");
                    if (idx > -1 && digitalData.page.category.primaryCategory == "Download") {
                        fileName = href.substring(idx);
                    }

                    var idxRedirect = fileName.indexOf("&redirect=true");   // the redirect query string parameter gets added on our 404 page
                    if (idxRedirect > -1) { fileName = fileName.substring(0, idxRedirect); }

                    digitalData.page.download.fileName = fileName;  // set JS variable for Adobe DTM
                    if (typeof (_satellite) != "undefined") { _satellite.track("filedownload"); }   // fire direct call rule in Adobe DTM

                    // add timeout so that the direct call rule has time to process on the Adobe server before a redirect            
                    if (idxRedirect > -1) {
                        setTimeout(function () {
                            window.location.replace(fileName);    // redirect to file
                        }, 250);
                    }
                }
            });
            //*** END: track file downloads in Adobe Analytics (this handles hyperlinks only)

        });
    </script>
    <script type="text/javascript">if (typeof (_satellite) != "undefined") { _satellite.pageBottom(); }</script>


