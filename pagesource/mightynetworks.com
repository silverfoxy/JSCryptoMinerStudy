            return $.doTimeout(2000, function() {
              return phoneNumberInput.removeClass('error-input');
            });
          };
          if (!phoneNumberInput || !submitButton || !phoneNumber) {
            message = 'Please enter a phone number.';
            return textMeTheAppFail(errorElement, message);
          } else {
            linkData = {
              channel: 'Mighty Networks Web',
              feature: 'Homepage Carousel'
            };
            if (sharingMeta) {
              linkData['data'] = {
                $always_deeplink: true,
                $deeplink_path: sharingMeta.deep_link_path,
                $og_title: sharingMeta.title,
                $og_description: sharingMeta.description,
                $og_image_url: sharingMeta.image_url
              };
            }
            return branch.sendSMS(phoneNumber, linkData, {
              make_new_link: true
            }, function(error) {
              if (error) {
                if (/[^\d]429$/.test(error.message)) {
                  message = 'Oops! Carrier limit reached, please try again later.';
                } else if (/[^\d]404$/.test(error.message)) {
                  message = 'Oops! Please try again later.';
                } else {
                  message = 'Oops! Enter a valid phone number.';
                }
                return textMeTheAppFail(errorElement, message);
              } else {
                return container.slideUp(function() {
                  successElement.slideDown();
                  return $.doTimeout(2000, function() {
                    return successElement.slideUp(function() {
                      return container.slideDown();
                    });
                  });
                });
              }
            });
          }
        };
      })(this);
    });
  
  }).call(this);
</script>
