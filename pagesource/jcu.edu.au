<div id="dialog-form" title="Ask Us">
<br>
  <form id="sendQuestion">
  <fieldset name="legend">
    <legend><p class="validateTips">Your question:</p></legend>
    <textarea name=legend title="question" id="question" style="width:90%" rows="2" class="text ui-widget-content ui-corner-all"></textarea>
    <p><label for="usertype">Are you a current JCU student?</label></p>
    <input title="usertype" name="usertype" value="Yes" class="radio" checked="checked" type="radio">Yes 
    <input title="usertype" name="usertype" value="No" class="radio" type="radio">No
  </fieldset>
    <br> 
    <div class="ui-dialog-buttonset"><button type="submit" id="askbutton" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" role="button" aria-disabled="false"><span class="ui-button-text">Ask a Question</span></button></div>
  </form>

</div>  
<br>
</div>
<script>
$("#askbutton").click(function( e ) {

if(e.preventDefault){ e.preventDefault()}
    else{e.stop()};

    e.returnValue = false;
    e.stopPropagation();

    var question = $( "#question" ),
      usertype = $("input[type=radio][name=usertype]:checked").val(),
			url = "app/answers/list/kw/",
      allFields = $( [] ).add( question ).add( usertype ),
      tips = $( ".validateTips" );
 
    function updateTips( t ) {
      tips
        .text( t )
        .addClass( "ui-state-highlight" );
      setTimeout(function() {
        tips.removeClass( "ui-state-highlight", 1500 );
      }, 500 );
    }
 
    function checkLength( o, n, min, max ) {
      if ( o.val().length > max || o.val().length < min ) {
        o.addClass( "ui-state-error" );
        updateTips( "Length of " + n + " must be between " +
          min + " and " + max + "." );
        return false;
      } else {
        return true;
      }
    }
	
    function checkType( ) {
	    user = $("input[type=radio][name=usertype]:checked").val();
      if (user == "Yes" ) {      
				 url= "https://jcu.custhelp.com/" + url;				
      } else {
        
				url= "https://jcu1.custhelp.com/" + url;				
      }
			return true;	
    } 

    var bValid = true;
          
    bValid = bValid && checkLength( question, "question", 5, 300 );
    bValid = bValid && checkType();

    if ( bValid ) {
	window.location.href = url + question.val() + "/search/1";
        return false;
    }
});
</script>



