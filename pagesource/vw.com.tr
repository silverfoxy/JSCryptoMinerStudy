	if(mevcutKonum < sonNokta)
	{ mevcutKonum += 1; }
	else
	{ mevcutKonum -= 1; }
	$('div#binek ul').attr('class', 'binekleft'+mevcutKonum);
	if(mevcutKonum==sonNokta){ clearInterval(timer);}
}

