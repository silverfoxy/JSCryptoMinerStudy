            });
            // Mass Banner JS End Here

            // Testimonial JS Start Here
            var swiper = new Swiper('.swiper-container.testimonialSwiper-Container', {
                nextButton: '.testimonial-swiper-button-next',
                prevButton: '.testimonial-swiper-button-prev',
                loop: true,
                intialSlide: 0,
                centeredSlides: true,
                slidesPerView: 'auto',
                spaceBetween: 30,
                grabCursor: true
            });
            // Testimonial JS End Here

            // Mobile Nav JS Start Here
            var anchor = document.querySelectorAll('button');

            [].forEach.call(anchor, function (anchor) {
                var open = false;
                anchor.onclick = function (event) {
                    event.preventDefault();
                    if (!open) {
                        this.classList.add('close');
                        open = true;
                    } else {
                        this.classList.remove('close');
                        open = false;
                    }
                }
            });
            // Mobile Nav JS End Here


        });
