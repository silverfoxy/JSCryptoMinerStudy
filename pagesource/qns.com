        function checkInputOnEmail(str) {
            var pattern = /^([a-z0-9_\.-])+@[a-z0-9-]+\.([a-z]{2,4}\.)?[a-z]{2,4}$/i;
            if (str !== '') {
                if (pattern.test(str)) {
                    return true;
                } else {
                    return false;
                }
            } else {
                return false;
            }
        }

        function isUrlValid(url) {
            return /^(http[s]?:\/\/)?(www\.)?([\d\w\.-]+)\.([\w\.]{2,5})/i.test(url);
        }


