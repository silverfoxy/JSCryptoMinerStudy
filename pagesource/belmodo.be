<script type="text/javascript">

	/* ****** menu ****** */

	$(window).on("scroll",function(){
		if ($(document).scrollTop() > 123)
		{
			$('body').addClass("scrolled");
		}
		else
		{
			$('body').removeClass("scrolled");
		}
	});

	$(window).on("resize",function(){
		mobilemenu();
	});

	$(document).ready(function(){
		mobilemenu();
	});

	$(window).on("scroll",function(){
		mobilemenu();
	});

	function mobilemenu()
	{
		if ($(window).width() <= 1024)
		{
			$("#menuwrapper > ul > li").each(function(){
				var dit = this;
				if ($(dit).find(".submenu").length > 0)
				{
					var href= $(dit).find("a").first().prop("href");
					$(dit).find("a").first().attr("data-href",href);
					$(this).find("a").first().removeAttr("href");
				}
			});

			var toph = $("#top").outerHeight();
			$("#menuwrapper").css("top",toph);
			$(" body.scrolled #menu #menuwrapper").css("top",toph - 10);
		}
		else
		{
			$("#menuwrapper").css("top",5);
			$(" body.scrolled #menu #menuwrapper").css("top",5);
		}
	}

	$("#menuwrapper > ul > li > a").on("click",function(){

		var dit = this;
		setTimeout(function(){
			var href = $(dit).data("href");
			console.log(href);
			$(dit).prop("href",href);
		},100);

		$(dit).parent().find(".submenu").slideDown();
	});

	$("#menubutton").on("click",function(){

		$("#menuwrapper").toggleClass('toonmenu');
	});

	/* ****** ****** */

	function slidetotop()
	{
		$("html, body").animate({ scrollTop: 0 }, "slow");
	}

	var catbol = $(".catbol");

	for(var i=0;i<catbol.length;i++) {
		var letter = catbol.eq(i).text();
		catbol.eq(i).text('');
		catbol.eq(i).append('<div><div>'+letter+'</div></div>');	
	}

	$(document).ready(function() {

		var wwww = 0;
		var wwwww = 0;

		$('#sliderbottom .sideblock').each(function() { wwwww += $(this).outerWidth(true) + 20; })

		$('#sliderbottom > div').width(wwwww);

		$(".dotdotdot").dotdotdot();

		if(!localStorage.getItem("belmodovisit"))
		{
			showcookies();
		}
		showcookies();

		$("#gsc-i-id1").attr("placeholder", "Geef hier uw zoekopdracht in");

		winkelmandje();
	});

	$("#zoekbutton").on("click",function(){


		$("#zoekbutton img").toggle();
		$("#topsearch").toggleClass("active");


		$("#winkelmandje").hide();
		$("#uppermenu > div > div:nth-child(2) > div:nth-child(1)").hide();

		if ($("#topsearch").hasClass("active"))
		{
			$("#winkelmandje").hide();
			$("#uppermenu > div > div:nth-child(2) > div:nth-child(1)").hide();
		}
		else
		{
			$("#winkelmandje").show();
			$("#uppermenu > div > div:nth-child(2) > div:nth-child(1)").show();
		}

	});

	$(window).on('resize',function(){
		if (typeof googleadWidth !== 'undefined') {
			googleadWidth();
			console.log('Functie googleaWidth gevonden.');
		} else {
			console.log('Functie googleadWidth niet gevonden.');
		}
		$(".dotdotdot").dotdotdot();
		$("#container").css("min-height","0");

		if ($(window).scrollTop() > 40)
		{
			$('#blueres, #taalres, #taalres2').hide();
		}
		else
		{
			if ($(window).width < 1025 && $(window).width > 550)
				$('#taalres').show();
			else if ($(window).width < 550)
				$('#blueres, #taalres2').show();
		}

	});

	$(window).on('scroll',function(){

		if ($(window).scrollTop() > 40)
		{
			$('#blueres, #taalres, #taalres2').hide();
		}
		else
		{
			if ($(window).width() < 1025 && $(window).width() > 550)
			{
				$('#taalres').show();
			}
			else if ($(window).width() < 550)
				$('#blueres, #taalres2').show();
		}

	});

	function winkelmandje()
	{
		$.post("/shop.php", { func: "whatInBag" }, function(data){
			var data = $.trim(data);
			if(data != "empty")
			{
				$("#winkelmandje_items").html("");
				var arrData = JSON.parse(data);
				if(arrData.length > 0)
				{
					$("#winkelmandje .aantal").text(arrData.length);
					var extrahtml = "";
					var totprijs = 0;
					$.each(arrData,function(key,line)
					{
						var prodnaam = line.Titel_NL;
						var prodaantal = line.Aantal;
						var prodprijs = line.Prijs;
						var div = '<div class="winkelmandje_item"><h2>'+prodnaam+'</h2>';

						$.each(line.Modifiers, function(key,value){
							div += '<span>'+value.Name_NL+': '+value.Optie_NL+'</span><br />';
						});
						div += '<span>Quantity: '+prodaantal+'</span>\
						<span>€'+prodprijs * prodaantal+'</span></div>';
						totprijs = totprijs + (parseFloat(prodprijs) * prodaantal);
						$("#winkelmandje_items").append(div);
					});

					$("#winkelmandje_subtotal_price").text("€" + totprijs);
					$("#winkelmandje").show();
				}
				else
				{
					$("#winkelmandje_items").html("");
					$("#winkelmandje .aantal").text("0");
				}
			}
		});
	}


	function showcookies()
	{
		if (!localStorage.getItem("belmodovisitt"))
			$("#cookieinfo").show();
	}

	function hidecookies()
	{
		$("#cookieinfo").hide();
		localStorage.setItem("belmodovisitt", "ja");
	}


	$("#btnnavtext").on("click", function(){
		$(".maincontainer header nav ul").slideToggle();
	});

	$(".menukruis").on("click",function(){
		$(this).toggleClass("box_rotate");
		$(this).parent().find(".submenu").slideToggle();
	});

	$(".share .sharebox a").on("click", function(e) {
		e.stopPropagation();
	});

	$(".share .sharebox").on("mouseenter", function(e){

		$(this).find(".shareboxbox").show();

		$(this).find(".shareboxbox a:nth-child(1)").show();

		setTimeout(function (){

			$(".shareboxbox:visible a:nth-child(2)").show();

		}, 50);

		setTimeout(function (){

			$(".shareboxbox:visible a:nth-child(3)").show();

		}, 100);

		setTimeout(function (){

			$(".shareboxbox:visible a:nth-child(4)").show();

		}, 150);
	});

	$(".shareboxbox").on("mouseleave", function(){
		$('.share .shareboxbox').hide();		
	});

	$("#searchform").on("submit",function(e){
		e.preventDefault();
		window.location = "/nl/zoekresultaten/" + $("#searchform input[name='googlesearch']").val();
	});
</script>

