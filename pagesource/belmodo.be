<script>
	$(document).ready(function(){
		$("#mainwrapper").css("padding-top",$("#top").height() + 25);
		bannerscript();

	});

	$(window).on("scroll",function(){
		bannerscript();
	});

	$(window).on("resize",function(){
		bannerscript();
	});

	function bannerscript()
	{
		$("#mainwrapper").css("padding-top",$("#top").height() + 25);
		
		$("#top").removeClass("takeovertop");
		$("footer").removeClass("takeoverfooter");
		// takeover

		if ($(".takeover").is(":visible") && $(window).width() > 640)
		{
			$("#mainwrapper").css("padding-top","0");

			if (!$("#top").hasClass("takeovertop"))
			{
				$("#top").addClass("takeovertop");
			}

			if (!$("footer").hasClass("takeoverfooter"))
			{
				$("footer").addClass("takeoverfooter");
			}

			if ($('body').hasClass("scrolled"))
			{
				if (!$("#top").hasClass("fixed"))
				{
					$("#top").addClass("fixed");
					$("#mainwrapper").css("padding-top","125px");
				}
			}
			else
			{
				$("#top").removeClass("fixed");
			}
		}

		// skyscraper

		if ($('body').hasClass("scrolled"))
		{
			var spaceright = $(window).width() - $(".maincontainer").outerWidth() - $(".maincontainer").offset().left;
			var spacetop = $("#top").offset().top - $(window).scrollTop() + $("#top").outerHeight() + 50;

			$(".skyscraper").removeClass("skyscraper").addClass("noscrolledskyscraper");
			$(".noscrolledskyscraper").css("right",spaceright - $(".noscrolledskyscraper").width());
			$(".noscrolledskyscraper").css("top",spacetop);

			$(".wideskyscraper").removeClass("wideskyscraper").addClass("noscrolledwideskyscraper");
			$(".noscrolledwideskyscraper").css("right",spaceright - $(".noscrolledwideskyscraper").width());
			$(".noscrolledwideskyscraper").css("top",spacetop);
		}
		else
		{
			$(".noscrolledskyscraper").removeClass("noscrolledskyscraper").addClass("skyscraper").removeAttr("style");
			$(".noscrolledwideskyscraper").removeClass("noscrolledwideskyscraper").addClass("wideskyscraper").removeAttr("style");
		}

		// leaderboard op mobile 
		var leaderboarddiv = $(".xl_leaderboard > div");
		if ($(window).width() < 995 && $(".xl_leaderboard").is(":visible"))
		{
			var ml = $(window).width() - leaderboarddiv.width();
			leaderboarddiv.css("margin-left",ml/2);
		}
		else
		{
			leaderboarddiv.css("margin-left","0px");
		}
	}

</script>

