			vorige.find("h2").height("auto");
			dit.find("h2").height("auto");
			vorige.find("h3").height("auto");
			dit.find("h3").height("auto");

			var h1 = vorige.find("h2").height();
			var h2 = dit.find("h2").height();

			var hh1 = vorige.find("h3").height();
			var hh2 = dit.find("h3").height();

			if (h1 > h2)
			{
				dit.find("h2").height(h1);
			}
			else if (h2 > h1)
			{
				vorige.find("h2").height(h2);
			}

			if (hh1 > hh2)
			{
				dit.find("h3").height(hh1);
			}
			else if (hh2 < hh1)
			{
				vorige.find("h3").height(hh2);
			}
		});

	}

