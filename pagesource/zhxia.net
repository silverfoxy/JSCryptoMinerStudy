        //改变下拉框时定位到相应站
        $(document).ready(function (e) {
            $(".form-control").change(function () {
                var years = $("#years").children('option:selected').val();
				var month= $("#month").children('option:selected').val();
				var day= $("#day").children('option:selected').val();


                if(years!="" && month !="" && day!="")
				{
					years=years.replace("年","");
					month=month.replace("月","");
					day=day.replace("日","");
					solar2lunar(years,month,day);	
				}


            });
        });



	function solar2lunar(year,month,day) {

    var lunar = calendar.solar2lunar(year,month,day);
    $('#solarlunar').html('阳历：'+lunar.cYear + '年' +lunar.cMonth +  '月' + lunar.cDay +'日<br />农历：'+lunar.lYear + '年' +lunar.IMonthCn+lunar.IDayCn+'');
	}
</script>


