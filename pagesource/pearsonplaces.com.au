<script>
    $(document).ready(function () {
        $('#SchoolTextBox').autocomplete({
            source: function (request, response) {
                $.ajax({

                    autoFocus: true,
                    type: 'POST',
                    autoSelect: true,
                    url: '/DesktopModules/PPLogin/PPLoginService.asmx/GetSchools',
                    contentType: 'application/json; charset=utf-8',
                    dataType: "json",
                    data: "{ term: '" + request.term + "' }",
                    success: function (json) {
                        {
                            var data = $.parseJSON(json.d);
                            var cleanData;
                            $.each(data, function (k, v) {
                                data[k].label = data[k].label.replace(/..School ID..\d*./, '');
                                data[k].value = data[k].value.replace(/..School ID..\d*./, '');
                            });
                            response(data);
                        }
                    }

                });
            },
            select: function (event, ui) {
                //var dirtyString = ui.item.value;
                //var cleanString = dirtyString.replace(/..School ID..\d*./, '');
                //console.log(cleanString);
                $('#SchoolTextBox').val(ui.item.value);
                $('#SchoolIdHiddenField').val(ui.item.id);
                return false;
            },
            change: function (event, ui) {
                if (!ui.item) {
                    $('#SchoolTextBox').val('');
                    $('#SchoolIdHiddenField').val('');
                }
            },
            minLength: 2
        });
    });
</script>
