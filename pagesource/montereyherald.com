<script type="text/javascript">
(function($) {
   var domain = "http://local.montereyherald.com";
   //JSON file lives in Akamai netstorage
   var jsonToGet = domain + '/assets/verticals.json';
   $.ajax({
      url: jsonToGet,
      dataType: 'jsonp',
      contentType: 'application/json',
      cache: true,
      async: false,
      jsonpCallback: 'pubData',
      success: function (data) {
         //Check to see if each item exists, and if it does, append it to verticalsLinks list in FinishHead.pbo
        if (typeof data.verticalsCustom !== 'undefined'  && data.verticalsCustom.length > 0) 
       {
          for(var i=0;i<data.verticalsCustom.length;i++)
          {
            $('ul#verticalsLinks').append('<li><a href="'+ data.verticalsCustom[i].link +'">' + data.verticalsCustom[i].title + '</a></li>');
            if(i<(data.verticalsCustom.length-1))
              $('ul#verticalsLinks').append('<li>|</li>');
          }         
       }
       else
       {
           //Autos
           if(typeof data.verticals.autos.link !== 'undefined' && data.verticals.autos.link !== '') {
              if(data.verticals.autos.link.substring(0,1) == "/") {
                 data.verticals.autos.link = "http://www.montereyherald.com" + data.verticals.autos.link;
              }
              $('ul#verticalsLinks').append('<li><a href="'+ data.verticals.autos.link +'">' + data.verticals.autos.title + '</a></li><li>|</li>');
           }
           //Real Estate
           if(typeof data.verticals.realestate.link !== 'undefined' && data.verticals.realestate.link !== '') {
              if(data.verticals.realestate.link.substring(0,1) == "/") {
                 data.verticals.realestate.link = "http://www.montereyherald.com" + data.verticals.realestate.link;
              }
              $('ul#verticalsLinks').append('<li><a href="'+ data.verticals.realestate.link +'">' + data.verticals.realestate.title + '</a></li><li>|</li>');
           }
           //Jobs
           if(typeof data.verticals.jobs.link !== 'undefined' && data.verticals.jobs.link !== '') {
              if(data.verticals.jobs.link.substring(0,1) == "/") {
                 data.verticals.jobs.link = "http://www.montereyherald.com" + data.verticals.jobs.link;
              }
              $('ul#verticalsLinks').append('<li><a href="'+ data.verticals.jobs.link +'">' + data.verticals.jobs.title + '</a></li>');
           }
        }
      },
      error: function(jqXHR, textStatus, errorThrown) {
         //log the error to the console
         console.log("The following error occured: " + textStatus, errorThrown);
