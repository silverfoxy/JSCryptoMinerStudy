		<style>
body,h1,h2,h3{margin:0}
#cari,#me,#toTop:active,#toTop:visited{color:#fff}
#footer,#subtitle,#tdef,#toTop,form,h1,p.popular-words{text-align:center}
#footer a,#me,#toTop,#tooltip-close,#word li a,.link-kata{text-decoration:none}
*,:after,:before{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}
body{padding:10px;font:400 13px "Helvetica Neue",Helvetica,Arial,sans-serif;background-color:#ddd}
h1{font-size:16px}
h2{font-size:14px}
h3{font:400 14px Georgia,Times}
#subtitle{font:italic 12px Georgia,Times;color:#999}
#cari,#me,#w{font-size:inherit}
#me{padding:5px 21px;border:1px solid #95A5A6;border-radius:3px;background:#95A5A6;margin-left:3px}
#me:hover{background-color:#ccc}
#toTop{position:fixed;bottom:10px;width:36px;right:20px;border-radius:20px;padding:10px 13px;background:#2b2b2b;background:rgba(0,0,0,.6);display:none}
#toTop:hover{background:#f39c12;background:rgba(243,156,18,.6)}
.ad-link{background:#fff;margin-left:16.667%;border-left:1px solid #d7d7d7;padding:5px 0}
#w{background-position:99% 50%;background-repeat:no-repeat;border:1px solid #AAA;border-right-width:0;border-radius:3px 0 0 3px;padding:5px 11px;width:50%}
#cari{background-color:#3498DB;border:1px solid #2980B9;padding:5px 13px;margin-left:0;border-radius:0 3px 3px 0}
#wrap1,#wrap2,.content{width:100%;max-width:1140px;min-width:460px;min-height:200px;margin:10px auto;overflow:hidden;outline:#c9c9c9 solid 1px;border:1px solid #fff;background-color:#e9e9e9}
#wrap1{min-height:60px;padding:10px 20px}
.col a,.popular-words a{text-decoration:none;padding:5px 7px;background-color:#f7f7f7;border-radius:3px;display:inline-block;min-width:50px;margin:0 1px 7px;text-align:center}
.col a{padding:5px 2px;display:block}
.col{float:left;width:173px;margin-right:10px;min-height:450px;margin-bottom:20px}
#about,#info,#tdef{margin-bottom:10px}
#info{padding:0 10px;background-color:#F4FF81;color:#333;line-height:200%}
strong.found,strong.not-found{font-weight:400;padding:2px 5px;border-radius:3px;color:#FFF}
strong.found{background-color:#28CE23}
strong.not-found{background-color:#FF5252}
#social{width:320px;margin:0 auto}
#wrap2{padding:10px 20px}
#wrap2 ul ul,#wrap2>ul{padding-left:25px}
.content:after{content:"";display:table;clear:both}
#word{width:16.667%;border-right:0 solid #d9d9d9;padding:10px 10px 20px}
#word ul{padding:0 0 0 15px;margin:5px 0 10px}
#word li{list-style:none}
#word li a{color:#00f;display:block;margin-bottom:1px;background:url(img/arrow.png) 2px 2px no-repeat;padding-left:10px}
#word a:hover,.col a:hover,.popular-words a:hover{color:#FFF;background-color:#55F}
#w3 li{color:#777}
#word a.cur{background-color:#F39C12;font-weight:400;color:#fff}
#w1{margin-bottom:5px}
sup{font-size:8px}
#desc{width:83.333%;border-left:1px solid #d9d9d9;background-color:#fff;padding:10px 20px 20px}
#tdef{border-bottom:1px dotted #ccc;padding-bottom:10px}
.link-kata{padding:2px 5px;font-size:12px;background:#448AFF;margin-left:5px;color:#fff;border-radius:3px}
#tooltip,#tooltip-close{display:none;font-size:small;position:absolute;font-family:serif}
.link-kata:hover{color:#FF6}
#next-popular{color:#fff;background-color:#3498DB}
#next-popular:hover{background-color:#F39C12}
#desc,#word{float:left;border-color:#d7d7d7;min-height:200px}
#jsdata{display:none}
#tooltip{background-color:#F2B579;color:#0D0C0C;padding:5px 30px 5px 10px;border-radius:5px;max-width:220px}
#tooltip-close{background-color:#E24B4B;color:#fff;font-weight:700;padding:5px 7px;cursor:pointer}
em.jk{margin:0 3px;color:#F0F;border-bottom:1px dotted #F0F;cursor:pointer}
b.main{font-size:16px}
.mjk{color:#060}
.tur{padding-left:0}
.turmjk{color:#606}
.per-suku{font-style:italic;color:#333;background:#EEE;margin-left:5px}
.notfound{font:400 14px Consolas,monospace;background:#F9F8A9;padding:3px 5px;color:#D51616}
.highlight{background:#ff7}
.num{padding:0 5px;background:#C6E4C7;color:#000;border-radius:9px;text-shadow:1px 1px 2px #fff;font-size:12px}
.num1{background:#F8E1A2}
em.pb{color:#F50;border-bottom:1px dotted #FA5;cursor:default}
em{color:#2A7FFF}
.sumber{font:italic 11px Georgia;border-top:1px solid #ddd;margin:5px 0 0;color:#E7814C}
.eks-def{font-weight:700;margin:0 0 3px;color:#ED4F73}
#footer{color:#666;text-shadow:1px 1px 1px #fff;font-size:11px}
#footer a{color:#333;border-bottom:1px dotted #666}
#footer a:hover{color:#000}
@media screen and (max-width:740px){#word{width:20%}
.ad-link{margin-left:0;border-width:0}
#w{width:70%}
#desc{width:80%}
}
@media (max-width:480px),handheld and (orientation:portrait){#word ul,body{padding:0}
#w{width:60%}
#wrap1,#wrap2,.content{width:100%;min-width:240px;margin-left:0;margin-right:0;padding-left:0;padding-right:0}
#w3 li,#word li a{margin:0 3px 3px 0}
#desc,#word,#wrap1,#wrap2{padding:10px}
#desc,#word{width:auto;float:none;margin:0}
#word{min-height:0;max-height:120px;overflow:scroll;overflow-y:scroll;overflow-x:hidden}
#word h3{margin:0 10px 0 0;float:left;font:700 13px "Helvetica Neue",Helvetica,Arial,sans-serif}
#word ul{margin:0}
#word li,#word li a{display:inline-block;min-width:50px;text-align:center}
#word li a{padding:3px 5px;border-radius:3px;background:#95A5A6;color:#fff}
#w3 li{border:1px dotted #aaa;border-radius:3px;padding:1px 3px;color:#777}
#desc{margin-top:0}
.col{width:47%}
}
