	var player;
	var windowWidth;
	var windowHeight;
	var alertHeight;
	
	function onYouTubeIframeAPIReady(windowWidth,windowHeight) {
    	var mediaSelect = 'true';
          if(mediaSelect == 'false'){
            if($(window).width() >= 1024) {
                      windowWidth = $(window).width();
                      windowHeight = $(window).height();
                      player = new YT.Player('player', {
                          height: 500,
                          width: windowWidth,
                          videoId: 'itrc9kT_284',
                          playerVars: { 
                              'modestbranding': 1,
                              'controls': 0, 
                              'autoplay': 1,
                              'rel' : 0,
                              'loop': 1,
                              'wmode': 'opaque',
                              'showinfo': 0
                          },
                          events: {
                              'onReady': onPlayerReady,
                              'onStateChange': onPlayerStateChange
                          }
                      });
              } else {
                  if(!$("#hp-slideshow.useMMR-false img").length) {
                      $("<img alt='Seneca Valley School District' src='/cms/lib/PA01001234/Centricity/Template/GlobalAssets/images///Faces/default-man.jpg' />").prependTo("#hp-slideshow.useMMR-false");
                  }
              }
          } 
	}
	//the API will call this function when the video player is ready.
	function onPlayerReady(event) {
		csVideoResize();
		$(window).resize(csVideoResize);
        player.mute();
	}
	
	//The API calls this function when the players state changes.
	//The function indicates that when playing a video (state=1),
	//the player should play for six seconds and then stop.
	var done = false;
	function onPlayerStateChange(event) {
		if (event.data == YT.PlayerState.PLAYING && !done) {
			
		} else {
			//pausePlayVideo();
		}
        
        if(event.data == YT.PlayerState.ENDED) {
        	player.playVideo();
        }
	}
	function pausePlayVideo() {
		
	}
	function stopVideo() {
		player.pauseVideo();
	}
	function csVideoResize() {
		if($("#player").length) {
			var screenWidth = $(window).width();
			var screenHeight = $(window).height();
			var vidWidth = screenWidth, vidHeight = 0;
			
			// GET IMAGE RATIO
			var initW = 853.5, initH = 480;
			var ratio = initH / initW;
			vidHeight = vidWidth * ratio;
            var vidMargin = vidHeight / 2;
            //console.log(vidWidth);
            //vidHeight = vidHeight - 500;
			
			// APPLY THE DIMENSIONS
			$("#player").attr("tabindex","-1").css({"height": vidHeight + "px" , "width": screenWidth + "px"});
            $(".html5-video-container").css({"position": "absolute !important" , "top":"-50% !important" , "margin-top": vidMargin + "px !important"});
            //$("#player").attr("tabindex","-1").css({"width": screenWidth + "px"});
		}
	}
