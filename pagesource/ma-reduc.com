            if ($R.viewport < $R.MD) {
                $carousel.find('.item').removeClass('active');
                $carousel.find('.item:first-child').addClass('active');
                $carousel.carousel();

                // Active le support des actions tactiles ("swipe")
                $carousel.swipe({
                    swipeLeft: function() {
                        $carousel.carousel('next');
                    },
                    swipeRight: function() {
                        $carousel.carousel('prev');
                    },
                    threshold: 40
                });
            } else {
                $carousel.find('.item').addClass('active');
            }
        }

        onViewportChange();

        window.addEventListener('resize', $R.debounce(onViewportChange, 300));

        $('.count').each(function () {
            $(this).prop('Counter',0).animate({
                Counter: $(this).text()
            }, {
                duration: 2000,
                easing: 'swing',
                step: function (now) {
                    var num = Math.ceil(now).toString();
                    if(Number(num) > 999){
                        while (/(\d+)(\d{3})/.test(num)) {
                            num = num.replace(/(\d+)(\d{3})/, '$1' + ' ' + '$2');
                        }
                    }
                    $(this).text(num);
                }
            });
        });
    })
	</script>
	
