                cartData: $('#productDetailsAddToCartForm').serialize()
            };
            $.ajax({
                datatype: "json",
                cache: false,
                async: true,
                type: "POST",
                data: postData,
                url: postURL,
                success: function (msg) {
                    result = (msg == 'success') ? true : false;
