	})

	// AROUND TOWN
	var at = $("#around-town");
	at.owlCarousel({
		items : 4, // 2 items above 1000px browser width
		itemsDesktop : [1000,4], // 2 items between 1000px and 901px
		itemsDesktopSmall : [900,3], // 2 items betweem 900px and 601px
		itemsTablet: [768,2], // 2 items between 600 and 0
		itemsMobile : true, // itemsMobile enabled - inherit from itemsTablet option
		autoHeight : false,
		loop : true,
		pagination : false
	});
	// Custom Navigation Events
	$(".at-next").click(function(){
		at.trigger('owl.next');
	})
	$(".at-prev").click(function(){
	at.trigger('owl.prev');
	})

	// ACCOLADES
	var acc = $("#accolades");
	acc.owlCarousel({
		items : 4, // 2 items above 1000px browser width
		itemsDesktop : [1000,4], // 2 items between 1000px and 901px
		itemsDesktopSmall : [900,3], // 2 items betweem 900px and 601px
		itemsTablet: [768,2], // 2 items between 600 and 0
		itemsMobile : true, // itemsMobile enabled - inherit from itemsTablet option
		autoHeight : false,
		loop : true,
		pagination : false
	});
	// Custom Navigation Events
	$(".acc-next").click(function(){
		acc.trigger('owl.next');
	})
		$(".acc-prev").click(function(){
		acc.trigger('owl.prev');
	})

	// EVENTS (Sidebar)
	var events = $("#events-sidebar");
	events.owlCarousel({
		items : 1, // 2 items above 1000px browser width
		itemsDesktop : [1000,1], // 2 items between 1000px and 901px
		itemsDesktopSmall : [900,1], // 2 items betweem 900px and 601px
		itemsTablet: [768,1], // 2 items between 600 and 0
		itemsMobile : true, // itemsMobile enabled - inherit from itemsTablet option
		autoHeight : true,
		loop : true,
		margin : 0,
		autoPlay : 6000,
		pagination : false
	});

	// Custom Navigation Events
	$(".events-next").click(function(){
		events.trigger('owl.next');
	})
		$(".events-prev").click(function(){
		events.trigger('owl.prev');
	})

	var accs = $("#accolades-sidebar");
	accs.owlCarousel({
		items : 2, // 2 items above 1000px browser width
		itemsDesktop : [1000,2], // 2 items between 1000px and 901px
		itemsDesktopSmall : [900,1], // 2 items betweem 900px and 601px
		itemsTablet: [768,1], // 2 items between 600 and 0
		itemsMobile : true, // itemsMobile enabled - inherit from itemsTablet option
		autoHeight : true,
		loop : true,
		margin : 0,
		autoPlay : 6000,
		pagination : false
	});

	// Custom Navigation Events
	$(".acc-sb-next").click(function(){
		accs.trigger('owl.next');
	})
		$(".acc-sb-prev").click(function(){
		accs.trigger('owl.prev');
	})

	// Multimedia Image Gallery page
	var gal = $("#multi-gal");
	gal.owlCarousel({
		navigation : false, // Show next and prev buttons
		singleItem : true,
		transitionStyle : 'fade',
		pagination: true,
		//Basic Speeds
		slideSpeed : 500,
		paginationSpeed : 500,
		rewindSpeed : 500,
		//Autoplay
		autoPlay : 6000,
		stopOnHover : false,
		//Mouse Events
		dragBeforeAnimFinish : true,
		mouseDrag : false,
		touchDrag : true
	});
	// Custom Navigation Events
	$(".next").click(function(){
		gal.trigger('owl.next');
	})
	$(".prev").click(function(){
		gal.trigger('owl.prev');
	});


	// Set height of search bar at top of page
	var pageTop = $("#page-top").height();
	$("#page-top input").css("height", pageTop);

	$("#page-top button").css("height", pageTop);
	$("#page-top button").css("width", pageTop);

	// Add rel=lightbox to all images inside of article content & add img-responsive class
	$('#article .article-content a:has(img)').attr('rel', 'lightbox');
	$('#article .article-content img').addClass('img-responsive');

	// Adding blue bottom border to Most Read h2 tag in sidebar
	$('.sidebar .widget_top-posts h2').addClass('btm-border');
	$( ".sidebar .widget_top-posts h2" ).wrapInner( "<span class='blue'></div>" );

	// Adding orange bottom border to BizPoll h2 tag in sidebar
	$('.sidebar .widget_polls-widget h2').addClass('btm-border');
	$( ".sidebar .widget_polls-widget h2" ).wrapInner( "<span class='orange'></div>" );

	// Back to Top button
	if ( ($(window).height() + 100) < $(document).height() ) {
	    $('#back-to-top').removeClass('hidden').affix({
	        // how far to scroll down before link "slides" into view
	        offset: {top:100}
	    });
	}
});
</script>

