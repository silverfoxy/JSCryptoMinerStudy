<div class="venue-finder js-venue-menu" ng-class="menuOpen" ng-hide="removeFromTab" aria-hidden="{{!isopen}}">

    <div class="loading" data-ng-hide="hideLoading">
        <div class="loader"></div>
    </div>

    <div class="venue-finder__inner">
            <div class="venue-finder__wrp" ng-class="hideFinder">
                <div class="venue-finder__title">

                    <div class="venue-finder__icon-wrp">
                        <div class="venue-finder__line"></div>
                        <div class="venue-finder__icon icon-pin"></div>
                    </div>

                    <h2 class="venue-finder__title-text">Find a pub or hotel</h2>
                </div>

                <p class="venue-finder__caption">
                    Please enter a location<span class="hide-mobile">, pub or hotel name</span>. If you are looking for something specific try our <a href="/pubs/all-pubs">advanced search</a>
                </p>

                <div class="venue-finder__form">
                    <label class="venue-finder__form-label" for="venue-search">Enter a location, postcode or pub name</label>
                    <div class="venue-finder__form-input">
                        
                        <div class="venue-finder__input-wrapper js-venue-finder-wrapper" ng-init="hideOnClick();">
                            <input class="venue-finder__input" id="venue-search" type="text" name="Search" ng-enter="loadResult()" ng-click="resultIndex = -1" ng-model="pubSearchTerm" ng-keyup="predictivePubSearch($event);">
                            <div class="venue-finder__predictive" ng-show="showPreditive">
                                <div class="venue-finder__predictive-caption">Suggested Pub and Hotels</div>

                                <a href="{{ result.url }}" class="venue-finder__predictive-item" ng-class="{'js-selected': activeResult($index)}" ng-repeat="result in pubPredictiveResults | slice:0:2">
                                    
                                    <b class="venue-finder__predictive-icon icon-airport" ng-if="result.isAirport === true">Airport</b>
                                    <b class="venue-finder__predictive-icon icon-accomodation" ng-if="result.isHotel === true">Hotel</b>
                                    {{ result.name }}
                                    
                                    <span class="venue-finder__predictive-item-details">{{ result.address1 }} <br>
                                          {{ result.city }} {{ result.postcode }}
                                    </span>
                                </a>
                                <a class="venue-finder__predictive-view-more" ng-class="{'js-selected': activeResult(2)}" ng-if="pubPredictiveResults.length > 2" href="/pubs/all-pubs?searchterm={{ pubSearchTerm }}">See more results</a>
                            </div>
                        </div>

                        <input class="btn--venue-finder" type="submit" value="Search" onclick="return false;" ng-click="search()">
                        
                        <a href="javascript:void(0);" oncontextmenu="return false;" ng-click="geoLocationSearch();" ng-show="locationSupport" title="Use my location" class="venue-finder__location-cta icon-geo-location">Use my location</a>

                        <div class="venue-finder__error">
                            <span class="venue-finder__error-msg" ng-show="lengthError">Please enter at least 3 characters</span>
                            <span class="venue-finder__error-msg" ng-show="inputError">Please enter a location, postcode or pub name</span>
                            <span class="venue-finder__error-msg" ng-show="geoError">Sorry couldn't find a location</span>
                        </div>

                    </div>
                </div>

            </div>

            <div class="venue-finder__wrp" ng-class="hideResults">
                <div class="venue-finder__title">

                    <div class="venue-finder__icon-wrp">
                        <div class="venue-finder__line"></div>
                        <div class="venue-finder__icon icon-pin"></div>
                    </div>

                    <h2 class="venue-finder__title-text" ng-show="isPubName">Pubs or hotels matching the name '{{ pubSearchTerm }}'</h2>
                    <h2 class="venue-finder__title-text" ng-hide="isPubName">Check out your nearest pub or hotel</h2>

                </div>

                <div class="venue-finder__result-list">

                    <div class="venue-finder__result" ng-repeat="x in pubs">

                        <a href="{{ x.url }}" class="venue-finder__venue-title" title="{{ x.name }}" tabindex="{{ $index }}" ng-class="{true: 'is-pubname', false: ''}[isPubName]">
                            <div class="venue-finder__distance" ng-hide="isPubName">
                                {{ x.distanceTo }} <span>miles</span>
                            </div>
