<script>
function slider(obj, pointer){
	var size = $("."+obj).find("li").length;  //获取轮播图片的数量
	var num = 0,speed = 8000,detetime;   //初始化
	var str="";
	for(var i=0;i<size;i++){
		str+="<li></li>";
	}
	$("."+pointer).append(str);   //插入导航按钮
	$("."+pointer).find("li").eq(0).addClass("cur");      
	$("."+pointer).siblings(".txt").html($("."+obj).find("li").eq(0).find("span").html());      //初始化轮播图
	// 鼠标划过导航按钮事件
	$("."+pointer).find("li").mouseover(function(){
		num = $(this).index();
		$("."+obj).find("li").stop(false,true).hide();
		$("."+obj).find("li").eq(num).fadeIn(1000);
		$("."+pointer).find("li").removeClass("cur").eq(num).addClass("cur");
		$("."+pointer).siblings(".txt").html($("."+obj).find("li").eq(num).find("span").html());
	}).eq(0).mouseover();

	// 鼠标滑过轮播图区域
	$("."+obj).parent().hover(function(){
		clearInterval(detetime);
	},function(){
		detetime = setInterval(function(){
			num++;
			if(num==size){
				num=0;
			}
			$("."+obj).find("li").stop(false,true).hide();
			$("."+obj).find("li").eq(num).fadeIn(1000);
			$("."+pointer).find("li").removeClass("cur").eq(num).addClass("cur");
			$("."+pointer).siblings(".txt").html($("."+obj).find("li").eq(num).find("span").html());
		},speed);
	}).trigger("mouseleave");
	// 鼠标点击左右导航箭头事件
	$("."+obj).siblings(".prev").click(function(){
		num--;
		if(num<0){
			num=size-1;
		}
		$("."+obj).find("li").stop(false,true).hide();
		$("."+obj).find("li").eq(num).fadeIn(1000);
		$("."+pointer).find("li").removeClass("cur").eq(num).addClass("cur");
		$("."+pointer).siblings(".txt").html($("."+obj).find("li").eq(num).find("span").html());
	})

	$("."+obj).siblings(".next").click(function(){
		num++;
		if(num>=size){
			num=0;
		}
		$("."+obj).find("li").stop(false,true).hide();
		$("."+obj).find("li").eq(num).fadeIn(1000);
		$("."+pointer).find("li").removeClass("cur").eq(num).addClass("cur");
		$("."+pointer).siblings(".txt").html($("."+obj).find("li").eq(num).find("span").html());
	})
}

slider("imglist" , "numlist");

</script>












