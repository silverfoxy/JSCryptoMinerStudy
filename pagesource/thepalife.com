/* Custom Button CSS Hiral
--------------------------------------------- */
.red::before{
	background: linear-gradient(60deg,#c42f2f,#d90a0a);
}
.red::after{
		background: linear-gradient(60deg,#d90a0a,#d90a0a);
}
.green::before{
	background: linear-gradient(60deg,#009946,#14ad5a);
}
.green::after{
		background: linear-gradient(60deg,#14ad5a,#14ad5a);
}
.btn::before
{
	position:absolute;
	top:0;
	left:0;

box-shadow: 0 4px 8px 0 rgba(20,173,90,.08),0 8px 24px 0 rgba(0,0,0,.08);
text-shadow: 0 0 16px hsla(0,0%,100%,0);
z-index: 5;
content: "";
width: 100%;
height: 100%;
transition: all .2s ease-in-out 0s;
border-radius: 2px;
opacity: 1;
}
.btn::after{
	position:absolute;
	top:0;
	left:0;
	
box-shadow: 0 4px 16px 0 rgba(20,173,90,.16),0 12px 32px 0 rgba(0,0,0,.16);
text-shadow: 0 0 16px hsla(0,0%,100%,.56);
z-index: 3;
	content: "";
width: 100%;
height: 100%;
transition: all .2s ease-in-out 0s;
border-radius: 2px;
opacity: 1;
}
.btn
{
	position:relative;
width: auto;
height: 3.5em;
transition: all .2s ease-in-out;
z-index:10;
color: #fff !important;
text-decoration: none;
text-transform: uppercase;
padding: .9em 1.75em;

border-radius: 2px;
-webkit-user-select: none;
-moz-user-select: none;
-ms-user-select: none;
user-select: none;
}
.btn:hover:before
{
	opacity:0;
}
.btn:hover:after {
    -webkit-transform: scale(1.03, 1.08);
    transform: scale(1.03, 1.08);
}
.btn:hover
{
	text-shadow: 0 0 16px hsla(0, 0%, 100%, .56);
	padding: 1em 2em;
}
.btn span
{
	letter-spacing: .08em;
position: relative;
z-index: 10;
text-shadow: 0 0 16px hsla(0,0%,100%,0);
transition: all .2s ease-in-out;
	color:#fff;
}
