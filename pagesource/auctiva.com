                    $this.addClass('newGlobalBtn').wrapInner('<span>');
                    if (!$.browser.mozilla && !$.browser.msie) { $this.find('span').css('padding-top', '1px') };
                } else if (tag === "input" || tag === "button") {
                    $this.addClass('newGlobalBtn').wrap('<span class="newGlobalBtn"></span>');
                    if ($.browser.mozilla) { $this.css('padding-bottom', '3px') };
                }
            });
        };
        $('.custom-button, .custom-button-link, .custom-button-disable, .button').newGlobalBtn();
