.flip-box-container {
	position: relative;	
}
.flip-box-container:after {
	content: '';
	display: block;
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: -1;	
}
.flip-box > div {
    width: 100%;
	height: 100%;	
    position: absolute;
	box-sizing: border-box;

    backface-visibility: hidden;
    transform-origin: 50% 50%;

	-webkit-filter: blur(0);
	   -moz-filter: blur(0);
	     -o-filter: blur(0);
    	    filter: blur(0);	
	
	-webkit-transition: all 0.8s cubic-bezier(0.2, 0.8, 0.5, 1);
	   -moz-transition: all 0.8s cubic-bezier(0.2, 0.8, 0.5, 1);
