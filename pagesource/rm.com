    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
    }
    .quote-image:hover .image-bg, .quote-image:focus .image-bg,
    .quote-image:hover img, .quote-image:focus img,
    .quote-image:hover video, .quote-image:focus video,
    .overlay-box.with-img:hover img, .overlay-box.with-img:focus img,
    .hover-effect:hover .small-slider, .hover-effect:focus .small-slider,
    .quote-link:hover .overlay-box.with-img img, .quote-link:focus .overlay-box.with-img img,
    .quote-link:hover .overlay-box.with-img video, .quote-link:focus .overlay-box.with-img video,
    .quote-link:hover .quote-image .image-bg, .quote-link:focus .quote-image .image-bg,
