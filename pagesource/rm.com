    /* webinar pages (end) */
    /* find a venue - dropdown (start) */
    .dropdown {
    width: 100%;
    float: left;
    margin: 15px 0;
    }
    /* shadows */
    .dropdown,
    .dropdown-content {
    -webkit-box-shadow: 5px 8px 20px 0 rgba(0, 0, 0, 0.2);
    -moz-box-shadow: 5px 8px 20px 0 rgba(0, 0, 0, 0.2);
    box-shadow: 5px 8px 20px 0 rgba(0, 0, 0, 0.2);
    }
    /* dropdown button */
    .dropbtn {
    color: #000;
    padding: 20px 30px 20px 105px;
    font-size: 1.5em;
    border: none;
    cursor: pointer;
    position:  relative;
    background-color: #fff;
    }
    /* left green pin box */
    .dropbtn:before {
    width: 75px;
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    top: 0;
    background-color: #00c8ae;
    background-image: url('/~/media/Images/Live/Seminars/Venue/map_pin_bg.png');
    background-repeat: no-repeat;
    background-position: center;
    }
    /* down arrow */
    .dropbtn:after {
    top: 50%;
    right: 30px;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
    border-color: rgba(178, 181, 186, 0);
    border-top-color: #b2b5ba;
    border-width: 8px;
    margin-top: -4px;
    }
    /* dropdown button on hover & focus */
    .dropbtn:hover, .dropbtn:focus {
    background-color: #f1f1f1;
    }
    .dropbtn:hover:after, .dropbtn:focus:after {
    border-top-color: #00c8ae;
    }
    /* the container
    <div> - needed to position the dropdown content */
    .dropdown {
    position: relative;
    display: inline-block;
    }
    /* dropdown Content (hidden by default) */
    .dropdown-content {
    width: 100%;
    min-width: 400px;
    font-size: 1em;
    display: none;
    position: absolute;
    background-color: #f9f9f9;
    z-index: 10000;
    }
    /* links inside the dropdown */
    .dropdown-content a {
    color: black;
    padding: 15px 30px;
    text-decoration: none;
    display: block;
    border: none;
    }
    /* location */
    .dropdown-content a > strong {
    font-size: 1.3em;
    }
    /* location details */
    .dropdown-content a > span {
    display: block;
    line-height: 1;
    }
    /* change color of dropdown links on hover */
    .dropdown-content a:hover {
    background-color: #f1f1f1;
    }
    /* show the dropdown menu (use JS to add this class to the .dropdown-content container when the user clicks on the dropdown button) */
    .dropdown-content.show {
    display: block;
    }
