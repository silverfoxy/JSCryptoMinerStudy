    /* underlined text */
    .anim-border > span,
    .intro .btn > span,
    .intro .addtocalendar .atcb-link {
    background-size: 0 100%;
    background-repeat: no-repeat;
    -webkit-transition: background-size 0.25s ease-in-out;
    -moz-transition: background-size 0.25s ease-in-out;
    -o-transition: background-size 0.25s ease-in-out;
    transition: background-size 0.25s ease-in-out;
    }
    .anim-border > span {
    background-image: -webkit-linear-gradient(top, transparent 90%, #fdc800 0);
    background-image:      linear-gradient(180deg, transparent 90%, #fdc800 0);
    }
    .intro .btn > span {
    background-image: -webkit-linear-gradient(top, transparent 0, #363636 0);
