<script type="text/jscript">
$(document).ready(function(){
	
	/* rotate flip box */
	$(".rotate").on('click', function(){
		setTimeout(function() {
		   $(".flip-box").toggleClass("flip");
	   }, 100);		
	});
	
	
	/* remove when menu closes flip box */
	$(".flip-box .close").on('click', function(){
		setTimeout(function() {
		   $(".flip-box").removeClass("flip");
	   }, 500);		
	});
	
		
	/* fade sibling menu items */
	$(".flip-box .menu a").hover(
		function () {
			$(this).parent().siblings().addClass('fade-this');
		},
		function () {
			$(this).parent().siblings().removeClass('fade-this');
		}
	);


	/* show parent menu item related submenus */	
	$(".flip-box .front a").click(function(){
		var id = $(this).attr("id");		
		$(".flip-box .back > .menu").removeClass("show");
		$(".flip-box .back > .menu." + id).addClass("show");
	});
	

	/* menu model box (start) */
	var appendthis =  ("<div class='flip-box-overlay'></div>");
	
	$('a[data-modal-id]').click(function(e) {
		e.preventDefault();
		$("body").append(appendthis);
		$(".flip-box-overlay").fadeTo(600, 0.8);
		var flipBoxModel = $(this).attr('data-modal-id');
		$('#'+flipBoxModel).fadeIn($(this).data());
		$(".flip-box-model").removeClass("slide-down");
		$(".flip-box-model").addClass("slide-up");
	});	
	
	$(".flip-box .close, .flip-box-overlay").click(function() {
		$(".flip-box-model").removeClass("slide-up");
		$(".flip-box-model").addClass("slide-down");
		
		$(".flip-box-model, .flip-box-overlay").fadeOut(300, function() {
			$(".flip-box-overlay").remove();			
		});
	});
	
	$(window).resize(function() {
		$(".flip-box-model").css({
			top: ($(window).height() - $(".flip-box-model").outerHeight()) / 2,
			left: ($(window).width() - $(".flip-box-model").outerWidth()) / 2
		});
	});
	
	$(window).resize();
	/* menu model box (end) */
				
});
</script>



