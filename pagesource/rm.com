    <script type="text/javascript">
        $(document).ready(function () {

            var setHeight = function (height, $iframe) {
                $iframe.height(height + 20);
            };

            if ((typeof modalSelectors !== 'undefined' && modalSelectors != '') || typeof mforeacodalSelectors !== 'undefined') {
                $(modalSelectors).click(function (e) {

                    var $this = $(this);
                    var width = $this.data('modalwidth') != undefined ? $this.data('modalwidth') : '';
                    var margin = $this.data('modalmargin') != undefined ? $this.data('modalmargin') : '';

                    //if (margin != '') {
                    //    margin = margin.replace("auto", "15px");
                    //}

                    var padding = $this.data('modalpadding') != undefined ? $this.data('modalpadding') : '';
                    var href = $this.attr('href');

                    var $modal = $('#modalContainer');
                    var $modalDialog = $modal.find('.modal-dialog');
                    var $modalContent = $modal.find('.modal-content');
                    $modal.css("padding", "0 15px");
                    $modalDialog.css('width', width).css('margin', margin);
                    $modalDialog.data('modalwidth', width).data('modalmargin', margin);

                    $modalContent.css('padding', padding);

                    var $iframe = $modal.find('iframe');
                    $iframe.attr('src', href);

                    if (($modalDialog.width() > $(document).width()) || ($(document).width() < 768)) {
                        $modalDialog.css('width', '');
                    }

                    $iframe.load(function () {

                        var body = $iframe[0].contentWindow.document.body;

                        var height = body.offsetHeight;
                        if (height == 0) {

                            setTimeout(function () {
                                var body = $('#modalContainer iframe')[0].contentWindow.document.body;
                                setHeight(body.offsetHeight, $iframe);
                            }, 500);
                        }
                        else {
                            setHeight(height, $iframe);
                        }
                    });

                    e.stopPropagation();
                    e.preventDefault();
                    $('#modalContainer').modal('show');

                    return false;
                });
            }
        });
    </script>   

    

