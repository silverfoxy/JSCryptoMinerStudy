    background-image:      linear-gradient(180deg, transparent 0, #fff 0);
    }
    .anim-border {
    cursor: pointer;
    }
    .anim-border:hover > span,
    .anim-border:focus > span,
    a.quote-link:hover .anim-border > span,
    a.quote-link:hover .anim-border > span,
    .intro .btn:hover > span,
