    /* blinking red dot */
    .anim-record {
    position: relative;
    padding-right: 25px;
    }
    .anim-record:after {
    width: 15px;
    height: 15px;
    content: "";
    display: block;
    position: absolute;
    right: 0;
    bottom: 20%;
    border-radius: 100%;
    background-color: #f00;
    -webkit-animation: blinker 1s linear infinite;
    -moz-animation: blinker 1s linear infinite;
    -o-animation: blinker 1s linear infinite;
    animation: blinker 1s linear infinite;
    }
    h5 .anim-record {
    padding-right: 12px;
    }
    h5 .anim-record:after {
    width: 8px;
    height: 8px;
    }
    @keyframes blinker {
    50% {opacity: 0;}
