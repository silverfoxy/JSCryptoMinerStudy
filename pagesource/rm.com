}

/* front & back */
.front {
	z-index: 1;
}
.flip .back {
	z-index: 2;
}
.front, .flip .back {
	-webkit-transform: rotateY(0deg) translateZ(170px);
		-ms-transform: rotateY(0deg) translateZ(170px);
		    transform: rotateY(0deg) translateZ(170px);
}
.back {
	-webkit-transform: rotateY(90deg) translateZ(170px);
		-ms-transform: rotateY(90deg) translateZ(170px);
			transform: rotateY(90deg) translateZ(170px);
}
.flip .front {
	-webkit-transform: rotateY(-90deg) translateZ(170px);
		-ms-transform: rotateY(-90deg) translateZ(170px);
