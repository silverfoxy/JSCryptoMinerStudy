<script type="text/javascript" language="JavaScript">
    $('#searchContainer').hide();
    
    var video = document.getElementById("Video1");
    $('body').scrollspy({target: '#navbar-subnav'});
    $('#navbar-subnav .navbar-nav li>a').click(function () {
        var link = $(this).attr('href');
        var posi = $(link).offset().top - ( topPadding + $('#navbar-subnav').height());
        $('body,html').animate({scrollTop: posi}, 1000, 'easeInOutExpo');
    });
    $('#navbar-subnav').affix({
        offset: {top: $('#bannerHeader').height()}
    });
    $('#navbar-subnav').on('affixed.bs.affix', function () {
        video.pause();
    });
    $(function () {
        $(window).scroll(function () {
            video = document.getElementById("Video1");
            var winTop = $(window).scrollTop();
            if (winTop >= 50) {
                video.pause();
            } else {
                video.play();
                $('a.play-button').show();
            }
        });
    });

    $('a.play-button').not('.paused').on('click', function(){
        video.pause();
        $('a.play-button').hide();
    });
    $('a.play-button.paused').on('click', function(){
        video.play();
        $('a.play-button.paused').removeClass('paused');
    });

        function resizeWindow() {
        $('#navbar').affix({
            offset: {top: $('#bannerHeader').height()}
        });
        if (checkWindowWidth()) {
//        if (1==2) {
            $('.tab-content.desktop').each(function () {

                var $this = $(this);
                targetID = this.id;
                $this.addClass('mobile').removeClass('desktop').addClass('showMore').removeClass('showLess');
                $('#' + targetID + ' .field-name-body')
                    .attr('id', targetID + '-body')
                    .collapse('hide')
                    .addClass('collapse')
                    .on('shown.bs.collapse', function () {
                        $(this).parent().addClass('showLess').removeClass('showMore');
                    }).on('hidden.bs.collapse', function () {
                    $(this).parent().addClass('showMore').removeClass('showLess');
                });

                $('h2', $this).attr('data-toggle', "collapse")
                    .attr('data-target', '#' + targetID + '-body')
                    .attr('aria-controls', targetID + '-body')
                    .attr('aria-expanded', "false");

//data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne"

            }).addClass('mobile');
            $('.tab-content .field-name-body:first').collapse('show');
        } else {
            $('.tab-content.mobile').each(function () {
                var $this = $(this);
                targetID = this.id;
                $this.removeClass('mobile');
                $('.tab-content .field-name-body').collapse('show');
                $('.tab-content > h2').removeClass('showMore');
            }).addClass('desktop')
        }
    }
    $(function () {
        $('[data-toggle="popover"]').popover()
    });
    $('.tab-content').removeClass('mobile').addClass('desktop');
    resizeWindow();

    $('[data-toggle="popover"]').popover();
    $('.searchLabel').on('click', function () {
        $('#searchContainer').show();
        $('#___gcse_0 input[type="text"]').focus();
            $('.gsc-results-close-btn').on('blur',function () {
            $('#searchContainer').slideUp();
        });
    });
    $('#mobileNav').hide();
    $('#mobileNavButton').on('click',function(){
        $('#mobileNav').fadeIn();
    })
    $('#mobileNavCloseButton').on('click', function () {
        $('#mobileNav').fadeOut();
    })
    $(window).on('resize', function () {
        (!window.requestAnimationFrame) ? setTimeout(resizeWindow, 300) : window.requestAnimationFrame(resizeWindow);
    });
    function checkWindowWidth() {
        //check window width (scrollbar included)
        var e = window,
            a = 'inner';
        if (!('innerWidth' in window)) {
            a = 'client';
            e = document.documentElement || document.body;
        }
        if (e[a + 'Width'] <= 767) {
            return true;
        } else {
            return false;
        }
    }
</script>
