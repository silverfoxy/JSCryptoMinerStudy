(function($) {
	$(document).ready(function() {
 $( '.primary_38 ul.sub-menu li a, .primary_38 ul.submenu-items li a' ).prepend( '<i class="glyphicon glyphicon-chevron-right"></i>' );$(document).scroll(function () {
								var y = $(document).scrollTop(),
								mega_container = $( '.wr-megamenu-container.primary_38' );
								responsive = $( '.it-responsive-mega' );
								if (y > mega_container.parent().offset().top) {
									mega_container.addClass( 'stick' );
									responsive.addClass( 'stick' );
								} else {
									mega_container.removeClass( 'stick' );
									responsive.removeClass( 'stick' );
								}
							});

			$( '.primary_38 .mega-item.wr-megamenu-fixed, .primary_38 .menu-item-has-children.menu-default' ).on( 'hover', function () {

				var
					mega_inner = null,
					mega_menu = $(this).find( ' > .wr-megamenu-inner' ),
					sub_menu = $(this).find( ' > .sub-menu' ),
					is_mega_inner = false
					;

				if (sub_menu.length > 0) {
					mega_inner = sub_menu;
				} else if (mega_menu.length > 0) {
					mega_inner = mega_menu;
					is_mega_inner = true;
				}
				if (mega_inner != null) {
					if (mega_inner.outerWidth() > ($(window).outerWidth() - mega_inner.offset().left ) ) {
						if (!is_mega_inner) {
							mega_inner.addClass( 'wr-megamenu-rtl' );
							$( '.primary_38 ul.sub-menu li.wr-megamenu-item' ).css({'text-align': 'right', 'background': 'inherit'});
							$( '.primary_38 ul.wr-mega-menu ul ul.sub-menu' ).css( 'right', '99%' );
							$( '.primary_38 ul.wr-mega-menu ul li' ).on( 'hover', function () {
								$(this).find( '> ul.sub-menu' ).css({'right': '100%'});
							});
							$( '.wr-megamenu-container.primary_38 ul ul' ).addClass('pull-right');
						} else {
							mega_inner.addClass('wr-megamenu-left-inner');
						}
					}
				}
			});
		
	});
})(jQuery);
</script>


