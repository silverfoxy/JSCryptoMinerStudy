	var isIE11 = !!navigator.userAgent.match(/Trident.*rv[ :]*11\./)

	$(window).load(function() {
		var p = $('#megamenu-sidebar');
		var position = p.offset();
		if(position.top > 0){
			$('#site-menu-bar').css( 'top' , position.top + 'px' );
		}
		//setPageHeight();
	});

	var wrapper = "#zoom-wrap";
	
	$('#font-plus').on('click',function(){

		if ($.browser.mozilla && !isIE11){
			var step = 0.08;
			currFFZoom = 1 + step*1
			$(wrapper).css('MozTransformOrigin','top center').css('MozTransform','scale(' + currFFZoom + ')');
			setMenuBarPos();
		} else {
			var step = 8;
			currIEZoom = 100 + step*1
			$(wrapper).css('zoom', ' ' + currIEZoom + '%');
			setMenuBarPos();
		}

		ieFixZoomMargin(wrapper,currIEZoom);
		$("#zoom-wrap2").css('overflow','visible').css('height','auto');
	});
	
	$('#font-revert').on('click',function(){
		if ($.browser.mozilla && !isIE11){
			var step = 0.08;
			currFFZoom = 1 + step*0
			$(wrapper).css('MozTransform','scale(' + currFFZoom + ')');
			setMenuBarPos();
		} else {
			var step = 8;
			currIEZoom = 100 + step*0
			$(wrapper).css('zoom', ' ' + currIEZoom + '%');
			setMenuBarPos();
		}
		ieFixZoomMargin(wrapper,currIEZoom);
		$("#zoom-wrap2").css('overflow','visible').css('height','auto');
	});

	$('#font-minus').on('click',function(){
		var prevLeft= $(wrapper).offset().left
		//alert(prevLeft);
		if ($.browser.mozilla && !isIE11){
			var step = 0.08;
			currFFZoom = 1 - step*1
			$(wrapper).css('MozTransformOrigin','top center').css('MozTransform','scale(' + currFFZoom + ')');
			setMenuBarPos();
		} else {
			var step = 8;
			currIEZoom = 100 - step*1
			$(wrapper).css('zoom', ' ' + currIEZoom + '%');
			setMenuBarPos();

		}
		
		ieFixZoomMargin(wrapper,currIEZoom);
		var winH = $(wrapper).height();
		var h=0;

		if ($.browser.mozilla && !isIE11){
			h = winH*currFFZoom;
		}else{
			h = winH*currIEZoom/100;
		}

		//alert(h+" vs "+ $("#zoom-wrap").height())
		$("#zoom-wrap2").height(h).css('overflow','hidden');
	});
});

function ieFixZoomMargin(wrapper,zoom){
	jQuery(function($) {	
	//alert("ieFixZoomMargin:"+wrapper);
	//alert(navigator.userAgent);
	if (navigator.userAgent.indexOf('MSIE') > -1 || navigator.userAgent.indexOf('rv:11') > -1) {
		//alert("ieFixZoomMargin isIE:"+zoom);

		if(zoom == 100){
			$(wrapper).css('left','0');
			/*$("#sitemap-footer").css('width',($("#footer-wrapper").width()-2)+"px");
			$("#sitemap-footer").css('bottom',0).css('left',0);*/
		}else{
			var winWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
			var ol = winWidth/2-1024/2;
			var left = winWidth/2-(1024*(zoom/100))/2;
			$(wrapper).css('position','relative').css('left',(left-ol)+"px");
			//alert(left-ol);
		}

		if ($('html').is('.ie6, .ie7, .ie8')) {

			var w = ($("#footer-wrapper").width()-2)*(zoom/100)
			$("#sitemap-footer").css('width',w+"px");
			var leftDiff = w -$("#footer-wrapper").width()
			$("ul[id^='menu-sitemap'] li:nth-last-child(4), #sitemap-close").css('right',leftDiff+"px")

			if(zoom != 100){
				var hDiff = $(wrapper).height()*(1-(zoom/100))*.9;
				$("#sitemap-footer").css('bottom',hDiff+"px");
			}else{
				$("#sitemap-footer").css('bottom',0);
			}

			$("#sitemap-footer").css('left',0)
			var xDiff =   $("#colophon").offset().left - $("#sitemap-footer").offset().left
			$("#sitemap-footer").css('left',xDiff+"px")
    		}
	}
	})
}

function setMenuBarPos(){
	jQuery(function($) {		
		var zoom = 1;

		var isIE11 = !!navigator.userAgent.match(/Trident.*rv[ :]*11\./)

	if ($.browser.mozilla && !isIE11){
			zoom = currFFZoom;
		} else {
			zoom = currIEZoom/100
		}
		var top = $("ul[id^='menu-megamenu']").offset().top
		if (!(navigator.userAgent.indexOf('MSIE') > -1 || navigator.userAgent.indexOf('rv:11') > -1 || $.browser.mozilla)) {
			top = top*zoom
		}
		$('#site-menu-bar').css( 'top' , top + 'px' );
		$('#site-menu-bar .header-blue-line').height(30*zoom+"px")
		$('#site-menu-bar .header-gold-line').height(5*zoom+"px")

		$('#zoom-wrap2').css( 'width' , Math.max(1024,1024*zoom) + 'px' );
	});
}

