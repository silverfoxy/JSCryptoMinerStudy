<script type="text/javascript">

var currFFZoom = 1;
var currIEZoom = 100;

jQuery(document).ready(function($){
			jQuery("#header-search-form").submit(function(){
				if($('input[name=sopt]:checked', '#header-search-form').val() == 'publications'){
					var q = $("#header-search-input input").val();
					window.open("http://spfind.ust.hk/spfind/Search/Results?lookfor="+q+"&type=AllFields&submit=Find");
					event.preventDefault();
