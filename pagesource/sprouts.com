	$(document).ready(function() {			
		var loadingMessage = '<p id="loadingMessage">Loading products...</p>';
		$(loadingMessage).insertBefore('unata-product-collection');
		$('unata-product-collection').hide();
		setTimer();
	});
	function setTimer() {
		setTimeout(function () {
			if ($('unata-product-collection unata-product-cell').length > 0 ){
				initializeProductCarousel();
			} else {
				setTimer();
			}
		}, 50);
	}
	function initializeProductCarousel() {
		if ($('unata-product-collection unata-product-cell').length > 4 || mobilecheck()) {								
			$('.collection').slick({
				arrows: true,
				dots: true,
				variableWidth: true,
				infinite: false,
				slidesToScroll: 4,
				slidesToShow: 4,
				focusOnChange: true,
				responsive: [
					{
						breakpoint: 959,
						settings: {
							slidesToShow: 3,
							slidesToScroll: 3
						}
					},
					{
						breakpoint: 749,
						settings: {
							slidesToShow: 2,
							slidesToScroll: 2
						}
					},
					{
						breakpoint: 640,
						settings: {
							slidesToShow: 1,
							slidesToScroll: 1
						}
					}]
			});
		}
		$('#loadingMessage').remove();
		$('unata-product-collection').show();
	}
</script>


