$( document ).on('click', '.drawer-inner .quickshop', function() {
  var product_handle = $(this).data('handle');
    $.getJSON('/products/'+product_handle+'.js').then(function(product) {
      var product_sku = product.variants[0].sku;
      var product_variant = product.variants[0].title;

      analytics.track('Product Viewed', {
       product_id: product.id.toString(),
       sku: product_sku,
       category: '',
       name: product.title,
       brand: product.vendor,
       price: parseInt(Shopify.formatMoney(product.price).replace('$', '').replace(',', '')),
       price_original: parseInt(Shopify.formatMoney(product.compare_at_price).replace('$', '').replace(',', '')),
       variant: product_variant,
       page_type: 'Page',
    });
  });
});

$( document ).on('click', '#segCheckout, #bCheckout, #paypal-express-button, .amazon-payments-pay-button', function(e) {
    my_id = $(this).attr('id');
    if(my_id == 'paypal-express-button') {
      var my_method = 'PayPal';
    }
    if(my_id == 'segCheckout' || my_id == 'bCheckout') {
      var my_method = 'Shopify';
    }

    if($(this).hasClass('amazon-payments-pay-button')) {
      var my_method = 'Amazon Pay';
    }

    var cart_values = [];

    $.ajax({
      url: '/cart.js',
      dataType: 'json',
      async: false,
      success: function(cart) {
        $.each(cart.items, function(i,line_item){
          cart_values.push({
            'product_id': line_item.id,
            'sku': line_item.sku,
            'category': line_item.product_type,
            'name': line_item.title,
            'brand': line_item.vendor,
            'price': parseInt(Shopify.formatMoney(line_item.price).replace('$', '').replace(',', '')),
            'quantity': line_item.quantity,
          });
        })

        analytics.track('Checkout Started', {
          cart_size: cart.item_count,
          cart_total: parseInt(Shopify.formatMoney(cart.total_price).replace('$', '').replace(',', '')),
          payment_method: my_method,
          products: cart_values
        });

      }
    });
});

function trackCartRemove(product_handle, variant_id) {
  $.getJSON('/products/'+product_handle+'.js').then(function(product) {
    var result = $.grep(product.variants, function(e){ return e.id == variant_id; });
    var product_sku = result[0].sku
    var product_variant = result[0].title

    analytics.track('Product Removed', {
      product_id: product.id.toString(),
      sku: product_sku.toString(),
      category: '',
      name: product.title,
      brand: product.vendor,
      price: parseInt(Shopify.formatMoney(product.price).replace('$', '').replace(',', '')),
      price_original: parseInt(Shopify.formatMoney(product.compare_at_price).replace('$', '').replace(',', '')),
      variant: product_variant,
      quantity: 1
    });
  });
}

function trackCartView() {
  var cart_values = [];
  $.getJSON('/cart.js', function(cart){
    $.each(cart.items, function(i,line_item){
      cart_values.push({
        'product_id': line_item.id,
        'sku': line_item.sku,
        'category': line_item.product_type,
        'name': line_item.title,
        'brand': line_item.vendor,
        'price': parseInt(Shopify.formatMoney(line_item.price).replace('$', '').replace(',', '')),
        'quantity': line_item.quantity,
      });    
    })
    
    analytics.track('Cart Viewed', {
      cart_size: cart.item_count,
      cart_total: parseInt(Shopify.formatMoney(cart.total_price).replace('$', '').replace(',', '')),
      products: cart_values
    });
  });
}

var entityMap = {
  '&': '&amp;',
  '<': '&lt;',
