      <div class="hover-reveal">
        <img src="{{ this.main_image }}" alt="product image" />
      </div>
      <div class="details">
        <p class="vendor">
          {{ this.vendor }}
        </p>
        <h2 class="h2">{{ this.title }}, {{ this.price_formatted }}</h2>
      </div>
    </a>
  </li>
</script>


  
