<script type="text/javascript">
	var group_id = "0";
    var keyword = "";
    var sub_id = "";
    var sub_id2 = "";
    var ret = "";
    var version = "";
    var bOnExit = 0;
    var iAlert = 0;
    var sVersion = "";
    var iVersion = 1;
    if (jQuery.browser.mobile)
    {
        window.location.href = '?mobile=1&group_id=' + encodeURIComponent(group_id) + '&ret=' + encodeURIComponent(ret) + '&keyword=' + encodeURIComponent(keyword) + '&subid=' + encodeURIComponent(sub_id) + '&subid2=' + encodeURIComponent(sub_id2) + '&alert=' + iAlert + '&v=' + sVersion + '&version=' + iVersion + '&p=' + bOnExit;
    }
    var aMsec = [800, 800, 800];
    var aText = ['Answers approved!', 'Free registrations found!', 'Matching sex partners found!'];
    var iItem = 0;
    $(document).ready(function ()
    {
        var wWhite = 1005;
        var hWhite = 632;
        var rWhite = wWhite / hWhite;
        var idCounterDown;

        $('#email').focus();
        $('#email').keyup(function ()
        {
            $('#error').text('').hide();
        });
        $('.button', '.buttons').click(function ()
        {
            var currentStep = $(this).parents('.steps');
            var nextStep = currentStep.next();
            currentStep.hide();
            nextStep.show();
            if (nextStep.hasClass('step4'))
            {
                animateItem();
            }
        });
        function animateItem()
        {
            var currentLi = $('.stepInfo ul li:nth-child(' + (iItem + 1) + ')');
            currentLi.fadeIn(300);
            if (iItem < aMsec.length)
            {
                var ms = aMsec[iItem];
                setTimeout(function ()
                {
                    currentLi.removeClass('grey');
                    currentLi.find('.text').text(aText[iItem]);
                    iItem++;
                    setTimeout(animateItem, 400);
                }, ms);
            }
            else
            {
                $('.titleBlack', '.step4Title').hide();
                $('.titleRed', '.step4Title').fadeIn(500);
                $('.keeping').fadeIn(500);
                $('.questionBox').fadeIn(500);
                idCounterDown = setInterval(counterDown, 1000);
            }
        }

        function counterDown()
        {
            var m = parseInt($('.min', '.keeping').text());
            var s = parseInt($('.sec', '.keeping').text());
            if (m + s > 0)
            {
                s--;
                if (s == -1)
                {
                    s = 59;
                    m--;
                }
                $('.min', '.keeping').text(m > 9 ? m : '0' + m);
                $('.sec', '.keeping').text(s > 9 ? s : '0' + s);
            }
            else
            {
                clearInterval(idCounterDown);
            }
        }

        function setSize()
        {
            var wBox = $('.wrapBox').width();
            var hBox = $('.wrapBox').height();
            var rBox = wBox / hBox;
            if (rWhite < rBox)
            {
                $('.mainBox').width(hBox * rWhite);
                $('.mainBox').height('100%');
            }
            else
            {
                $('.mainBox').height(wBox / rWhite);
                $('.mainBox').width('100%');
            }

            var tempW = $('#imageGirl').width();
            $('.flag').width((tempW * 100 / 418) + '%');

            var x = parseInt($('.mainBox').width() * 100 / 936);
            var y = parseInt(x * 57 / 100);
            var wI = parseInt(x * 24 / 100);
            $('body').css('font-size', x + '%');
            $('.warning img').width(x + '% ').height(x + '%');
            $('.step4Title').height(parseInt(x * 65 / 100 + 'px'));
            $('.ring').css('background-size', x + '% ' + x + '%');
            $('.stop').width(x * 193 / 100 + 'px').height(x * 230 / 100 + 'px').css('background-size', x + '% ' + x + '%');
        }

        setSize();
        $(window).resize(function ()
        {
            setSize();
        });

