<script type="text/javascript">
    var busy = false;
    var limit = 12;
    var offset = 0;
    var uri = '/';
        function displayRecords(lim, off) {
        jQuery.ajax({
            type: "GET",
            async: false,
            url: "https://jplay.com.au/wp-content/themes/jplay/getrecords.php?uri=" + uri,
            data: "limit=" + lim + "&offset=" + off,
            cache: false,
            beforeSend: function () {
                jQuery("#loader_message").html("").hide();
                jQuery('#loader_image').show();
            },
            success: function (html) {
                jQuery("#results").append(html);
                jQuery('#loader_image').hide();
                if (html == "") {
                    jQuery("#loader_message").html('<button class="btn btn-default width-100" type="button">No more records.</button>').show()
                    window.busy = true;
                    exit;
                } else {
                    jQuery("#loader_message").html('<div id="loader_image"><img src="https://jplay.com.au/wp-content/themes/jplay/images/preloader.gif" alt="" > </div>').show();
                }
                window.busy = false;


            }
        });
    }

    jQuery(document).ready(function () {
        if (busy == false) {
            busy = true;
            displayRecords(limit, offset);
        }
        jQuery(window).scroll(function () {
            if (jQuery(window).scrollTop() + jQuery(window).height() > jQuery(document).height() - 10 && jQuery("#results").height() != 0 && !busy) {
                busy = true;
                offset = limit + offset;
                setTimeout(function () {
                    displayRecords(limit, offset);
                }, 500);
            }
        });

    });

</script>

