<script type="text/javascript">

    var searchTimer = 0;

    $(document).ready(function() {
        registerRideFinderSearchInput($("#ride-location-container.desktop"), $("#ride-location-trigger.desktop"), $("#ride-location-results.desktop"));
        registerRideFinderSearchInput($("#ride-location-container.mobile"), $("#ride-location-trigger.mobile"), $("#ride-location-results.mobile"));
        createDistanceSlider("desktop");
        createDistanceSlider("mobile");
        createDistanceSlider("modal-edit");
        setInitialStarRating("desktop");
        setInitialStarRating("mobile");
        setInitialStarRating("modal-edit");

        $('#ride-finder-modal').on('hidden.bs.modal', function (e) {
            e.stopPropagation();
        });
    });

    function registerRideFinderSearchInput(container, input, results) {
        var clearBackground = $("#clear-background");

        input.focus(function () {
            showSearchResults(clearBackground, container, results);
        });

        clearBackground.click(function () {
            hideSearchResults(clearBackground, container, results);
        });

        input.keyup(function () {
            clearTimeout(searchTimer);
            searchTimer = setTimeout(function () {
                rideFinderLocationSearch(input, results, "ride-finder")
            }, 150);
        });
    }

    function rideFinderLocationSearch(inputEl, resultsEl, source) {
        var query = inputEl.val();

        if (query.length > 2) {
            if (typeof searchAjaxRequest !== "undefined") {
                searchAjaxRequest.abort();
            }
            searchAjaxRequest = $.ajax({
                url: inputEl.data("ajax") + "?q=" + encodeURIComponent(query) + "&source=" + source,
                success: function (data) {
                    resultsEl.html(data);
                }
            });
        }
    }

    function createDistanceSlider(type) {
        var projectValues = getProjectDistanceValues();
        var formattedValues = formatSliderDisplayValues(projectValues, projectValues);

        $("#min-distance." + type).html(formattedValues[0]);
        $("#max-distance." + type).html(formattedValues[1]);

        $("#ride-distance-slider." + type).slider({
            range: true,
            min: projectValues[0],
            max: projectValues[1],
            values: projectValues,
            slide: function(e, ui) {
                updateSliderDisplayValues(ui.values, type);
                updateFilters(type);
            },
            stop: function(e, ui) {
                updateFilters(type);
            }
        });
    }

    function getProjectDistanceValues() {
        var rangeValues = [];
        rangeValues["hike"] = [0, 20];
        rangeValues["mtb"] = [0, 40];
        rangeValues["trailrun"] = [0, 25];
        rangeValues["ski"] = [0, 15];

        var projectType = "hike";
        return rangeValues[projectType];
    }

    function displayStarRating(rating, starColor, type) {
        onRFStarOver("rating", rating, starColor, type);
    }

    function onRFStarOver(id, score, starColor, type) {
        for (var i = 1; i <= 4; i++) {
            var fullStarSrc = "star" + starColor + ".svg";
            var emptyStarSrc = "starYellowEmpty.svg";
            var src = (score >= i) ? fullStarSrc : emptyStarSrc;
            $("#star-" + i + "-" + id + "." + type).attr("src", gsCDNUrl + "/img/stars/" + src);
        }
    }

    function rideFinderLocationClick(title, x, y, radius) {
        var type = ($("#desktop-container").is(":visible")) ? "desktop" : "mobile";

        var query = $("#ride-location-trigger." + type).val();
        logReiAnalytics("search-click", title);
        logReiAnalytics("search", query);

        $("#ride-location-trigger." + type).val(title);
        $("#ride-location-results." + type).hide();
        $("#clear-background").hide();

        var bounds = [
            [x2lon(x - radius), y2lat(y - radius)],
            [x2lon(x + radius), y2lat(y + radius)]
        ];

        window.APMap.fitBounds(bounds, {animate: true});
    }

    function getDifficulty(type) {
        var green = 0;
        var greenBlue = 0;
        var blue = 0;
        var blueBlack = 0;
        var black = 0;
        var dblack = 0;

        $(".diff-selector." + type + ":checked").each(function() {
            switch ($(this).val()) {
                case "easy":
                    green = 1;
                    greenBlue = 1;
                    break;
                case "intermediate":
                    blue = 1;
                    blueBlack = 1;
                    break;
                case "difficult":
                    black = 1;
                    dblack = 1;
                    break;
            }
        });

        return {
            "green": green,
            "greenBlue": greenBlue,
            "blue": blue,
            "blueBlack": blueBlack,
            "black": black,
            "dblack": dblack
        };
    }

    function getDistance(type) {
        var distanceValues = $("#ride-distance-slider." + type).slider("option", "values");
        distanceValues = checkValuesForDistanceLimits(distanceValues, getProjectDistanceValues());
        return distanceValues;
    }

    function checkValuesForDistanceLimits(newValues, initialValues) {
        var newMin = newValues[0];
        var newMax = newValues[1];
        var initialMin = initialValues[0];
        var initialMax = initialValues[1];

        if (newMin == initialMin) {
            newMin = -1;
        }

        if (newMax == initialMax) {
            newMax = -1;
        }

        return [newMin, newMax];
    }

    function getRating(type) {
        return $("#score-rating." + type).val();
    }

    function setRating(rating, type) {
        $("#score-rating." + type).val(rating);
        if (type == "modal-edit") {
            onRFStarOver("rating", rating, "Red", "mobile");
        }
        updateFilters(type);
    }

    function updateSliderDisplayValues(values, type) {
        values = formatSliderDisplayValues(values, getProjectDistanceValues());
        $("#min-distance." + type).html(values[0]);
        $("#max-distance." + type).html(values[1]);
        if (type == "modal-edit") {
            $("#min-distance.mobile").html(values[0]);
            $("#max-distance.mobile").html(values[1]);
        }
    }

    function formatSliderDisplayValues(newValues, initialValues) {
        var newMin = newValues[0];
        var newMax = newValues[1];
        var initialMin = initialValues[0];
        var initialMax = initialValues[1];

        if (newMax == initialMax) {
            newMax = newMax + "+";
        }

        return [newMin, newMax];
    }

    function updateFilters(type) {
        var difficulty = getDifficulty(type);
        validateDifficulty(difficulty, type);
        var distance = getDistance(type);
        var rating = getRating(type);

        logReiAnalytics("filter-change");

        window.APMap.updateTrailFilters(rating, distance[0], distance[1], difficulty);
    }

    function validateDifficulty(difficulty, type) {
        if (type == "modal-edit") {
            if (!difficulty.green && !difficulty.blue && !difficulty.black) {
                $("#see-results").prop("disabled", true);
                $("#no-diff-indicator.mobile").show();
            }
            else {
                $("#see-results").prop("disabled", false);
                $("#no-diff-indicator.mobile").hide();
            }
        }
    }

    function resetFilters(e, type) {
        e.preventDefault();
        resetLocation(type);
        resetDifficulty(type);
        resetDistance(type);
        resetRating(type);
        updateFilters(type);
    }

    function resetLocation(type) {
        if (type == "modal-edit") type = "mobile";
        $("#ride-location-trigger." + type).val("");
        $("#ride-location-results." + type).html("");
    }

    function resetDifficulty(type) {
        $("input.diff-selector." + type).prop("checked", true);
        if (type == "modal-edit") {
            $("input.diff-selector.mobile").prop("checked", true);
        }
    }

    function resetDistance(type) {
        var projectValues = getProjectDistanceValues();
        updateSliderDisplayValues(projectValues, type);
        $("#ride-distance-slider." + type).slider("option", "values", projectValues);
    }

    function resetRating(type) {
        $("#score-rating." + type).val(0);
        setInitialStarRating(type);
        if (type == "modal-edit") {
            $("#score-rating.mobile").val(0);
            setInitialStarRating("mobile");
        }
    }

    function setInitialStarRating(type) {
        var starColor = (type == "desktop") ? "White" : "Red";
        displayStarRating(getRating(type), starColor, type);
    }

    function alignDifficulty(checkbox, type) {
        if (type == "modal-edit") {
            $(checkbox + "-mobile").prop("checked", $(checkbox + "-" + type).prop("checked"));
        }
    }

    function logReiAnalytics(description) {
        logReiAnalytics(description, "");
    }

    function logReiAnalytics(description, value) {
        if (window.utag) {
            var linkName = "ap:" + gsProjectType + "_rei-get-outside_ride-finder_" + description;
            if (value != "") {
                linkName += "_" + value;
            }

            window.utag.link({
                linkName: linkName,
                pageName: window.pageName
            });
        }
    }

    function toggleToView(view) {
        toggleIcon(view);
        toggleView(view);
    }

    function toggleIcon(view) {
        if (view == "list") {
            $("#list-toggle-mobile").hide();
            $("#map-toggle-mobile").show();
        }
        else if (view == "map") {
            $("#list-toggle-mobile").show();
            $("#map-toggle-mobile").hide();
        }
    }

    function toggleView(view) {
        $("#details-window").toggleClass('hidden-xs-down');
        var width = (view == 'list') ? '100%' : '200px';
        $("#details-window").css("width", width);
        $("#details-window-container").css("width", width);
    }

    function hideRideFinderModal() {
        $('#ride-finder-modal').modal('hide');
    }

</script>
