                         onclick="setRating(2, 'modal-edit');"
                         onmouseover="displayStarRating(2, 'Red', 'modal-edit');" />
                                    <img src="https://cdn.apstatic.com/img/clear.gif" id="star-3-rating" class="star modal-edit"
                         onclick="setRating(3, 'modal-edit');"
                         onmouseover="displayStarRating(3, 'Red', 'modal-edit');" />
                                    <img src="https://cdn.apstatic.com/img/clear.gif" id="star-4-rating" class="star modal-edit"
                         onclick="setRating(4, 'modal-edit');"
                         onmouseover="displayStarRating(4, 'Red', 'modal-edit');" />
