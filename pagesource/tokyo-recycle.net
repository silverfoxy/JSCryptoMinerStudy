        $(".js-include--instagram").append(
        $("<li>").addClass("itemList__unit").append($("<a>").attr("href", d.link).attr("target", "_blank").append($("<img>").attr("src", d.images.low_resolution.url))));
        }
      }
  });
};
});
</script>

