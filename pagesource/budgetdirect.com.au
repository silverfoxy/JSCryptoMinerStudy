    <!-- Lazy Load Images -->
    <script>
        var lazyImageLoad = (function(){
            var $lazyImages = null;
            var $lazyImagesProcessed = null;
            var $lazyContainerImages = null;
            var $lazyContainerImagesProcessed = null;
            var viewport = {
                height: 0,
                scrollTop: 0,
                scrollBottom: 0
            };
            var distancePastBottom = 500;
            var runDelay = null;
            
            return {
                setup: function(){
                    setup();
                },
                init: function(){
                    setup();
                    run();

                    window.removeEventListener('scroll', run);
                    window.addEventListener('scroll', run);
                }
            };
            
            function setup(){
                setGlobals();
                process();
            }
            
            function setGlobals(){
                $lazyImages = document.querySelectorAll('.lazy-image');
                $lazyImagesProcessed = document.querySelectorAll('.lazy-images-processed');
                $lazyContainerImages = document.querySelectorAll('.content-container--lazy-image');
                viewport.height = window.innerHeight;
                viewport.scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                viewport.scrollBottom = viewport.scrollTop + viewport.height;
            }
            
            function getTopOffset(elem){
                var offsetTop = 0;
                if (elem.offsetParent != null) {
                    while(elem){
                        offsetTop += elem.offsetTop;
                        elem = elem.offsetParent;
                    }
                }
                return offsetTop >= 0 ? offsetTop : 0;
            }
            
            function hideImages($lazyImage){
                if($lazyImage.querySelectorAll('img') && !$lazyImage.classList.contains('lazy-images-processed')){
                    $lazyImage.classList.add('lazy-images-processed');
                    $lazyImage.classList.remove('lazy-image');
                    var $lazyImages = $lazyImage.querySelectorAll('img');

                    for(var j = 0; j < $lazyImages.length; j++){
                        var $image = $lazyImages[j];

                        var src = $image.getAttribute('src');
                        $image.setAttribute('data-src', src);
                        $image.setAttribute('src', '');
                    }
                }
            }
            
            function showImages($lazyImage){
                if($lazyImage.querySelectorAll('img') && $lazyImage.classList.contains('lazy-images-processed')){
                    $lazyImage.classList.remove('lazy-images-processed');
                    var $lazyImages = $lazyImage.querySelectorAll('img');

                    for(var j = 0; j < $lazyImages.length; j++){
                        var $image = $lazyImages[j];

                        var src = $image.getAttribute('data-src');
                        $image.setAttribute('src', src);
                        $image.removeAttribute('data-src');
                    }
                }
            }
            
            function showContainerImages($lazyImage){
                if($lazyImage.classList.contains('content-container--lazy-image')){
                    $lazyImage.classList.remove('content-container--lazy-image');
                }
            }
            
            function process(){
                for(var i = 0; i < $lazyImages.length; i++){
                    var $lazyImage = $lazyImages[i];

                    hideImages($lazyImage);
                }
            }
                
            function run(){
                if(runDelay !== null){
                    clearTimeout(runDelay);
                }
                runDelay = setTimeout(function(){
                    setGlobals();
                    
                    for(var i = 0; i < $lazyImagesProcessed.length; i++){
                        var $lazyImage = $lazyImagesProcessed[i];
                        var offsetTop = getTopOffset($lazyImage);

                        if(offsetTop < (viewport.scrollBottom + distancePastBottom)){
                            showImages($lazyImage);
                        }
                    }
                    
                    for(var i = 0; i < $lazyContainerImages.length; i++){
                        var $lazyContainerImage = $lazyContainerImages[i];
                        var offsetTop = getTopOffset($lazyContainerImage);

                        if(offsetTop < (viewport.scrollBottom + distancePastBottom)){
                            showContainerImages($lazyContainerImage);
                        }
                    }
                }, 30);
            }
        })();
    </script>

