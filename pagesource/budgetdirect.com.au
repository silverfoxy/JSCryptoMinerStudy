    <script>
        var digitalData = {};
            
        var $legacyPageName = document.querySelector('meta[property="legacy-pagename"]');
        var legacyPageName = 'Not Set';
        if($legacyPageName && $legacyPageName.getAttribute("content")){
            legacyPageName = $legacyPageName.getAttribute("content");
        }

        var pageName = window.location.pathname;
        pageName = pageName.split('.')[0];
        pageName = pageName.replace('/content/budget-direct-desktopsite/en/', '');

        var xhr = new XMLHttpRequest();
        xhr.open('get', "https://www.budgetdirect.com.au/interactives/clientuid/", true);
        xhr.responseType = 'json';
        xhr.onload = function () {
            var status = xhr.status;
            var hashIP = 'Not Set';
            var realIP = 'Not Set';
            if (status == 200) {
                hashIP = xhr.response.uid;
                realIP = xhr.response.ip;
            }

            bdDDL.create({
                app: 'aem',
                legacyPageName: legacyPageName,
                pageName: pageName,
                hashIP: hashIP,
                realIP: realIP
            }, function(result){
                digitalData = result;
                var eventSignalData = new Event('signalData');
                document.dispatchEvent(eventSignalData);
            });
        };
        xhr.send();
    </script>
    
