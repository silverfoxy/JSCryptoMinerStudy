                    products.push({
                        'category': category,
                        'name': value.Description().toLowerCase(),
                        'price': value.ItemPrice(),
                        'quantity': '1'

                    });
                }

            });
            if (privateRepeats != null)
            {
                $.each(privateRepeats, function (key, value) {
                    if (value.Selected()) {
                        var category = "private";
                        var variant = value.Quantity() + " " + value.Units();
                        products.push({
                            'category': category,
                            'name': value.Description().toLowerCase(),
                            'price': value.ItemPrice(),
                            'variant': variant,
                            'quantity': '1'

                        });
                    }

                });
            }


            dataLayer.push({
                'event': 'visitor interaction',
                'interaction_name': 'commence checkout',
                'checkout_loc': 'repeat prescription',
                'ecommerce': {
                    'checkout': {
                        'actionField': { 'step': 1 },
                        'products': products
                    }
                }
            });
        }

        defer(function () {
