            var onlineDoc = '/ssl/product/OnlineDoctorService/' + preperation;
            if (url.indexOf('?') == -1) {
                url = url + "?";
            }
            else {
                url = url + "&";
            }
            var urlPrivate = url + "pm=pv";
            var urlPay = url + "pm=pp";
            var urlExempt = url + "pm=pe";
            var data = { urlPrivate: urlPrivate, urlPay: urlPay, urlExempt: urlExempt, urlOnlineDoc: onlineDoc };

            prescriptionOptionViewModel = ko.mapping.fromJS(data);
            ko.applyBindings(prescriptionOptionViewModel, $("#prescription-options")[0]);

