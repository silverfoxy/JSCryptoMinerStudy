                var ProductGuid = $(this).attr('data-addbasketitemguid');
                var PreperationGuid = $(this).attr('data-addprepitemguid');
                var p2uPreparationId = $(this).attr('data-addp2uprepitemguid');
                var Quantity = $(this).attr('data-quantity');
                var overwriteitem = $(this).attr('data-overwriteitem');
                var inputname = $(this).attr('data-inputname');

                var discoutnfix = $(this).attr('data-discountfix');

                //console.log("addbasketitemguid" + ProductGuid);
                //console.log("addprepitemguid" + PreperationGuid);
                //console.log("addp2uprepitemguid" + p2uPreparationId);
                //console.log("quantity" + Quantity);
                //console.log("inputname" + inputname);


                if ((PreperationGuid != null && PreperationGuid != 0) || (p2uPreparationId != null && p2uPreparationId != 0)) {
                    ProductGuid = null;
                }
                if (Quantity === undefined || Quantity.length == 0) {
                    Quantity = $('select[name=quantity' + (ProductGuid ? ProductGuid : "") + (PreperationGuid ? PreperationGuid : "") + (p2uPreparationId ? p2uPreparationId : "") + '] option:selected').val();
                }
                console.log("quantity" + Quantity);

                if (Quantity === undefined && inputname !== undefined) {
                    Quantity = $('#' + inputname).val();
                }
                console.log("quantity" + Quantity);


                var Exempt = $(this).attr('data-exempt');
                var Prescription = $(this).attr('data-prescription');
                var nameHtml = $('.productName', $(this).parentsUntil('.Product')).html();

                // Discount qty change workaround.
                for (var i = 0; i < (discoutnfix == 'true' ? 2 : 1); i++) {


                    PostAPI('product/updatebasketproduct', { productGuid: ProductGuid, preperationGuid: PreperationGuid, p2uPreparationId: p2uPreparationId, productQuantity: Quantity, isNHSExempt: Exempt, IsNHSPrescription: Prescription, overwrite: overwriteitem }, "json", function (data) {
                        //UpdateMVCBasketHeader();
                        UpdateSideBasket();
                        if (typeof CheckoutBasketPage !== 'undefined' && CheckoutBasketPage != undefined && CheckoutBasketPage) {
                            UpdateBasketInnerItems();
                        }
                    },
                    function (err) {
                        // error
                    });

                }

                //Leap3 DataLayer from Mini Basket

                if (Quantity != "0")
                {
                    //Add to Basket
                    DataLayerAddBasket();
                }
                else {
                    var itemName = $(this).attr('data-ItemName');
                    var price = $(this).attr('data-price');
                    var nhs = $(this).attr('data-prescription');
                    var exempt = $(this).attr('data-exempt');
                    var category = "private"

                    console.log("nhs 1: " + nhs);
                    console.log("exempt: " + exempt);


                    var roundedPrice = (Math.round(price * 100) / 100);
                    price = roundedPrice.toFixed(2);

                    if (nhs.toLowerCase() == "true") {
                        category = "nhs exempt";

                        if (price != 0.00) {
                            category = "nhs paid";
                        }

                    }
                    DataLayerRemoveBasket(itemName.toLowerCase().replace(' ','-'), price, category);
                }


                return false;
            });

        });
