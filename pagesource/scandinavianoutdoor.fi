    $(function() {
        function getClosedNotices() {
            var closedNotices = [];
            if (sessionStorage.getItem("closedNotices") !== null) {
                closedNotices = JSON.parse(sessionStorage.getItem("closedNotices"));
            }
            return closedNotices;
        }

        function getDeliveryCountries() {
            $.ajax({
                type: 'GET',
                url: '/api/delivery_countries/',
                success: function(data) {
                    var SELECTED_DELIVERY_COUNTRY = data.SELECTED_DELIVERY_COUNTRY;
                    var SELECTED_DELIVERY_COUNTRY_ISO = data.SELECTED_DELIVERY_COUNTRY_ISO;
                    var DELIVERY_COUNTRIES = data.DELIVERY_COUNTRIES;
                    var RUSSIAN_COUNTRY_ISOS = data.RUSSIAN_COUNTRY_ISOS;

                    var selectedCountryIsoLower = SELECTED_DELIVERY_COUNTRY_ISO.toLowerCase();
                    var selectedCountryHtml = SELECTED_DELIVERY_COUNTRY;
                    selectedCountryHtml += ' <span class="flag ' + selectedCountryIsoLower + '">';

                    $('.selected-country').html(selectedCountryHtml);

                    $.each(DELIVERY_COUNTRIES, function(index, value) {
                        var uri = '/delivery_country/' + '?country_iso=' + value[0];
                        var countryIsoLower = value[0].toLowerCase();
                        var li_element = '<li><a href="' + uri + '">' + value[1];
                        li_element += '<span class="flag ' + countryIsoLower + '"></span></a></li>';
                        $('#delivery-countries').append(li_element);
                        $('#delivery-countries-mobile').append(li_element);
                    });

                    if(data.AUTO_SET_DELIVERY_COUNTRY) {
                        var noticeText = "Toimitusmaaksi on automaattisesti valittu " + SELECTED_DELIVERY_COUNTRY;
                        noticeText += ". Voit vaihtaa maata alla olevasta pudotusvalikosta.";

                        var noticeHtml = '<div class="delivery-country-notice text-center"><span>' + noticeText + '</span><div class="close-notice right"><i class="fa fa-times-circle"></i></div></div>';
                        $(".header-top").prepend(noticeHtml);

                        $(".delivery-country-notice .close-notice").click(function() {
                            $(".delivery-country-notice").addClass("hide");
                        });
                    }

                    // Don't show on .ru site
                    if (window.location.href.indexOf('.ru') === -1) {
                        var closedNotices = getClosedNotices();
                        if (RUSSIAN_COUNTRY_ISOS.indexOf(SELECTED_DELIVERY_COUNTRY_ISO) > -1 && closedNotices.indexOf('ru') === -1) {
                            var russianNotice = '<div class="urgent-notices russia"><div class="urgent-notice-container" data-id="ru"><div style="text-align: center;margin: 0 20px 0 20px;padding: 10px 10px 10px 10px;line-height: 15px;"><a href="https://scandinavianoutdoor.ru/"><p style="color: #000;line-height: 15px;"> !      .      .</p></a></div><div class="close-notice"><i class="fa fa-times-circle"></i></div></div></div>';
                            $(".urgent-notices").prepend(russianNotice);
                            $(".russia .close-notice").click(function() {
                                $(".urgent-notices .russia").addClass("hide");
                                var closedNotices = getClosedNotices();
                                closedNotices.push('ru');
                                sessionStorage["closedNotices"] = JSON.stringify(closedNotices);
                            });
                        }
                    }
                },
                error: function(data) {
                }
            });
        }
        getDeliveryCountries();
    });
</script>


