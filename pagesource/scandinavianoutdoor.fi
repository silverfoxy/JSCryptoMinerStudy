<script type="text/javascript">
    $(function() {
        if($(".product-cards-container").length) {
            var isProductCardMutation;
            var productCardContainerMutated = false;
            var observer = new MutationObserver(function(mutations) {
                mutations.forEach(function(mutation, index) {
                    // reset this on first loop
                    if (index === 0) {
                        productCardContainerMutated = false;
                    }

                    isProductCardMutation = $(mutation.target).prop("class") === "lazyload";
                    if(isProductCardMutation) {
                        if(typeof StampedFn !== "undefined") {
                            StampedFn.loadBadges();
                        }
                    } else {
                        // only run once for product card container mutation(observer for search page)
                        if($(mutation.target).prop("id") === "search-direct-results") {
                            if(!productCardContainerMutated) {
                                StampedFn.loadBadges();
                                productCardContainerMutated = true;
                            }
                        }
                    }
                });
            });

            var observerConfig = {
                childList: true,
                subtree: true
            };

            $.each($(".product-cards-container"), function() {
                observer.observe($(this)[0], observerConfig);
            });
        }
    });
</script>

        
