    function ajax_clock_pop_up(content_type_id, object_id){
        div_name = "#ajax_clock_" + content_type_id + "_" + object_id;
        div_url = '/mediaon_fan/save_object_ajax/' + content_type_id + '/' + object_id;
        //alert(div_name + ' ' + div_url)
        $(div_name).show();
        $(div_name).load(div_url);
        //alert();
    }
</script>


<script type="text/javascript">
    $(document).ready(function () {

        // hide #back-top first
        $("#logoup").hide();
        $(".menu01b").hide();
        var done_kauppa = 0;
        //disabilitato
        var done_announces = 1;
        //end disabilitato
        var done_ccc = 0;
        var done_video_box = 0;
        var done_rubriche = 0;
        var done_cordata = 0;
        if ( $("#day_video_big_box").length ) {
            var videoBoxFromTop = $("#day_video_big_box").offset().top;
        } else {
            var videoBoxFromTop = 0;
        }


        if ($("#mobile_kauppa").length) {

                done_kauppa = 1;

                $.ajax("https://live.mediaon.it/live/ajax/media_adv/?site_id=13&content_type_id=715&class_object=prodotto&template_size=mobile&province_id=Monza e Brianza", {
                    cache: false,
                    crossDomain: true,
                    xhrFields: {
                        'withCredentials': false
                    },
                    success: function(data, textStatus, jqXHR) {
                        $("#mobile_kauppa").html(data);
                    },
                    dataType:"html"
                });

        };

        // fade in #back-top
        //funzioni che richiedono scroll
        setInterval(function () {
            if (didScroll) {
                didScroll = false;
                var scrollTop = $(this).scrollTop();
                //inserimento menù navigazione
                if (scrollTop > 500) {
                    $('#logoup').fadeIn();
                    $('.menu01b').fadeIn();
                    $('.bar1').fadeOut();
                    $('.menusopra').show();
                    $(".menusopra").css("visibility", "visible");
                } else {
                    $('#logoup').fadeOut();
                    $('.menu01b').fadeOut();
                    $('.bar1').fadeIn();
                    $('.menusopra').hide();
                    $(".menusopra").css("visibility", "hidden");

                }

                //Live cordata
                if (done_cordata == 0){
                    if ( $("#box_orobie_tre_notizie").length ) {
                        if (scrollTop > 200) {
                            done_cordata = 1;
                            $.ajax("https://live.mediaon.it/live/ajax/cordata/?no_object=3&province_id=Bergamo&site_id=12&target=_blank&content_type_id=14&source_type_id=24&template=cityrun", {
                                cache: false,
                                crossDomain: true,
                                xhrFields: {
                                    'withCredentials': false
                                },
                                success: function (data, textStatus, jqXHR) {
                                    $("#box_orobie_tre_notizie").html(data);
                                },
                                dataType: "html"
                            });
                        }
                    }
                }



                //banner Kauppa
                if (done_kauppa == 0){
                    if ($("#banner_kauppa_col2").length) {
