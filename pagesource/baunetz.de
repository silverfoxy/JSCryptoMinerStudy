<script type="text/javascript">
// <![CDATA[

var faktorX = -10;
var faktorY = 360;

var banner_link_aktuell = false;
var gedimmt = false;
var aktueller_tooltip_footer = -1;
var footer_tooltips = '';

//Verlassen des Links
function bout(obj, el) {
	if (!$(banner_link_aktuell).descendantOf(obj) || banner_link_aktuell == obj) {
		hideTooltip('btooltip'+el);
	}
}

function bin(obj, el) {
	if (aktueller_tooltip_footer > -1) {
		hideTooltip('btooltip'+aktueller_tooltip_footer);
	}
  gedimmt = true;
	showTooltip('btooltip'+el,'info_blase');
  aktueller_tooltip_footer = el;
	bdimmen(obj);
}

//Dimmen der anderen Banner, das aktuelle bleibt hell
function bdimmen(obj) {
	if (obj) {
		var sib = $(obj).siblings();

		//Aktuelles Bild
		var child = $(obj).childElements();
		child.each(function(name,index) {
			if (name.tagName == 'IMG') {
				name.style.opacity = 1;
				name.style.filter = 'Alpha(opacity=100, finishopacity=0, style=4);';
			}
		});

		sib.each(function(links,index) {
			if (links.tagName == 'A') {
				var childs = $(links).childElements();
				childs.each(function(name,index) {
					if (name.tagName == 'IMG') {
						name.style.opacity = 0.5;
						name.style.filter = 'Alpha(opacity=50, finishopacity=0, style=4);';
					}
				})
			}
		})
	}
}

var rotating_footer_banner = false;

function Rotation_Banner_Werbung_Footer() {
	//Die anderen 4 Banner werden angezeigt
	/*var childs = $('footer_banner_werbung').childElements();
	$(childs[0]).addClassName('hide');
	$(childs[1]).removeClassName('hide');*/
	if ($('bgross1')) {
		if (!rotating_footer_banner) {
			new Effect.Fade('bgross1', {duration: 1.0, queue: 'front'});
			new Effect.Appear('bgross2', {duration: 1.0, queue: 'end'});
			rotating_footer_banner = true;
		} else {
			new Effect.Fade('bgross2', {duration: 1.0, queue: 'front'});
			new Effect.Appear('bgross1', {duration: 1.0, queue: 'end'});
			rotating_footer_banner = false;
		}

    if (typeof(footer_tooltips) == "object" && (footer_tooltips instanceof Array)) {
      if (aktueller_tooltip_footer > -1) {
        //Es wurde ein Tooltip ausgewählt
        var l = $('btooltip'+aktueller_tooltip_footer).style.left;
        var t = $('btooltip'+aktueller_tooltip_footer).style.top;
        //Index des aktuellen Tooltips herausfinden
        var pos = -1;
        for (var i = 0;i< footer_tooltips.length; i++) {
          if (footer_tooltips[i] == aktueller_tooltip_footer) {
            pos = i;
            break;
          }
        }
        if (pos > -1) {
          var ebene2 = -1;
          if (pos < 4) {
            ebene2 = footer_tooltips[pos+4]
          } else {
            ebene2 = footer_tooltips[pos-4];
          }
          if (ebene2 > -1 && $('btooltip'+ebene2)) {
            //Anderen Tooltip einblenden aber nur dann wenn der Mauszeiger innerhalb des Werbefelds ist
            if ($(banner_link_aktuell).descendantOf('footer_banner_werbung')) {
              $('btooltip'+ebene2).style.left = l;
              $('btooltip'+ebene2).style.top = t;
              hideTooltip('btooltip'+aktueller_tooltip_footer);
              showTooltip('btooltip'+ebene2,'info_blase');
              aktueller_tooltip_footer = ebene2;
            }
          }
        }
      }
    }

	}
	window.setTimeout("Rotation_Banner_Werbung_Footer()", 10000);
}

Event.observe(window, 'load', function() {
	if (document.getElementById('footer_banner_werbung') && document.getElementById('bgross2')) {
			window.setTimeout("Rotation_Banner_Werbung_Footer()", 10000);
	}
});

Event.observe(document, 'mousemove', function (el) {
	//banner_link_aktuell = el.target;
	//Problem mit descendantOf: http://stackoverflow.com/questions/4283713/descendantof-causes-js-error-in-ie
	banner_link_aktuell = el.element();

  /*if ($(banner_link_aktuell).descendantOf('footer_banner_werbung')) {
   //Nach dem Ausblenden der anderen Werbung soll der Tooltip schon seine aktuellen X,Y Werte vom vorherigen Tooltip übernehmen
   if (banner_link_aktuell.tagName == 'IMG' || banner_link_aktuell.tagName == 'A') {
    //if ($$(banner_link_aktuell));
   }
	}*/

  if (!$(banner_link_aktuell).descendantOf('footer_banner_werbung')) {
		if (banner_link_aktuell != $('btooltip0') && banner_link_aktuell != $('btooltip1') && banner_link_aktuell != $('btooltip2') && banner_link_aktuell != $('btooltip3') && banner_link_aktuell != $('btooltip4') && banner_link_aktuell != $('btooltip5') && banner_link_aktuell != $('btooltip6') && banner_link_aktuell != $('btooltip7')) {
if (gedimmt) {
      gedimmt = false;
			for (var i = 0; i < 8 ; i++) {
				hideTooltip('btooltip'+i);
			}
      //Alle Dimmungen wieder auf den ursprünglichen Zustand setzen
      $$('#bgross1 a img').each( function(name,index) {
          name.style.opacity = 1;
  				name.style.filter = 'Alpha(opacity=100, finishopacity=0, style=4);';
      });
      $$('#bgross2 a img').each( function(name,index) {
          name.style.opacity = 1;
  				name.style.filter = 'Alpha(opacity=100, finishopacity=0, style=4);';
      });
      $$('#footer_banner_werbung a img').each( function(name,index) {
          name.style.opacity = 1;
  				name.style.filter = 'Alpha(opacity=100, finishopacity=0, style=4);';
      });
}
		}
	}
});


</script>
