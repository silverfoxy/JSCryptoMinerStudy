    <script>
        //textWidth() returns width of text in a specific selector - used for Tab Nav and Tab component
        $.fn.textWidth = function(){
            var html_org = $(this).html();
            var html_calc = '<span>' + html_org + '</span>';
            $(this).html(html_calc);
            var width = $(this).find('span:first').width();
            $(this).html(html_org);
            return width;
        };// End text width function
        
        function resizeLeftColumnFiller(){       
            columnHeightBalance($('.two.parent-columns.alpha + .ten.parent-columns .twelveColGrid.section'),$('#left-nav'));
            columnHeightRebalance($('.two.parent-columns.alpha + .ten.parent-columns .twelveColGrid.section'),$('#left-nav'));
        }
        //Resizes the unbalanced height based on the desired main height
        function columnHeightBalance(mainHeight,unbalancedHeight){
            var mh = mainHeight.height();
            var uh = unbalancedHeight.height();
            var h = ( mh > uh ) ?  mh : uh;
            unbalancedHeight.height(h);
        }
        
        //RJO - Resizes the height again if the height changes after the page initially loads - EX(Toggling the gencontainer for the calculator page)
        function columnHeightRebalance(mainHeight,unbalancedHeight){
            var mh = mainHeight.height();
            var uh = unbalancedHeight.height();
            var reh = ( uh > mh ) ?  mh : uh;
            unbalancedHeight.height(reh);
        }   
             

        /*=============================================================================
            Resizes sub masthead
          =============================================================================*/       
        function subHeadMastSizer(){
            tabHeightSizer($('#subNav ul li a'));
        }
        
        function tabHeightSizer(tabSelector){
            //Will keep all tabs same height, despite line breaks
            
            var maxHeight = -1,regHeight = -1;
            tabSelector.each(function() {
                regHeight = regHeight > $(this).height() ? regHeight : $(this).height();
            
              maxHeight = maxHeight > $(this).outerHeight() ? maxHeight : $(this).outerHeight();
              if(maxHeight == 80){$(this).css({'width':'200px','height':'60px'});maxHeight = 60;}
            });
        
            tabSelector.each(function() {
              $(this).height(maxHeight);
            });
            
            //Sets and calls gradient height for sub masthead
            subMastheadGradientHeight(regHeight);
        }//END tabHeightSizer
        
        function subMastheadGradientHeight(h){
            var LINEHEIGHT = 20;
            var H1bottomMargin = 5;
            var maxH = h;
            var hVal = $('#shm-treatment').css('height');
            var maxBG  = parseInt(hVal);           
            var titleLineBreak = $('.title.section > h1').height() > 50? 40: 0;
            var netMaxHeight = maxBG + (maxH - LINEHEIGHT + titleLineBreak);
            //execute only when not in edit mode, due to layout conflict(s)
                //if creative space does not exist at all
                if( (!$('.creative.creativeSpace').length) || ($('html').hasClass('touch')) || ($('body').hasClass('mobile-first'))  ){
                    $('.title.section > h1').css('margin-bottom', '10px');
                }                

                //centers the title in the creative space if there is no subheader(breadcrumb) or secondary tab nav 
                else if(!$('.subHeader').length && maxH <= 0){                
                    var subMastHead = $("#sub-header").height();
                    if(subMastHead < 20){netMaxHeight -= titleLineBreak;}
                    $('#shm-treatment').css('max-height', netMaxHeight);
    
                    var titleHeight = 110 - $('.title.section').height();
                    titleHeight = ( titleHeight <= 0 ) ? 10 : titleHeight / 2; //responsive fix (title height drastically increases in mobile view due to line breaks)
                    var topBtmMargin = titleHeight + "px 0";
                    $('.title.section').css('margin', topBtmMargin);
                }   
                                   
                //fallback if tab nav doesn't exist
                else if(maxH <= 0){
                    var subMastHead = $("#sub-header").height();
                    if(subMastHead < 20){netMaxHeight -= titleLineBreak;}
                    $('#shm-treatment').css('max-height', netMaxHeight);
                    
                    var bufferSum = $('.subHeader').height() + $('.title.section > h1').outerHeight(true) + H1bottomMargin;

                    //120 is the height of the submasthead when there is no 2nd level tab nav
                    if (subMastHead < 20){titleLineBreak = 0;}
                    bufferSum = 120 - bufferSum + titleLineBreak; 

                    $('.title.section').css('margin-bottom', bufferSum);
                }
                //when tab nav exists
                else{
                    $('#shm-treatment').css('max-height', netMaxHeight);
                }
            
        }//END subMastheadGradientHeight
        /*============================END OF SUB MASTHEAD RESIZER ==============================*/
        resizeLeftColumnFiller();
        subHeadMastSizer();   

