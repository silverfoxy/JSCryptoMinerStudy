            var level = 1;
            var html = '';
            var list = document.createElement( "ul" );
            list.className = 'main-nav__mega-menu main-nav__sub-nav list--unstyled';

            Object.keys(countriesWithRegionsMobile[countryId]).map(function(id) {
                var r = countriesWithRegionsMobile[countryId][id];
                var region = r.region;
                var checked = filterParamsMob && filterParamsMob['region'] && filterParamsMob['region'].includes(region.uid);

                if (r.level > level) {
                    html += '<ul class="main-nav__sub-nav list--unstyled' + (singleCountry == country.pid && level == 1 ? " expanded" : "")+'">';
                } else if (r.level < level) {
                    for (var i = 0; i <= (level - r.level); i++) {
                        html += '</ul>';
                    }
