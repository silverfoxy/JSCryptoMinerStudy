;font-size : 13px;color : #000;} /* caractères */ .car_survol {font-size:30px;color:#CA0002;} .py_survol {color: #666;font-size:12px;} .tab_contenu_survol {padding:5px;} .texte_info_recherche {FONT-SIZE: 10pt; FONT-STYLE: normal; text-decoration: none; color:#2967AD;} .texte_description_survol {MARGIN-LEFT: 5px; COLOR: #000000; line-height: 12px; font-size:10pt; } /* 500 phrases */ .pincode900 span {FLOAT: left;PADDING: 2px;TEXT-ALIGN: center;} .pincode900 span:first-line {COLOR: #787878;HEIGHT: 16px;} .pincode900 {FONT-SIZE: 14px;MARGIN: 0px 0px 0px 20px;WIDTH: auto; COLOR: #ed1e78;}  /* COULEURS */ .noir,.black {color : #000;} .white,.blanc {color:#FFF;} .red,.rouge {color: #c1272d; } .grey {color:#666;} .grey2 {color:#606060;}  /* onglets de modif */ #onglets_art {border-bottom:1px solid #CCC;margin-bottom:10px;} #onglets_art a {text-decoration:none;border:none;display:block;float:left;background:url(/images/bac_ong.png) repeat-x; border:1px solid #CCC;margin-bottom:-1px;} #onglets_art a span {display:block;padding:5px}  /* BOX DES LISTES */ .top_box {clear:both;text-align:left;width: 655px;padding:0;} .titre_box {float:left;padding:5px;} .plus_box { float:right;padding:5px;margin:0;padding-bottom:0;} .plus { float:right;font-size:11px;margin:10px;} .box{width:650px;padding-top:8px;padding-bottom:5px;margin-bottom:10px;margin-right:10px;text-align:left;} #contenu .box ul { padding-left:0; }  /* NAVIGATION DES PAGES */ /*barre navigation*/ .navig {color:#666;text-decoration:none;font-family:Verdana, Geneva, sans-serif} #barre_navig_in {margin:15px;margin-left:10px;margin-bottom:20px} #barre_navig {color:#555;min-height:24px;max-height:24px;overflow:hidden;border-bottom:1px solid #F5F5F5;margin-bottom:10px} #barre_navig a {color:#555; padding-bottom:1px;border-bottom:1px dotted #E2E2E2} #barre_navig a:hover {color:#C1272D;text-decoration:none} #barre_navig h1,#barre_navig div {padding:5px;margin:0;color:#555;font-size:10px;text-align:left;} #barre_navig h1 {padding-bottom:0;font-size:11px;} /*Pages*/ .numpage,.numpage2 {font-size:14px;-moz-border-radius:24px;-webkit-border-radius:24px;border-radius:24px;cursor:pointer;padding:4px;text-align:center;text-decoration:none;border:1px solid #999;color:#999;display:inline-block;margin-right:1px;margin-left:1px} .numpage:hover{background:#F5F5F5} a.numpage:hover {text-decoration:none;}  .numpage2 {background:#666;border:1px solid #666;color:#FFF; font-weight:bold} .menu_navig {text-align:center;padding-top:15px}   /* ONGLETS DYNAMIQUES (cf voyage) */  #onglets_revue {margin:0;padding:0;padding-bottom : 21px;border-bottom : 1px solid #CCC;} #onglets_revue li { float : left;list-style:none;background-image: none;padding:4px;margin-left:5px;color: #1b629a; border : 1px solid #e5ecf1;background-image: url(/actualite/chine/back_onglet.png);background-repeat:repeat-x;border-bottom:1px solid #CCC;font-family:Verdana;padding-right:7px;padding-left:7px;font-size:10px;cursor:pointer;letter-spacing:1px;} #onglets_revue li:hover {border-color:#CCC} #onglets_revue li.ongactif {background-image:none;border:1px solid #ccc; color:#333;border-bottom:1px solid #fff;background-color:#FFF}  /* TAGS */ /* Tags dans les articles */ #corps .linkin {text-decoration:underline} #corps .linkin:hover {text-decoration:none} #corps .linkin strong {font-weight:inherit} #introtxt .linkin strong {font-weight:inherit} .list_txt .linkin strong {font-weight:inherit} /* Page tag */ .page_tags h1 {color:#999;font-size:28px;margin-top:10px}  /* MENU NEWS */ #plus_info_menu {display:none} .line_newsa {text-decoration:none;display:block;color:#333;padding-top:3px;padding-bottom:3px} .line_news1, .line_newst {height:35px;float:left;width:35px;font-weight:bold;text-align:center;} .line_news1 div, .line_newst div {background-color:#c1272d;color:#FFF} .line_news1b, .line_newstb {float:right;width:220px;font-size:14px} .line_news1b a, .line_newstb a {color:#222} .line_news1 .date {padding-top:2px} /* top news*/ .line_newsa:hover