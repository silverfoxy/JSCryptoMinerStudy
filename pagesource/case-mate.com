        'domReady!'
    ], function ($) {
        'use strict';
        $(document).ready(function(){

        	$.ajax({
	          url: 'https://telize-v1.p.mashape.com/geoip',
	          type: 'GET',
	          dataType: 'json',
	          success: function(json) { 
	          var countries = ["AT","BE","BG","CZ","DK","EE","FI","FR","DE","GB","GR","HU","IE","IT","LV","LT","LU","NL","PL","PT","RO","SK","SI","ES","SE","UK"];
	            if($.inArray(json.country_code,countries) >=0) {
	            	var index = $.inArray(json.country_code,countries);
	              	var cCode = countries[index];
	              	$(document).ready(function(){ 
	  			 		$('body').redirectModal(cCode); 
					});
	            }
	          
	          
	          },
	          error: function() {  },
		      beforeSend: function(xhr) { 
			  	xhr.setRequestHeader('x-mashape-key', 'oUH2dGeeBgmshSr5WOdWliRFlheYp13jmCCjsnrNAbRlST5veB'); 
		      }
	        });

        	$.fn.redirectModal = function(countryCode) {

			      // Handle cookies - Functions adapted from W3Schools: http://www.w3schools.com/js/js_cookies.asp
			      function getCookie() {
			          var name = "case-mate-redirect";
			          var cookieVals = document.cookie.split(';');
			          for(var i = 0; i < cookieVals.length; i++) {
			              var cookie = cookieVals[i];
			              while (cookie.charAt(0)==' ')
			                  cookie = cookie.substring(1);
			              if (cookie.indexOf(name) === 0)
			                  return cookie.substring(name.length, cookie.length);
			          }
			          return "";
			      }

			      function setCookie(choice) {
			          // Set to last 1 days
			          var date = new Date();
			          date.setTime(date.getTime() + 1*24*60*60*1000);
			          document.cookie = "case-mate-redirect="+choice+";expires="+date.toGMTString();
			      }

			      // Check if cookie exists. If no, show the redirect modal.
			       var cookieRedirect = getCookie();
			       if (cookieRedirect === "") {

			          var countryUrl;
			          if (countryCode === 'GB') {
			             countryCode = 'UK';
			             countryUrl = 'https://www.case-mate.co.uk';
			          } else if (countryCode === 'FR') {
			             countryUrl = 'https://www.case-mate.eu/fr';
			          } else if (countryCode === 'DE') {
			             countryUrl = 'https://www.case-mate.eu/de';
			          } else {
			             countryCode = 'EU';
			             countryUrl = 'https://www.case-mate.eu';
			          }

			          var body = $('body');

			          // Append transparent black overlay to site
			          var cover = $('<div id="redirect-cover"></div>');
			          cover.appendTo(body);

			          // Build modal elements
			          var modal = $('<div id="redirect-modal"></div>');
			          var modalInner = $('<div id="redirect-container"></div>');
			          var modalMessage = $('<p id="redirect-message"></p>');
			          var modalOptions = $('<ul id="redirect-options"></ul>');

			          // Append modal elements
			          modal.appendTo(body);
			          modalInner.appendTo(modal);
			          modalMessage.appendTo(modalInner).text("It appears you're visiting us from outside the US. If you would like to visit your regional store, please choose from the options below.");
			          modalOptions.appendTo(modalInner);

			          for (var i = 0; i < 2; i++) {
			             // Build options
			             var option = $('<li class="redirect-option"></li>');
			             var flag = $('<a href="javascript:void(0)" class="redirect-current"><img src="https://cdn.shopify.com/s/files/1/0796/3401/files/flag-us.jpg" alt="Case-Mate US"/></a>');
			             var actionCall = $('<a href="javascript:void(0)" class="redirect-current">Continue on Case-Mate US</a>');
			             // Append options
			             option.appendTo(modalOptions);
			             flag.appendTo(option);
			             actionCall.appendTo(option);
			             if (i === 1) {
			                // Configure suggested option
			                option.attr('id','redirect-suggested');
			                option.children('a').attr('data-redirect-code',countryCode).removeClass().addClass('redirect-link');
			                var flagSrc = 'https://cdn.shopify.com/s/files/1/0796/3401/files/flag-'+countryCode.toLowerCase()+'.jpg';
			                flag.children('img').attr('id','redirect-suggested-flag').attr('src',flagSrc).attr('alt','Case-Mate '+countryCode);
			                actionCall.attr('id','redirect-suggested-message').text('Switch to Case-Mate '+countryCode);
			             }
			          }

			          // Handle closing modal without redirecting
			          body.on('click','.redirect-current',function(){
			             setCookie('continue');
			             cover.remove();
			             modal.remove();
			          });

			          // Handle redirect
			          $('.redirect-link').click(function(){
			             setCookie('redirect');
			             window.location.href = countryUrl;
			          });

			       }

			    return this;

			};
            
        });
    });
