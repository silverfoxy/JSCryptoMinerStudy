<script type="text/javascript">
    var bannerUpdateInterval = (VmiAdParams.reload_ads != 'undefined') ? parseInt(VmiAdParams.reload_ads) : 0;
    var bannerRefreshTime = (VmiAdParams.refresh_time != 'undefined') ? parseInt(VmiAdParams.refresh_time) : 0;
    var vmi_ads = {};
    var vmi_ads_to_render = {};
    /**
     * Display anzeige text near the add
     * @param elem
     */
    function showAnzeige(elem){
		if((jQuery(elem).data('pos') == 'x74' || jQuery(elem).data('pos') == 'x92') && jQuery(elem).class != 'fixed') {
				return;
		}
        if(jQuery(elem).find('.anzeige').length==0 && jQuery(elem).find('iframe').length >0){
            jQuery(elem).append('<div class="anzeige" style="position:absolute; top:14px; font-size: 11px; left: -30px; transform: rotate(-90deg); -webkit-transform: rotate(-90deg); -ms-transform: rotate(-90deg); font-family:sans-serif;">Anzeige</div>');
        }
    }

    var rev6 = rev6 || {};
    var isDev = VmiAdParams.pageVariables.isDev;
    window.rev6 = rev6;
    var screen_size =  jQuery(window).width()<768 ?   'mobile' : jQuery(window).width()<992 ? 'tablet' : 'desktop';
    var oas_tag = oas_tag || {};
    oas_tag.url = 'oasc-eu1.247realmedia.com'; //Define OAS URL
    oas_tag.query = '&coUsrGr='+VmiAdParams.pageVariables.userGroup;
    oas_tag.anchorElementPrefix = 'rect'; // generates ids #rect_<pos> and generates iframes automaticaly
    oas_tag.allowSizeOverride = true;
    oas_tag.site_page = VmiAdParams.adSpecificDomain;  //Define OAS Site page
    //for debug
    //oas_tag.site_page = 'www.ke-next.de/werbung';
    oas_tag.sizes = function () { //size is required [width, height]
        //we will define all width as 1px 1px than we will resize those divs because for some ads sizes are dynamic
        //sizes fore ads are get via json
        if(screen_size == 'mobile'){
            var oas_tag_list_positions = [
                'x25','x26','x27','x28','x29','x30','x31','x32','x33','x34','x35','x36','x37','x38','x39',
                'x71','x76',
                'x80','x81','x83','x84','x85','x86','x87','x88','x89',
                'x90','x93','x94','x95'
            ];
        }
        else if(screen_size == 'tablet'){
            var oas_tag_list_positions = [
                'x25','x26','x27','x28','x29','x30','x31','x32','x33','x34','x35','x36','x37','x38','x39',
                'x72','x76',
                'x80','x81','x82','x83','x84','x85','x86','x87','x88','x89',
                'x90','x93','x94','x95'
            ];
        }
        else{
            var oas_tag_list_positions = [
                'x25','x26','x27','x28','x29','x30','x31','x32','x33','x34','x35','x36','x37','x38','x39',
                'x73','x74','x75','x76','x77','x78','x79',
                'x80','x81','x82','x83','x84','x85','x86','x87','x88','x89',
                'x90','x91','x92','x93','x94','x95'
            ];
        }
        oas_tag_list_positions.forEach(function(position) {
            oas_tag.definePos(position, [1,1]); //we add 1px by 1px size for ad ,  we never know how much some add wil have width and high untill we don't get him from ad provider
        });
    };
    ////////////////// Optional Paramaters below //////////////////////////
    oas_tag.callbackHandler = function () {
        oas_tag.addHandler('initialize', function(data) {
            if(data.width != 'undefined'){
                for(var pos in data){
                    if(data.hasOwnProperty(pos)){
                        var $elem = jQuery('.rect[data-pos="'+pos+'"]');
                        var ad = data[pos];
                        if($elem.length>0 && ad.width != undefined && ad.width>0){
                            //fix position
                            if($elem.is('.left') || $elem.is('.left-bottom')){
                                var to_left = -1*(10+parseInt(ad.width));
                                $elem.css({'left':to_left})
                            }
                            if($elem.is('.right')){
                                var to_right = -1*(10+parseInt(ad.width));
                                $elem.css({'right':to_right})
                            }
                            if($elem.data('pos') =='x73'){
                                var to_left = 19 + parseInt(data['x74'].width);
                                $elem.css({'left':to_left});
                            }
                            if($elem.data('pos') =='x74'){
                                var to_top = parseInt(data['x73'].height);
                                $elem.css({'top':to_top});
                            }
                            $elem.width(ad.width);
                            $elem.height(ad.height);
                            $elem.removeClass('hidden');
                            if($elem.data('pos')!='x74'){
                                showAnzeige($elem);
                            }
                            vmi_ads[pos] = ad;
                        }
                        else{
                            if(ad.width>0){
                                vmi_ads[pos] = ad;
                                vmi_ads_to_render[pos] = ad;
                            }
                            if(isDev && ad.width != undefined && ad.width>0){
                                console.warn("Theres an ad on position:"+pos+' but it don\'t have placeholder div in body.');
                            }
                            $elem.addClass('hidden');
                        }
                    }
                }
            }
        });
    };
    //////////////////End Optional Paramaters //////////////////////////
    // Make async call - do not modify //

    (function() {
        oas_tag.version = '1';
        var oas = document.createElement('script');
        var protocol = (document.location.protocol == 'https:') ? 'https://' : 'http://';
        var node = document.getElementsByTagName('script')[0];
        oas.type = 'text/javascript';
        oas.async = true;
        //oas_tag.logging = true; for debug - gives you information about ad position
        oas.src = protocol + oas_tag.url + '/om/' + oas_tag.version + '.js';
        node.parentNode.insertBefore(oas, node);
    })();
    rev6.handleBanner = function(position, data) {
        var $placeholder = jQuery(oas_tag.getOasAdElement(position)).parent();
        var offsetTopLeftRight = 0;
        var $body = jQuery('body');
        var $placeholderOuterHeight = $placeholder.outerHeight(true);
        var hide = (data.height == 0 && data.width == 0); // data.creativeId == "empty.gif" ||
        if(data.height == undefined){
            hide = data.creativeId == 'empty.gif';
        }
        if (hide) {
            $placeholder.addClass('hidden');
            jQuery('.rect[data-pos="'+position+'"]').hide();
            if(position == 'x88' && jQuery('#rect_x80').is(':visible') == false){
                jQuery('#rect_x80-x88').hide();
            }
        } else {
            // fix dimensions
            $placeholder.css({width: data.width});
            $placeholder.css({height: data.height});
            showAnzeige($placeholder);
            $placeholder.removeClass('hidden');

        }
    };
</script>
<script type="text/javascript">

    function setCustomInterval(callback, delay, repetitions)
    {
        if(repetitions>0)
        {
            var x = 0;
            var intervalID = window.setInterval(function () {

                callback();

                if (++x === repetitions) {
                    window.clearInterval(intervalID);
                }
            }, delay);
        }
    }
    //this will refresh all ads in defined criteria
    jQuery(document).ready(function(){
        var iqads = ['#iqadtile1','#iqadtile2','#iqadtile6','#iqadtile8','#iqadtile9'];
        if(bannerUpdateInterval > 0)
        {
            window.setCustomInterval(function () {
                for(var pos in vmi_ads){
                    if(vmi_ads.hasOwnProperty(pos)){
                        var $elem = jQuery('.rect[data-pos="'+pos+'"]');
                        var ad = vmi_ads[pos];
                        if($elem.length>0 && ad.width != undefined && ad.width>0){
                            //fix position
                            if($elem.is('.left') || $elem.is('.left-bottom')){
                                var to_left = -1*(10+parseInt(ad.width));
                                $elem.css({'left':to_left})
                            }
                            if($elem.is('.right')){
                                var to_right = -1*(10+parseInt(ad.width));
                                $elem.css({'right':to_right})
                            }
                            $elem.width(ad.width);
                            $elem.height(ad.height);
                            $elem.removeClass('hidden');
                            showAnzeige($elem);
                        }
                    }
                }
            }, bannerUpdateInterval, bannerRefreshTime);
        }
        setTimeout(function(){
            if(isDev){
                showDuplicateAdsPositions();
            }
            //solution for anzeige for iqads
//            jQuery(iqads).each(function(index,value){
//                showAnzeige(jQuery(value));
//
//            })
        },5000);

//        jQuery(iqads).each(function(index,value){
//            jQuery(value).bind('DOMNodeInserted', function(event) {
//                if (event.type == 'DOMNodeInserted') {
//                    showAnzeige(this);
//                }
//            });
//        });

    });

    function showDuplicateAdsPositions(){
        for(var pos in vmi_ads){
            if(jQuery('[id=rect_'+pos+']').length>1){
                jQuery('[id=rect_'+pos+']').append('<div class="bg-danger" style="position:absolute; top:20px; left:20px; height:60px; width:180px; padding:10px;">Duplicate ad position '+pos+'!</div>');
            }
        }
    }

    /**
     * Supporting old calls and reverse redirects.
     *
     * Creates a new placeholder, tries to load the ad and hides if that failed.
     */
    function OAS_AD(position) {
        console.warn(position, 'called via outdated command');
        var elemToAppend = null;
        var elementToHide = '';
        var elementContainer = jQuery(".mod_gridsystem:visible").length ? ".mod_gridsystem:visible" : "body";
        switch (position) {
            case 'x93':
                elemToAppend = jQuery("[data-pos='x81']", elementContainer);
                elementToHide = '#iqadtile8';
                jQuery('#rect_'+position).width('auto').height('auto');
                jQuery('#rect_'+position).width('auto').height('auto').find('.anzeige').remove();
                break;
            case 'x94':
                elemToAppend = jQuery("[data-pos='x82']", elementContainer);
                elementToHide = '#iqadtile9';
                jQuery('#rect_'+position).width('auto').height('auto').find('.anzeige').remove();
                break;
            default: // unsupported format, i.e. should not be called this way
                return;
        }

        if (!elemToAppend.prev().is('#rect_' + position)) {
            elemToAppend.before("<div id='rect_" + position + "' class='rect hidden' data-pos='" + position + "'></div>");
        } else {
            elemToAppend.prev().show();
        }

        oas_tag.reloadAds([position]);
        jQuery(elementToHide).addClass('hidden'); // always hide if this call was made
    }
</script>

