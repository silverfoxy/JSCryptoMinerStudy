function onProductClick(id, name, category, brand, position, list, destination ) {
ga('ec:addProduct', {
'id': '\'' + id + '\'',
'name': '\'' + name + '\'',
'category': '\'' + category + '\'',
'brand': '\'' + brand + '\'',
