function PinterestPixel() {

    var htmlImg = function (queryString) {
        if (!queryString) {
            queryString = '';
        }

        return '<img ' +
            'height="1" ' +
            'width="1" ' +
            'style="display:none;" ' +
            'alt="" ' +
            'src="https://ct.pinterest.com/v3/?tid=2615287242708&noscript=1' + queryString + '" />';
    };

    return {
        init: function () {
            jQuery('head').append(htmlImg(''));
        },
        addToCart: function () {
            jQuery('head').append(htmlImg('&event=AddToCart'));
        },
        checkout: function (value, order_quantity) {
            jQuery('head').append(htmlImg('&event=checkout&ed[value]=' + value + '&ed[order_quantity]=' + order_quantity + '&ed[currency]=USD'));
        }
    };
}
