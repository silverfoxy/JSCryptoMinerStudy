        handleResizeWindow(wbnsScreenWidth);
        jQuery("div#colorScroller").smoothDivScroll("jumpToElement", "number", scrolledElementNumber || 1);
        jQuery('#slider').slider('value', scrolledElementNumber || 1);
    });

    jQuery(document).ready(function($) {
        var uiHandleMove = true, prevElement = null;

        if(jQuery('#colorScroller').width() < 984){
            var colorSliderRatio = 5;
        } else {
            var colorSliderRatio = 6;
        }
        //Start position for sliders
        rollBackStartPosition = 30;
        jQuery("a[href*='lovestruck']").first().parent().parent().attr('id','tieksSliderStartPosition');
        minValueOfWidth = jQuery('#tieksSliderStartPosition').width();
        jQuery('#tieksSliderStartPosition').width(minValueOfWidth);

        $("div#colorScroller").smoothDivScroll({
            startAtElementId: "tieksSliderStartPosition",
            speedBooster: 0,
            rightScrollInterval: 1,
            leftScrollInterval: 1,
            scrollStep: 1,
            touchScrolling: ('ontouchend' in document),
            hotSpotScrolling: false,
            mousewheelScrolling: false,
            setupComplete: function(eventObj, data) {
                scrolledElementNumber = 30;;
                jQuery('#tieksSliderStartPosition').width(minValueOfWidth);
                handleResizeWindow(wbnsScreenWidth);
                jQuery("div#colorScroller").smoothDivScroll("jumpToElement", "number", scrolledElementNumber || 1);
                jQuery('#slider').slider('value', scrolledElementNumber || 1);
                $(eventObj.target).parent().prepend('<div class="left-scroll"></div><div class="right-scroll"></div>');
                $('.left-scroll').on('click', function(event) {
                    if (scrolledElementNumber > 1) {
                        $("div#colorScroller").smoothDivScroll("scrollToElement", "number", --scrolledElementNumber);
                        $('#slider').slider('value', scrolledElementNumber);
                    }
                });
                $('.right-scroll').on('click', function(event) {
                    if (scrolledElementNumber < (count - colorSliderRatio)) {
                        $("div#colorScroller").smoothDivScroll("scrollToElement", "number", ++scrolledElementNumber);
                        $('#slider').slider('value', scrolledElementNumber);
                    }
                });
            }
        });

        $("#slider").slider({
            animate: true,
            range: 'max',
            value: rollBackStartPosition,
            step: 0.05,
            min: 1,
            max: count - colorSliderRatio,
            create: function(event, ui) {
                $('.ui-slider-handle').append('<div class="outer"><div class="triangle"></div></div>');
            },
            slide: function(event, ui) {
                uiHandleMove = false;
                var elementNumber = Math.round(ui.value);
                if(prevElement != elementNumber) {
                    $("div#colorScroller").smoothDivScroll("scrollToElement", "number", elementNumber);
                    prevElement = elementNumber;
                    scrolledElementNumber = elementNumber;
                    //scrolledOffset = undefined;
                }
            }
        });
