        /* Swipe for iPad */
        $("div#colorScroller").on('swipeMoved', function(event, data) {
            if(data.scrollLeft && uiHandleMove) {
                var scrollWidth = $('#colorScroller .scrollableArea').width();
                var topLimit = scrollWidth - $('#slider').width()*0.95;
                var leftPos = (data.scrollLeft*100)/topLimit;
                if(leftPos > 94.64){leftPos = 94.64;}
                else if(leftPos < 0){leftPos = 0;}
                $('.ui-slider-handle').css({left: leftPos +'%'});
            }
        });
        $("div#colorScroller").on('swipeStopped', function(event, data){
            uiHandleMove = false;
        });
        var swipableElement = $('#colorScroller .scrollableArea')[0];
        Hammer(swipableElement, {
            stop_browser_behavior: {
                touchAction: 'auto',
                touchCallout: 'auto'
            }
        }).on('touch', function(event) {
            uiHandleMove = true;
        });
    });
