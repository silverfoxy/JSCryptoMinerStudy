      // If the ads are not loaded, track the event
      if ((ad && ad.innerHTML.replace(/\s/g, "").length == 0)||(!document.querySelector("div.GoogleActiveViewClass")))  {

        if (typeof ga !== 'undefined') {

            // Log an event in Universal Analytics
