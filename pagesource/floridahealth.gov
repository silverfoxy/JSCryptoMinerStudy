var quick_refresh = 0;
/*======on load + 4sec.======*/
$(window).load(function() {
  $('#Florida > path').mouseout(function() {
    quick_refresh = setInterval(function() {
      fbTimer();
      clearInterval(quick_refresh);
    }, 4000); //Fry 1000 = 1sec
  });
});

/*======on scroll======*/
$(window).on('scroll',function() {
    if (checkVisible($('div.facebook-header'))) {
        fbTimer();
        clearInterval(quick_refresh);
        quick_refresh = undefined;
        if ( getQueryVariable('alert') ) {
                alert("scroll ran");
                }
        $(window).off('scroll');
    } else {
        // do nothing
    }
});

function checkVisible( elm, eval ) {
    eval = eval || "object visible";
    var viewportHeight = $(window).height(), // Viewport Height
        scrolltop = $(window).scrollTop(), // Scroll Top
        y = $(elm).offset().top,
        elementHeight = $(elm).height();   
    
    if (eval == "object visible") return ((y < (viewportHeight + scrolltop)) && (y > (scrolltop - elementHeight)));
    if (eval == "above") return ((y < (viewportHeight + scrolltop)));
}
/*======changes on time======*/
var fb_auto_refresh = setInterval(function (){
    fbTimer();      //alert("1");           
}, 30000); //Fry 1000 = 1sec

</script>


