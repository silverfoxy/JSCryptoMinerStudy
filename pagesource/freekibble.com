		<script>
			jQuery(document).ready(function($) {
				var the_enchilada;
				if ( Cookies.get('odometer-meals') ) {
					the_enchilada = Cookies.get('odometer-meals');
				}
				var meals_kat = 13473432.68;
				var meals_dog = 7959540.38;
				if ( meals_kat > 0 && meals_dog > 0 ) {
					// we made it
					if ( Cookies.get('odometer-meals') === undefined ) {
						the_enchilada = Math.round(meals_kat + meals_dog);
					}
				} else {
					// we got nothin'
					if ( Cookies.get('odometer-meals') ) {
						the_enchilada = Cookies.get('odometer-meals');
					} else {
						$('.served').remove();
					}
				}
				if (window.location.href.indexOf('/thanks') != -1) {
					// giving user instant feedback by showing total growing
					the_enchilada++;
				}
				// https://github.com/js-cookie/js-cookie/wiki/Frequently-Asked-Questions
				var in_fifteen_minutes = new Date(new Date().getTime() + 15 * 60 * 1000);
				if ( the_enchilada != 'undefined' ) {
					Cookies.set('odometer-meals', the_enchilada, {
						expires: in_fifteen_minutes
					});
				}
				var the_enchilada_with_commas = the_enchilada.toString().replace(/,/g, '').replace(/\B(?=(\d{3})+(?!\d))/g, ',');
				$('#odometer').html(the_enchilada_with_commas);
			});
		</script>
	
