<script>

	function popShortcut(){
		
		document.getElementById("kstdDiv").style.display = 'block';
		
	}

function get_offset(obj){
		var offsetparent = obj;
		var left=0,top=0;
		
		while(offsetparent != null && offsetparent.nodeName != "BODY"){
			 left += offsetparent.offsetLeft;
			 top += offsetparent.offsetTop;
			offsetparent=offsetparent.parentNode;
		}
		return left;
	}

function restict(o,e){
	  var x=0,y=0;
	  var l=get_offset(o), t=o.offsetTop+2;
	  var w=o.offsetWidth, h=o.offsetHeight;
	  
	  x=e.pageX||e.clientX, y=e.pageY||e.clientY;
	  if((x>l && x<(l+w)) &&(y>t && y<(t+h))){
	    if(window.event){e.cancelBubble=true; return}
	    else{e.stopPropagation();  return }//stopPropagation是MOM标准的，IE6下的不行的，故必须颠倒一下顺序！
	  }
	 
	  o.style.display='none';
	}

	
</script>
