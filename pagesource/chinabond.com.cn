	        var parentDivid = selfObj.parentNode.parentNode.parentNode.id;
	        var tags = selfObj.parentNode.parentNode.getElementsByTagName("li");
	        var tagNumber = tags.length;
	        for (var index = 0; index != (tagNumber-1); index++){
	            var tag = tags[index];
	            tag.className = "";
