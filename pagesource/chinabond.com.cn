	jgzb_tag_num = num;
	jgzb_change();
}

var dateList=new Array("2018-03-23","2018-03-23","2018-03-23","2018-03-23","2018-03-23","2018-03-23");
var jgzb_ctl = setInterval(jgzb_change ,jgzb_time);
//自动切换标签方法
function jgzb_change(){

	for(var i=1;i<=6;i++){
		if(i == jgzb_tag_num){
			 $("#smore").html(dateList[i-1]);
			$("#jgzb_div_"+i).show();
			$("#cent_list5_li_"+i).attr("class","se");
		}else{
			$("#jgzb_div_"+i).hide();
			$("#cent_list5_li_"+i).removeClass("se");
		}
	}

	if(jgzb_tag_num >= 6){
		jgzb_tag_num = 1;
	}else {
		jgzb_tag_num++;
	}
}
//停止自动运行函数
function jgzb_stop(){
	clearInterval(jgzb_ctl);
}
//启动自动运行函数
function jgzb_start(){
	jgzb_ctl = setInterval(jgzb_change ,jgzb_time);
}
function gozzsj(){
	window.open("http://www.chinabond.com.cn/d2s/cbData.html");
}

</script>



