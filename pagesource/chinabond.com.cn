	//div display control
	var currentFocusPanel = null;
	function showOptions(obj)
	{
		if (currentFocusPanel != null)
			currentFocusPanel.style.display = "none";
		var optPanel = document.getElementById(obj.id + "_panel");
		position = new PositionGetter(obj.parentNode);
		optPanel.style.top = position.getElementTop() + 25 + 'px';
		optPanel.style.left = position.getElementLeft() + 'px';
		optPanel.style.display = "block";
		currentFocusPanel = optPanel;
		return false;
	}
    function hideDIV(event)
    {
        event = event ? event : window.event;
        var targetObj = event.target ? event.target : event.srcElement;	// 跨浏览器设定，前者是非 IE ，后者是 IE
        if (targetObj.id == "query1") return;
        if (targetObj.id == "query2") return;
		if (targetObj.id == "jdcxFormeYear") return;
		if (targetObj.id == "jdcxFormzqxz") return;
        if (currentFocusPanel != null)
        {
            if ((currentFocusPanel.id == "query1_panel" || currentFocusPanel.id == "query2_panel") && currentFocusPanel.style.display == "block")
            {
                if ((event.clientX < currentFocusPanel.style.pixelLeft || event.clientX > (currentFocusPanel.style.pixelLeft + currentFocusPanel.clientWidth))
                        || (event.clientY < currentFocusPanel.style.pixelTop || event.clientY > (currentFocusPanel.style.pixelTop + currentFocusPanel.clientHeight)))
                {
                    currentFocusPanel.style.display = "none";
                    currentFocusPanel = null;
                }
            }
        }
    }
    document.onclick = hideDIV;
	function SubmitController()
    {
		this.innerTosearch = function() // 提交前判断
		{
			var searchForm = document.getElementById("innerSearchForm");
			var searchKeyword = document.getElementById("innerSearchFormKeyword");
			if (searchKeyword.value == '' || searchKeyword.value == '请输入关键字')
