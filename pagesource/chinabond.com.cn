				alert("请输入关键字");
				return false;
			}  
			var cid = jQuery("#area").val();
			var qt =encodeURI(jQuery.trim(jQuery("#innerSearchFormKeyword").val()));
			jQuery("#innerSearchFormKeyword").val(jQuery.trim(jQuery("#innerSearchFormKeyword").val()));
			jQuery("#cid").val(cid);
			jQuery("#qt").val(qt);
			searchForm.action="/jsp/searchEngine/searchResultPage.jsp";
			searchForm.method="post";
			searchForm.submit();
		}
		this.quickLink = function(clickObj,qtTemp)
		{
			var searchForm = document.getElementById("innerSearchForm");
			var searchKeyword = document.getElementById("innerSearchFormKeyword");
			var originalKeyword = searchKeyword.value; 
			var qt = encodeURI(encodeURI(qtTemp));
			searchKeyword.value = qt;
			searchForm.action = "/jsp/searchEngine/searchResultPage.jsp?qt="+qt+"&pageResultNum=10&page=1";
			searchForm.method = "post";
			searchForm.submit();
			searchKeyword.value = originalKeyword;
			return false;
		}
