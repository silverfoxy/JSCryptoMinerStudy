		function initDivContent(divId,divArray)
		{
			var content='';//div内部HTML内容
			var maxWidth=680;//显示区域总宽度
			var countWidth=0;//统计显示文字的宽度
			var singleWordWidth=12;//单个字符的宽度
			var moreFlag=0;//更多按钮标志位
			var nextId=0;
			//alert(divArray[].length)
			for(var t=0;t<divArray.length;t++)
			{							
				if(t==divArray.length-1)
				{
					countWidth+=divArray[t].name.length*singleWordWidth;
				}else
				{
					countWidth+=divArray[t].name.length*singleWordWidth+18;
				}
				if(countWidth>maxWidth)
				{
					nextId=t;
					moreFlag=1;
					break;
				}
				content=content+' '+divArray[t].link+' |';
			}	
			

			if(moreFlag==1)
			{
				document.getElementById(divId).innerHTML=content.substr(0,content.length-1)+"<img id='"+divId+"ImgNext' src='/static/CB_CN/images/zyn_ernav_13.jpg'  />";
				
				//document.getElementById(divId+'ImgPrev').onclick=function (){prev(divId,divArray,0)};
				//alert(divId+' '+nextId);
				document.getElementById(divId+'ImgNext').onclick=function (){next(divId,divArray,nextId-1)};
				
			}else
			{
				document.getElementById(divId).innerHTML=content.substr(0,content.length-1);
			}
			
		}
		
		function prev(divId,divArray,id)
		{
			//alert('prev');
			if(id==0)
			return;
			var content='';//div内部HTML内容
			var maxWidth=680;//显示区域总宽度
			var countWidth=0;//统计显示文字的宽度
			var singleWordWidth=12;//单个字符的宽度
			var moreFlag=0;//更多按钮标志位
			var prevId=0;
			for(var t=id;t>=0;t--)
			{			
				prevId=t;
				if(t==divArray.length-1)
				{
					countWidth+=divArray[t].name.length*singleWordWidth;
				}else
				{
					countWidth+=divArray[t].name.length*singleWordWidth+18;
				}
				if(countWidth>maxWidth)
				{
					if(t==0)
					{
						moreFlag=1;
					}
					break;
				}
				content=' '+divArray[t].link+' |'+content;
			}	
			

			if(moreFlag==1)
			{
				document.getElementById(divId).innerHTML="<img id='"+divId+"ImgPrev' src='/static/CB_CN/images/zyn_ernav_10.jpg' />"+content.substr(0,content.length-1)+"<img id='"+divId+"ImgNext' src='/static/CB_CN/images/zyn_ernav_13.jpg' />";
				
				document.getElementById(divId+'ImgPrev').onclick=function (){prev(divId,divArray,prevId)};
				document.getElementById(divId+'ImgNext').onclick=function (){next(divId,divArray,id)};
				
			}else
			{
				
				document.getElementById(divId).innerHTML=content.substr(0,content.length-1)+"<img id='"+divId+"ImgNext' src='/static/CB_CN/images/zyn_ernav_13.jpg'  />";
				
				//document.getElementById(divId+'ImgPrev').onclick=function (){prev(divId,divArray,prevId)};
				document.getElementById(divId+'ImgNext').onclick=function (){next(divId,divArray,id)};
			}
		}
		
		function next(divId,divArray,id)
		{
			//alert(id+'arrayLength= '+divArray.length);
			
			var content='';//div内部HTML内容
			var maxWidth=680;//显示区域总宽度
			var countWidth=0;//统计显示文字的宽度
			var singleWordWidth=12;//单个字符的宽度
			var moreFlag=0;//更多按钮标志位
			var nextId=0;
			for(var t=id+1;t<divArray.length;t++)
			{			
				nextId=t;
				if(t==divArray.length-1)
				{
					countWidth+=divArray[t].name.length*singleWordWidth;
				}else
				{
					countWidth+=divArray[t].name.length*singleWordWidth+18;
				}
				if(countWidth>maxWidth)
				{
					if(nextId==divArray.length-1)
					{
						moreFlag=1;
					}
					break;
				}
				content=content+' '+divArray[t].link+' |';
			}	
			

			if(moreFlag==1)
			{
				
					document.getElementById(divId).innerHTML="<img id='"+divId+"ImgPrev' src='/static/CB_CN/images/zyn_ernav_10.jpg' "+'&nbsp;&nbsp;'+content.substr(0,content.length-1)+"<img id='"+divId+"ImgNext' src='/static/CB_CN/images/zyn_ernav_13.jpg' />";
					
					document.getElementById(divId+'ImgPrev').onclick=function (){prev(divId,divArray,id)};
					document.getElementById(divId+'ImgNext').onclick=function (){next(divId,divArray,nextId)};
				
				
			}else
			{
				document.getElementById(divId).innerHTML="<img id='"+divId+"ImgPrev' src='/static/CB_CN/images/zyn_ernav_10.jpg' />"+'&nbsp;&nbsp;'+content.substr(0,content.length-1);
					
					document.getElementById(divId+'ImgPrev').onclick=function (){prev(divId,divArray,id)};
					//document.getElementById(divId+'ImgNext').onclick=function (){next(divId,divArray,nextId)};
			}

		}

		function NavObj(name,link)
		{
			this.name=name;
			this.link=link;
		}
	</script>
