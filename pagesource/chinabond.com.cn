				function MarqueeContentOperator()
				{
					this.init = function()
					{
						var infoContentArea = document.getElementById("rollingInformations");
						var infoArray = infoContentArea.childNodes;
						var infoNumber = infoArray.length;
						for (var index = 0; index != infoNumber; index++)
						{
							var info = infoArray[index];
							if (info.nodeName != "SPAN") continue; // This statement used to avoid pure white space text node.
							info.className = "Red"; // Outter <span> style
							var tempFlag = true; // This variable used to tell if faced tip text. False means faced.
							var tempArray = info.childNodes;
							for (var sectionId = 0; sectionId < tempArray.length; sectionId++)
							{
								var section = tempArray[sectionId];
								if (section.nodeName == "A")
								{
									section.target = "_blank";
								}
								else if (section.nodeName == "SPAN")
								{
									var text = section.innerText;
									
									if (text.length > 0)
									{
										if (text.indexOf('↑')>0)										
											section.className = "Red";
										else if (text.indexOf('↓')>0)
											section.className = "Green";
										else section.className = "Black";
									}
								}
							}
							var separator = document.createElement("span");
							separator.className = "x";
							separator.innerText = "|";
							info.appendChild(separator);
						}
					}
				}
				var marqueeContentOperator = new MarqueeContentOperator();
				marqueeContentOperator.init();
