	<script language="JavaScript">
	function navigationInit() // 用以给导航栏每个频道加载鼠标事件
	{
		var navs = document.getElementById("navList").childNodes;
		for (var index = 0; index != navs.length; index++)
		{
			var nav = navs[index];
			var toucher = new Toucher(nav, "current", "normal");
			nav.onmouseover = toucher.moveOn();
			nav.onmouseout = toucher.moveOut();
		}

		document.getElementById('divNavArea').style.visibility='hidden';
	}
	navigationInit();
	function Toucher(obj, currentClass, normalClass) // 此对象通过闭包技术返回函数
	{
		this.moveOn = function() {
			var eventFunction = function() {
				//alert(obj.innerText);
				obj.className = currentClass;

				if(obj.innerText=='首 页' || obj.innerText=='关于中债')
				{
					document.getElementById('divNavArea').style.visibility='hidden';
				}
				
			};
			return eventFunction;
		};
		this.moveOut = function() {
			var eventFunction = function() {
				obj.className = normalClass;
			};
			return eventFunction;
		};
		this.getCurrentClass = function() {
			return currentClass;
		}
	}
	
	//首页新的页头制作JS
	function displayChildMenu(id)
	{
		for(i=1;i<=7;i++)
		{		
			document.getElementById('divNav'+i).style.display="none";		
		}
		
		if(id == 'divNav7'){

		}else{
		document.getElementById('divNavArea').style.visibility="visible";
			document.getElementById(id).style.display="block";
		}		
		
	}

	
	function get_offset(obj){
		var offsetparent = obj;
		var left=0,top=0;
		
		while(offsetparent != null && offsetparent.nodeName != "BODY"){
			 left += offsetparent.offsetLeft;
			// alert(left);
			 top += offsetparent.offsetTop;
			offsetparent=offsetparent.parentNode;
		}
		return left;
	}


	function restict1(o,e){           
		  var x=0,y=0;
		  var l=get_offset(o), t=47;
		
		  
		  l=(document.documentElement.scrollWidth-950)/2+254;		
			
		  var w=694, h=77;
		 // alert(l);
		  //alert(w);
		  x=e.pageX||e.clientX, y=e.pageY||e.clientY;
		//  alert("boolean="+(x>l && x<(l+w))+"   x="+x+"   l="+l+"  w="+w);

		  if((x>l && x<(l+w)) &&(y>t && y<(t+h))){
			  
			if(window.event){
				//alert('event');
				e.cancelBubble=true; return}
			else{//alert('stopP');
			e.stopPropagation();  return }//stopPropagation是MOM标准的，IE6下的不行的，故必须颠倒一下顺序！
		  }
		  document.getElementById('divNavArea').style.visibility="hidden";

    }
	</script>

