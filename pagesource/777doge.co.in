        (function(){
            $("[data-timer]").each(function(){
                var _do_interval = function(elm)
                {
                    return function()
                    {
                        var doTime = elm.data("timer"),
                            datum = Math.floor((new Date()).getTime() / 1000),
                            datumDiff = doTime - datum,
                            minutes = 0,
                            seconds = 0;
