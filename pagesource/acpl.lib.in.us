<script type="text/javascript">

$(document).ready(function(){
    var slideIndex = 0;

    $.ajax({headers: {"accept":"application/json"}, 
            url:"https://amapi.acpl.lib.in.us:8443/BestSellerWebService/rest/operations/getVisibleLists",
           success:function(response){
               
        var count = response.lists.length;
        
        var loopCount = 0;
        for(i = 0;i<10;i++){
            var listIndex = Math.floor(Math.random() * (count-1));
            var listName = response.lists[listIndex].listNameEncoded
            
            $.ajax({headers:{"accept":"application/json"}, 
            url:"https://amapi.acpl.lib.in.us:8443/BestSellerWebService/rest/operations/getListItems?listNameEncoded="+listName,
           success:function(response){
                
               var hitFound = false;
               
               var numItems = response.listItems.length;
               var itemIndex = Math.floor(Math.random() * (numItems - 1));
               
               var isbn = response.listItems[itemIndex].primaryIsbn;
               var title = response.listItems[itemIndex].title;
               
               $.ajax({headers:{"accept":"application/json"}, 
                url:"http://catalog.acpl.lib.in.us:8080/symws/rest/standard/searchCatalog?clientID=sdIPhoneApp&term1="+isbn+"&json=true",
                success:function(response){
                   
                   if(response.totalHits>=1){
                        var imgUrl = 'https://secure.syndetics.com/index.aspx?isbn='+isbn+'/lc.gif&client=260-421-1200';

                        var div = document.getElementById('bestsellerWidget');

                        div.innerHTML +='<a aria-label="'+title+'" style="height:200;" href="http://alpl.ent.sirsi.net/client/default/search/results?te=ILS&qu='+isbn+'"><img class="mySlides" style="width:125px;margin:auto;display:none;" alt="'+title+'" src="'+imgUrl+'"></a>';
                   }
                    
               
                       loopCount++;
                       if(loopCount>=10){
                            carousel();
                       }
                    
                }});
               

           }});
        }       
        

               
        
    }});

		
    function carousel() {
    var i;
    var x = document.getElementsByClassName("mySlides");
    for (i = 0; i < x.length; i++) {
      x[i].style.display = "none"; 
    }
    slideIndex++;
    if (slideIndex > x.length) {slideIndex = 1} 
    x[slideIndex-1].style.display = "block"; 
    setTimeout(carousel, 4000); // Change image every 2 seconds
}
});




