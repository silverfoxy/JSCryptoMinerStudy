 <script>

    // Get weather cookie
function getWeatherCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
    return "";
}

// Check weather warning cookie
function checkWeatherWarning() {
    var weatherCookie = getWeatherCookie("weatherWarning");
    if (weatherCookie != "true") {
        var weatherWarning = "<!'+'-- Wetaher Message --'+'>\n<div class=\"alert alert-weather\" style=\"border-color: #164053; background-color: #164053; color: #fff; margin-top: 0;\">\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-xs-3 col-sm-2 col-md-1\">\n                <em class=\"fa fa-info-circle margin-top\" style=\"font-size: 60px;\"><\/em>\n            <\/div>\n            <div class=\"col-xs-9 col-sm-8 col-md-9\">\n                <h5>Update 7th December 3pm: <\/h5>\n                <p>Deliveries and collections have been affected by flooding in some areas of the UK, for a full list <a href=\"https:\/\/www.myhermes.co.uk\/help\" style=\"color:white;\">click here<\/a>. We are working hard to deliver parcels where we can; where roads are closed it’s inevitable that deliveries and collections will be affected.<\/p>\n            <\/div>\n            <div class=\"col-xs-12 col-sm-2 text-right\">\n                <a href=\"#\" class=\"btn btn-white btn-lg btn-block-xs margin-top\">Dismiss<\/a>\n            <\/div>\n        <\/div>\n    <\/div>\n<\/div>\n";
        $(weatherWarning).insertBefore('.alert-global');
        //Accept and close weather div
        $(".alert-weather .btn").click(function(){
            $(".alert-weather").hide();
        });
        setCookie("weatherWarning", 'true', 365);

    }
}
