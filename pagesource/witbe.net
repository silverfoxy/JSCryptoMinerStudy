<script type="text/javascript">

/**
* @param a variable id of any element
* @return boolean if the variable is set jquery wise
**/
function issetForJquery( aVariableId ) {
  return ( jQuery( aVariableId ).length > 0 );
}

/**
* @param a variable of any type
* @return boolean if the variable is defined
**/
function isset( aVariable ) {
  return ( typeof aVariable != 'undefined' );
}

/**
* @see http://stackoverflow.com/questions/979975/how-to-get-the-value-from-the-url-parameter
* @do code below retrieve all the url params and store it in the QueryString object
**/
var QueryString = function () {
  // This function is anonymous, is executed immediately and 
  // the return value is assigned to QueryString!
  var query_string = {};
  var query = window.location.search.substring(1);
  var vars = query.split("&");
  for (var i=0;i<vars.length;i++) {
    var pair = vars[i].split("=");
        // If first entry with this name
    if (typeof query_string[pair[0]] === "undefined") {
      query_string[pair[0]] = decodeURIComponent(pair[1]);
        // If second entry with this name
    } else if (typeof query_string[pair[0]] === "string") {
      var arr = [ query_string[pair[0]],decodeURIComponent(pair[1]) ];
      query_string[pair[0]] = arr;
        // If third or later entry with this name
    } else {
      query_string[pair[0]].push(decodeURIComponent(pair[1]));
    }
  } 
  return query_string;
}();

// the code below display the chosen job ad on the page
if ( isset( QueryString.job_post_id) ) {
  var jobLinkId = 'a.w-tabs-item-h[href="#' + QueryString.job_post_id + '"]';
  
  if ( issetForJquery( jobLinkId ) ) {
    var jobLink = jQuery( jobLinkId );

    jobLink.trigger('click');
  }
}
</script></body>
