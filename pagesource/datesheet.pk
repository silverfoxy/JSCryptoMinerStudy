    <script>
        $(function () {
            BindLevels();
        });
        // Validation
        $('#btnSubmit').click(function () {
            var ddlvalue = $("#data-levels option:selected").val();
            if (ddlvalue == '-1') {
                $("#data_levels_chosen").addClass('error');
                return false;
            }
            else {
                $("#data_levels_chosen").removeClass('error');
            }
            var ddlboards = $("#data-boards option:selected").val();
            if (ddlboards == '-1') {
                $("#data_boards_chosen").addClass('error');
                return false;
            }
            else {

                $("#data_boards_chosen").removeClass('error');
            }
            var ddlclass = $("#data-classes option:selected").val();
            if (ddlclass == '-1') {
                $("#data_classes_chosen").addClass('error');
                return false;
            }
            else {
                $("#data_classes_chosen").removeClass('error');
                return true;
            }
        });

        // Level Chnage
        $("#data-levels").change(function () {

            $("#data-boards option").remove();
            $("#data-classes option").remove();

            $("#data-classes").trigger("chosen:updated");
            $("#data-boards").trigger("chosen:updated");

            var lvId = $(this).val();
            if (lvId == '-1') {
                $("#data_levels_chosen").addClass('error');
                return false;
            }
            else {
                $("#data_levels_chosen").removeClass('error');
            }

            $("#spinner-boards").show();


            var id = $(this).val();
            $.get("/api/DatesheetApi/GetBoardsByLevelId", { LevelId: id }, function (data) {
                var v = "<option value='-1'>--Select Board/Uni--</option>";
                $.each(data, function (i, v1) {
                    v += "<option value=" + v1.Value + ">" + v1.Text + "</option>";
                });
                $("#data-boards").html(v);
                $("#data-boards").trigger("chosen:updated");
                $("#data-boards").trigger("liszt:updated");

            }).done(function () {
                $("#spinner-boards").hide();
            }).fail(function () {
                $("#spinner-boards").hide();
            });

        });
        $("#data-classes").change(function () {
            var clId = $(this).val();
            if (clId == '-1') {
                $("#data_classes_chosen").addClass('error');
                return false;
            }
            else {
                $("#data_classes_chosen").removeClass('error');
            }
        });
        $("#data-boards").change(function () {

            $("#data-classes").empty();
            $("#data-classes").trigger("chosen:updated");

            var boId = $(this).val();
            if (boId == '-1') {
                $("#data_boards_chosen").addClass('error');
                return false;
            }
            else {
                $("#data_boards_chosen").removeClass('error');
            }

            $("#spinner-class").show();

            var id = $(this).val();
            var lvId = $("#data-levels option:selected").val();
            $.get("/api/DatesheetApi/GetClassByLevelIdBoardId", { LevelId: lvId, BoardId: id }, function (data) {
                var v = "<option value='-1'>--Select Class--</option>";
                $.each(data, function (i, v1) {
                    v += "<option value=" + v1.Value + ">" + v1.Text + "</option>";
                });
                $("#data-classes").html(v);
                $("#data-classes").trigger("chosen:updated");
                $("#data-classes").trigger("liszt:updated");
            }).done(function () {
                $("#spinner-class").hide();
            }).fail(function () {
                $("#spinner-class").hide();
            });
        });
        function BindLevels() {
            $.get("/api/DatesheetApi/GetLevels", null, function (data) {
                var v = "<option value='-1'>--Select Level--</option>";
                $.each(data, function (i, v1) {
                    v += "<option value=" + v1.Value + ">" + v1.Text + "</option>";
                });
                $("#data-levels").html(v);
                $("#data-levels").trigger("chosen:updated");
                $("#data-levels").trigger("liszt:updated");
            }).done(function () {

            }).fail(function () {

            });
        }
    </script>

