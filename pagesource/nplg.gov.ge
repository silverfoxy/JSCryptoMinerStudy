<script type="text/javascript">
$(document).ready(function()
{
	$(".CheckBoxClass").click(
	function()
	{
		SwitchRadiobutton();
	});
	
	SwitchRadiobutton();
	
	function SwitchRadiobutton()
	{
		$(".radioButtons input").each(
			function()
			{
				if($(this).is(":checked"))
				{
					jQuery("#" + $(this).attr("id") + "_label").addClass("LabelSelected");
				}
				else
				{
					jQuery("#" + $(this).attr("id") + "_label").removeClass("LabelSelected");
				}
			}
		);
	}
});
</script>

