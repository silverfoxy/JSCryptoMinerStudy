<script type="text/javascript">
    $(document).ready(function () {
        $('#navbar-wrap').removeClass('in');
        $('#navbar-wrap').hide();
        $('body').removeClass('block-scroll');
        $('.navbar-toggle').click(function () {
            $('#navbar-wrap').show();
            $(this).toggleClass('close-nav');
            $('#navbar-wrap').toggleClass('in');
            $('body').toggleClass('block-scroll');
            setTimeout(function () {
                $('#mainLob').trigger('click');
            }, 100);
            $('.nav-overlay').toggleClass('in');
            $('.list-filter, .enquiry-wrap').css('z-index', '0');
        });
        $('.nav-overlay').click(function () {
            $('.navbar-toggle').trigger('click');
            $('.list-filter, .enquiry-wrap').css('z-index', '5');
        });

        $('.dropdown-menu').click(function (e) {
            e.stopPropagation();
        });

        $(".panel-heading").click(function () {
            var showId = $(this).attr("data-code");
            $('.dropdown-arrow').removeClass("arrowup");
            if ($('#' + showId).is(":visible")) {
                $('#' + showId).slideUp();
            } else {
                $('.currency-container').slideUp();
                $('#' + showId).slideDown();
                $(this).find('.dropdown-arrow').addClass("arrowup");
            }
        });

        var currencyCode = 'USD';

        $(".currency-li").each(function () {
            if ($(this).attr("title") == currencyCode) {

                $('#loadCurrencytext').text($(this).attr("data-code"));
            }
        });

        var currCode = $('#GetCurrency').val();
        $('.currency-li').click(function () {
            var currentVal = $.trim($(this).attr('title'));

            if ($.trim(currentVal) != $.trim(currencyCode)) {
                var url = $('#ConvertCurrencyURL').val() + "?currency=" + currentVal;
                $.ajax({
                    type: 'Get',
                    url: url,
                    async: true,
                    success: function () {
                        var curUrl = window.location.href;
                        window.location.href = curUrl;
                        return false;
                    },
                    error: function () {
                        return false;
                    }
                });
            }
            return false;
        });


        $("#openNav").bind("click", (function () {
            $('#mainLob').dropdown('toggle');
        }));
        $('#contact-no').click(function () {
            $('.contact-info-wrap').toggleClass('contact_show', 1000);
        });       
    });
    function openIframe() {
        var iframe = $("#TraveazyLoginIframe");
        $(".sign-loader").show();
        iframe.attr("src", decodeURIComponent($("#TraveazyLoginIframe").attr('data-src')));
        setTimeout(function () {
            $(".sign-loader").hide();
        }, 800);
    }

    $(document).click(function () {
        $('.contact-info-wrap').removeClass('contact_show', 1000);
        $('.contact-info-wrap').addClass('hidden-xs', 1000);

    });
    $(".contact-info-wrap").click(function (e) {
        //e.stopPropagation(); // This is the preferred method.
        return false;        // This should not be used unless you do not want
        // any click events registering inside the div
    });
    $(".contact-btn").click(function (e) {
        //e.stopPropagation(); // This is the preferred method.
        return false;        // This should not be used unless you do not want
        // any click events registering inside the div
    });
    $('#lngEnglish').attr('href', window.location.href);
    var requestUrl = window.location.href;
    $('#lngArabic').attr('href', requestUrl.replace('https://www.holidayme.com', 'https://www.holidayme.com.sa'));

    $('#lngArabic').on("click", function () {
        var currentUrl = $('#lngArabic').attr('href');
        if (currentUrl.indexOf('?') > -1) {
            if (currentUrl.indexOf('langsw') > -1) 
                currentUrl = currentUrl.replace("langsw=ar", "langsw=en");
            else 
                currentUrl = currentUrl + "&langsw=en";
        } else 
            currentUrl= currentUrl + "?langsw=en";
        $('#lngArabic').attr('href', currentUrl);
    });
</script>

