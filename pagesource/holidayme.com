    <script>
        $(document).ready(function () {
            $(document).click(function (event) {
                var target = $(event.target);

                if (target.parents("div").attr("id") != "currlist" && target.parents("li").attr("id") != "lnkSelectCurr") {
                    if ($(window).width() >= 480) { $(".currency-select_wrap").hide(); }
                }
                if (target.parents("li").attr("id") != "liMemberArea") {
                    $('.user-area-wrap').hide();
                }
            });
            $('#btnSendAppLink').click(function () {
                var mobileNumber = $('#txtAppCountryCode').val() + $('#txtAppMobileNumber').val();
                $.ajax({
                    url: "/base/SendSms",
                    data: { mobileNo: mobileNumber },
                    success: function (data) {
                        if (data.Error == "" || data.Error == undefined) {
                            dataLayer.push({ 'Appdownload': 'text-message', 'event': 'sms-sent-success' });
                            dataLayer.push({ 'event': 'appdownload', 'phone': mobileNumber });
                            $('#txtAppMobileNumber').val("");
                        }
                    }
                });
            });
        });
        function getCookie(cname) {
            var name = cname + "=";
            var cookies = document.cookie.split(';');
            for (var i = 0; i < cookies.length; i++) {
                var c = cookies[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }
        if (Number.isInteger(parseInt(getCookie("HCC")))) {
            document.cookie = 'HCC=AE;domain=.holzoo.com;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        }


        $('.lazy').lazy();

        //For Lazy Load Images on Slide & Pagination Click
        $('.swiper-pagination-bullet, .offer_card_overlay').click(function () {
            $('.lazy').lazy({
                bind: "event"
                //delay: 0
            });
        });

    </script>
