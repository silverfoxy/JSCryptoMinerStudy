    if(!o2) return;

    var w2=$("#kakaFocus2 .banner_pic").width(), $o2=$('#'+o2),i=0,l=0;arr2= [],t2= null,

        $infoLi2 = $o2.find('.banner_info li'), len2= $infoLi2.length*2,
        $ul2=$o2.find('.banner_pic>ul');
    $ul2.append($ul2.html()).css({'width':len2*w2, 'left': -len2*w2/2});
    $infoLi2.eq(0).addClass('current');
    //add banner_pages element
    arr2.push('<div class="banner_pages"><ul>');
    $infoLi2.each(function(i){
        if(i==0){
            arr2.push('<li class="current"><span>'+ (i+1) +'</span></li>');
        }else{
            arr2.push('<li><span>'+ (i+1) +'</span></li>');
        }
    });
    arr2.push('</ul></div>');
    $o2.append(arr2.join(''));
    var $pagesLi2 = $o2.find('.banner_pages li');
    //mouse
    $pagesLi2.children('span').click(function(){
        var p2 = $pagesLi2.index($o2.find('.banner_pages li.current'));
        i2 = $pagesLi2.children('span').index($(this));
        if(i2==p2) return;
        l2 = parseInt($ul2.css('left')) + w2*(p2-i2);

        addCurrent2(i2,l2);
        return false;
    })
    if(len2>2){
        $("#kakaFocus2 .btn_prev").show();
        $("#kakaFocus2 .btn_next").show();
        $o2.children('a.btn_prev').click(function(){
            i2 = $pagesLi2.index($o2.find('.banner_pages li.current'));
            (i2==0)? i2=(len2/2-1):i2--;
            l2 = parseInt($ul2.css('left')) + w2;
            addCurrent2(i2,l2);
            return false;
        })
        $o2.children('a.btn_next').click(function(){
            i2 = ($pagesLi2.index($o2.find('.banner_pages li.current'))+1)%(len2/2);
            l2 = parseInt($ul2.css('left')) - w2;
            addCurrent2(i2,l2);
            return false;
        })
    }else{

        $("#kakaFocus2 .btn_prev").hide();
        $("#kakaFocus2 .btn_next").hide();
    }

    //auto focus
    t2 = setInterval(init2,8000);
    $o2.hover(function(){
        clearInterval(t2);
    }, function(){
        if(len>2){
           t2 = setInterval(init2,8000);
        }

    });
    function init2(){
        $o2.children('a.btn_next').trigger('click');
    }
    //add focus
    function addCurrent2(i,l){
        if($ul2.is(':animated')) return;
        $ul2.animate({'left':l},500,function(){
            $o2.children('.banner_count').text(i+1);
            $infoLi2.not($infoLi2.eq(i).addClass('current')).removeClass('current');
            $pagesLi2.not($pagesLi2.eq(i).addClass('current')).removeClass('current');
            if(l== (1-len2)*w2){
                $ul2.css({'left': (1-len2/2)*w2});
            }else if(l== 0){
                $ul2.css({'left': -len2*w2/2});
            }
        });
    }
}
}

