                },
                error: function(xhr){
                    $('.panel-body').html("<h1 class='text-center'>Thank you for your application!</h1><p class='text-center'><em>(If you have been successful, we'll be in touch soon.)</em></p><a href='/refer-a-friend' class='text-center alert alert-success col-xs-12'><strong>Refer a friend for £100, click here</strong></a>");
                }
            })

        }

        uploadForm.addEventListener('submit', function(e) {

            //var uploadFiles = document.getElementById('js-upload-files').files;
            e.preventDefault()
            $('.error').remove();

            if($('#js-upload-files').val() =='')
            {
                $('#js-upload-files').after('<div class="error alert-danger" role="alert"> <small> <span class="glyphicon glyphicon-remove" aria-hidden="true"> </span> <span class="sr-only"> Error: </span> Select a CV file in .doc, .docx or .pdf only </small></div>');

            }else{
                var uploadFiles = new FormData(this);
                startUpload(uploadFiles)
            }

        })

        dropZone.ondrop = function(e) {
            e.preventDefault();
            this.className = 'upload-drop-zone';
            var uploadFiles = new FormData();

            uploadFiles.append('files', e.dataTransfer.files[0]);
            uploadFiles.append('_token', $('#js-upload-form input[name=_token]').val());

            startUpload(uploadFiles)
        }

        dropZone.ondragover = function() {
            this.className = 'upload-drop-zone drop';
            return false;
        }

        dropZone.ondragleave = function() {
            this.className = 'upload-drop-zone';
            return false;
        }

    }(jQuery);


</script>


