							<div class="card-image" v-if="(item._embedded['wp:featuredmedia'][0].media_details.sizes.uams_news.source_url)"><a :href="item.link" :title="item.title.rendered" target="_blank"><img :src="item._embedded['wp:featuredmedia'][0].media_details.sizes.uams_news.source_url" :alt="item._embedded['wp:featuredmedia'][0].alt_text"></a></div>
							<div class="card-image" v-else><img src="http://via.placeholder.com/560x350?text=Not%20Available" alt=" "></div>
							<div class="card-body">
								<h3>
									<a :href="item.link" :title="item.title.rendered" target="_blank" class="title" >
										{{ item.title.rendered | truncate(60) }}
									</a>
								</h3>
								<div v-html="this.$options.filters.truncate(item.excerpt.rendered)" class="card-excerpt"></div>
								<a :href="item.link" :title="item.title.rendered" target="_blank" class="read-more uams-btn btn-sm btn-red">Read more</a>
							</div>
						</div>

					</template>

					<template id="hero">

						<div class="uams-hero-image hero-height" :class="this.imgcolor" :style="getImageBackgroundCSS(this.imageurl)">
						    <div id="hero-bg">
						      <div id="hero-container" class="container">
						        <h1 class="uams-site-title">{{ this.imgtitle }}</h1>
						        <span class="udub-slant"><span></span></span>
						        <div class="hero-content" v-html="this.imgtext"></div>
						        <div class="hero-button" v-if="this.imgbuttontxt"><a class="uams-btn btn-sm" :class="this.imgbuttoncolor" :href="this.imgbuttonurl" :title="this.imgtitle">{{ this.imgbuttontxt }}</a></div>
						      </div>
						    </div>
						</div>

					</template>

