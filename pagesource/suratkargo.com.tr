var ana_menu_timeout;

var selected_menu = '';

function ana_menu_ac(id) {

	if (selected_menu == id) {
		ana_menu_kapat();
		return;
	}

	selected_menu = id;
	$(".ana-menu-panel").hide();
	$("#panel-"+id).show();
	$("#ana-menu-paneller").slideDown(200);
}

function ana_menu_kapat() {
	selected_menu = '';
	$("#ana-menu-paneller").slideUp(200);
}

$(document).ready(function(){

	$(".menuler").hover(
		function () {
			clearTimeout(ana_menu_timeout);
			//ana_menu_ac($(this).attr('id'));
		},
		function () {
			ana_menu_timeout = setTimeout("ana_menu_kapat();", 500);
		}
	).click(function(){
		ana_menu_ac($(this).attr('id'));
	});

	$("#ana-menu-paneller").hover(
		function () {
			clearTimeout(ana_menu_timeout);
		},
		function () {
			ana_menu_timeout = setTimeout("ana_menu_kapat();", 500);
		}
	);

});

