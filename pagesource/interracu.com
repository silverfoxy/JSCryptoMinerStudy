.contain {width: 100%; max-width: 1000px; margin: 0 auto; }

nav {width: 100%;position: relative; float: left; text-align: left; z-index: 100}


div.header.bar-main, div.header-mobile, div#submenu.header-menu.clearfix {z-index: 99000;}


div.header div.menu-main div.inner {border-bottom: 1px solid #b4be35; overflow: hidden;}


div.nav-main { margin: 0; line-height: normal;  padding: 0; border: none;  width: 100%; background-color: #b4be35;  position: relative; z-index: 10;max-width: 640px;}
div.nav-main a.on {background-image: none; background-color: #ededed; color: #666;}


div.nav-main ul.nav-menu {margin: auto;/*display: table !important; */width: 100%;  max-width: 700px; padding: 0;  text-align: left;}

div.nav-main ul.nav-menu li.nav-item {display: inline-block; line-height: normal !important; margin: 0 !important; text-align: center;float: left; }
div.nav-main ul.nav-menu li.nav-item a {display: block; padding: 7px 20px 10px 20px; border: none; font-weight: 100; color: #fff;  margin-top: 3px; text-decoration: none; }
div.nav-main ul.nav-menu li.nav-item a:hover {background-image: -webkit-gradient(linear, left top, left bottom, from(#f5f5f5), to(#fff)); 	background-image: -moz-linear-gradient(top, #f5f5f5, #fff); text-decoration: none;  text-decoration: none;}
div.nav-main ul.nav-menu li.nav-item ul li {display: block; width: auto;}



div.nav-main li {padding: 0; display: table-cell !important; float: none !important;}

div.nav-main li:last-child {border-right: none; margin-right: 0; padding-right: 0;}
div.nav-main ul.nav-menu li.on a, div.nav-main ul.nav-menu li a:hover {	color: #005584;	background-image: none !important; background: #EDEFF1; 	-webkit-transition: background .6s ease; -moz-transition: background .6s ease; transition: background .6s ease;}
div.nav-main ul.nav-menu li.on a {font-weight: normal; text-decoration: none;}
div.nav-main ul.nav-menu li a.on {background: #EDEFF1; text-decoration: none;}
div.header-mobile {display: none;}


.clearfix {clear: both;}


