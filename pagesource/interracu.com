	ul#main-nav-mobile li a.active {background-color: #aaa !important; color: #fff; text-shadow: none !important; border: solid 1px #7e2332 !important; opacity: .8; background-image: none; text-decoration: none;}

	div.nav-sub-mobile ul li {margin-bottom: 5px;}
	div.nav-sub-mobile ul li.nav-item a.parent {border-bottom: 1px solid #ededed; font-weight: 500;  text-decoration: none;}
	div.nav-sub-mobile ul li.nav-item a.on.parent {text-decoration: none;  text-decoration: none;}
	div.nav-sub-mobile ul li.nav-item a.on.parent:hover {text-decoration: underline;}

	div.nav-main-mobile  ul.nav-menu,  div.nav-sub-mobile ul.nav-menu{margin: 0; padding: 0 0 0 10px;}
	div.nav-sub-mobile ul li a:hover {text-decoration: underline;}
	.onehalf {width: 50%; float: left;}


