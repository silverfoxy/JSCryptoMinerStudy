<script>
    document.onreadystatechange = () => {
        if (document.readyState === 'complete') {
            // document ready
            $(function () {
                var resizeTime = 100;
                var resizeDelay = 100;
                resizeDrillDownMap();
                function resizeDrillDownMap() {
                    var drillDownMap = $(".img-map-responsive");
                    var maxWidth = drillDownMap.closest('.shape_image').innerWidth();
                    var maxHeight = drillDownMap.closest('.shape_image').innerHeight();
                    var width = drillDownMap.width();
                    var height = drillDownMap.height();
                    var ratioX = (maxWidth / width);
                    var ratioY = (maxHeight / height);
                    var ratio = Math.min(ratioX, ratioY);

                    var newWidth = (width * ratio);
                    var newHeight = (height * ratio);
                    drillDownMap.mapster('resize', newWidth, newHeight, resizeTime);
                }

                function onWindowResize() {
                    window.setTimeout(function () {
                        resizeDrillDownMap();
                    }, resizeDelay);
                }

                $(window).bind('resize', onWindowResize);
            });
        }
    };
</script>
