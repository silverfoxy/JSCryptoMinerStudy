			// 상단 메뉴 마우스오버, 아웃, 포커스인, 포커스아웃 시 처리
			var currentOnMenu = $('a[id^="link_"].menuSelect').attr('id');
			var SSMenu = '';

			$('div#mtnGnbGroup li.1depth, div.2depth').bind('mouseenter', function(){

				$('div.2depth[id^="sub_"]').hide();
				if($(this).attr('id') != 'tab_news' && $(this).attr('id') != 'sub_news' ) 
				{
					if( SSMenu == 'news_index' || SSMenu == 'news_program' ) $('#btnMore').show();
					//$('#btnMore').show();
					$('div#gnbMenuSub').hide();
				}
				
				$('#' + $(this).attr('id').replace('tab_', 'sub_')).show();
				$('#' + $(this).attr('id').replace('sub_', 'link_')).addClass('menuSelect');

			});


			$('div#mtnGnbGroup li.1depth, div.2depth').bind('mouseleave', function() {
				$('a[id^="link_"]').removeClass('menuSelect');
				$('div.2depth[id^="sub_"]').hide();

				// 마우스아웃 시 원래 메뉴로 다시 포커스 인 처리
				if(currentOnMenu != null) {
					$('#' + currentOnMenu).addClass('menuSelect');
					$('#' + currentOnMenu.replace('link_', 'sub_')).css('display', 'block');
					
					if( currentOnMenu == 'link_program' ) $('#sub_program').css('display', 'none');
					if( (SSMenu != 'news_index' && SSMenu != 'news_program') && currentOnMenu == 'link_news' ) 
					{
						$('#btnMore').hide();  
						$('div#gnbMenuSub').show();
					}

				}
			});

			// 현재 선택한 메뉴의 하위메뉴가 있을 경우, 기본 보임으로 설정
			if(currentOnMenu != null) {
				$('#' + currentOnMenu.replace('link_', 'sub_')).css('display', 'block'); 
				if( currentOnMenu == 'link_program' ) $('#sub_program').css('display', 'none');
				if( (SSMenu != 'news_index' && SSMenu != 'news_program') && currentOnMenu =='link_news' ) $('div#gnbMenuSub').css('display', 'block'); 
			}

		});
	</script>
	
