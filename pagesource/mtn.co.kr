        <script type="text/javascript">
// 좌우 슬라이드
function showBroadSlide(id, max, arrow, current){

	try{
		if(typeof(eval("val"+id)) == 'undefined')
			eval("val"+id+"="+current);
	}
	catch(e){
		eval("val"+id+"="+current);
	}
	var tmpVal	=	parseInt(eval("val"+id));


	switch(arrow){
		case 'left' :	if(tmpVal > 1)		tmpVal -=1;
						else				tmpVal = max;
						break;

		case 'right' :	if(tmpVal == max)	tmpVal =1;
						else				tmpVal +=1;
						break;
	}
	for(var i=1;i<=max;i++)
		document.getElementById(id+i).style.display="none";
		document.getElementById(id+tmpVal).style.display="block";
		eval("val"+id+"="+tmpVal);
}
</script>
