<SCRIPT LANGUAGE="JavaScript" type="text/javascript">

	// valida que se haya puesto algo en el buscador y seg√∫n el lugar de busqueda deriva a la pagina correspondiente
	function buscar_validar() {
	
		var formOK = false;
		var doc = document.buscar_form;

		if ( doc.buscar_texto.value != "" ) {
			//document.encuestaForm.submit()
			formOK = true;
		}

		if (formOK){
			switch(doc.buscar_donde.value) {
				case "1": // en intramed
					doc.action = "/buscar_resultado.asp";
					break;
				case "2##": // en pubmed viejo
					doc.action = "http://www.ncbi.nlm.nih.gov/entrez/query.fcgi";
					doc.target = "_blank";
					doc.term.value = doc.buscar_texto.value;
					//alert(doc.term.value);
					doc.cmd.value = "search";
					doc.db.value = "PubMed";
					doc.dispmax.value = "20";
					break;
				case "2": // en pubmed jam 2016
					doc.action = "https://www.ncbi.nlm.nih.gov/pubmed/";
					doc.target = "_blank";
					doc.term.value = doc.buscar_texto.value;
					doc.cmd.value = "search";
					doc.db.value = "PubMed";
					doc.dispmax.value = "20";
					break;
				case "3": 
					// google CSE funciona con cuenta de fbraga de google
					//doc.action = "/buscar_resultado_google.asp?cx=partner-pub-1166250582411247:76dlqe-yk5c&cof=FORID:11&ie=UTF-8&q=" + doc.buscar_texto.value;;
					// google CSE cuenta google jam, search entire web
					doc.action = "/buscar_resultado_google.asp?cx=015711661000872611851:aicwn4yfigs&cof=FORID:11&ie=UTF-8&q=" + doc.buscar_texto.value;
					break;
				case "4": // google CSE cuenta google jam, search only site
					doc.action = "/buscar_resultado_google.asp?cx=015711661000872611851:akmd3-7z6nm&cof=FORID:11&ie=UTF-8&q=" + doc.buscar_texto.value;
					break;					
				case "5": // usuarios
					doc.action = "/buscar_resultado_usuarios.asp?buscar_texto=" + doc.buscar_texto.value;
					break;					
			}
			doc.submit();
		}else{
			alert("Ingrese una palabra, varias, o una frase entre comillas");
		}
	}
	
	// valida que se haya puesto algo en el buscador movil y seg√∫n el lugar de busqueda deriva a la pagina correspondiente
