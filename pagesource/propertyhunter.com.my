			<script>
	$(document).ready(function(){  
		//submiting verification code
		$(".search-type").change(function(){
			var searchtype = $(".typesearch").val();
			
			$('.search-buy').hide();
			$('.search-rent').hide();
			$('.search-new').hide();
			$('.search-news').hide();
			$('.search-agent').hide();
			$('.search-services').hide();
			$('.search-'+searchtype).show();
			$('.search-type').val(searchtype);
			$('.typesearch').val(searchtype);
		});
	});
	
	//search filtering
	//get state
	$(document).ready(function(){  
		var searchtype = $(".typesearch").val();

		if(searchtype == "buy"){
			var state = $("#stateSell").find(":selected").val();
		}else if(searchtype == "rent"){
			var state = $("#stateRent").find(":selected").val();
		}else if(searchtype == "new"){
			var state = $("#stateNew").find(":selected").val();
		}else{
			var state = ""
		}

		var data = "transaction=" + searchtype + "&state_name=" + state ;
		
		$.ajax({
			type: "POST",
			url: "get-state_search.php",
			data: data,
			success: function(html){
				if(searchtype == "buy"){
					$("#stateSell").html(html);
				}else if(searchtype == "rent"){
					$("#stateRent").html(html);
				}else{
					$("#stateNew").html(html);
				}
			}
		});
	});
	
	function getState(val) {
		var data = "transaction="+val;
		$(".typesearch").val(val);
		
		if(val == "buy" || val == "rent" || val == "new"){
			$.ajax({
				type: "POST",
				url: "get-state_search.php",
				data: data,
				success: function(html){
					if(val == "buy"){
						$("#stateSell").html(html);
					}else if(val == "rent"){
						$("#stateRent").html(html);
					}else{
						$("#stateNew").html(html);
					}
				}
			});
		}
	}

	//get city
	$(document).ready(function(){  		
		var searchtype = $('.search-type').find(":selected").val();
		
		if(searchtype == "buy"){
			var state = $("#stateSell").find(":selected").val();
			var area = $("#cityFilterSell").find(":selected").val();
		}else if(searchtype == "rent"){
			var state = $("#stateRent").find(":selected").val();
			var area = $("#cityFilterRent").find(":selected").val();
		}else if(searchtype == "new"){
			var state = $("#stateSellNew").find(":selected").val();
			var area = $("#cityFilterNew").find(":selected").val();
		}
		
		var data = "transaction=" + searchtype + "&state_name=" + state + "&area_id=" + area;
		
		if(state != ""){
			if(searchtype == ""){
				if(searchtype == "buy"){
					$("#cityFilterSell").hide();
				}else if(searchtype == "rent"){
					$("#cityFilterRent").hide();
				}else{
					$("#cityFilterNew").hide();
				}
			}else{
				if(searchtype == "buy"){
					$("#cityFilterSell").show();
				}else if(searchtype == "rent"){
					$("#cityFilterRent").show();
				}else{
					$("#cityFilterNew").show();
				}
			}

			$.ajax({
				type: "POST",
				url: "get-city_search.php",
				data: data,
				success: function(html){
					if(searchtype == "buy"){
						$("#cityFilterSell").show();
						$("#citySell").html(html);
					}else if(searchtype == "rent"){
						$("#cityFilterRent").show();
						$("#cityRent").html(html);
					}else{
						$("#cityFilterNew").show();
						$("#cityNew").html(html);
					}
				}
			});
		}
	});
	
	
	function getCity(val) {
