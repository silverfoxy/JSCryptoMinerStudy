//login
$(document).ready(function(){
	$('#submit').click(function(){
		$('#submit').hide()
		$('#loadingbtn').show();
		$(".msg").hide();
		var username = $("#username").val();
		var password = $("#password").val();
		var result = $(".msg");
		var data = 'username='+ encodeURIComponent(username) + '&password=' + encodeURIComponent(password) 
		$.ajax({
			url: 'https://www.propertyhunter.com.my/login.php',
			type: 'POST',
			data:  data,
			dataType: 'json',
			success: function(response){
			  if(response == 0){
					result.show().html('Username or password incorrect');
					$('#submit').show()
					$('#loadingbtn').hide();
			  }else if (response == 1){
					location.href = "https://www.propertyhunter.com.my/dashboard.php"
					 $("#loginBtn").hide();
					 $("#logoutBtn").show();
			  }
			},
			error: function(response){

				// Error
			}          
		});

	});
	
	$('#loginBtn').click(function(){
		$("#loginModal").css("display", "block");
	});
	
	$('.closeLogin').click(function(){
		$("#loginModal").css("display", "none");
	});
	$(window).click(function(event) {
		if (event.target.id == $("#loginModal").attr('id')) {
			$("#loginModal").css("display", "none");
		}
	});	
});
</script>
