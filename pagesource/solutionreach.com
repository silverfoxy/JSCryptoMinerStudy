	function switchVerticals( verticalData ) { 
		// set the pseudo menu to the selection
		$(".pseudoMenu").hide();
		$(".verticalSelect").empty();
		if ( verticalData.subVertical ) {
			$.cookie("menuVertical", verticalData.subVertical)
			// select the subvertical in the hidden form
			$("#sub_industry__c-d8c877ad-c37a-4ef5-9237-bb9acf643206").val(verticalData.subVertical);
			
			fillCards(verticalData.vertical.toLowerCase(), verticalData.subVertical.toLowerCase())
		} else {
			$.cookie("menuVertical", verticalData.vertical)
			fillCards(verticalData.vertical.toLowerCase(), false)
		}
		// set what is shown in the dropdown
		$(".verticalSelect").append(verticalData.headname)
		$(".verticalSelectWrap").show();

		// set hero image
		$(".heroWrap").css("background-image", "url(" + verticalData.heroImage + ")" )

		// hide current menus
		$(".site-header__main-menu .hs-menu-wrapper").css("display", "none");
		$(".site-footer__primary .vertical_solutions .hs-menu-wrapper").css("display", "none");
		// clear out the subHero and refill it
		$(".subHero").css("max-height", "0px");
		setTimeout(function(){
			$(".subHero .subHeroTitle").empty();
			$(".subHero .subHeroBody").empty();
			$(".subHero .subHeroTitle").append(verticalData.title);
			$(".subHero .subHeroBody").append(verticalData.description);
			$(".subHero").css("max-height", "300px");
		}, 1000)
		// clear out the page title and replace it with vertical specific text
		$(".verticalSelectTitleWrap .verticalSelectTitle").empty();
		$(".verticalSelectTitleWrap .verticalSelectTitle").append("Patient Relationship Management for your " + verticalData.headname + " Practice");
		// hide the current resources so the selected one can be shown
