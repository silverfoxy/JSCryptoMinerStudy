			$.each(cardValue, function( cardContentKey, cardContentValue ){
				if ( subVertical && subVertical !== "undefined" && cardContentValue["verticals"].includes(subVertical) ) {
					$("#" + cardKey + " .graphDataBack .graphDataTitle").append(cardContentValue["backTitle"]);
					$("#" + cardKey + " .graphDataBack .graphDataBody").append(cardContentValue["backBody"]);
					$("#" + cardKey + " .pseudoButton").append(cardContentValue["pseudoButton"]);

					$("#" + cardKey + " .header").append(cardContentValue["header"]);
					$("#" + cardKey + " .card__icon img").attr("srcset", cardContentValue["icon"]);
					$("#" + cardKey + " .card__icon img").attr("style", cardContentValue["iconCSS"]);
					$("#" + cardKey + " .card__content").append("<h4>&nbsp;</h4>" + cardContentValue["content"]);
					$("#" + cardKey + " .card__link-text a").append(cardContentValue["linkText"]);

					$("#" + cardKey + " .card__link-text a").attr("href", cardLinks[subVertical][cardKey]);
					$("#" + cardKey + " .backLink").attr("href", cardLinks[subVertical][cardKey]);

				} else if ( cardContentValue["verticals"].includes(vertical) ) {
					$("#" + cardKey + " .graphDataBack .graphDataTitle").append(cardContentValue["backTitle"]);
					$("#" + cardKey + " .graphDataBack .graphDataBody").append(cardContentValue["backBody"]);
					$("#" + cardKey + " .pseudoButton").append(cardContentValue["pseudoButton"]);

					$("#" + cardKey + " .header").append(cardContentValue["header"]);
					$("#" + cardKey + " .card__icon img").attr("srcset", cardContentValue["icon"]);
					$("#" + cardKey + " .card__icon img").attr("style", cardContentValue["iconCSS"]);
					$("#" + cardKey + " .card__content").append("<h4>&nbsp;</h4>" + cardContentValue["content"]);
					$("#" + cardKey + " .card__link-text a").append(cardContentValue["linkText"]);

					$("#" + cardKey + " .card__link-text a").attr("href", cardLinks[vertical][cardKey]);
					$("#" + cardKey + " .backLink").attr("href", cardLinks[vertical][cardKey]);
					
				}
			})
		})
	}

