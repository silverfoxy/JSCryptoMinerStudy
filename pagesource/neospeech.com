    function populateVoices(languageTypeId) {
        var html = '';
        var gender = '';
        var first = true;

        var voiceCount = 0;

        for (var i = 0; i < voices.length; i++) {
            if (voices[i].languageTypeId === languageTypeId) {
                voiceCount++;
                gender = voices[i].gender === 'M' ? 'male' : 'female';
                
                if(voices[i].languageTypeId == 1)//en-US
                {
                	 if(voices[i].name == "James")//default voice
                     {
                		 first = true;     
                     }
                	 else
                	 {
                		 first = false;
                	 }
                }
               
                if (first) {
                    html += '<li vid="' + voices[i].voiceId + '" demo="' + voices[i].ttsDemoText + '" class="selected"><img src="/images/' + gender + '.png" />' + voices[i].name + ' (' + gender + ')</li>';
                    first = false;
                } else {
                    html += '<li vid="' + voices[i].voiceId + '" demo="' + voices[i].ttsDemoText + '" ><img src="/images/' + gender + '.png" />' + voices[i].name + ' (' + gender + ')</li>';
                }
            }
        }
        $('#selected-voice').html('');
        $('.voices').html(html);
        $('.voices li.selected').clone().appendTo('#selected-voice');
        $('#tts-text').val($('.voices li.selected').attr('demo'));
        $('.voices').hide();
        if (voiceCount < 2) {
            $('#voice-down').hide();
        } else {
            $('#voice-down').show();
        }
    }


    $(document).ready(function () {

        $('.languages').find('>:first-child').addClass('selected');
        var voices = $('.voices');
        voices.find('>:first-child').addClass('selected');

        $('.languages li.selected').clone().appendTo('#selected-language');
        $('.voices li.selected').clone().appendTo('#selected-voice');

        populateVoices('1');

        $('#marquees').slick({
            dots: false,
            infinite: true,
            speed: 500,
            slidesToShow: 1,
            centerMode: true,
            variableWidth: true,
            cssEase: 'linear',
            autoplay: true,
            autoplaySpeed: 7000
        });

        $('#selected-language, #language-down').click(function ($event) {
            $event.stopPropagation();
            $('.voices').hide();
            $('.languages').toggle();
        });

        $('.languages li').click(function () {
            $('.languages').hide();
            $('#selected-language').html('');
            $(this).clone().appendTo('#selected-language');
            $('.languages li').removeClass('selected');
            $(this).addClass('selected');
            populateVoices($(this).attr('lid'));
        });

        $('#selected-voice, #voice-down').on('click', function ($event) {
            $event.stopPropagation();
            $('.languages').hide();
            $('.voices').toggle();
        });

        $('.voices').on('click', 'li', function () {
            $('.voices').hide();
            $('#selected-voice').html('');
            $(this).clone().appendTo('#selected-voice');
            $('.voices li').removeClass('selected');
            $('#tts-text').val($(this).attr('demo'));
            $(this).addClass('selected');
        });

        $('body').click(function () {
            $('.languages').hide();
            $('.voices').hide();
        });

        $("#tts-play").click(function () {
            if (getWordCount('#tts-text', '#selected-voice li') < 1) {
                alert('Please provide a text.');
                $('#tts-text').focus();
                return false;
            }

            if ($('#tts-play').hasClass('disabled')) {
                return false;
            }
            if ($('#tts-play').hasClass('playing')) { // need to stop
                $('#tts-audio').attr('src', '');
                $('#tts-play').removeClass('playing').html('PLAY');
                return false;
            }

            $('#tts-play').addClass('disabled').html('Waiting...');

            var voiceId = $("#selected-voice li").attr("vid");
            var audioElement = $('#tts-audio')[0];
		var i = 0;
		
            $.get("/service/demo?voiceId=" + voiceId + "&content=" + encodeURIComponent($("#tts-text").val()))
                .done(function (data) {
			//alert(data);
                    if (data === "error") {
                        alert("Something went wrong.  Please try again.");
                        $('#tts-play').removeClass('disabled').removeClass('playing').html('PLAY');
		   			 } else if(data === "not available") {
		   				alert("Demo is not available. Please contact  Neospeech sales team: sales@neospeech.com.");
                        $('#tts-play').removeClass('disabled').removeClass('playing').html('PLAY'); 
		   			 }else {

                        var audioUrl = data.audioUrl;
                        $('#tts-play').removeClass('disabled').addClass('playing').html('Stop');
                        audioElement.setAttribute('src', audioUrl);
                        audioElement.play();
                    }
			sleep(10000);
                });

            //audioElement.setAttribute('src', "http://neospeech.com/service/demo?voiceId="+voiceId+"&content="+$("#tts-text").val());
            audioElement.play();
        });

        $('#tts-audio').on('ended', function () {
            $('#tts-audio').attr('src', '');
            $('#tts-play').removeClass('disabled').removeClass('playing').html('PLAY');
        });
	
    });

