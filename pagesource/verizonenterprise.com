var ctryLookup=new function() {
 var lookup=this;
 var c=readCookie('SetCountry');
 var open;
 this.l="";
 this.t;

 // save cookie
 this.set=function(c) {
  if (c) {
   var exp = new Date();
   exp.setTime(exp.getTime()+(365*5*24*60*60*1000));
   document.cookie = "SetCountry="+(c ? c : '')+";expires="+exp.toGMTString()+"; domain="+vzb.cookiedomain+"; path=/";
  }
 }

 // IP info returned
 this.gotIP=function(response) {
     var c=lookup.l || (typeof(response.ctry)=="string" ? response.ctry.toLowerCase() : false);
     if (!c) { 
      vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup','Error','No Country',null,null,null,1);
      return false; }
     if (false && $('#ctryLookup .country option[value="'+c+'"]').size()) { // main country
       lookup.l=c;
       $('#ctryLookup .country').val(c);
       lookup.t=new Date() - vzb.start;
       lookup.set(c);
       if (lookup.t < 2000) {
        vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup',"Quick",c,null,lookup.t,1);
        if (c=='us') { 
         $.fancybox.close(true);
        } else {
         //window.location="/"+c+"/";
        }
       } else if (c!="us" && lookup.t < 8000) {
        lookup.ask();
       } else {
        vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup',"Set",c,null,lookup.t,1);
       }
     }
     else if (false && $('#ctrysel .other option[value="'+c+'"]').size()) {
       $('#ctryLookup .other').val(c);
       $('#ctryLookup .country').val('other');
       if (lookup.t < 8000) { 
         lookup.ask();
       }
     }
     else if ($('#countryselector a[data-ctry="'+c+'"]').size()) { // just set cookie
       lookup.t=Math.round((new Date() - vzb.start)/1000);
       vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup',"Set",c,null,lookup.t,1);
       lookup.set(c);
     }
 }

 // form submit
 this.submit=function(e) {
      var c=$('#ctryLookup select.country').val();
      if (c=='us') { $(this).attr("action","/"); e.preventDefault(); }
      else if (c.length==2) {
       $(this).attr("action","/"+c);
      } else {
       c=$('#ctryLookup select.other').attr('name','country').val();
       if ($('#ctryLookup .country option[value="'+c+'"]').size()) { $(this).attr("action","/"+c); } // is a main country
       else { $(this).attr("action","/support/contact/"); }
      }
      if ($('#ctryLookup input.remember:checked').val()) {
        lookup.set(c);
      }
      vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup',"Select",c);
      $.fancybox.close();
 }
 // ask user
 this.ask=function() {
   if ($('#ctryLookup').size()) {
     $('#ctryLookup select.country').change(function () {
      if ($(this).val()=='other') {
       $('#ctryLookup select.other').show().focus();
      } else {
       $('#ctryLookup select.other:visible').hide();
      }
      var n=$('#ctryLookup select.country').val();
      vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup',"Change",lookup.l+"->"+n)
     });
     $('form#ctryLookup').submit(lookup.submit);
     if (lookup.l!="us") {
      $.fancybox({content:$("#ctryLookup"),complete:function(){open=new Date()}});
      vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup',"Lightbox",lookup.l,null,null,1);
     }
   }
 }


 if (typeof(vzb)!="undefined" && vzb.root == '/') {
  var f=window.location.search.match(/forceCountry=(..)/);
