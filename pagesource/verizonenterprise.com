     hpI.industry=ind.sort(function(a,b){return section.industry[b]-section.industry[a]});
    } else { hpI.industry=[]; }
    if (section.contact && section.contact.industry) { hpI.industry.unshift(section.contact.industry.toLowerCase()); }
    if (typeof(Demandbase)!="undefined") { try {
     hpI.industry.push(Demandbase.IP.CompanyProfile.industry.toLowerCase());
    } catch(e){}} else if (!ind.length && !(n>2)) {
    //  setTimeout("hpIndustry.getIndustry("+(n ? n+1 : 1)+")",1000);
    }
   if (ind.length) {hpI.gotIndustry()};
 }
