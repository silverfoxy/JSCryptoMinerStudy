     vztrackevent(vzb.section,'Template',vzb.subsection,'CountryLookup','Error',s,null,null,null,1);
    }});
   }
  }
 }
}


homehero=new function() {
 var homehero=this;
 var interval=5000; // 4s rotation
 var current=0;
 var n=1; // how many heros
 var c=0; // rotation counter;
 var active=-1; // active rotation
 var t;
 

 this.next=function(a) {
   if (a) { active=a; }
   if ($('.heros a:hover, .heros a:focus').size()) { active=0; }
   if (active>0){
    clearTimeout(t);
    $('div.heros').addClass('enable');
    $('div.heros ul.main>li:visible').each(function(i,v) {
     if ($(v).hasClass('p1')) { $(v).removeClass('p1').addClass('p2');}
     else if ($(v).hasClass('p2')) { $(v).removeClass('p2').addClass('p3'); }
     else if ($(v).hasClass('p3')) { $(v).removeClass('p3').addClass('p1'); }
    });
    c++;
   }
   t=setTimeout("homehero.next()",(c >= n ? 20000 : interval)+(c*c)); // normal rotation
 }

 // start autorotate
 this.start=function() {
  if ($(window).width()>992 && $('div.heros.rotate .main>li').size() > 2) {
   if (!$('div.heros .enable').size()) {
    $('div.heros ul.main').prepend("<li class='enable'/>");
    $('div.heros ul.main>li').each(function(i,v){if(i){$(v).addClass('p'+i)}});
    //$('div.heros li.first+li').addClass('p2');
    //$('div.heros li.second+li').addClass('p3');
    //$('div.heros').addClass('enable');
   }
   t=setTimeout("homehero.next(1)",6000); // 6s first rotation
  }
 }
 // stop autorotate
 this.stop=function() {
  clearTimeout(t);
  active=-1;
 }
 
 // pause when mouse over
 this.over=function(e) {
   clearTimeout(t);
   if (e.type=='mouseleave' && active==0){
     t=setTimeout("homehero.next(1)",4000); // 4s after leaving
   } else if (e.type=='mouseenter') {
     active=0;
   } else if (e.type=='mousemove'){
     //t=setTimeout("homehero.next(1)",interval*4); // if over but still
   }
 }
 // handle links getting focus
 this.focus=function(e) {
  var h=$(e.target).parents('div.hero');
  if (h.size() && !h.hasClass('on')) { // hero not visible
   homehero.goto(h.index());
   $(window).scrollTop(h.offset().top);
  }
 }

 // load non-first images
 this.imgs=function() {
  $('div.heros .bg[data-img]').each(function(i,v) {
    $(v).css({'background-image':'url('+$(v).attr('data-img')+')'});
  });
 }

 // setup heros
 this.init=function() {
  $('div.heros .msg').on('mouseenter mouseleave mousemove',homehero.over);
  $('div.heros a').focus(homehero.focus);
  n=$('div.heros ul.main>li').size();
  homehero.imgs();
 }
 vzInit.onReady(homehero.init);
 vzInit.onLoad(homehero.start);
}



homenews=new function() {
 var homenews=this;
 var timer;
 var div=$('div.news div.marquee');

 // next item, e=click event, not present for interval
 this.next=function(e) {
   if (e && div.children('.next.end').size()) { } // end
   else if (e || !$('ul#news:hover,ul#news a:focus').size()) {
     var n=$('ul#news li:visible:first').hide().nextAll('li');
     if (n.size()==1) { div.children('.next').addClass('end'); } // last
     else { div.children('.next.end').removeClass('end'); }
     if (!n.size()) { n=$('ul#news li:first'); } // loop
     div.children('.prev.end').removeClass('end');
     n.first().show();
    }
    if (e) {
     e.preventDefault();
     clearInterval(timer);
     vztrackevent(vzb.section,'PageActivity',vzb.subsection,'News','Next');
    }
 }

 // previous item
 this.prev=function(e){
   e.preventDefault();
   var n=$('ul#news li:visible:first').hide().prevAll('li');
   //if (n.size()<2) { n=$('ul#news li:first').show(); div.children('.prev').addClass('end'); }
   //else if (!n.size()) { n=$('ul#news li:first').show(); }
   if (n.size()==1) { div.children('.prev').addClass('end'); }
   if (!n.size()) { n=$('ul#news li:first').show(); }
   else { n.first().show(); }
   div.children('.next.end').removeClass('end');
   clearInterval(timer);
   vztrackevent(vzb.section,'PageActivity',vzb.subsection,'News','Prev');
 }

 // insert news and setup controls
 this.setup=function(content) {
  if (content.indexOf('data-news')>0) {
   timer=setInterval("homenews.next()",4500);  //4.5 seconds
   div.prepend(content).prepend('<div class="prev end vzi-cleft"></div><div class="next vzi-cright"></div>');
   div.children('.next').click(homenews.next);
   div.children('.prev').click(homenews.prev);
   $('ul#news a').click(function(e) {
    var label=$(this).attr('data-id')+": "+$(this).text();
    vztrackevent(vzb.section,'PageActivity',vzb.subsection,'News',label);
   }).keydown(function(e){
     if (e.keyCode==39) { homenews.next(e); $('ul#news a:visible:first').focus() }
     else if (e.keyCode==37) { homenews.prev(e); $('ul#news a:visible:first').focus() }
   });
  } else {
    // news missing
  }
 }

 // load news feed
 this.init=function() {
  $.ajax({url:"/home/news-rss.xml",dataType:"html",cache:true,success:homenews.setup,timeout:30000,error:function(){
  }});
 }
 vzInit.onLoad(homenews.init);
}


// video loop
hpVideo=new function() {
 var n=0; // n loops
 var video=false;
 var promodiv=false;
 var times=[]; // load time
 var visible=1;
 var ytlabel; // youtube id:label
 var q=""; // quality

 this.scroll=function(e) { // scroll arrow clicked
  if(e) { e.preventDefault(); }
  $('html,body').animate({scrollTop:$('.promo').height()+$('.promo').offset().top});
 }
 this.resize=function(e) { // window scrolled or resized
  var ph=promodiv.height();
  var st=$(window).scrollTop();
  var b=ph+promodiv.offset().top-st;
  var w=$(window).height()-24;
  var p=Math.max(24,b-w);
  if (p>24) {
   promodiv.find('a.scroll').css({'bottom':p+"px"});
  }
  if (!visible && n<0 && ph > st) { setTimeout("hpVideo.start(1)",500); } // back in view
  visible = ph > st;
 }
 this.close=function() { // overlay closed
   $('.promo').removeClass('play');
    video.get()[0].play();
 }
 this.start=function(t) { //start video after it has stopped
    video.get()[0].currentTime=(t ? t : 0);
    video.show().get()[0].play();
 }
// this.track=function() { dataLayer.push({event:'youtube',gaEventAction:'play',gaEventLabel:ytlabel}); } // start tracking
 this.track=function() { dataLayer.push({event:'fancybox_youtube'}); } // start tracking
 this.play=function() { video.get()[0].pause();} // play full video
 this.stop=function() { video.hide().get()[0].pause(); } // stop and hide loop
 this.ready=function() { // onready
  promodiv=$('.promo');
  $('.promo a.scroll').click(hpVideo.scroll);
  $(window).on('scroll.promo resize.promo',hpVideo.resize);
  setTimeout("hpVideo.resize()",200);
 }
 this.load=function() { // window loaded
  if ($('a#videoloop').size()) {
   setTimeout("hpVideo.init()",150);
  }
 }
 this.init=function() { // initialize
  var pt; // progress timer
  var button=$('.promo a.video');
  var w=Math.min(1280,$(window).width());
  button.fancybox({padding:0,width:w,height:Math.min(w*0.5625,$(window).height()),beforeLoad:hpVideo.play,afterLoad:hpVideo.track,afterClose:hpVideo.close});
  if (button.size()) {
   var yi=button.attr('href').match(/youtu.be\/(.*)/);
   ytlabel=(yi ? yi[1]+":" : '')+button.attr('data-title');
  }
  var loop=$('a#videoloop');
  if ($('.promo .aspect:visible').size()) { // not mobile
  times[0]=new Date() - vzb.start;
  var src=loop.attr('data-video');
  if (times[0] > 4000 && loop.attr('data-low')) {
   src=loop.attr('data-low');
   q="-low";
  }
  if (times[0] > 8000) {
   dataLayer.push({event:'video-loaded','gaEventLabel':'Too slow to load','load-time':times[0]});
  } else {
   loop.html("<video autoplay='' poster='"+loop.attr('data-poster')+"'><source src='"+src+"' type='video/mp4'/></video>");
   video=$('#videoloop video');
   video.on('ended',function() {
    n++;
    if (n<20 && visible) { hpVideo.start(); }
    else { video.hide(); n=-1; }
   }).on('progress',function(e) { if (!times[3]) {
    times[2]=new Date() - vzb.start;
    if (pt) { clearTimeout(pt); }
    var buf=video.prop('buffered');
    var tt=times[2]-times[0];
    if (tt > 3000 && buf.length && buf.end(0) < tt/1500) {
     //console.log("too slow "+tt+" at "+buf.end(0));
     $('#videoloop source').remove();
     video.attr('src','').remove();
     dataLayer.push({event:'video-loaded','gaEventLabel':'Abort','load-time':buf.end(0)*1000});
    } else { pt=setTimeout("hpVideo.loaded()",2000); }
   }}).on('canplaythrough',function() {
    if (!times[1]) {
     video.addClass('on');
     times[1]=new Date() - vzb.start;
     }
   });
   promodiv.on('mouseenter',function() {
   if (visible && n<0) {
    hpVideo.start(0.25);
    n=0;
   }
   });
  }
  }
 }
 this.loaded=function() { // finished loading
  if (!times[3]) { times[3]=times[2] - times[0];
   dataLayer.push({event:'video-loaded','gaEventLabel':'Video'+(q?q:''),'load-time':times[3]});
  }
 }
}


