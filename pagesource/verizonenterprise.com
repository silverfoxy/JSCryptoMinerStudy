   if (li.size()>2) {
    $('ul.industry>li>span[data-industry]').each(function(i,v){ $('#debugIndustry').append($(v).attr('data-industry')+", "); })
    $('#debugIndustry').prepend('<b>modules:</b> ').append("<div/>");
    var a=0;

    for (var n=hpI.industry.length-1;n>=0;n--) {
     var m=li.has("span[data-industry = '"+hpI.industry[n]+"']");
     if (m.size()) { m.prependTo($('ul.industry')); a++; $('#debugIndustry div').prepend("<span style='color:#f00;'>"+hpI.industry[n]+"</span>, ")}
     else  { $('#debugIndustry div').prepend(hpI.industry[n]+", "); }
    }
    if (a) {
     dataLayer.push({'variableContent':$('ul.industry>li:first span[data-industry]').attr('data-industry')});
     //console.log("industry: "+$('ul.industry>li:first span[data-industry]').attr('data-industry'));
    }
    $('#debugIndustry div').prepend('<b>profile:</b> ');
    //$('ul.industry>li:gt(1)').remove();
    $('ul.industry>li:gt(1)').each(function(i,v){$(v).attr('data-bgimgoff',$(v).attr('data-bgimg')).removeAttr('data-bgimg');});
   }
 }
 this.viewAll=function() {
  var n=$('ul.industry>li').size()-1;
  $('ul.industry>li:gt(0)').each(function(i,v){
   if ($(v).attr('data-bgimgoff')) { $(v).css({'background-image':'url('+$(v).attr('data-bgimgoff')+')'}); }
   $(v).css({'position':'absolute','left':(50*(i+1)/n)+'%'});
  }).show();
  $('ul.industry>li').on('mouseenter',function() {
    $(this).css({'z-index':9}).nextAll().each(function(i,v){$(v).css({'z-index':(8-i)})})
    $(this).prevAll().each(function(i,v){$(v).css({'z-index':(8-i)})});
  });
 }
 $('a.viewallindustry').click(function(e) {
  e.preventDefault();
  hpI.viewAll();
  $(this).hide();
 });
 vzInit.onReady(hpI.getIndustry);
}

