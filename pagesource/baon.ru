    <div class="float-cabinet" ng-controller="FloatCabinetController" ng-show="$root.currentShowFloatBlock=='cabinet';" ng-mouseleave="totalCloseWindows();" ng-init="currentViewBlock='auth-form';" ng-cloak>
        <img src="/public/new-design/main/top-pimpa.png" class="basket-pimpa" alt=""/>

        <div class="alert alert-danger" role="alert" ng-show="errorMessage">
            <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
            <span class="sr-only">Ошибка:</span>
            <span ng-bind="errorMessage"></span>
        </div>


        <form class="form-horizontal" name="authForm" ng-show="currentViewBlock=='auth-form'">

            <h4>Авторизация</h4>
            <div class="form-group">
                <label class="col-sm-3 control-label">Логин/e-mail</label>
                <div class="col-sm-7">
                    <input type="text" class="form-control" ng-model="login" placeholder="email" required >
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 control-label">Пароль</label>
                <div class="col-sm-7">
                    <input type="password" class="form-control" ng-model="passwd" placeholder="пароль" required>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 control-label">&nbsp;</label>
                <div class="col-sm-7">
                    <span class="dotted-underline-black pointer" ng-click="currentViewBlock='loss-pass-form'; $event.preventDefault();">Забыли пароль?</span> / <span ng-click="currentViewBlock='new-customer-form';" class="dotted-underline-black pointer">Регистрация</span>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 control-label">&nbsp;</label>
                <div class="col-sm-7">
                    <a class="golden-button enter-button only-border" href="javascript:void(0);" ng-click="userLogin()">ВОЙТИ</a>
                </div>
            </div>

        </form>

        <form class="form-horizontal" name="newCustomerForm" ng-show="currentViewBlock=='new-customer-form'">
            <h4>Зарегистрируйте аккаунт покупателя Baon</h4>
            <div ng-hide="newUserAdded">
                <div class="form-group">
                    <label class="col-sm-3 control-label">Имя <sup class="required">*</sup></label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control" ng-model="customer.i" placeholder="введите имя" required>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Фамилия <sup class="required">*</sup></label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control" ng-model="customer.f" placeholder="введите фамилию" required>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Отчество </label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control" ng-model="customer.o" placeholder="введите отчество">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Телефон <sup class="required">*</sup></label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control"  ng-model="customer.phone" ui-mask="+7 (999) 999-99-99"  ng-model="phone" required/>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">E-mail <sup class="required">*</sup></label>
                    <div class="col-sm-7">
                        <input type="email" class="form-control" ng-model="customer.email" placeholder="введите email" required><br />
                        <input type="checkbox" value="1" ng-model="customer.subscriptionApprove"/> Подписаться на новости
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Пол</label>
                    <div class="col-sm-7">
                        <select class="form-control" ng-model="customer.sex">
                            <option value="">-- выберите пол --</option>
                            <option value="m">мужской</option>
                            <option value="f">женский</option>
                        </select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-3 control-label">Пароль <sup class="required">*</sup></label>
                    <div class="col-sm-7">
                        <input type="password" class="form-control" ng-model="customer.passwd" required>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Пароль еще раз <sup class="required">*</sup></label>
                    <div class="col-sm-7">
                        <input type="password" class="form-control" ng-model="customer.passwd2" required>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">&nbsp;</label>
                    <div class="col-sm-7">
                        <a href="javascript:void(0);" class="dotted-underline-black"  ng-click="currentViewBlock='auth-form'">У меня уже есть аккаунт</a>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">&nbsp;</label>
                    <div class="col-sm-7">
                        <div>Нажимая кнопку Зарегистрироваться Вы соглашаетесь с <a href="/index/privacy/" target="_blank">Политикой конфиденциальности</a>  и <a href="/index/privacy-cookie/" target="_blank">Политикой обработки cookies</a> </div>
                        <a class="golden-button enter-button only-border" ng-click="createCustomer(); $event.preventDefault();" href="javascript:void(0);">ЗАРЕГИСТРИРОВАТЬСЯ</a>
                    </div>
                </div>
            </div>
            <div ng-show="newUserAdded">
                <p>Вы успешно зарегистрированы на сайте baon.ru</p>
                <p>&nbsp;</p>
                <p style="text-align: center;"><a class="golden-button only-border" href="/customer/cabinet-new/">ПЕРЕЙТИ В КАБИНЕТ</a></p>
                <p>&nbsp;</p>
            </div>



        </form>

        <form class="form-horizontal" name="lossPassForm" ng-show="currentViewBlock=='loss-pass-form'">
            <h4>Востановление пароля</h4>
            <div ng-hide="passwordSend">
                <p>Введите адрес электронной почты, который вы указывали при региcтрации</p>
                <div class="form-group">
                    <label class="col-sm-3 control-label">E-mail <sup class="required">*</sup></label>
                    <div class="col-sm-7">
                        <input type="email" class="form-control" ng-model="email" placeholder="введите email" required>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">&nbsp;</label>
                    <div class="col-sm-7">
                        <a class="golden-button enter-button only-border" href="javascript:void(0);" ng-click="sendPassword();">ВЫСЛАТЬ ПАРОЛЬ</a>
                    </div>
                </div>
            </div>
            <div ng-show="passwordSend">
                Вам на почту выслано письмо с дальнейшими инструкциями
            </div>
        </form>

        <div class="already-login-in"  ng-show="currentViewBlock=='login-block'">
            <p>  </p>
            <p><a class="golden-button only-border" href="/customer/cabinet-new/">ПЕРЕЙТИ В КАБИНЕТ</a></p>
            <p><a class="dotted-underline-black" href="/customer/logout/">Выйти</a></p>
        </div>


        <div class="social-gate" ng-hide="currentViewBlock=='login-block'">
            <hr/>
            <label class="col-sm-3 control-label">Войти через социальные сети</label>
            <div class="col-sm-7">
                <div style="text-align: center;" id="uLogin" data-ulogin="display=panel;fields=first_name,last_name,email;providers=vkontakte,odnoklassniki,mailru,facebook,yandex,twitter;redirect_uri=http%3A%2F%2Fbaon.ru%2Fcustomer%2Fsocial%2F"></div>
            </div>
            <p>&nbsp;</p>
            <p>&nbsp;</p>
            <p>&nbsp;</p>
        </div>

    </div>
    <!--/noindex-->
    <!--noindex-->
    <div class="float-basket" ng-controller="FloatBasketController" ng-show="$root.currentShowFloatBlock=='basket';" ng-mouseleave="totalCloseWindows();" ng-cloak>
        <img src="/public/new-design/main/top-pimpa.png" class="basket-pimpa" alt=""/>

        <div height-correction="-3" shift="129" animate-duration=100 ng-show="$root.basketObject.length > 0" ng-style="" vertical-scroll float-basket>
            <img src="/public/new-design/main/arrow-top-big.png" class="top-arrow" ng-show="$root.basketObject.length > 2" ng-click="topMove()">
            <img src="/public/new-design/main/arrow-bottom-big.png" class="bottom-arrow" ng-show="$root.basketObject.length > 2" ng-click="bottomMove()">

            <ul>
                <li ng-repeat="item in $root.basketObject">
                    <img src="/public/new-design/main/cross-small.png" alt="" class="cross-small" ng-click="removeItem(item)"/>
                    <div class="price-item" ><span class="red-strike nowrap" ng-if="item.discount&&item.discount!=item.price"><span ng-bind="item.discount"></span><span class="b-rub">Р</span></span><span class="model-price nowrap"><span ng-bind="item.price"></span> <span class="b-rub">Р</span></span><br /><span class="basket-cnt"><span ng-bind="item.cnt"></span> шт.</span></div>
                    <img ng-src="{{ item.img }}" alt=""/>
                    <div>
                        <a href="/catalog/{{ item.modelId }}/{{ item.transliteratedName }}/{{ item.color }}/"  ng-href="/catalog/{{ item.modelId }}/{{ item.transliteratedName }}/{{ item.color }}/"><span ng-bind="item.name"></span>, <nobr>арт. <span ng-bind="item.articul"></span></nobr></a><br />
                         <span class="basket-stock"  ng-if="item.stock<$root.currentShowAvailabilityCount">Осталось только <span ng-bind="item.stock>10?10:item.stock"></span> шт.</span>
                         <div class="basket-size">Размер: <span ng-bind="item.size"></span></div>
                    </div>

                </li>
            </ul>
        </div>

        <div class="total">Всего {{ totalCnt }} товар{{ totalSuffix }} на сумму {{ totalSum }} <span class="b-rub">Р</span> </div>
        <div class="btn-block text-center"  ng-show="showButtonBlock==true">
            <a class="red-button only-border" href="/customer/quick-order-form/" yandex-event="move_to_basket" marketing>КУПИТЬ В 1 КЛИК</a>
            <a class="red-button" href="/customer/basket/" yandex-event="move_to_basket" marketing>ОФОРМИТЬ ЗАКАЗ</a>
        </div>

    </div>
    <!--/noindex-->
    <!--noindex-->
    <div class="float-favorite" ng-controller="FloatFavoriteController" ng-show="$root.currentShowFloatBlock=='favorite';" ng-mouseleave="totalCloseWindows();" ng-cloak>
        <img src="/public/new-design/main/top-pimpa.png" class="basket-pimpa" alt=""/>

        <ul>
            <li ng-repeat="item in favoriteObject">
                <img src="/public/new-design/main/cross-small.png" alt="" class="cross-small" ng-click="removeItem(item)"/>
                <div>
                    <a href="/catalog/{{ item.modelId }}/{{ item.specialTranslatedName }}/{{ item.color }}/" class="dotted-underline-black" ng-href="/catalog/{{ item.modelId }}/{{ item.specialTranslatedName }}/{{ item.color }}/">{{ item.name }}</a><br />
                    <span>Артикул:</span> {{ item.articul }}<br />
                    <span>Цена:</span> {{ item.price }}<br />
                </div>
                <img ng-src="{{ item.img }}" alt=""/>
            </li>
        </ul>

        <div class="total">В избранном <span ng-bind="totalCnt"></span> товар<span ng-bind="totalSuffix"></span></div>
        <div class="btn-block" ng-show="showButtonBlock==true">
            <a class="red-button only-border" href="/catalog/favorite/">ПЕРЕЙТИ К ТОВАРАМ</a>
        </div>

    </div>
