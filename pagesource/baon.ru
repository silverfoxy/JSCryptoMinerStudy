        <img src="/public/new-design/main/cross-small.png" class="close-window" ng-click="totalCloseWindows();" alt=""/>
        <div>
            <form action="/catalog/search/" name="searchForm">
                <input type="text" class="form-control" name="searchString" ng-keyup="search($event)" ng-click="$event.stopPropagation();" ng-model="searchString" placeholder="введите запрос"/>
                <button type="submit" class="btn btn-danger">Искать</button>
            </form>
            <div ng-bind-html="searchResult"></div>
