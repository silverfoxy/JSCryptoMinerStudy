	    function slidesticky(disorder) {
	        var arrsticky = $("[data-displayorder='"+disorder+"']");
	        arrsticky.each(function (index) {
	            if ($(this).hasClass("sticky-mobile-active")) {
	                var indextoshow = 0;
	                if (index==arrsticky.length-1) {
	                    indextoshow = 0;
	                }
	                else {
	                    indextoshow = index + 1;
	                }
	                $(this).removeClass("sticky-mobile-active").hide();
	                arrsticky.eq(indextoshow).addClass("sticky-mobile-active").fadeIn();
	                doSetTimeoutsticky(disorder);
	                return false;
	            }
	            

	        })
	    }

	    function update_removesticky_array(autoid){
	        var remove_array=[]
	        var value=getCookie_sticky("ckdelsticky")
	        if (value.length>0) {
	            remove_array=JSON.parse(value);
	        }
	        if (remove_array.indexOf(autoid) < 0) {
	            remove_array.push(autoid);
	            setCookie_sticky("ckdelsticky",JSON.stringify(remove_array))
	        }
	    }
	    function Remove_Sticky(autoid){
	        update_removesticky_array(autoid)
	        $("[data-autoid="+autoid+"]").remove();
	    }

	    function setCookie_sticky(name, value, expires, path, domain, secure) {
	        try {
	            var curCookie = name + "=" + escape(value) +
                ((expires) ? "; expires=" + expires.toGMTString() : "") +
                ((path) ? "; path=" + path : "") +
                ((domain) ? "; domain=" + domain : "") +
                ((secure) ? "; secure" : "")
	            if ((name + "=" + escape(value)).length <= 4000) {
	                document.cookie = curCookie
	            }
	            else {
	                document.cookie = curCookie
	            }
	        }
	        catch (err) {
	            ErrorHandle(err, "adstickymanager.js");
	        }
	    }
	    function getCookie_sticky(name) {
	        try {
	            var prefix = name + "="
	            var cookieStartIndex = document.cookie.indexOf(prefix)
	            if (cookieStartIndex < 0) {
	                return '';
	            }
	            var cookieEndIndex = document.cookie.indexOf(";", cookieStartIndex + prefix.length)
	            if (cookieEndIndex < 0) {
	                cookieEndIndex = document.cookie.length
	            }
	            return unescape(document.cookie.substring(cookieStartIndex + prefix.length, cookieEndIndex))
	        }
	        catch (err) {
	            ErrorHandle(err, "adstickymanager.js");
	        }
	    }
