  // 2. Runs when the JavaScript framework is loaded

  function onLinkedInLoad() {

    IN.Event.on(IN, "auth", onLinkedInAuth);

  }



  // 2. Runs when the viewer has authenticated

  function onLinkedInAuth() {

    IN.API.Profile("me").fields(["id","headline","firstName","lastName","positions:(company)","email-address","location:(name)","industry","site-standard-profile-request"]).result(displayProfiles);

  }



  // 2. Runs when the Profile() API call returns successfully

  function displayProfiles(profiles) {

		member 				= profiles.values[0];

		var title 			= member.headline;

		var id				= member.id;

		var first_name 		= member.firstName;

		var last_name		= member.lastName;

		var email			= member.emailAddress;

		var company			= member.positions['values'][0]['company']['name'];

		var location 		= member.location['name'].split(",");

		var city 			= location[0];

		var country 		= location[1];

		var state			= '';

		var profile_link	= member.siteStandardProfileRequest['url'];

	

		var val 			= first_name+"=="+last_name+"=="+email+"=="+company+"=="+city+"=="+state+"=="+country+"=="+profile_link;

		

		var ajax_url = $('#ajax_url').val();

		if(ajax_url == 1){

			url_ajax = 'https://www.happiestminds.com/?session='+val;

		}else{

			url_ajax = 'https://www.happiestminds.com/?id='+val;

		}

		

		//alert(url_ajax);

		

		$.ajax({

			url: url_ajax,

			success:function(data){

				$('#or_displaying').css('display','none');

				$('#00Nd0000007lbJ8').val(first_name);

				$('#company').val(company);

				$('#email').val(email);

				$('#lastname').val(last_name);

				$('#city').val(city);

				$('#or_hidden').val(1);

			},

			error:function(request, status, error){

				alert(request.responseText);

			}

		});

  }

  

	$(function(){

