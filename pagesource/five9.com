                $(sourceDetailField).attr("value", $.cookie('mktoPPCProgram'));
            }
            $(programField).attr("value", $.cookie('mktoPPCProgram'));
            $(campaignField).attr("value", $.cookie('mktoPPCCampaign'));
            $(adGroupField).attr("value", $.cookie('mktoPPCAdGroup'));
            $(gclidField).attr("value", $.cookie('mktoGCLID'));
            $(matchTypeField).attr("value", $.cookie('mktoPPCMatchType'));
            $(networkTypeField).attr("value", $.cookie('mktoPPCNetworkType'));
            $(deviceTypeField).attr("value", $.cookie('mktoPPCDeviceType'));
            $(creativeIdField).attr("value", $.cookie('mktoPPCCreativeId'));
            $(placementField).attr("value", $.cookie('mktoPPCPlacement'));
            $(placementCategoryField).attr("value", $.cookie('mktoPPCPlacementCategory'));
            $(adPositionField).attr("value", $.cookie('mktoPPCAdPosition'));
            $(utmSourceField).attr("value", $.cookie('f9utmSource'));
            if (($.cookie('mktoPPCKeyword') != undefined) && ($.cookie('mktoPPCKeyword') != "")) {
                $(payPerClickKeywordField).attr("value", $.cookie('mktoPPCKeyword'));
            }
            if ($.cookie('mktoLeadAction') != null) {
                $(leadActionField).attr("value", $.cookie('mktoLeadAction'));
            } else {
                $(leadActionField).attr("value", mktoLeadAction);
            }
            if (($.cookie('mktoLandingPageURL') == undefined) || ($.cookie('mktoLandingPageURL') == "")) {
                $.cookie('mktoLandingPageURL', window.location, { expires: 730, path: '\/', domain: cookieDomain });
            }
        })();
    });
</script>
