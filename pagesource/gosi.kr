<script type="text/javascript" src="/js/cookies.js" ></script>
<script type="text/javascript">
$(function() {
	$('#box2, #box3, #box4, #box5, #box6, #box7, #box8, #box9, #box10, #box11, #box12').hide();
    $("#select_table").change(function(){
    	$("#" + this.value).show().siblings().hide();
    });

   	/* 탭메뉴 */
   	tab('#tab', 0);
   	
   	/* 슬라이드 탭메뉴 */
   	tab('#slidetab', 0);
   	
   	$("#slidetab_wrap").touchSlider({
        view: 6,
        btn_prev: $("#slidetab_wrap").next().find(".btn_prev"),
        btn_next: $("#slidetab_wrap").next().find(".btn_next")
    });
   	
   	/* 슬라이드 탭메뉴(모바일) */
   	tab('#slidetab_m', 0);
   	
   	$("#slidetab_wrap_m").touchSlider({
   		breakpoints : {
            640: {
                view: 2
            }
        },
        view: 3,
        btn_prev: $("#slidetab_wrap_m").next().find(".btn_prev"),
        btn_next: $("#slidetab_wrap_m").next().find(".btn_next")
    });
    	
   	/* 상단팝업 */
   	if(getCookie('popupClose')=='Y'){
   		$("#chkbox2").prop("checked", true);
       	$('#topPopupIn').css('display', 'none'); 
       	$('#up').css('display', 'none');
        $('#down').css('display', 'inline');
    }else{
       	$('#topPopupIn').slideDown('slow');
       	$("#chkbox2").prop("checked", false);
    }
    	
    $('#up').click(function() {
    	$('#topPopupIn').slideUp('slow');
    	$('#up').css('display', 'none');
    	$('#down').css('display', 'inline');
    });
        
	$('#down').click(function() {
		$('#topPopupIn').slideDown('slow');
		$('#down').css('display', 'none');
		$('#up').css('display', 'inline');
	});
	
	$("#chkbox2").click(function(){
		if($("#chkbox2").prop("checked")){
			setCookie('popupClose', 'Y', 1);
		}else{
			setCookie('popupClose', 'N', 0);
		}
	})
});
	
function fn_egov_inqire_notice(nttId, bbsId) {
	document.board.nttId.value = nttId;
	document.board.bbsId.value = bbsId;
	document.board.action = "/cop/bbs/selectBoardArticle.do;jsessionid=FyjYtA5RHApzKa4RoR4nQFwr.node_cybergosiwas21";
	document.board.submit();			
}
    
function fn_egov_select_noticeList() {
	document.boardList.submit();	
}

function tab(e, num) {
    var num = num || 0;
    var menu = $(e).children();
    var con = $(e + '_con').children();
    var select = $(menu).eq(num);
    var i = num;

    select.addClass('on');
    con.eq(num).show();
    
    menu.click(function() {
        if (select !== null) {
            select.removeClass("on");
            con.eq(i).hide();
        }

        select = $(this);
        i = $(this).index();

        select.addClass('on');
        con.eq(i).show();
        
        if(e == '#tab'){
        	if(i == '0'){
        		document.boardList.examGubuncd.value= "EXAM09";
        	}else if(i == '1'){
        		document.boardList.examGubuncd.value= "EXAM01";
        	}else if(i == '2'){
        		document.boardList.examGubuncd.value= "EXAM02";
        	}else if(i == '3'){
        		document.boardList.examGubuncd.value= "EXAM03";
        	}else if(i == '4'){
        		document.boardList.examGubuncd.value= "EXAM04";	
        	}
        }
    });
}
</script>

