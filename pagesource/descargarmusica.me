<script type="text/javascript">
    //    if (location.protocol !== 'https:') {
    //        window.location.href = 'https:' + window.location.href.substring(window.location.protocol.length);
    //    }

    if (typeof window.callPhantom !== 'function' &&
            typeof navigator !== 'undefined' &&
            typeof window.applicationCache !== 'undefined') {

        var startLoading = false;
        var appCache = window.applicationCache;

        var updater = document.getElementById("updater");
        var updating = document.getElementById("updating");
        var updated = document.getElementById("updated");
        var progressStatus = document.getElementById("updater-progress");
        var progressBarStatus = document.getElementById("updater-progress-bar");

        if (navigator.onLine) {

            appCache.addEventListener('progress', function (e) {

                try {
                    var percent = Math.round((e.loaded / e.total) * 100);

                    if (percent > 0) {

                        if (!startLoading) {
                            startLoading = true;
                            updater.style.display = 'block';
                            updater.className = 'animated bounceInUp';
                            updating.style.display = 'block';
                            updated.style.display = 'none';
                        }


                        if (percent >= 100) {

                            updater.className = 'animated pulse';
                            updating.style.display = 'none';
                            updated.style.display = 'block';

                            setTimeout(function () {
                                updater.className = 'animated bounceOutDown';
                                setTimeout(function () {
                                    updater.style.display = 'none';
                                }, 3000);
                            }, 4000);
                        }

                        progressStatus.innerHTML = percent + '%';
                        progressBarStatus.style.width = percent + '%';


                    }

                } catch (e) {
                    console.error(e.message);
                }
            }, false);

            try {
                appCache.update();
            } catch (e) {
                console.error(e.message);
            }

        }
    }
</script>
