    jQuery('#nlshelfwrapper').css('bottom', divHeight);
    jQuery('#nlshelfwrapper').addClass("collapsed");
    isOpen = false;
  }

  jQuery('#nlshelfwrapper').click( function(e){
      open_nl_shelf();
      stop(e);
  });

  jQuery('#nl_close_button').click( function(e){
