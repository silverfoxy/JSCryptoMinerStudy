<script type="text/javascript">
    // goods_category_virtual widget script

    jq(function() {
        //event
        jq('.top-header .nav .virtual .flyoutContainer .lv1').mouseenter(function() {
            jq(this).addClass('active').siblings('.lv1').removeClass('active');
            var lv2s = jq(this).next();
            lv2s.show().siblings('.subCategories').hide();
            // lv2s.find('li').each(function(){
            //   if(jq(this).hasClass('active')){
            //     ssdGetVirtualCatGoods(jq(this).attr('data-vid'),this);
            //   }
            // });
        });
        jq('.top-header .nav .virtual .flyoutContainer .lv2').mouseenter(function() {
            jq(this).parents('.virtual .flyoutContainer').find('.lv2').removeClass('active');
            jq(this).addClass('active');
            ssdGetVirtualCatGoods(jq(this).attr('data-url'), this);
        });
        //init
        jq('.virtual .flyoutContainer').each(function() {
            //show first
            jq(this).find('.subCategories').eq(0).show();
            //init active
            jq(this).find('.lv1').eq(0).addClass('active');
            // 加载第一批商品 & init active
            var first_lv2 = jq(this).find('.lv2').eq(0).addClass('active');
            var first_url = first_lv2.attr('data-url');
            ssdGetVirtualCatGoods(first_url, first_lv2);
        });
    });

    function ssdGetVirtualCatGoods(url, This) {
        // alert(url);
        jq.ajax({
            url: url,
            success: function(result) {
                // alert(result);
                var data = JSON.parse(result);
                // console.log(data);
                // alert(data.length);
                var flyoutContainer = jq(This).parents('.flyoutContainer');
                for (var i = 0; i < 3; i++) {
                    if (data[i]) {
                        flyoutContainer.find('.ajaxProduct').eq(i).show();
                        flyoutContainer.find('.hf_title a').eq(i).html(data[i].name);
                        flyoutContainer.find('.pricePrice').eq(i).html(parseInt(data[i].price));
                        flyoutContainer.find('.hf_img1 img').eq(i).attr('src', data[i].thumbnail_pic);
                        flyoutContainer.find('.hf_add').eq(i).attr('href', data[i].add_cart_url);
                        flyoutContainer.find('.vgLink1').eq(i).attr('href', data[i].goods_url);
                        flyoutContainer.find('.vgLink2').eq(i).attr('href', data[i].goods_url);
                    } else {
                        flyoutContainer.find('.ajaxProduct').eq(i).hide();
                    }
                }
                jq(This).parents('.virtual .flyoutContainer').find('.hf_more').show().find('a').attr('href', jq(This).attr('href'));
                // console.log('append');
                ssdGoodsNameSpaceToBr();
            }
        });
    }
</script>
