        function TN_addToCart(id) {
            var qty = $("#qty_to_order").val();
            $("#add_to_cart_msg").html("Adding item to your cart...");
            tn_show_popup("#add_to_cart");
            $.get("/crt/add/" + id + "/" + qty, null, function (response) {
                var result = Drupal.parseJson(response);
                $("#add_to_cart_msg").html(result.data);
                var rn = Math.floor((Math.random() * 10000) + 1);
                $.get("/crt/total/" + rn, null, function (response) {
                    var result = Drupal.parseJson(response);
                    $("#midbar_cart").html(result.crt);
                });
            });
            return false;
        }
        function TN_addToWishlist(uid, id) {
            if (uid == 0) { tn_show_popup("#wishlist_login"); }
            else
            {
                $("#add_to_wishlist_msg").html("Adding item to your wish list...");
                tn_show_popup("#add_to_wishlist");
                $.get("/wl/add/" + id, null, function (response) {
                    var result = Drupal.parseJson(response);
                    $("#add_to_wishlist_msg").html(result.data);
                    var rn = Math.floor((Math.random() * 10000) + 1);
                    $.get("/crt/total/" + rn, null, function (response) {
                        var result = Drupal.parseJson(response);
                        $("#midbar_wish_list").html(result.wl);
                    });
                });
            }
            return false;
        }
        function TN_followShop(uid, shop_id) {
            if (uid == 0) { tn_show_popup("#favorite_shop_login"); }
            else
            {
                tn_show_popup("#favorite_shop");
                $("#favorite_shop_msg").html("Adding shop...");
                $.get("/db/fav/shop/" + shop_id, null, function (response) {
                    var result = Drupal.parseJson(response);
                    $("#favorite_shop_msg").html(result.data);
                    var rn = Math.floor((Math.random() * 10000) + 1);
                    $.get("/crt/total/" + rn, null, function (response) {
                        var result = Drupal.parseJson(response);
                        $("#midbar_fav_shops").html(result.fs);
                    });
                });
            }
            return false;
        }
            function TN_messageShop(uid, shop_id) {
                if (uid == 0) {
                    tn_show_popup("#message_shop_login");
                } else {
                    window.location = "/dashboard/m/messages/new/" + shop_id;
                }
            }
    </script>


