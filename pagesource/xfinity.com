    <script>
    
    document.dispatchEvent(new CustomEvent("c-tracking-init-end"));
    
    window.addEventListener("load", function () {
        document.dispatchEvent(new CustomEvent("c-tracking-log-page", {
            bubbles: true
        }));
    });
    
    
    $(document).on('keydown.polaris-nav', 'input[data-link-info="Search"]', function (ev) {
        if (ev.keyCode == 13 || ev.key == "Enter") // Enter
        {
            var event = new CustomEvent('c-tracking-log-dom', {
                detail: {
                    "eventAction": "Submit search form"
                },
                bubbles: true
            });
            ev.target.dispatchEvent(event);
        }
    });

    
    document.addEventListener('click', function (ev) {
        var element;

        if (ev.path && ev.path.length) {
            // Shadow DOM event
            element = ev.path[0];
        }
        else if (window.Polymer && typeof ev.composedPath === 'function') {
            // Polymer 2
            element = ev.composedPath()[0];
        }
        else if (window.Polymer && typeof window.Polymer.dom === 'function') {
            // Polymer 1;
            element = typeof window.Polymer.dom(ev).rootTarget;
        }
        else {
            // Standard Event
            element = ev.target;
        }

        if (element && element.closest('a:not([data-tracking]), button:not([data-tracking]), input[type=checkbox]:not([data-tracking]), input[type=radio]:not([data-tracking])')) {
            element.dispatchEvent(new CustomEvent('c-tracking-log-dom', {
                composed: true,
                bubbles: true
            }));
        }
    }, false);
</script>  

