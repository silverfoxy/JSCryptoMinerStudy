<script type="text/javascript">

  $(document).scroll(function () {
    var y = $(this).scrollTop();
    if (y >= 60) {
      $('.stock-feed-wrapper').hide();
      $('#navigation1').css({"position": "fixed", "top": "0", "width" : "100%"});
    } else {
      $('.stock-feed-wrapper').show();
      $('#navigation1').css({"position": "relative", "top": "0", "width" : "100%"});
    }

  });

  $(function() {

    $(window).scroll(function(){

      /* when reaching the element with id "last" we want to show the slidebox. Let's get the distance from the top to the element */

      var distanceTop = $('#last').offset().top - $(window).height();



      if  ($(window).scrollTop() > distanceTop)

        $('#slidebox').animate({'right':'0px'},300);

      else 

        $('#slidebox').stop(true).animate({'right':'-430px'},100);  

    });

    /* remove the slidebox when clicking the cross */

    $('#slidebox .close').bind('click',function(){

      $('#slidebox').remove();

    });


    $('.load-more').on("click", function(){
      $('.fa-spinner').show();
      wrapper = $('#' + this.id + '-wrapper');
      cat_slug = $(this).data('cat_slug');
      limit = $(this).data('limit');
      skip = $(this).data('skip');
      $(this).data()['skip'] += 8;
      site_url = $('meta[name="site_url"]').attr('content');
      params = {};
      params.cat_slug = cat_slug;
      params.skip = skip;
      params.limit = limit;
      params._token = $('meta[name="csrf-token"]').attr('content');
      $.ajax({
        url: 'https://thefinancialexpress.com.bd/public/get-more-news',
        type: 'POST',
        dataType: "html",
        data: params,
        success: function (data) {
          wrapper.append(data);
          $('.fa-spinner').hide();
        },
        error: function(data){
          console.log(data)
        }
      });
    });

  });

</script>
