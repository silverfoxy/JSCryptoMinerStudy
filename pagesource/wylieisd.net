			$(this).parent().addClass("active");
			
			$("html, body").animate({
				scrollTop: $("#" + clickedApp).offset().top
			}, 500, function(){
				$("html, body").stop(true, false);
			});
		});
	}	
	
	function useChannelForSection(){
		//DOC use channel name for pagelistnavigation header if one is not present
		if(!$("div.sp.column.one .ui-widget-header h1").length > 0) {
			$("div.sp.column.one .ui-widget-header").append("<h1></h1>");
		}
        }
        
	function dynamicContent(){
		// ********** MYSTART ********** //
		//DOC replace mystart dropdown arrows
		$(".sw-myaccount-chevron").text("");
		//$(".sw-dropdown-arrow").text("");
		$("#sw-mystart-outer #sw-search-button span").text("search");
		
		$(".sw-mystart-dropdown.schoollist .selector, .sw-mystart-dropdown.schoollist .sw-dropdown-selected").text("Select School");
		$(".sw-mystart-dropdown.translate .selector, .sw-mystart-dropdown.translate .sw-dropdown-selected").text("Select Language");
		
		$("#sw-mystart-left, #sw-mystart-right #sw-mystart-account, #sw-mystart-right .sw-mystart-button.manage, #sw-mystart-right .sw-mystart-button.signin, #sw-mystart-right .sw-mystart-button.register").addClass("hide768");
		$("#sw-mystart-search").addClass("hide320");
		
		// ********** HEADER ********** //
		
		var dtToday = new Date();
		var day = dtToday.getDate();
		$("#gb-global-icons .icn.calendar .date").text(day);
		
		// ********** CHANNEL BAR ********** //
		//DOC add first class to first channel item
		$("li.sw-channel-item").first().addClass("first");
		//DOC add last class to last channel item
		$("li.sw-channel-item").last().addClass("last");
		
		// ********** BODY ********** //
		//DOC rename first breadcrumb
		$("li.ui-breadcrumb-first > a > span").text("Home");
		//DOC replace more-link text
		$("a.more-link span").text("More ");
		//DOC auto-focus username signin field
		$("#swsignin-txt-username").focus();
		//DOC center signed out message and signin button
        	if($("div.ui-spn > div > p > span").text() == "You have been signed out."){
        		$("div.ui-spn > div").css({"text-align" : "center", "padding" : "50px 0px 50px 0px"});
        		//DOC add signed out breadcrumb
        		$(".ui-breadcrumbs").append("<li>Signed Out</li>");
        	}
        	//DOC add sign in breadcrumb
        	if($("#swlogin").length){
        		$(".ui-breadcrumbs").append("<li>Sign In</li>");
        	}
        	
        	//MOD EVENTS
		$(".ui-hp .upcomingevents").modEvents({
			columns:"yes",
			monthLong:"yes"
		});
		eventsByDay(".ui-hp .upcomingevents .ui-articles");
		
		var isiPad = navigator.userAgent.match(/iPad/i) != null;
		if (isiPad) {
			$("#gb-global-icons .icn.calendar .icon .date").css("top","10px");
		}
		
		
		// ********** FOOTER ********** //
		//DOC get Schoolwires footer links
		var feedbackURL = $("div#sw-footer-links li:eq(0) a").attr("href");
		var termsURL = $("div#sw-footer-links li:eq(2) a").attr("href");
		var policyURL = $("div#sw-footer-links li:eq(1) a").attr("href");
		
		$("a#feedback").attr("href", feedbackURL);
		$("a#terms").attr("href", termsURL);
		$("a#policy").attr("href", policyURL);			
	}
	
	function searchText() {
		//DOC custom mystart search placeholder
	        //SEARCH TEXT
	        $("div#sw-mystart-search input#sw-search-input").removeAttr("placeholder");
		$("div#sw-mystart-search input#sw-search-input").attr("value", "Enter Type...");
		$("div#sw-mystart-search input#sw-search-input").focus(function() {
			if($(this).val() == "Enter Type...") {
				$(this).val("");
			}
		});
		$("div#sw-mystart-search input#sw-search-input").blur(function() {
			if($(this).val() == "") {
				$(this).val("Enter Type...");
			}
		});
	}
	function tabbedApps(){	
		//TABBED APPS
        	if(!$(".tabs-container").length){
	        	$("div.hp.column.two div#hp-tabs div#sw-content-container3").tabBlocks();
	        	$(".tab-button:eq(0)").addClass("first");
		}	
	}
	function eventsByDay(container) {
		//DOC move all events with the same dates into the same container
		var countLi = 0;
		$(".ui-article", container).each(function(){
			if (!$(this).find("h1.ui-article-title.sw-calendar-block-date").size()){
				var moveArticle = $(this).html();
				$(this).parent().prev().children().children().next().append(moveArticle);
				$(this).parent().remove();
				
			};
		});
	}
	
	function calendarListView(){
		//DOC auto click function for schoolwires calendar list view for small mobile devices
		if(!$("a.ui-btn-toggle.list").hasClass("active")){
			$("a.ui-btn-toggle.list").click();
		}
	}
	
	function scroller() {
		// SET WIDTHS AND HEIGHTS
		var channelBarWidth = 0;
		$(".tab-button").each(function () {
			channelBarWidth += $(this).width();
		});
		var borderWidth = ($(".tab-button").length * 20);
		channelBarWidth += borderWidth;
		$(".tabs-container").css("width", channelBarWidth);
				
		// GET CURRENT WIDTH OF CHANNEL BAR CONTAINER
		var scrollWidth = 0;
		scrollWidth = channelBarWidth - $("#hp-tabs").width();
		if($(".tabs-container").width() > $("#hp-tabs").width()){
			$("#hp-tabs-arrow-right").removeClass("hidden");
			$("#hp-tabs-arrow-left").removeClass("hidden");
		}
	
		// RIGHT SCROLL FUNCTION
		$("#hp-tabs-arrow-right").live("mousedown touchstart", function() {
			$(".tabs-container").animate({
				marginLeft: "-" + scrollWidth + "px"
			}, 1500, "linear");
		});
		$("#hp-tabs-arrow-right").live("mouseup touchend", function() {
			$(".tabs-container").stop();
		});
		// LEFT SCROLL FUNCTION
		$("#hp-tabs-arrow-left").live("mousedown touchstart", function () {
			$(".tabs-container").animate({
				marginLeft: "0px"
			}, 1500, "linear");
		});
		$("#hp-tabs-arrow-left").live("mouseup touchend", function () {
			$(".tabs-container").stop();
		});
	}
	
	function imgResize(){
		var imgHeight = $("#hp-slideshow .rotating-images img").height();
		$("#hp-slideshow").css("height", imgHeight);
	}
	function contentHeight() {
		var hpHeight = $("#hp-content").height();
		$(".hp.column.two").css("height", hpHeight);
		$(window).resize(function() {
			$(".hp.column.two").css("height", "auto");
			var hpHeight = $("#hp-content").height();
			$(".hp.column.two").css("height", hpHeight);
		});
		$("#hp-content .region").resize(function() {
			$(".hp.column.two").css("height", "auto");
			var hpHeight = $("#hp-content").height();
			$(".hp.column.two").css("height", hpHeight);
		});
	}
