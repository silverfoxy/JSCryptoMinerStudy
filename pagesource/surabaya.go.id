$(function(){
  var kapalKiri = Snap.select("#kapal_x5F_kiri"),
      kapalKanan = Snap.select("#kapal_x5F_kanan"),
      bis = Snap.select("#bis"),
      bis2 = Snap.select("#bis1"),
      angkot1 = Snap.select("#angkot1"),
      angkot2 = Snap.select("#angkot2"),
      angkot3 = Snap.select("#angkot3"),
      sepeda = Snap.select("#sepeda"),
      pivots = [[44, 147], [300, 126]]

  animKapalKiri()
  function animKapalKiri(){
    setTimeout(function(){
      kapalKiri.animate({transform: "r" + [1, pivots[0]]}, 3000, mina.easeinout, resetKapalKiri)
    }, 500)
  }
  function resetKapalKiri(){
    kapalKiri.animate({transform: "r" + [-0.5, pivots[0]]}, 3000, mina.easeinout, animKapalKiri)
  }

  animKapalKanan()
  function animKapalKanan(){
    kapalKanan.animate({transform: "r" + [1, pivots[1]]}, 4000, mina.easeinout, resetKapalKanan)
  }
  function resetKapalKanan(){
    kapalKanan.animate({transform: "r" + [-0.1, pivots[1]]}, 5000, mina.easeinout, animKapalKanan)
  }

  bis.transform('t400, 0');
  animbis1()
  function animbis1(){
    bis.animate({transform: 't-800, 0'}, 11000, animbis2)
  }
  function animbis2(){
    bis.animate({transform: 't400, 0'}, 0, animbis3)
  }
  function animbis3(){
    bis.animate({transform: 't400, 0'}, 6000, animbis1)
  }

  bis2.transform('t100, 0');
  animbis21()
  function animbis21(){
    bis2.animate({transform: 't100, 0'}, 4000, animbis22)
  }
  function animbis22(){
    bis2.animate({transform: 't-600, 0'}, 8000, animbis23)
  }
  function animbis23(){
    bis2.animate({transform: 't100, 0'}, 0, animbis21)
  }

  angkot1.transform('t-250, 0');
  animangkot11()
  function animangkot11(){
    angkot1.animate({transform: 't800, 0'}, 6000, animangkot12)
  }
  function animangkot12(){
    angkot1.animate({transform: 't-250, 0'}, 0, animangkot13)
  }
  function animangkot13(){
    angkot1.animate({transform: 't-250, 0'}, 3000, animangkot11)
  }

  angkot2.transform('t-150, 0');
  animangkot21()
  function animangkot21(){
    angkot2.animate({transform: 't600, 0'}, 8000, animangkot22)
  }
  function animangkot22(){
    angkot2.animate({transform: 't-150, 0'}, 0, animangkot23)
  }
  function animangkot23(){
    angkot2.animate({transform: 't-150, 0'}, 3000, animangkot21)
  }

  angkot3.transform('t-250, 0');
  animangkot31()
  function animangkot31(){
    angkot3.animate({transform: 't100, 0'}, 5000, animangkot32)
  }
  function animangkot32(){
    angkot3.animate({transform: 't-250, 0'}, 0, animangkot33)
  }
  function animangkot33(){
    angkot3.animate({transform: 't-250, 0'}, 13000, animangkot31)
  }

  animsepeda31()
  function animsepeda31(){
    sepeda.animate({transform: 't1000, 0'}, 9000, animsepeda32)
  }
  function animsepeda32(){
    sepeda.animate({transform: 't-50, 0'}, 0, animsepeda33)
  }
  function animsepeda33(){
    sepeda.animate({transform: 't-50, 0'}, 8000, animsepeda31)
  }
})
</script>

