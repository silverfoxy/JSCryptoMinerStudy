<script type="text/javascript">

$(function(){
	$(window).on('load',function(){
		penla_slider();
		item_slide();
	});
});
	
	function penla_slider(){
		 var slick = $('.penla_slider').slick({
			infinite: true,
			edgeFriction: 0,
			initialSlide: 0,
			slidesToShow: 3,
			slidesToScroll: 1,
			arrows: true,//左右のナビゲーションは表示させるか
			swipe: true,
			autoplay: true, //自動再生させるか
			//autoplay: false, //自動再生させるか
			speed: 2000,
			touchMove: true,
			dots: false,
			prevArrow: '<a class="slick-prev"><img src="images/left.png" alt="Previous" style="width: 100%;"></a>',
      	    nextArrow: '<a class="slick-next"><img src="images/right.png" alt="Next" style="width: 100%;"></a>',

		});
	}
	
	function item_slide(){
		 var slick = $('.item_slide').slick({
			infinite: true,
			edgeFriction: 0,
			initialSlide: 0,
			slidesToShow: 1,
			slidesToScroll: 1,
			fade: true,
			arrows: false,//左右のナビゲーションは表示させるか
			swipe: false,
			autoplay:true, //自動再生させるか
			//autoplay: false, //自動再生させるか
			speed: 500,
			touchMove: false,
			dots: false,
		});
	}

</script>

