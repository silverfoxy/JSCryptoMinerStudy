	float:left;
	text-align:center;
	position:relative;
	padding: 11px 10px;
	margin-right: 3px;
	border:none;
}

#menu > li {
	border: 1px solid #bbbbbb;
	border-bottom: none;
	background: #d3d3d3;
	padding: 11px 10px;
	
	/* Background color and gradients */
	background: #dddddd;
	background: -moz-linear-gradient(top, #dddddd, #cccccc);
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#dddddd), to(#cccccc));

	/* Rounded corners */
	-moz-border-radius: 5px 5px 0px 0px;
	-webkit-border-radius: 5px 5px 0px 0px;
	border-radius: 5px 5px 0px 0px;
}

#menu li:hover {
	border: 1px solid #2d4c82;
	padding: 11px 10px;

	/* Background color and gradients */
	background: #3961A2;
	background: -moz-linear-gradient(top, #4c79c1, #3961A2);
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#4c79c1), to(#3961A2));

	/* Rounded corners */
	-moz-border-radius: 5px 5px 0px 0px;
	-webkit-border-radius: 5px 5px 0px 0px;
	border-radius: 5px 5px 0px 0px;
}

#menu li a {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
