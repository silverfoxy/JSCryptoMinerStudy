<body class="primary-app flex-parent__column">
	<!-- Google Tag Manager (noscript) -->
	<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-559ZMHF"
	height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
	<!-- End Google Tag Manager (noscript) -->
	<script type="text/javascript">
		if( window.location.pathname === '/' || window.location.pathname === '/home' ){
			document.getElementsByTagName( 'body' )[0].className += ' home';
		}
	</script>
	<!-- Header -->
	<div id="messageContainer" class="alert-message"></div>
	<header id="header" class="header flex-parent__column">
		<div class="width-wrapper header__container flex-child__fill flex-parent__column">
			<div class="header__main flex-child__auto flex-parent__wrap">
				<a href="/" title="BookScouter" class="header__logo flex-child__auto" id="BookScouterLogo">
					<img src="/dist/images/logos/bks_logo.svg" alt="BookScouter logo">
				</a>
				<div class="header__icon toggle--menu icon__container flex-child__auto"><i class="icon--menu"></i></div>
				<div class="header__icon toggle--search icon__container flex-child__auto"><i class="icon--search"></i></div>
				<div class="header__cta flex-parent flex-child collapsable collapsed">
					<a class="cta__login flex-parent__center-both flex-child__50 login action" href="#"><i class="icon--login"></i>Login</a>
					<a class="cta__register btn btn--accent flex-child__50 register action" href="#">Register</a>
				</div>
				<div class="header__menu flex-child__auto collapsable collapsed">
					<ul class="menu menu--primary flex-parent__wrap">
						<li class="menu__item menu__item--primary flex-child__50"><a href="/vendors" class="menu__link menu__link--primary">Vendors</a></li>
						<li class="menu__item menu__item--primary flex-child__50"><a href="/tools" class="menu__link menu__link--primary">Tools</a></li>
						<li class="menu__item menu__item--primary flex-child__50"><a href="/tips" class="menu__link menu__link--primary">Tips</a></li>
						<li class="menu__item menu__item--primary flex-child__50"><a href="/blog" class="menu__link menu__link--primary">Blog</a></li>
						<li class="menu__item menu__item--primary flex-child__50"><a href="/contact" class="menu__link menu__link--primary">Contact</a></li>
					</ul>
				</div>
			</div>
			<div class="header__search collapsable collapsed flex-child__fill">
				<h1 class="hero__title"><strong>Buy and Sell</strong> your textbooks for the best price</h1>
				<div id="searchContainer" class="input__wrapper input__wrapper--search">
					<i class="icon--search"></i>
					<input type="text" class="input--text input--search" placeholder="Search by ISBN Number...">
					<div tabindex=0 class="select select--search" data-select="Sell">
					    <i class="icon--carat"></i>
					    <div class="options options--search">
					        <div class="option option--search selected" data-option="Sell">Sell</div>
					        <div class="option option--search" data-option="Buy">Buy</div>
							<div class="option option--search" data-option="Rent">Rent</div>
					    </div>
					</div>
					<button tabindex=0 type="submit" class="btn btn--accent search__btn">Search</button>
				</div>
			</div>
		</div>
		<div class="header__instructions flex-child__auto">
			<div class="instruction__container flex-parent__center-main">
				<div class="instruction">
