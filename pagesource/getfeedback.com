    var mixpanelTryCounter = 0;
    var mixpanelInterval = window.setInterval(function() {
      mixpanelTryCounter++;
      if (window.mixpanel && window.mixpanel.get_property) {
        window.clearInterval(mixpanelInterval);
        gf('setParameters', {'mixpanel_id': window.mixpanel.get_property('MixpanelID')});
        gf('setParameters', {'initial_referrer': window.mixpanel.get_property('$initial_referrer')});
      } else if (mixpanelTryCounter > 5) {
        window.clearInterval(mixpanelInterval);
        gf('setParameters', {'mixpanel_id': 'mixpanel was not loaded'});
        gf('setParameters', {'initial_referrer': 'mixpanel was not loaded'});
      }
    }, 1000);
  }
</script>

</body>
