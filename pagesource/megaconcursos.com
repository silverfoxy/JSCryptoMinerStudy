        jQuery("#btnEnviar").on('click', function () {

            var txNome = jQuery("#varNome").val();
            var txEmail = jQuery("#varEmail").val();
            var txTelefone = jQuery("#telefone").val();
            var areas = [];
            jQuery('input.group1:checkbox:checked').each(function () {
                areas.push(this.value);
            });

            var checado = jQuery("input.group1:checkbox:checked").length > 0;

            if(txNome == "")
            {
                retorno = ''+
                    '<div class="col-md-12"> '+
                    '    <div class="alert alert-danger alert-dismissible" role="alert"> '+
                    '        <strong>Ocorreu um erro!</strong> Informe seu nome.'+
                    '    </div> '+
                    '</div>';
                jQuery('#loadingCadEmailRetorno1').html(retorno);
                jQuery('#loadingCadEmailRetorno1').show();
            }
            else if(txEmail == "")
            {
                retorno = ''+
                    '<div class="col-md-12"> '+
                    '    <div class="alert alert-danger alert-dismissible" role="alert"> '+
                    '        <strong>Ocorreu um erro!</strong> Informe seu e-mail válido.'+
                    '    </div> '+
                    '</div>';
                jQuery('#loadingCadEmailRetorno1').html(retorno);
                jQuery('#loadingCadEmailRetorno1').show();
            }
            else if((txTelefone === "") || (txTelefone === "99-99999-9999") || (txTelefone === "88-88888-8888") || (txTelefone === "77-77777-7777")
                || (txTelefone === "66-66666-6666") || (txTelefone === "55-55555-5555") || (txTelefone === "44-44444-4444") || (txTelefone === "33-33333-3333")
                || (txTelefone === "22-22222-2222") || (txTelefone === "11-11111-1111") || (txTelefone === "00-00000-0000"))
            {
                retorno = ''+
                    '<div class="col-md-12"> '+
                    '    <div class="alert alert-danger alert-dismissible" role="alert"> '+
                    '        <strong>Ocorreu um erro!</strong> Informe seu celular.'+
                    '    </div> '+
                    '</div>';
                jQuery('#loadingCadEmailRetorno1').html(retorno);
                jQuery('#loadingCadEmailRetorno1').show();
            }
            else if(!checado)
            {
                retorno = ''+
                    '<div class="col-md-12"> '+
                    '    <div class="alert alert-danger alert-dismissible" role="alert"> '+
                    '        <strong>Ocorreu um erro!</strong> selecione uma área de atuação.'+
                    '    </div> '+
                    '</div>';
                jQuery('#loadingCadEmailRetorno1').html(retorno);
                jQuery('#loadingCadEmailRetorno1').show();
            }
            else
            {
                var dataFields = {'nome': txNome, 'telefone': txTelefone, 'areas': areas, 'email': txEmail};

                jQuery.ajax({
                    url: "https://megaconcursos.com/wp-content/themes/megaconcursos2015/cadastro_email_novo.php",
                    type: "POST",
                    data: dataFields,
                    dataType: "html"
                })
                    .done( function( data )
                    {
                        var json = jQuery.parseJSON(data);
                        jQuery.each(json[0], function(key, value){
                            retorno = ''+
                                '<div class="col-md-12"> '+
                                '    <div class="alert alert-'+ key +' alert-dismissible" role="alert"> '+
                                '        <strong>'+ value +'</strong> '+
                                '    </div> '+
                                '</div>';
                            jQuery('#loadingCadEmailRetorno1').html(retorno);
                            jQuery('#loadingCadEmailRetorno1').show();
                        });
                    })
                    .fail( function( jqXHR, textStatus )
                    {
                        retorno = ''+
                            '<div class="col-md-12"> '+
                            '    <div class="alert alert-danger alert-dismissible" role="alert"> '+
                            '        <strong>Ocorreu um erro!</strong> Por favor tente novamente. '+
                            '    </div> '+
                            '</div>';
                        jQuery('#loadingCadEmailRetorno1').html(retorno);
                        jQuery('#loadingCadEmailRetorno1').show();
                    });
            }

            return false;
        });
    </script>

