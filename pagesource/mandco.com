                                     'affiliation': 'M&Co TEST',     // To be corrected for LIVE production
                                     'revenue': data.details.customerTotalPrice, //Total order price in customer's local currency
                                     'shipping': data.details.customerDiscountedShippingPrice, // Shipping price in customer's local currency
                                     'tax': data.details.customerTotalVAT, // Tax total in customer's local currency
                                     'currency': data.details.customerCurrency   // Customer's local currency (ISO 4217)
                                    });
              
                                    for(i=0;i<data.details.products.length;i++){ 
                                                            ga('ecommerce:addItem', {
                                                                                    'id': data.OrderId,                                                      
                                                                                    'name': data.details.products[i].name,                         //The product name
