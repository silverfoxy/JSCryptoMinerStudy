                                                                                    'price': data.details.products[i].customerPrice,               //The product price in the customer's local currency
                                                                                    'quantity': data.details.products[i].quantity,                 //The requested product quantity
                                                                                    'currency': data.details.customerCurrency                                                      //The request customer currency code
                                                                        });
                                     }        
                                    ga('ecommerce:send');						
									
					if (typeof AWIN != "undefined" && typeof AWIN.Tracking != "undefined") {
						AWIN.Tracking.Sale = {};
						AWIN.Tracking.Sale.amount = parseFloat(data.details.totalPrice).toFixed(2);
