<script type="text/javascript">
(function() {
	
 	var PeeriusCallbacks = window.PeeriusCallbacks = {},
 		peeriusLang = "default".replace("_", "-").toLowerCase(),
 		uv = window.universal_variable,
 		bi = window.qubitPeeriusBasketInfo,
 		oi = window.qubitPeeriusOrderInfo;
 		
	PeeriusCallbacks.smartRecs = {};
	
	var urlMap = {
		"search" : "searchresults",
		"storefront" : "home",
		"cart" : "basket",
		"product" : "product",
		"checkout" : "checkout",
		"orderconfirmation" : "order",
		"error"	: "error"
	};

	var currentPagePeerius = urlMap[qubitPeeriusPageType.type.toLowerCase()];
	if (typeof currentPagePeerius == 'undefined') {
		currentPagePeerius = 'other';
	}

	PeeriusCallbacks.track = {
		"type": currentPagePeerius,
		lang: (peeriusLang == "default") ? "en-gb" : peeriusLang,
		channel: "web",
		recContent :"refCodeOnly"
	};
	
	if (uv.hasOwnProperty("user")) {
		var peeriusCustomerEmail = "";
		if (peeriusCustomerEmail != '') {
			uv.user.email = peeriusCustomerEmail;
		}
		if (uv.user.name != '' && uv.user.email != '' ){
			PeeriusCallbacks.track.user = {
				"name": uv.user.name,
				"email": uv.user.email
			}
		}
	}
	
	// --------------------------------------------------- PRODUCT ---------------------------------------------------
	if (currentPagePeerius == 'product') {
		
	
	// --------------------------------------------------- SEARCH ---------------------------------------------------
	} else if (currentPagePeerius == 'searchresults' || currentPagePeerius == 'error') {
		
			var searchTerm = '';
			if (searchTerm) {
				// WE ARE IN SEARCH SCREEN OR NOT FOUND SEARCH PAGE

				PeeriusCallbacks.track.searchResults = {};
				PeeriusCallbacks.track.searchResults.term = searchTerm;
				
				var searchResults = [];
				jQuery("ul#search-result-items .product-tile").each(function( index ) {					
					searchResults.push({ refCode : jQuery(this).attr("refcode-id")});
				});
				PeeriusCallbacks.track.searchResults.results = searchResults;
			} else {
			// WE ARE IN CATEGORY SCREEN
				PeeriusCallbacks.track.type = "category";

				var peeriusBreadcrumbs = [];
				jQuery(".breadcrumb>div>a").each(function( index ) {
					if (index != 0) { // skip the "Home" breadcrumb
						peeriusBreadcrumbs.push(jQuery(this).attr("title"));
					}
				});
				
				PeeriusCallbacks.track.category = peeriusBreadcrumbs.join('>');
			
			}
			
			if (currentPagePeerius == 'error') {
			
				PeeriusCallbacks.track.type = "searchresults";
				
				PeeriusCallbacks.track.searchResults = {};
				PeeriusCallbacks.track.searchResults.term = (searchTerm) ? searchTerm : '';
				PeeriusCallbacks.track.searchResults.results = [];
			}
	
	// --------------------------------------------------- BASKET ---------------------------------------------------
	} else if (currentPagePeerius == 'basket') {
		// peeriusBasket created in minicart template
		PeeriusCallbacks.track.basket = bi.peeriusBasket;

	// --------------------------------------------------- CHECKOUT ------------------------------------------------
	} else if (currentPagePeerius == 'checkout') {
	
		// track checkout as 'other' page when basket is empty i.e. for login page
		if (jQuery.isEmptyObject(bi)) {
		
			PeeriusCallbacks.track.type = "other";
		
		} else {

			PeeriusCallbacks.track.type = "checkout";
			// peeriusBasket created in minicart template
			PeeriusCallbacks.track.checkout = bi.peeriusBasket;
	
			//checkoutBasketSubtotal, checkoutBasketShipping, checkoutBasketTotal created in minicart template
			PeeriusCallbacks.track.checkout.subtotal = bi.checkoutBasketSubtotal;
			PeeriusCallbacks.track.checkout.shipping = bi.checkoutBasketShipping;
			PeeriusCallbacks.track.checkout.total    = bi.checkoutBasketTotal;
		}

	// --------------------------------------------------- ORDER ---------------------------------------------------
	} else if(currentPagePeerius == 'order') {
		PeeriusCallbacks.track.type = "order";
		//orderObjectPeerius created in confirmation template
		PeeriusCallbacks.track.order = oi.orderObjectPeeris;
		
	}
	
	// --------------------------------------------------- CALLBACK -------------------------------------------------
	PeeriusCallbacks.smartRecs = function (args) {
		var peeriusObject = args[0],
			productsID = [],
			productsMap = {};

		if (!peeriusObject || !peeriusObject.recs) {
			return;
		}
		
	 	for (var i = 0; i < peeriusObject.recs.length; ++i) {
	 		var rec = peeriusObject.recs[i];
	 		
	 		productsID.push(rec.refCode);
	 		
	 		var masterProductId = (rec.refCode.length > 7) ? rec.refCode.substring(0, 7) : rec.refCode;
	 		productsMap[masterProductId] = rec.id;
	 	}
	 	
	 	var productsIDStringValue = productsID.join(",");
		var url = "/on/demandware.store/Sites-mandco-Site/default/Peerius-GetProducts?ids=" + productsIDStringValue;
		
		var onProductsReceive = function (data) {

			if (data && data != null && data.hasOwnProperty('products')) {
				data.products.forEach(function(productObj) {
					productObj.peeriusId = productsMap[productObj.masterProductId];
				});
				
				app.renderRecs(data);
			}
		}
		
		// Get products request
		$.ajax({
			async: true,
			dataType: 'json',
			type: 'GET',
			url: url,
			success: onProductsReceive
		});
		
	}

   	var availablePeeriusTypes = ["home", "category", "searchresults", "product", "basket", "checkout", "order"];
	if (availablePeeriusTypes.indexOf(PeeriusCallbacks.track.type) == -1) {
		PeeriusCallbacks.track.type = "other";
	}
	
})();
</script>


