<script src="js/libs/js.cookie.js"></script>
<script src="js/libs/jquery-1.11.0.min.js"></script>
<script>

	// glitch text

	var things = ['coding','knitting','drawing','drawing','drawing','drawing','drawing','designing','planning','writing','writing','writing','writing','writing','writing','writing','programming','programming','programming','programming','programming','concluding','programming','thinking','painting','painting','painting','painting','painting','sewing','sketching','ruminating','deliberating','pondering','contemplating','abstracting','abstracting','abstracting','abstracting','abstracting','abstracting','optimising','optimising','optimising','optimising','optimising','optimising','refactoring','refactoring','refactoring','objectifying','simplifying','decoupling','debugging','debugging','debugging','debugging','debugging','debugging','configuring','streamlining','searching','tweaking','editing'];
	var junk = ['#','@','%','*','&amp;','&lt;','&gt;','_','=','+','[',']','|','-','!','?','X'];

	function randomInt(min, max)
	{
		return Math.round(min + (Math.random() * (max-min)));
	}

	function tick()
	{
		var txt = things[randomInt(0, things.length-1)];
		var chars = txt.split('');
		var glitch = randomInt(0, 3);
		for (var i = 0; i < glitch; i++)
		{
			chars[randomInt(0, chars.length-1)] = junk[randomInt(0, junk.length-1)];
		}
		txt = chars.join('');
		$('#ing').html(txt);
		window.setTimeout(tick, randomInt(16,400));
	}

	if(document.getElementById('ing') != null) tick();





	// colour theme

	function themeSwitch()
	{
		var linkElement = $('#styleLink');
		var newStyle = (linkElement.attr('href') == 'css/style.css') ? 'css/style-alt.css' : 'css/style.css';
		linkElement.attr('href', newStyle);
		Cookies.set('musicForProgramming_theme', newStyle);
	}

	document.getElementById('themeLink').addEventListener('click', themeSwitch);





	// audio player

	String.prototype.toHHMMSS = function()
	{
		var sec_num = parseInt(this, 10);
		var hours   = Math.floor(sec_num / 3600);
		var minutes = Math.floor((sec_num - (hours * 3600)) / 60);
		var seconds = sec_num - (hours * 3600) - (minutes * 60);
		if (hours   < 10) {hours   = "0"+hours;}
		if (minutes < 10) {minutes = "0"+minutes;}
		if (seconds < 10) {seconds = "0"+seconds;}
		var time    = hours+':'+minutes+':'+seconds;
		return time;
	}

	function AudioPlayer()
	{
		this.timeout = null;
		this.autoUpdate = false;
		this.state = 'UNLOADED';
		this.timeTxt = $('#player_time');
		this.audio = $('#player')[0];
		this.btnPlayPause = $('#player_playpause');
		this.btnStop = $('#player_stop');
		this.btnRew = $('#player_rew');
		this.btnFfw = $('#player_ffw');
	}

	AudioPlayer.prototype.init = function()
	{
		this.audio.onloadedmetadata = this.loaded.bind(this);
		this.audio.load();
	}

	AudioPlayer.prototype.loaded = function()
	{
		this.timeTxt.html((''+this.audio.duration).toHHMMSS());
		this.state = 'STOPPED';
		this.btnPlayPause.addClass('active');
		this.btnPlayPause.click(this.playPause.bind(this));
	}

	AudioPlayer.prototype.playPause = function()
	{
		boop();

		if(this.state == 'STOPPED')
		{
			this.state = 'PAUSED';
			this.btnStop.addClass('active');
			this.btnStop.click(this.stop.bind(this));
			this.btnRew.addClass('active');
			this.btnRew.click(this.rew.bind(this));
			this.btnFfw.addClass('active');
			this.btnFfw.click(this.ffw.bind(this));
		}

		if(this.state == 'PAUSED')
		{
			this.state = 'PLAY';
			this.audio.play();
			this.autoUpdate = true;
			this.update();
			this.btnPlayPause.html('[PAUSE]');
		} else {
			this.state = 'PAUSED';
			this.audio.pause();
			clearTimeout(this.timeout);
			this.autoUpdate = false;
			this.update();
			this.btnPlayPause.html('[RESUME]');
		}
	}

	AudioPlayer.prototype.update = function()
	{
		this.timeTxt.html((''+this.audio.currentTime).toHHMMSS());
		if(this.autoUpdate) this.timeout = setTimeout(this.update.bind(this), 1000);
		if(this.audio.currentTime == this.audio.duration) this.stop();
	}

	AudioPlayer.prototype.rew = function()
	{
		boop();

		this.audio.currentTime = (this.audio.currentTime-30 < 0) ? 0 : this.audio.currentTime-30;
		this.update();
	}

	AudioPlayer.prototype.ffw = function()
	{
		boop();

		this.audio.currentTime = (this.audio.currentTime+30 > this.audio.duration-1) ? this.audio.duration-1 : this.audio.currentTime+30;
		this.update();
	}

	AudioPlayer.prototype.stop = function()
	{
		boop();

		this.state = 'STOPPED';
		this.audio.pause();
		this.audio.currentTime = 0;
		clearTimeout(this.timeout);
		this.autoUpdate = false;
		this.update();
		this.btnPlayPause.html('[PLAY]');
		this.btnStop.off('click');
		this.btnStop.removeClass('active');
		this.btnRew.off('click');
		this.btnRew.removeClass('active');
		this.btnFfw.off('click');
		this.btnFfw.removeClass('active');
	}

	var player;

	if(document.getElementById('player') != null)
	{
		player = new AudioPlayer();
		player.init();
	}

	// beep

	var context = new(window.AudioContext || window.webkitAudioContext || false);
	var oscillator = null;
	var gain = null;

	function boop()
	{
		if(!context) return; // web audio not supported
		oscillator = context.createOscillator();
		oscillator.type = "square";
		oscillator.frequency.value = 5555;
		gain = context.createGain()
		gain.gain.value = 0.125;
		oscillator.connect(gain);
		gain.connect(context.destination);
		oscillator.start(context.currentTime);
		oscillator.stop(context.currentTime + 0.025);
	}

	// keyboard control
	window.addEventListener("keydown", onKeyDown, false);
	var prevEpisode = $('.multi-column .selected').prev().prev().attr('href');
	var nextEpisode = $('.multi-column .selected').next().next().attr('href');
	$('#episodes .selected')
	function onKeyDown(event)
	{
		var keyCode = event.keyCode;
		switch (keyCode)
		{
			case 32:
				// space
				if(player)
				{
					if(player.state == 'STOPPED' || player.state == 'PAUSED' || player.state == 'PLAY')
					{
						player.playPause();
					}
				}
				break;

			case 37:
				// left
				if(prevEpisode) window.location.href = '/'+prevEpisode;
				break;

			case 39:
				// right
				if(nextEpisode) window.location.href = '/'+nextEpisode;
				break;
		}
	}



</script>




