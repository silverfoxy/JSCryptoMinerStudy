                    $('.typebox-vcard').show();
                    $(this).css({ opacity: '0.4' });
                });
                $('.wa-message').keyup(function () {
                    $('.wa-message-c').html(255 - parseInt($(this).val().length));
                });
                $('.anonyemrg').change(function () {
                    var anemr = $(this).val();
                    if (anemr == 'anonymous') {
                        $('.emergency_name').hide();
                    } else if (anemr == 'emergency') {
                        $('.emergency_name').show();
                    }
                });
                $("input[type=checkbox]").bootstrapSwitch({
                    onColor: 'primary',
                    size: 'small'
                });
                $('input[type=file]').change(function () {
                    var msgtype = $('.msg-type').val();
                    var file = $(this)[0].files[0];
                    checkfile(file, msgtype);

                });
                $('.send-message').click(function () {
                    if ($('#country').val() == '0') {
                        $.growl.error({ message: 'Please choose a country!' });
                        return false;
                    }
                    if ($('#number').val().length < 5) {
                        $.growl.error({ message: 'Number to short!' });
                        return false;
                    }
                    else if ($('#number').val().length > 15) {
                        $.growl.error({ message: 'Number to long!' });
                        return false;
                    }

                    var msgtype = $('.msg-type').val();
                    if (msgtype == 'image') {
                        var file = $('.file-image')[0].files[0];
                    }
                    else if (msgtype == 'video') {
                        var file = $('.file-video')[0].files[0];
                    }
                    else if (msgtype == 'audio') {
                        var file = $('.file-audio')[0].files[0];
                    }
                    else if (msgtype == 'vcard') {
                        var file = $('.file-vcard')[0].files[0];
                    }



                    if (msgtype == 'message' || msgtype == 'location') {
                        if (msgtype == 'message') {
                            if ($('#message').val().length < 10) {
                                $.growl.error({ message: 'Message to short!' });
                                return false;
                            }
                            if ($('#message').val().length > 255) {
                                $.growl.error({ message: 'Message to long!' });
                                return false;
                            }
                        }
                        $('.message-form').submit();
                    }
                    else {
                        if (checkfile(file, msgtype)) {
                            $('.message-form').submit();
                        }
                    }
                });
                //setInterval('moveCar();', 15);
            });
        </script>


    </div>
