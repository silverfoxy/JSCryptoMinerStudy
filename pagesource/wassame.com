                    }
                    else if (RLcar == 'moto') {
                        RLcar = 'bus';
                        $('.car2').html('<img alt="WhatsApp car" class="car" src="assets/cars/bus.gif">');
                    }

                    $('.car2').css({ left: screen.width });
                }
