        <script>
            var vehicle = 'car1';
            var RLcar = 'bus';
            function checkfile(file, msgtype) {
                if (!file.size) return false;
                if (!file.type) return false;
                var split = file.type.split('/');
                if ((split[0] != 'image' && msgtype == 'image') || (split[0] != 'video' && msgtype == 'video') || (split[0] != 'audio' && msgtype == 'audio')) {
                    $.growl.error({ message: "Wrong file type!" });
                    return false;
                }
                else if (msgtype == 'image') {
                    if (file.size > (5 * (1024 * 1024))) {
                        $.growl.error({ message: 'Image to big!' });
                        return false;
                    }
                }
                else if (msgtype == 'video') {
                    if (file.size > (20 * (1024 * 1024))) {
                        $.growl.error({ message: 'Video to big!' });
                        return false;
                    }
                }
                else if (msgtype == 'audio') {
                    if (file.size > (5 * (1024 * 1024))) {
                        $.growl.error({ message: 'Audio to big!' });
                        return false;
                    }
                }
                else if (msgtype == 'vcard') {
                    if (file.size > (1 * (1024 * 1024))) {
                        $.growl.error({ message: 'Vcard to big!' });
                        return false;
                    }
                }
                return true;
            }
            function hideBoxes() {
                $.each($('.typebox'), function (i, data) {
                    $(data).hide();
                });
                $.each($('.type-icon'), function (i, data) {
                    $(data).css({ opacity: '1' });
                });
            }
            function resize() {
                $('.boxer2').css({ height: parseInt($('.boxer1').height()) + 20 });
            }
            function moveCar() {
                var speed = 2;
                left = ($('.car1').position().left) + speed;
                if (left < screen.width) {
                    $('.car1').css({ left: left });
                }
                else {
                    if (vehicle == 'car4') {
                        vehicle = 'car1';
                        $('.car1').html('<img alt="WhatsApp car" class="car" src="assets/cars/car1.gif">');
                    }
                    else if (vehicle == 'car1') {
                        vehicle = 'car2';
                        $('.car1').html('<img alt="WhatsApp car" class="car" src="assets/cars/car2.gif">');
                    }
                    else if (vehicle == 'car2') {
                        vehicle = 'car3';
                        $('.car1').html('<img alt="WhatsApp car" class="car" src="assets/cars/car3.gif">');
                    }
                    else if (vehicle == 'car3') {
                        vehicle = 'car4';
                        $('.car1').html('<img alt="WhatsApp car" class="car" src="assets/cars/car4.gif">');
                    }
                    $('.car1').css({ left: '-200px' });
                }

                left = ($('.car2').position().left) - (speed - 0.5);
                if (left > -200) {
                    $('.car2').css({ left: left });
                }
                else {
                    if (RLcar == 'bus') {
                        RLcar = 'car'
                        $('.car2').html('<img alt="WhatsApp car" class="car" src="assets/cars/car.gif">');
