<script language="JavaScript">
function toggleSearchDiv()
{
	var div = document.getElementById('index_search');
	var elt = document.getElementById('bt_search_incit');
	if ( div.style.display == 'none' ) {
		div.style.display = 'block';
		elt.setAttribute('class','fa fa-chevron-circle-up');
	} else {
		div.style.display = 'none';
		elt.setAttribute('class','fa fa-chevron-circle-down');
	}
}

var lookStarted = false;

function callStarted(str,src)
{
	if ( isExtId(str) ) joinRoomX(str);
	else {
		window.location.href="started.php?src=" + src + "&q=" + encodeURIComponent(str);
	}
}

function lookForVideo()
{
	if ( lookStarted ) return;
	lookStarted = true;
	document.getElementById('video_url').style.cursor='wait';
	document.getElementById('video_url_img').style.cursor='wait';
	callStarted(document.getElementById('video_url').value,'youtube');
}

function searchVideos(src)
{
	if ( ! src ) src = 'youtube';
	callStarted(document.getElementById('video_url').value, src);
}

var rgExtId = new RegExp('^\\d{2,6}-\\d{2,6}-\\d{2,6}$');

function joinRoomByExtId()
{
	joinRoom(document.getElementById('ext_id'));
}

function isExtId(str)
{
	var match = rgExtId.test(str);
	if ( match && str.length == 12  ) return true;
	return false;
}

function joinRoom(ext_id)
{
	//mycircletv.consoleLog('joinRoom : ' + ext_id.value);
	if ( ! isExtId(ext_id.value) ) {
		document.getElementById('search_err1').innerHTML = 'This code is not Ok';
		ext_id.focus();
		return;
	}
	joinRoomX(ext_id.value);
}

function joinRoomX(ext_id)
{
	if ( typeof(_gaq) != 'undefined' ) _gaq.push(['_trackEvent', 'misc', 'join', 'by_code', 1, true]);
	xmlhttp=mycircletv.createXHR();
	xmlhttp.open("GET","/check_ext_id.php?ext_id=" + ext_id, true);
	xmlhttp.onreadystatechange=checkComplete;
	xmlhttp.send();
	return true;

	function checkComplete() {
		lookStarted = false;
		if (xmlhttp.readyState==4 && xmlhttp.status==200){
			var projpriv = JSON.parse(xmlhttp.responseText);
			if (projpriv.id != 0){
				window.location.href="/" + ext_id;
			}
			else
			{
		document.getElementById('search_err1').innerHTML = 'This code is unknown.';
			}
		}
	}
}

var curFeature;
var addthisdiv;

function customizeAddthis()
{
	addthisdiv = document.getElementById('at4-share');
	if ( addthisdiv == null || addthisdiv.nodeName != 'DIV' ) setTimeout(customizeAddthis,250);
	else {
		addthisdiv.style.left = '30px';
		addthisdiv.style.top = '445px';
		setTimeout(shareIncit,4000); 
	}
}

function shareIncit()
{
	document.getElementById('share_incit').style.display = 'block';
}


$(document).ready(function(e) {	
	document.getElementById(curFeature = 'feature_0').setAttribute('class','lkfeature_on');
	document.getElementById('div_' + curFeature).style.display = 'block';
	$("#sample_rooms").mCustomScrollbar({
			axis:"x",
			theme:"dark-3"
		});
});

function switchFeature(event)
{
	var elt = event.target;
	if ( elt.getAttribute('class') == 'lkfeature_on' ) return;
	document.getElementById(curFeature).setAttribute('class','lkfeature_off');
	document.getElementById('div_' + curFeature).style.display = 'none';
	curFeature = elt.id;
	elt.setAttribute('class','lkfeature_on');
	document.getElementById('div_' + curFeature).style.display = 'block';
}
</script>