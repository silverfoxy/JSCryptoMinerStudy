function switchToLanguage(lang)
{
	var uri = '/';
	var re = new RegExp('/en/');
	if ( re.test(uri) ) uri = uri.replace(re,'/' + lang + '/');
	re = new RegExp('lang=en');
	if ( re.test(uri) ) uri = uri.replace(re, 'lang=' + lang);
	else {
		if ( uri.indexOf('?') > 0 ) uri += '&';
		else uri += '?';
		uri += 'lang=' + lang;
	}
	if ( typeof(_gaq) != 'undefined' ) _gaq.push(['_trackEvent', 'lang', 'en', lang, 1, true]);
	window.location= uri + window.location.hash;
}

var validInscr_once = true;

function validInscrError(msg)
{
	document.getElementById('err2').innerHTML = msg;
	validInscr_once = true;
}

function validInscr()
{
	if ( ! validInscr_once ) return;
	//mycircletv.consoleLog('validInscr');
	validInscr_once = false;
	var email = document.getElementById('email');
	var vnom = document.getElementById('nom').value;
	if ( ! vnom ) {
		validInscrError("Please enter a name");
		return;
	}
	if ( ! email.value || ! isEmail(email) ) {
		validInscrError("The email address you entered does not seem valid");
		return;
	}
	var pwd = document.getElementById('passwd1').value;
	if ( pwd.length == 0 || pwd != document.getElementById('passwd2').value ) {
		validInscrError("The passwords you entered does not match");
		return;
	}
	if ( ! document.getElementById('chkterms').checked ) {
		validInscrError("You have to accept the terms of use to go on");
		return;
	}
   var req = new XMLHttpRequest();
   var cpte = {
		nom: vnom,
		email : email.value,
		password: pwd,
		//stay_connected: document.getElementById('stay_connected').checked ? 1:0
		};
   req.open('GET', '/newcpte.php?cpte=' + encodeURIComponent(JSON.stringify(cpte)) +
		'&t=' + Math.random().toString().substring(2), true);
   req.onreadystatechange = function (aEvt) {
	  if (req.readyState == 4) {
		 if(req.status == 200) {
			validInscr_once = true;
			//console.log('newcpte : ' + req.responseText);
			if ( req.responseText == 'ok' ) {
				document.getElementById("err2").innerHTML = '';
				openAlert('Thank you for joining.', function() {
						toggleLoginDiv('login');
						document.getElementById('login').value = document.getElementById('email').value;
						document.getElementById('password').value = document.getElementById('passwd1').value;
					});
				//window.location.href = "/en/login?new=1";
			} else document.getElementById("err2").innerHTML = req.responseText;
		 } else {
			//console.log("Erreur (" + req.status + ") on " + url + "\n" + req.responseText);
		 }
	}
	};
	req.send(null);
}

function toggleLoginDiv(cmd)
{
	var div = document.getElementById('connect_widget');
	if ( cmd == 'close')   {
		div.style.display = 'none';
		return;
	}
	if ( div.style.display != 'inline-block' ) {
		// mycircletv.consoleLog('toggleLoginDiv display block');
		div.style.display = 'inline-block';
	}
	if ( cmd == 'register' ) {
		document.getElementById('div_register').style.display = 'inline-block';
		document.getElementById('div_login').style.display = 'none';
	} else {
		document.getElementById('div_register').style.display = 'none';
		document.getElementById('div_login').style.display = 'inline-block';
	}
}

function getNewPwd() {

	xmlhttp=mycircletv.createXHR();
	var login = document.getElementById("login").value;
	xmlhttp.open("GET","/getpwd.php?login=" + login, true);
	xmlhttp.onreadystatechange= function() {
		if (xmlhttp.readyState==4 && xmlhttp.status==200){
			document.getElementById("err").innerHTML = xmlhttp.responseText;
		}
	};
	xmlhttp.send();
}

function logout() {

	xmlhttp=mycircletv.createXHR();
	xmlhttp.open("GET","/logout.php?json=1", true);
	xmlhttp.onreadystatechange=logoutComplete;
	xmlhttp.send();
	return false;

	function logoutComplete() {
		if (xmlhttp.readyState==4 && xmlhttp.status==200){
			if ( window.location.href.indexOf('account.php') > 0 ) {
				window.location = '/';
				return;
			}
			var div = document.getElementById('div_logon');
			div.style.display = 'none';
			div = document.getElementById('div_logoff');
			div.style.display = 'inline-block';
		}
	}
}


function checkLogin() {

	xmlhttp=mycircletv.createXHR();
	var login = document.getElementById("login").value;
	var password = document.getElementById("password").value;
	xmlhttp.open("GET","/login.php?login=" + login + '&password=' + password + (document.getElementById("stay_connected").checked ? '&stay_connected=1':''), true);
	xmlhttp.onreadystatechange=loginComplete;
	xmlhttp.send();
	return false;

	function loginComplete() {
		if (xmlhttp.readyState==4 && xmlhttp.status==200){
			finishLogin(xmlhttp.responseText);
		}
	}
}

function finishLogin(resp)
{
	// mycircletv.consoleLog('finishLogin : ' + resp);
	var newuser = JSON.parse(resp);
	if ( newuser.id == 0 ) {
		document.getElementById("err").innerHTML = "Verify email / password";
		return;
	}
	var div = document.getElementById('div_logoff');
	div.style.display = 'none';
	document.getElementById('div_logon').style.display = 'inline-block';;
	document.getElementById('div_logon_uname').innerHTML = newuser.user_name;
	if ( typeof(mycircletv.notifLogin) == 'function') mycircletv.notifLogin(newuser);
	toggleLoginDiv('close');
	if ( newuser.msgperso ) {
		document.getElementById('notif_perso').style.display = 'block';
		document.getElementById('notif_perso_msg').innerHTML = newuser.msgperso;
	}
}

function displayEditField(editf)
{
	mycircletv.consoleLog('displayEditField');
	var child = editf.childNodes[0];
	child.style.display = 'inline-block';
	child.childNodes[0].focus();
}

function disableEditField(editfield)
{
	editfield.childNodes[0].style.display = 'none';
}

function getToStartedFromHeader(src) {
	var div = document.getElementById('mobile_search_div');
	var q;
	if ( div && div.style.display == 'block' ) q=document.getElementById("search_mobile_input").value;
	else {
		div = document.getElementById('div_search_options');
		q=document.getElementById("search_head_input").value;
	}
	//if (q=="Search videos (or paste video link)") s="";
	current_channel = src;
	window.location.href="/started.php?" + (isAutomatic() ? 'ac=1&':'') + "src=" + src + "&q=" + encodeURIComponent(q);
}

