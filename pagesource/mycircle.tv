        elem.attachEvent( "on" + type, eventHandle ); 
    } 
};

mycircletv.createXHR = function(){

	var result=null;
	
	try { // Test pour les navigateurs : Mozilla, Opera...
		result= new XMLHttpRequest();
	}
	catch (Error) {
		try { // Test pour les navigateurs Internet Explorer > 5.0
			result= new ActiveXObject("Msxml2.XMLHTTP");
		}
		catch (Error) {
			try { // Test pour le navigateur Internet Explorer 5.0
				result= new ActiveXObject("Microsoft.XMLHTTP");
			}
			catch (Error) {
				result= null;
			}
		}
	}
	return result;
};

mycircletv.dateHMSX = function(time) {
	if ( time == 0 ) return '00:00';
	return mycircletv.dateHMS(time);
};

mycircletv.dateHMS = function(time) {
	if ( time == 0 ) return '';
	var addZero = function(v) { return v<10 ? '0' + v : v; };
	var t = [];
	var m = Math.floor(time/60);
	var h = Math.floor(m/60);
	if ( h > 0 ) {
		t.push(h);
		m = m%60;
	}
	t.push(addZero(m));
	t.push(addZero(Math.floor(time%60)));
	return t.join(':');
};

