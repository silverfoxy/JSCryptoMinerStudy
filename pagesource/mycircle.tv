function getAutoPub(sdivpub)
{
	var divpub;
	if ( typeof(sdivpub) == 'string' ) divpub = document.getElementById(sdivpub);
	if ( ! divpub ) return;
	var style = window.getComputedStyle(divpub,null);
	var req = new XMLHttpRequest();
	var url = '/srv/autopub.php?r=' + currentRating + '&w=' + style.width + '&h=' + style.height + '&divpub=' + sdivpub;
	req.open('GET', url, true);
	req.onreadystatechange = function (aEvt) {
		if (req.readyState == 4 ) {
			if ( req.status == 200) {
				divpub.innerHTML = req.responseText;
			} else divpub.innerHTML = '';
		}
	}
	req.send(null);
}

