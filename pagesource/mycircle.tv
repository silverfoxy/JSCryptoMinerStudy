function dump(arr,level) {
	var dumped_text = "";
	if(!level) level = 0;
	
	//The padding given at the beginning of the line.
	var level_padding = "";
	for(var j=0;j<level+1;j++) level_padding += "    ";
	
	if(typeof(arr) == 'object') { //Array/Hashes/Objects 
		for(var item in arr) {
			var value = arr[item];
			
			if(typeof(value) == 'object') { //If it is an array,
				dumped_text += level_padding + "'" + item + "' ...\n";
				//dumped_text += dump(value,level+1);
			} else {
				dumped_text += level_padding + "'" + item + "' => \"" + value + "\"\n";
			}
		}
	} else { //Strings/Chars/Numbers etc.
		dumped_text = "===>"+arr+"<===("+typeof(arr)+")";
	}
	return dumped_text;
}

var alertCallback;

function openConfirm(msg,callback)
{
	openAlertX(msg,true,callback);
}

function openAlertX(msg,confirm,callback)
{
	alertCallback = callback;
	var dac = document.getElementById('div_alert_content');
	if ( dac == null ) {
		var da = document.createElement('div');
		da.setAttribute('class', 'modal fade in');
		da.setAttribute('id', 'div_alert');
		dac = document.createElement('div');
		dac.setAttribute('id', 'div_alert_content');
		da.appendChild(dac);
		document.body.appendChild(da);
	}
	dac.innerHTML = msg + '<p style="margin-top:20px"><input type="button" class="submit_button" value="OK" onclick="closeAlert(1)">' +
		(confirm ? '&nbsp;&nbsp;<input type="button" class="submit_button" value="Cancel" onclick="closeAlert(0)">':'') + '</p';
	$('#div_alert').modal('show');
	setTimeout(function() {
		var dac = document.getElementById('div_alert_content');
		mycircletv.consoleLog('openAlertX height : ' + dac.offsetHeight);
		var div = document.getElementById('div_alert');
		div.style.height = (dac.offsetHeight+45) + 'px';
		div.style.overflow = 'hidden';
	}, 200);
}

function openAlert(msg, callback)
{
	openAlertX(msg,false,callback);
}

function closeAlert(cr)
{
	mycircletv.consoleLog('closeAlert');
	$('#div_alert').modal('hide');
	if ( alertCallback ) {
		var callback = alertCallback;
		alertCallback = null;
		callback(cr);
	}
}
