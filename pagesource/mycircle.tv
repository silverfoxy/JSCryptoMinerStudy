mycircletv.getEltOffset = function(el,debug) {
	//console.log('getOffset el : ' + el);
	var _x = 0;
	var _y = 0;
	while( el && !isNaN( el.offsetLeft ) && !isNaN( el.offsetTop ) ) {
		if ( debug ) mycircletv.consoleLog('getOffset el.tagName : ' + el.tagName + ', el.id : ' + el . id + ', el.offsetLeft : ' + el.offsetLeft + ', el.offsetTop : ' + el.offsetTop);
		_x += el.offsetLeft - el.scrollLeft;
		_y += el.offsetTop - el.scrollTop;
		if ( debug ) mycircletv.consoleLog('getOffset x : ' + _x + ', y : ' + _y);
		el = el.offsetParent;
	}
	if ( debug ) mycircletv.consoleLog('getOffset x : ' + _x + ', y : ' + _y);
	return { top: _y, left: _x };
};

