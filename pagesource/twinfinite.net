h1.mvp-arch-head { background: #fff !important; margin-bottom: 0 !important; padding: 1em 0 !important; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; }
h4.mvp-post-bot-head { background: #fff!important; margin-bottom: 0 !important; padding: 1em 0 1em .5em !important; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; }

