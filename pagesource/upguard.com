<script type="text/javascript">
    function removeActive() {
        var active = document.querySelectorAll(".active");
        for (var i = 0; i < active.length; i++) {
            active[i].classList.remove("active");
        }
    }

    function handleNav() {
        removeActive();
        var loc = window.location.pathname;
        var ID;
        switch (loc) {
            case "/":
                ID = "product-header"
                break;
            case "/solutions":
                ID = "product"
                break;
            case "/pricing":
                ID = "pricing"
                break;
            case "/partners":
                ID = "partners";
                break;
            case "/resources":
                ID = "resources";
                break;
            case "/about":
                ID = "about";
                break;
            case "/blog":
                ID = "blog";
                break;
            case "/press":
                ID = "press";
                break;
            case "/breaches/research-guidelines":
                ID = "about-our-research";
                break;
        }
        try {
            var headerLink = document.querySelector(".links > a[href='" + loc + "']");
            headerLink.classList.add("active");
            document.getElementById(ID).classList.add("active");
        } catch (e) {}
    }

    function toggleOverlay() {
        var overlays = document.getElementById("main-nav").querySelectorAll(".overlay");
        for (var i = 0; i < overlays.length; i++) {
            overlays[i].classList.toggle("open");
        }
    }

    function toggleProductDropdown(event) {
        event.target.classList.toggle("active");
        var dropdown = event.target.querySelector("#product-dropdown");
        dropdown.focus();
        dropdown.addEventListener("blur", closeProductDropdown);
        dropdown.addEventListener("click", function(e) {e.stopPropagation();});
    }
    
    function closeProductDropdown() {
        var headerLink = document.getElementById("product-header");
        setTimeout(function() {
            headerLink.classList.remove("active");
        }, 500);
        var dropdown = headerLink.querySelector("#product-dropdown");
        dropdown.removeEventListener("blur", closeProductDropdown);
    }

    (function() {
        document.getElementById("hamburger").addEventListener("click", function() {
            toggleOverlay();
        });
        var closeActions = document.querySelectorAll("#close-overlay");
        for (var i = 0 ; i < closeActions.length ; i++ ) {
            closeActions[i].addEventListener("click", function() {
                toggleOverlay();
            });
        }
    })();
    
    (function() {
        var CHROME = /Chrome/i.test(navigator.userAgent);
        if (CHROME) {
            var el = document.getElementById("only-chrome");
            el.setAttribute("style", "display: -webkit-box; display: -moz-box; display: -ms-flexbox; display: -webkit-flex;");
        }
    })();

    (function() {
        var el = document.getElementById("product-header");
        el.addEventListener("click", toggleProductDropdown);
    })();
    
    handleNav();
    
    window.getBrowser = function() {
        var ua=navigator.userAgent,tem,M=ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || []; 
        if(/trident/i.test(M[1])){
            tem=/\brv[ :]+(\d+)/g.exec(ua) || []; 
            return {name:'IE',version:(tem[1]||'')};
        }
        if(M[1]==='Chrome'){
            tem=ua.match(/\bOPR|Edge\/(\d+)/)
            if(tem!=null)   {return {name:'Opera', version:tem[1]};}
        }
        M=M[2]? [M[1], M[2]]: [navigator.appName, navigator.appVersion, '-?'];
        if((tem=ua.match(/version\/(\d+)/i))!=null) {M.splice(1,1,tem[1]);}
        return {
            name: M[0],
            version: M[1]
        };
    }
</script>

            
            
