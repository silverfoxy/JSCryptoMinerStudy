            <!-- <div class="modal-footer">
              <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div> -->
          </div>


          <!-- Modal content-->
          <div id="emi-calculator" style="display:none" class="modal-content">
            <div class="modal-header">
            <h4 class="modal-title" style="text-align: center;">Want to know your EMI</h4>
              <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
              <div class="EMI-widget" style="margin-top: 0px;">
            <div class="input-amount spacer">
              <div class="form-group">
                <label class="sr-only" for="amount">Amount</label>
                <div class="input-group">
                  <div class="input-group-addon"><i class="fa fa-inr" aria-hidden="true"></i></div>
                  <input type="text" class="form-control" id="amount" pattern="[0-9]+" onclick="getMixpanelEvent()" onkeyup="formatCurrencyTest(this.value)" placeholder="Enter required loan amount" name="loanAmount" maxlength="11">
                </div>
              </div>
            </div>
            <input type="text" id="jh" onchange="this.value=formatCurrency(this.value)" style="display: none;">
            <div id="maxAmountError"></div>
              <div id="amountRequired"></div>
              <div id="amountRequiredError"></div>
            <div id="amountLess"></div>
            <div id="amountMore"></div>
            <div id="amountLessError"></div>
            <div class="info-block">
              <div class="spacer">
                <h6>Your Monthly Indicative EMI * </h6>
                <h5 id="loanCalc">It will calculate based on amount…</h5>
                <h5 id="emiAmount"></h5>
                <p class="actual-emi-info">* Actual EMI will depend on your credit evaluation.</p>
              </div>
              <hr>
              <div class="spacer">
                <h6>Want a different loan duration?</h6>
              </div>
              <div class="spacer">
                <input id="monthSlider" type="text" data-slider-value="6"  data-slider-min="3" data-slider-max="12" data-slider-ticks-labels='["3 months", "12 months"]' onchange="getEmiCalculation();"/>
              </div>
              <div id="loanTenureRequired"></div>
            </div>
            <div class="spacer text-center">
              <a href="http://app.lendingkart.com/lkart/check_eligibility.html#!/home?from=EMICal" class="btn btn-primary" onclick="return submitForm();">Check Eligibility</a>
            </div>
          </div>
