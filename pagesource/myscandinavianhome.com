}
.css-carousel .css-img:nth-child(2) {
-webkit-animation-delay: 5s;
-moz-animation-delay: 5s;
-ms-animation-delay: 5s;
animation-delay: 5s;
}
.css-carousel .css-img:nth-child(3) {
