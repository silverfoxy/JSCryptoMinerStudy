    function toggleCourseComparisonTableRow(el, selector) {
    $el = $(el);
    $el.nextAll(selector).toggle();
    if ($(selector).first().is(':visible')) {
    $el.find('td').css('border-top', '3px solid #bbbbbb');
    $(selector).last().find('td').css('border-bottom', '3px solid #bbbbbb');
    } else {
    $el.find('td').css('border-top', '1px solid #dddddd');
    $(selector).last().find('td').css('border-bottom', '1px solid #dddddd');
    }
    }
