function mobile_sort(orderby){ /*if(window.innerWidth >= 768){return;}*/ if(orderby=="TRENDING" && window.orderedby!='TRENDING'){ localStorage.setItem('title','TRENDING'); window.orderedby="TRENDING"; document.getElementById('trending').style.textDecoration = "underline"; document.getElementById('recent').style.textDecoration = "none"; var elems = document.getElementsByClassName("content_box"); var ad_elems = document.getElementsByClassName("ad_div"); var array = []; for (var i = elems.length >>> 0; i--;) { array[i] = elems[i];} var ad_array = []; for (var i =  0;i < ad_elems.length; i++) { ad_array[i] = ad_elems[i];} array.sort(function(a, b) { return Number(b.getAttribute('data-trendingvalue')) - Number(a.getAttribute('data-trendingvalue'));}); var output = ""; for (var i = 0; i < array.length; i++) { output += array[i].outerHTML; if(i>0 && (i+1)%6==0){ if(ad_array.length>0){output += ad_array.shift().outerHTML;}} } document.getElementById('content_container').innerHTML = output + "<div style='clear:both;'></div>"; for (i = 0; i < window.adDivArray.length; i++) { if(document.getElementById(window.adDivArray[i]).offsetHeight>59){ googletag.pubads().refresh([window.adSlotArray[i]]);}} window.scrollTo(0,0); window.lastWidth=0; layout();/*end of if*/} else if(orderby=="RECENT" && window.orderedby=='TRENDING') { localStorage.setItem('title','RECENT'); window.orderedby="RECENT"; document.getElementById('recent').style.textDecoration = "underline"; document.getElementById('trending').style.textDecoration = "none"; var elems = document.getElementsByClassName("content_box"); var ad_elems = document.getElementsByClassName("ad_div"); var array = []; for (var i = elems.length >>> 0; i--;) { array[i] = elems[i];} var ad_array = []; for (var i =  0;i < ad_elems.length; i++) { ad_array[i] = ad_elems[i];} array.sort(function(a, b) { return Number(b.getAttribute('data-timestamp')) - Number(a.getAttribute('data-timestamp'));}); var output = ""; for (var i = 0; i < array.length; i++) { output += array[i].outerHTML; if(i>0 && (i+1)%6==0){ if(ad_array.length>0){output += ad_array.shift().outerHTML;}} } document.getElementById('content_container').innerHTML = output + "<div style='clear:both;'></div>"; for (i = 0; i < window.adDivArray.length; i++) { if(document.getElementById(window.adDivArray[i]).offsetHeight>59){ googletag.pubads().refresh([window.adSlotArray[i]]);}}window.scrollTo(0,0); window.lastWidth=0; layout();/*end of else*/ }}
