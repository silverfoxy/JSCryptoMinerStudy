$(document).ready(function(){ 
$("ul.thumb li").hover(function() {
	$(this).css({'z-index' : '0'});
	$(this).find('img').addClass("hover").stop()
		.animate({
			marginTop: '0px', 
			marginLeft: '0px', 
			top: '0', 
			left: '0', 
			width: '98px', 
			height: '95px',
			padding: '2px' 
		}, 400);
	
	} , function() {
	$(this).css({'z-index' : '0'});
	$(this).find('img').removeClass("hover").stop()
		.animate({
			marginTop: '0', 
			marginLeft: '0',
			top: '0', 
			left: '0', 
			width: '98px', 
			height: '95px', 
			padding: '2px'
		}, 200);
});
 
	$("ul.thumb li a").mouseover(function() {
		
		var mainImage = $(this).attr("id");  
		$("#main_view img").attr({ src: mainImage });
		return false;		
	});
 
});
  
function tabshowed(r){
var tabs = Array('aus-tab','can-tab','den-tab','nz-tab','hon-tab','ire-tab','sing-tab','uk-tab','usa-tab');
var containers = Array('aus_container','can_container','den_container','nz_container','hon_container','ire_container','sing_container','uk_container','usa_container','click');
for(i=0; i<containers.length ;i++)
{
if(r==containers[i])
{
	document.getElementById(tabs[i]).className=tabs[i]+'_hover'; 	
	document.getElementById(containers[i]).style.display = 'block';
}
else{
	if(document.getElementById(tabs[i]))
		document.getElementById(tabs[i]).className=tabs[i]; 
	if(document.getElementById(containers[i]))	
		document.getElementById(containers[i]).style.display = 'none';
} } }

function trimLeadZero(s) {
	return s.replace(/^0+/, "");
}

function isNumberKey(evt)
{
 var charCode = (evt.which) ? evt.which : event.keyCode
 if (charCode > 31 && (charCode < 48 || charCode > 57))
	return false;

 return true;
}
function enquiry_validate(){
	var frm=document.enquiry_form;
	var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	var re = /^[A-Za-z ]+$/;
	var name = frm.txtName.value;
	var email = frm.txtEmail.value;
	//var phone = frm.txtPhoneNo.value;
	//var plength = phone.length;
	
	var phcodereg = /^(\+?\d+(-\d+)*)$/;
	var phone = trimLeadZero(frm.txtPhoneNo.value);
			
	if(name == '' || name == 'Name'){
		alert('Enter Your Name');
		frm.txtName.focus();
		return false;
	} 
	else if(name.charAt(0)==' ')
	{
	alert('Name Should Not Start with Space');
	
	frm.txtName.focus();
	frm.txtName.value='';
	return false;
	}	
	else if(re.test(name)==false) {
		alert('Enter Only Alphabets');
		frm.txtName.focus();
		return false;
	}	
	else if(name.length<3 || name.length>40){
		alert("Name Field accepts from 3 to 40 Alphabets");
	frm.txtName.focus();
	frm.txtName.value='';
	return false;
	} else if(email == 'Email' || email == ''){
		alert('Enter Your Email');
		frm.txtEmail.focus();
		frm.txtEmail.value='';
		return false;
	}else if(!validateEmail(email)){
		alert('Enter Valid email ');
		frm.txtEmail.focus();
		return false;
	} else if(email!="" && reg.test(email) == false) {
      alert('Invalid Email Address');
	  frm.txtEmail.focus();
	  frm.txtEmail.value='';
      return false;
   }
	else if (email.indexOf('..',0) != -1) 
		{	
		alert('Invalid Email! Please check once again.');
  		return false;
		}
else if (email.indexOf('.@',0) != -1)
{	
	alert('period must not immediately precede @ in email address');
	return false;
}	

else if(email.charAt(0)=='_' || email.charAt(0)=='.')
{
alert('Email Should Not Start with Special Characters(.,@,_)');
return false;
}    	

else if(frm.txtCode.value=='' || frm.txtCode.value=='Code'){
		alert("Please Enter Code");
		frm.txtCode.focus();
		frm.txtCode.value='';
		return false;
	}
	if(frm.txtCode.value!='' && phcodereg.test(frm.txtCode.value) == false) {
		alert("Please Enter Valid Code ");
		frm.txtCode.focus();
		return false;
	}
	if(frm.txtCode.value.length<2 || frm.txtCode.value.length>6){
		alert("Code should be in range 2-6 digits");
		frm.txtCode.focus();
		return false;

	}
  if(frm.txtPhoneNo.value =='' || frm.txtPhoneNo.value =='Mobile No'){			
		alert('Enter Mobile Number ');
		frm.txtPhoneNo.focus();
		return false;
	}
	if(isNaN(frm.txtPhoneNo.value))
	{
		alert('Enter Numbers only');	
		frm.txtPhoneNo.value='';	
		frm.txtPhoneNo.focus();
		return false;
	}
	if(/\./.test(phone) == true)
	{
		alert('Enter Numbers only');	
		frm.txtPhoneNo.focus();
		return false;
	}
	if(phone.length < 10 || phone.length > 10)
	{
		alert(" Your Mobile Number must have 10 digits");
		frm.txtPhoneNo.focus();
		return false;
	}
			
	
	else if(frm.ddlBranch.value ==''){			
		alert('please select branch');
		frm.ddlBranch.focus();
		return false;
	}
	else if(frm.txtEnquiry.value =='Enquiry'){			
		alert('Enter Comments ');
		frm.txtEnquiry.focus();
		return false;
	}else {
		return true;
	}
	
}
