    position: relative;
    text-align: center;
    margin-bottom: 6px;
    width: 100%;
    z-index: 360;
}

.sl-cms2-module-dropzone-arrow-up:before {
    content: '';
    position: absolute;
    top: 10px;
    left: 0;
    height: 10px;
    width: 51%;
    background: #f90;
    /*rgba(255,254,245,1);*/
    -webkit-transform: skew(0deg, -6deg);
    -moz-transform: skew(0deg, -6deg);
    -ms-transform: skew(0deg, -6deg);
    -o-transform: skew(0deg, -6deg);
    transform: skew(0deg, -6deg);
}

.sl-cms2-module-dropzone-arrow-up:after {
    content: '';
    position: absolute;
    top: 10px;
    right: 0;
    height: 10px;
    width: 50%;
    background: #f90;
/*rgba(255,254,245,1);*/
    -webkit-transform: skew(0deg, 6deg);
    -moz-transform: skew(0deg, 6deg);
    -ms-transform: skew(0deg, 6deg);
    -o-transform: skew(0deg, 6deg);
    transform: skew(0deg, 6deg);
}​

