.sl-cms2-sites-homepage-card-header {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    border-top: 8px solid #009900;
}

.sl-cms2-sites-homepage-card-header__title-div {
    display: flex;
    flex-direction: row;
    padding: 2px;
}
.sl-cms2-sites-homepage-card-header__title-page-name {
    font-size: 16px;
    color: #009900;
    margin: 5px;
}

.sl-cms2-sites-homepage-card-header__settings {
    text-align: right;
}

.sl-cms2-sites-homepage-card-header__icon-home {
    height: 30px;
}

.sl-cms2-sites-homepage-card-header__icon {
    margin: 5px;
    height: 24px;
    float: left;
}

.sl-cms2-sites-homepage-card-preview {
    background: #cccccc;
    overflow: hidden;
    height: 160px;
    border: 5px solid #cccccc;
    position: relative;
}

.sl-cms2-sites-homepage-card-preview__image {
    width: 100%;
}

.sl-cms2-sites-homepage-card-footer {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 15px;
}

.sl-cms2-sites-homepage-card-footer__link {
    color: #6d93dc;
    font-size: 12px;
    text-decoration: none;
    margin-bottom: 1em;
}

.sl-cms2-sites-homepage-card-footer__link--bottom {
    margin-bottom: 0;
}

.sl-cms2-sites-homepage-card-footer__right-div {
    display: flex;
    flex-direction: column;
    text-align: right;
}

.sl-cms2-sites-homepage-card-preview-edit-overlay {
    display: none;
}

.sl-cms2-sites-homepage-card-preview:hover .sl-cms2-sites-homepage-card-preview-edit-overlay {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background-color:rgba(0, 0, 0, 0.5);
    width: 100%;
    height: 160px;
    position: absolute;
    top: 0;
}


.sl-cms2-sites-homepage-card-preview-edit-overlay__link {
    display: inline-block;
    text-decoration: none;
    color: #ffffff;
    margin: 0 20px;
}

a.sl-cms2-sites-homepage-card-preview-edit-overlay__link:focus,
a.sl-cms2-sites-homepage-card-preview-edit-overlay__link:hover {
    color: #ffffff;
}

.sl-cms2-sites-homepage-card-preview-edit-overlay__link img {
    display: block;
    margin: 0 auto 5px auto;
}

