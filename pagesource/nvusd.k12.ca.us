    addImageHandler: function (e, data) {
        sl.log("addImageHandler");
        this.slideshowParent = this.blockElement.closest(".jsSlideshow").closest(".cms2-selectable-module"); //fix this!!!
        this.slideshowControllerInstance = this.slideshowParent.data("jsEditorHandler");
        if (this.slideshowControllerInstance != null && data.originalFiles.length > 1) {
            if (data.originalFiles[0] == data.files[0]) {
                sl.alert("Oops! Currently images have to be uploaded one at a time.")
            }
            //data.url = "/pf4/cms2/setSlideLayoutImage";
            //this.slideshowControllerInstance.addMultipleSlides(e, data);
        }
        else {
            var uploadFile = data.files[0];
            if (sl.verifyFileType(uploadFile) == false) {
                return false;
            }
            var imageFileTypes = (/\.(gif|jpg|jpeg|png|svg)$/i);
            if (data.originalFiles.length > 1) {
                if (vex.getAllVexes().length == 0) {
                    sl.alert("Only one image can be uploaded for this module. You might want to use a carousel instead.")
                }
                return false;
            } else if (imageFileTypes.test(uploadFile.name)) {
                this.initProgressBar();
                this.jsProgressItemHolder.show();
                this.jsProgressItemHolder.on({
                    "abort": jQuery.proxy(this.update, this)
                })
                this.blockElement.on({
                    'fileuploaddone': jQuery.proxy(this.fileDropDoneHandler, this),
                    'fileuploadprogress': jQuery.proxy(this.progressInstance.progressHandler, this.progressInstance)
                });
                this.jqXHR = this.progressInstance.addHandler(e, data, "image");
                return true;
            }
            else {
                sl.alert("Sorry, please upload valid image type (.jpg, .jpeg, .png, .gif, .svg)")
                return false;
            }
        }
    },

    addVideoHandler: function (e, data) {
        var uploadFile = data.files[0];
        if (sl.verifyFileType(uploadFile) == false) {
            return false;
        }

        var videoFileTypes = (/\.(mp4)$/i);
        if (videoFileTypes.test(uploadFile.name)) {
            this.initProgressBar();
            this.jsProgressItemHolder.show();
            this.blockElement.on({
                'fileuploaddone': jQuery.proxy(this.fileDropDoneHandler, this),
                'fileuploadprogress': jQuery.proxy(this.progressInstance.progressHandler, this.progressInstance)
            });

            this.jqXHR = this.progressInstance.addHandler(e, data);
            return true;
            sl.log("upload video");

        }
        else {
            sl.alert("Sorry, mp4 is the only supported video format.")
            return false;
        }
