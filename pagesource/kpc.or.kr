			function callBackData( resultData ){
				if ( resultData.result ){
					switch ( resultData.mode ) {
						case 'changePwdCheck' :
							if ( resultData.Articles.length > 0 ){
								var today = new Date();
								var changeday = new Date(resultData.Articles[0].PwdDate.substring(0, 4), Number(resultData.Articles[0].PwdDate.substring(4, 6))-1, resultData.Articles[0].PwdDate.substring(6, 8));
								var betweenday = (today.getTime() - changeday.getTime())/1000/60/60/24;

								if ( betweenday > 180 && (Number(resultData.Articles[0].PwdDate) > Number()) ){
									jQuery('#open_pass_change_pop').empty();
									jQuery('#footerLayerPop5').tmpl( '' ).appendTo('#open_pass_change_pop');
									jQuery('#open_pass_change_pop').bPopup({ closeClass : 'g_pop_close' });
								}
							}
							break;

						default: break;
					}
				} else {
					alert( resultData.errorMSG );
				}
				return false;
			}

