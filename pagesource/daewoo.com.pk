    <script>
        var app = angular.module('myApp', []);
        app.controller('myCtrl', function ($scope, $http) {
            $scope.DepartureTerminals = null;
            //Declaring the function to load data from database
            $scope.SeachLoad = function () {
                $http({
                    method: 'POST',
                    url: 'Index.aspx/GetDepartureOnLoad',
                    data: {}
                }).success(function (result) {
                    $scope.DepartureTerminals = result.d;
                    var date = new Date();
                    $scope.DepartureDate = ("0" + (date.getDate())).slice(-2) + "-" + ("0" + (date.getMonth() + 1)).slice(-2) + "-" + date.getFullYear().toString()
                });
            };
            $scope.SetDepartureInitialValue = function () {
                $scope.DepartureValue = "0";
            };

            $scope.onSelectChange = function () {
                $http({
                    method: 'POST',
                    url: 'Index.aspx/GetArrivalsByDeparture',
                    data: { 'DepartureValue': $scope.DepartureValue }
                }).success(function (result) {
                    $scope.ArrivalTerminals = result.d;
                    $scope.ArrivalValue = $scope.ArrivalTerminals[0].TerminalCode;
                });
            };

            $scope.GetSchedule = function () {
                $scope.Departure_Name = $("#ddldeparture option:selected").text();
                $scope.Arrival_Name = $("#ddlArrival option:selected").text();

                if ($scope.Departure_Name == "--Select Departure--") {
                    return;
                } else if ($scope.Arrival_Name == "--Select Arrival--") {
                    return;
                }
                ShowScheduleLoader();
                $scope.Departure_Date = $("#date").val();
                $http({
                    method: 'POST',
                    url: 'Index.aspx/GetSchedules',
                    data: { 'DepartureValue': $scope.DepartureValue, 'ArrivalValue': $scope.ArrivalValue, 'DepartureDate': $scope.Departure_Date }
                }).success(function (result) {
                    $scope.ScheduleList = result.d;
                    if ($scope.ScheduleList.length == 0) {
                        $(".schedule-table").hide();
                        $(".no-record-found").show();
                    } else {
                        $(".schedule-table").show();
                        $(".no-record-found").hide();
                    }
                    ShowSchedulePopup();
                });
            };
        });

    </script>

