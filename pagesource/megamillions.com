<div class="span12 row-fluid" id='homePageAnimationWrapper' style="">
    <div id="homePageAnimation" style="margin:0,auto"></div>
    </div>
<script type="text/javascript">
    var goldBallUrl = '/Themes/MegaMillions/Content/Images/Balls/home_ball_gold_9.png';
    var ball1Url = '/Themes/MegaMillions/Content/Images/Balls/home_ball_14.png';
    var ball2Url = '/Themes/MegaMillions/Content/Images/Balls/home_ball_38.png';
    var ball3Url = '/Themes/MegaMillions/Content/Images/Balls/home_ball_51.png';
    var ball4Url = '/Themes/MegaMillions/Content/Images/Balls/home_ball_64.png';
    var ball5Url = '/Themes/MegaMillions/Content/Images/Balls/home_ball_70.png';
    var homeBgUrl = '/Themes/MegaMillions/Content/Images/home_bg.jpg';
    var homeLogoUrl = '/Themes/MegaMillions/Content/Images/animation_logolarge.png';
    var megaplierUrl = '/Themes/MegaMillions/Content/Images/animation_megaplier3x.png';


    function getDimentions() {
        var ratio = 400 / 690;
        var divwidth = getWidth();
        return { width: divwidth, height: (divwidth * ratio) }
    }

    function getWidth() {
        return $("#homePageAnimationWrapper").width();
    }



    var BouncyBall = function (paper, x, y, diameter, ballImage, angle) {
        this.paper = paper;
        this.x = x;
        this.y = y;
        this.r = angle;
        this.frameRate = 30;
        this.ballInstance = this.paper.image(ballImage, x, y, 106, 106);
        return this;
    };

    BouncyBall.prototype.calculateBounce = function (xVelocity, yVelocity, finalX, finalY, gravity, bounce, decelleration, rotations, rolldecelleration, rVelocity) {
        this.vx = xVelocity;
        this.vy = yVelocity;
        this.g = gravity;
        this.fac = bounce;
        this.finalRotation = rotations * 360;
        this.finalX = finalX;
        this.finalY = finalY;
        this.xfac = decelleration;
        this.rfac = rolldecelleration;
        this.vr = rVelocity;

        this.animating = $.Deferred();
        this.loadBouncePath();
    };
    BouncyBall.prototype.loadBouncePath = function(){
        this.animationSteps = [];
        var notfinished = true;
        var currentStep = {
            vy:this.vy,
            y:this.y,
            x:this.x,
            vx:this.vx,
            vr:this.vr,
            r:this.r
        };
        while(notfinished){
            currentStep.vy += this.g;
            currentStep.y += currentStep.vy;
            if (currentStep.y > this.finalY) {
                currentStep.y = this.finalY;
                currentStep.vy *= -this.fac;
            }
            var currentX = currentStep.x;

            currentStep.vx = currentStep.vx - (currentStep.vx * this.xfac);
            if (currentStep.vx < .25 && currentStep.vx > -.25) {
                if (currentStep.vx > 0) {
                    currentStep.vx = .25;
                } else {
                    currentStep.vx = -.25;
                }
            }
            currentStep.x += currentStep.vx;
            if (Math.abs(currentStep.x - this.finalX) > Math.abs(currentX - this.finalX)) {
                if (!this.logged) {

                    this.logged = true;
                }
                currentStep.x = this.finalX;
            }

            currentStep.vr = currentStep.vr - (currentStep.vr * this.rfac);
            if (currentStep.vr < 3 && currentStep.vr > -3) {
                if (currentStep.vr > 0) {
                    currentStep.vr = 3;
                } else {
                    currentStep.vr = -3;
                }
            }
            currentStep.r += currentStep.vr;
            if (Math.abs(currentStep.r) > Math.abs(this.finalRotation)) {
                currentStep.r = this.finalRotation;
            }
            this.animationSteps.push({
                y: currentStep.y,
                x: currentStep.x,
                transform: "r " + currentStep.r
            });
            if (!(currentStep.x == this.finalX && currentStep.y == this.finalY && currentStep.r == this.finalRotation)) {
                notfinished = true;
            }else{
                notfinished = false;
            }

        }
    };
    BouncyBall.prototype.bounce = function (frame) {
        if (frame < this.animationSteps.length) {
            var animate = this.animationSteps[frame];
            this.ballInstance.attr(animate);
            return true;
        }else{
            var animate = this.animationSteps[this.animationSteps.length - 1];
            this.ballInstance.attr(animate);
            return false;
        }
    };

     function imageLoader (src) {
         var deferred = $.Deferred();
        var img = new Image();
        img.onerror = function (err) {
            try {
                delete img.onload; //release memory - suggested by John Hann
                delete img.onerror;
            } catch (err) {
                img.onload = noop; // IE7 :(
                img.onerror = noop; // IE7 :(
            }
            deferred.resolve();
        };
        img.onload = function (evt) {
            try {
                delete img.onload; //release memory - suggested by John Hann
                delete img.onerror;
            } catch (err) {
                img.onload = noop; // IE7 :(
                img.onerror = noop; // IE7 :(
            }
            deferred.resolve();
        };
        img.src = src;
        return deferred;
    }

     var imageLoadingPromiseArray = [];
     var promise;
     imageLoadingPromiseArray.push(imageLoader(homeBgUrl));
     imageLoadingPromiseArray.push(imageLoader(ball1Url));
     imageLoadingPromiseArray.push(imageLoader(ball2Url));
     imageLoadingPromiseArray.push(imageLoader(ball3Url));
     imageLoadingPromiseArray.push(imageLoader(ball4Url));
     imageLoadingPromiseArray.push(imageLoader(ball5Url));
     imageLoadingPromiseArray.push(imageLoader(goldBallUrl));



     $.when.apply(this, imageLoadingPromiseArray).then(function () {
         var date = '3/20/2018';
         var size = getDimentions();
         var paper = Raphael(document.getElementById("homePageAnimation"), size.width, size.height);
         paper.setViewBox(0, 0, 690, 400, true);
         var bg = paper.image(homeBgUrl, 0, 0, 690, 400).attr({ transform: "r0" });
         var logoImg = paper.image(homeLogoUrl, 315, 82, 0, 0);
         var megaImg = paper.image(megaplierUrl, 170, -100, 350, 100);
         var text = paper.text(345, 370, date + " Winning Numbers").attr({
             'font-family': "'Oswald', Helvetica, Arial, san-sarif",
             'font-size': '34px',
             fill: "#fff"
         });
         
         $(window).resize(function () {
             var newSize = getDimentions();
             if (newSize.width != size.width) {
                 paper.setSize(newSize.width, newSize.height);
                 size = newSize;
             }
         });

         $("#homePageAnimationWrapper").resize(function () {
             var newSize = getDimentions();
             if (newSize.width != size.width) {
                 paper.setSize(newSize.width, newSize.height);
                 size = newSize;
             }
         });

         var ball1 = new BouncyBall(paper, 500, -106, 20, ball1Url, -70);
         var ball2 = new BouncyBall(paper, 420, -106, 20, ball2Url, 0);
         var ball3 = new BouncyBall(paper, -106, 399, 20, ball3Url, 90);
         var ball4 = new BouncyBall(paper, -106, 50, 20, ball4Url, 160);
         var ball5 = new BouncyBall(paper, 796, 199, 20, ball5Url, 10);
         var ball6 = new BouncyBall(paper, -106, -106, 20, goldBallUrl, 270);
         var ballAnimations = [];
         ball1.calculateBounce(-10, 4, 15, 175, .5, .6, .017, -2, .01, -10);
         ball2.calculateBounce(-8, 2, 126, 175, .4, .5, .025, -1, .06, -13);
         ball3.calculateBounce(10, 30, 237, 175, .6, .5, .025, 1, .01, 2);
         ball4.calculateBounce(16, 2, 348, 175, .8, .4, .025, 1, .01, 5);
         ball5.calculateBounce(-10, 30, 459, 175, .6, .5, .025, -1, .01, -6);
         ball6.calculateBounce(20, 20, 570, 175, .6, .5, .025, 2, .01, 10);

         var ball1Going = true;
         var ball2Going = true;
         var ball3Going = true;
         var ball4Going = true;
         var ball5Going = true;
         var ball6Going = true;
         var animating = true;
         var framerate = 30;
         var frame = 0;
         var actualFrame = 0;
         function runAnimations(){
                 var startTime = (new Date()).getTime();
                 if (ball1Going) {
                     ball1Going = ball1.bounce(frame);
                 }
                 if (ball2Going) {
                     ball2Going = ball2.bounce(frame);
                 }
                 if (ball3Going) {
                     ball3Going = ball3.bounce(frame);
                 }
                 if (ball4Going) {
                     ball4Going = ball4.bounce(frame);
                 }
                 if (ball5Going) {
                     ball5Going = ball5.bounce(frame);
                 }
                 if (ball6Going) {
                     ball6Going = ball6.bounce(frame);
                 }
                 if(!ball1Going 
                     && !ball2Going
                     && !ball3Going
                     && !ball4Going
                     && !ball5Going
                     && !ball6Going){
                     megaImg.animate({x:170, y:260,transform:"r0"},750,"bounce",function () {
                         logoImg.animate({
                             width: 388,
                             height: 213,
                             x: 150,
                             y: -15,
                             transform: "r0"
                         }, 2000, "elastic") });
                 }
                 else{
                     var endTime = (new Date()).getTime();
                     var diff = endTime - startTime;
                     var ratio = 34 / diff;
                     //console.log(diff);
                     if (ratio >= 1) {
                         framerate = 30;
                     } else {
                         framerate = 30 * ratio;
                     }
                     actualFrame = actualFrame + (30 / framerate);
                     frame = Math.round(actualFrame);
                     setTimeout(function(){
                         runAnimations();
                     }, 1000 / framerate);
                 }
         }
         runAnimations();
     });

</script>

</article></div>
