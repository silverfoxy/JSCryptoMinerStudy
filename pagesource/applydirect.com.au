             if(objJobtype!=null)
             {
                /*HungLe add 30/11 
                    if jobtype is HealthCare will change background of HealthBuzz
                */
                if(objJobtype.val() == "11"){
                    if(window.location.pathname.search('/jobs/')!=-1 )
                        $(".headerBackgroundImage").css("background-image","url('/Plugins/Core/Files/NewSkin_Images/bg_featureHealthBuzzSearch.jpg')");
                    else
                        $(".headerBackgroundImage").css("background-image","url('/Plugins/Core/Files/NewSkin_Images/bg_featureHealthBuzz.jpg')"); 
                    $(".logo-healthbuzz").show();
                }else{
                    $(".headerBackgroundImage").css("background-image",defaultImgBackground); 
                    $(".logo-healthbuzz").css('display','none');
                }//end
                
                var objctDiv="";
                var selectJobTypeVal = $( "#ctl00_ContentPlaceHolder1_uscSearchbox1_ddlJobType option:selected" ).val();
                objctDiv="#ctl00_ContentPlaceHolder1_uscSearchbox1_ddlSubType";
