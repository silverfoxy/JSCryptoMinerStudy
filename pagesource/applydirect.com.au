		    else
		    {
                $("#ctl00_ContentPlaceHolder1_uscSearchbox1_tbxKeyword").focus();
            }
            
            //Change property = attribute
            $('.feature-apply-searchresult input[type=text]:visible').each(function(){
                $(this).val($(this).attr('value'));
            });
            
            $('.feature-apply-searchresult select:visible').each(function(){
                $(this).find('option').each(function(){
                    if($(this).attr('selected') == 'selected')
                    {
                        $(this).prop("selected", true);
                    }
                });
		    })
		     $('.close-popup-healthBuzz').click(function () {
                $(".box-popup-healthBuzz").hide();
            });
            
            $.ajax({
                type: "POST",
                url: "/Services/wsJobAlert.asmx/LoadCurrentCandidateSetting",
                data: null,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data){
                    if(data.Code == "1")
                    {
                        var result = $.parseJSON(data.Result);
                        if (result.JobAlert)
                        {
                            $('#radJobAlertsYes').prop('checked', true);
                            $('#radJobAlertsNo').prop('checked', false);
                        }
                        else
                        {
                            $('#radJobAlertsYes').prop('checked', false);
                            $('#radJobAlertsNo').prop('checked', true);
                        }
                        if (result.Industry_Insights)
                        {
                            $('#radOffersYes').prop('checked', true);
                            $('#radOffersNo').prop('checked', false);
                        }
                        else
                        {
                            $('#radOffersYes').prop('checked', false);
                            $('#radOffersNo').prop('checked', true);
                        }
                        if (result.Newsletters)
                        {
                            $('#radNewslettersYes').prop('checked', true);
                            $('#radNewslettersNo').prop('checked', false);
                        }
                        else
                        {
                            $('#radNewslettersYes').prop('checked', false);
                            $('#radNewslettersNo').prop('checked', true);
                        }
                    }
                    //else{
                    //    Alert.notify("", "<span style='color:red;'><strong>Cannot load user settings</strong></span>");
