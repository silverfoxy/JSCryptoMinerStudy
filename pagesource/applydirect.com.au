        function closeHealthBuzzPopUp(){
            $(".box-popup-healthBuzz").hide();
        }
        function submitSearchQuery()
        {
            
            var searchqueryURL ='/query.html?';
            var searchprams = '';
            searchprams +="jt=" + $("#ctl00_ContentPlaceHolder1_uscSearchbox1_ddlJobType").val();
            searchprams +="&st=" + $("#ctl00_ContentPlaceHolder1_uscSearchbox1_ddlSubType").val(); 
            searchprams +="&s=" + $("#ctl00_ContentPlaceHolder1_uscSearchbox1_ddlState").val();
            searchprams +="&c=" + $("#ctl00_ContentPlaceHolder1_uscSearchbox1_ddlCity").val(); 
            var workTypeKeyword = $("input[value!=''].WorkTypeIDs:checkbox:checked").map(function () {
                if ($(this).val() != '')
                    return $(this).val();
            }).get().join(',');
            if ($("#ctl00_ContentPlaceHolder1_uscSearchbox1_hdIsSimpleSearch").val().toLowerCase() =="false")
            {
                ///advanced search
                searchprams +="&IsSimple=false"; 
                var company = $("#ctl00_ContentPlaceHolder1_uscSearchbox1_tbxCompany").val();
                company = company.replace(/\./g," ");
                searchprams +="&company=" + encodeURIComponent(company); 
                  
                var title =$("#ctl00_ContentPlaceHolder1_uscSearchbox1_tbxJobtitle").val();                    
                title = title.replace(/\./g," ");
                searchprams +="&title=" + encodeURIComponent(title); 
                  
                var desc = $("#ctl00_ContentPlaceHolder1_uscSearchbox1_tbxJobdescription").val();                    
                desc = desc.replace(/\./g," ");
                searchprams +="&desc=" + encodeURIComponent(desc); 
                
                searchprams += "&worktype=" + encodeURIComponent(workTypeKeyword);
            }
            else
            {
                //simple search
                var keywords =$("#ctl00_ContentPlaceHolder1_uscSearchbox1_tbxKeyword").val();
                keywords =  keywords.replace(/\./g," ");
                searchprams +="&keywords=" + encodeURIComponent(keywords); 
                searchprams += "&worktype=" + encodeURIComponent(workTypeKeyword);
            }
            
                if(searchprams.search('jt=11')!=-1){
                    $(".box-popup-healthBuzz").show();
                    setTimeout(closeHealthBuzzPopUp, 5000);
                }
            
            $(location).attr('href',searchqueryURL +  searchprams);
            $(".ajax-loading").show();
            return false;
        }
               
        var isMobile = {
			Android: function() {
				return navigator.userAgent.match(/Android/i) ? true : false;
			},
			BlackBerry: function() {
				return navigator.userAgent.match(/BlackBerry/i) ? true : false;
			},
			iOS: function() {
				return navigator.userAgent.match(/iPhone|iPad|iPod/i) ? true : false;
			},
			Windows: function() {
				return navigator.userAgent.match(/IEMobile/i) ? true : false;
			},
			any: function() {
				return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Windows());
			}
		};
		
