            $("#sign-up-emp-2").addClass(' active');
          }
          $('a.poplight[href^=#]').click(function(){
	            $("#ctl00_UscCandidateSignInUp1_txtUsername").focus();
	      });
          
           if($("#ctl00_UscCandidateSignInUp1_txtUsername").hasClass('error'))
           {
                $("#ctl00_UscCandidateSignInUp1_txtUsername").removeClass("error");
           }
            if($("#ctl00_UscCandidateSignInUp1_txtpassword1").hasClass('error'))
           {
                $("#ctl00_UscCandidateSignInUp1_txtpassword1").removeClass("error");
           }
          
          $("#popup-sign-in-cand").keydown(function(event) {
             event.stopPropagation();
            if (event.keyCode == '13') {
               login2();
               return false;
            }
          });
          
           $("#popup-forgotPassword").keydown(function(event) {
             event.stopPropagation();
            if (event.keyCode == '13') {
               forgotpasswordCandidate();
               return false;
            }
          });
          
           $('#popup-sign-in-cand').on('keypress', 'input[type=text],input[type=password]', function(event) {    
            event.stopPropagation();
