        function ResetField()
        {
           // $('#lblMessage2').html("");
            $("#ctl00_UscCandidateSignInUp1_txtUsername").val("");
            $("#ctl00_UscCandidateSignInUp1_txtUsername").removeClass("error");
            $("#lblErrorUsernameID1").html("");
            $("#ctl00_UscCandidateSignInUp1_txtpassword1").val("");
            $("#ctl00_UscCandidateSignInUp1_txtpassword1").removeClass("error");
            $("#lblErrorpassword1ID1").html("");
        }

      function login2()
      {
        var emailaddressVal = $("#ctl00_UscCandidateSignInUp1_txtUsername").val();
        if($.trim(emailaddressVal)=="" )
            {
                $("#ctl00_UscCandidateSignInUp1_txtUsername").addClass("error");
                Alert.notify("", "<span style='color:red;'><strong>Username is invalid.</strong></span>");
                $("#ctl00_UscCandidateSignInUp1_txtUsername").focus();
                return false;
            }else
            {
                $("#ctl00_UscCandidateSignInUp1_txtUsername").removeClass("error");
            }


        if($.trim($("#ctl00_UscCandidateSignInUp1_txtpassword1").val())=='')
        {
            $("#ctl00_UscCandidateSignInUp1_txtpassword1").addClass("error");
            Alert.notify("", "<span style='color:red;'><strong>Password is invalid</strong></span>");
            $("#ctl00_UscCandidateSignInUp1_txtpassword1").focus();
            return false;
        }else
        {   
            $("#ctl00_UscCandidateSignInUp1_txtpassword1").removeClass("error");
        }
        var username='';
        var password='';
        var remember=false;
        if($("#ctl00_UscCandidateSignInUp1_txtUsername").hasClass('error'))
        {
            $("#ctl00_UscCandidateSignInUp1_txtUsername").removeClass("error");
        }
        if($("#ctl00_UscCandidateSignInUp1_txtpassword1").hasClass('error'))
        {
            $("#ctl00_UscCandidateSignInUp1_txtpassword1").removeClass("error");
        }
        if(!ValidateLogin2()) return;
        username=$("#ctl00_UscCandidateSignInUp1_txtUsername").val();
        password=$("#ctl00_UscCandidateSignInUp1_txtpassword1").val();
        remember=$("#ctl00_UscCandidateSignInUp1_chkRemember").is(":checked"); 
        TryToLogin2(username,password,remember);
      }  
      
      function ValidateLogin2()
      {
        if($.trim($("#ctl00_UscCandidateSignInUp1_txtUsername").val())=='')
        {
            if(!$("#ctl00_UscCandidateSignInUp1_txtUsername").hasClass('error'))
            {
                $("#ctl00_UscCandidateSignInUp1_txtUsername").addClass("error");
            }
            Alert.notify("", "Email or username is required.");
            $("#ctl00_UscCandidateSignInUp1_txtUsername").focus();
            return false;
        }
        if($.trim($("#ctl00_UscCandidateSignInUp1_txtpassword1").val())=='')
        {
            if(!$("#ctl00_UscCandidateSignInUp1_txtpassword1").hasClass('error'))
            {
                $("#ctl00_UscCandidateSignInUp1_txtpassword1").addClass("error");
            }
            Alert.notify("", "Password is required.");
            $("#ctl00_UscCandidateSignInUp1_txtpassword1").focus();
            return false;
        }
         return true;
      }
      
      function TryToLogin2(username,password,remember)
        {
          //$(".ajax-loading").show();  
          $.ajax({
          type: 'POST',
          url: "/Plugins/Core/Pages/AjaxHandler.aspx",
          data: { "func": "Authenticate","username":username,"password":password,"remember":remember,"hashedID":"27026de2-375c-4fb0-ac53-b86ee547825a" },
          error: function(xhr, error){
                console.debug(xhr); console.debug(error);
                $('.ajax-loading').css('display','none');
                Alert.notify("", "<span style='color:red;'><strong>Login Failed: Email or Password is invalid</strong></span>");
         },
         beforeSend: function(data){
            $(".ajax-loading").show();
         },
          success: function(data){            
            UpdateLoginResult2(data); 
            $('.ajax-loading').css('display','none');
         },
          dataType: "json"
        });
    }
    
    function UpdateLoginResult2(data)
    {
        var validLogin = AJAXGetValueByKey(data, 'ValidLogin');
        var message = AJAXGetValueByKey(data, 'Message');
        var displayName =AJAXGetValueByKey(data, 'DisplayName'); 
        if (validLogin !='true')
        {
            //Alert.notify("", "<span style='color:red;'><strong>Login Failed: Email or Password is invalid.</strong></span>");
            Alert.notify("", "<span style='color:red;'><strong>" + message + "</strong></span>");
            if(!$("#ctl00_UscCandidateSignInUp1_txtUsername").hasClass('error'))
            {
                $("#ctl00_UscCandidateSignInUp1_txtUsername").addClass("error");
            }
            
            if(!$("#ctl00_UscCandidateSignInUp1_txtpassword1").hasClass('error'))
            {
                $("#ctl00_UscCandidateSignInUp1_txtpassword1").addClass("error");
            }
            $("#ctl00_UscCandidateSignInUp1_txtUsername").focus();
            
        }
        else
        {
         
            if($("#ctl00_UscCandidateSignInUp1_txtUsername").hasClass('error'))
            {
                $("#ctl00_UscCandidateSignInUp1_txtUsername").removeClass("error");
            }
            if($("#ctl00_UscCandidateSignInUp1_txtpassword1").hasClass('error'))
            {
                $("#ctl00_UscCandidateSignInUp1_txtpassword1").removeClass("error");
            }
            var returnUrl = getUrlVars()["returnUrl"];
            if (returnUrl !=null)
            {
              returnUrl = decodeURIComponent(returnUrl);
               $(location).attr('href',returnUrl);
            }
            else
            {
                Alert.notify("", "<span style='color:green;'><strong>Signed in successfully!</strong></span>");
            //update display on top
            //default is redirect to job alert page
             $(location).attr('href',"/CandidateDashboard.html");
            }
        }
    }
    
    function ValidateForgotPassword()
    {
        if($.trim($("#ctl00_UscCandidateSignInUp1_txtEmailaddress").val())=="")
        {
            Alert.notify("", "<span style='color:red;'><strong>Email is required!</strong></span>");
            $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").addClass("error");
            $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").focus();
            return false;
        }
        else
        {
            $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").removeClass("error");
        }
        
        var emailaddressVal = $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").val(); 
        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;  
        if(!emailReg.test(emailaddressVal))
        {
            Alert.notify("", "<span style='color:red;'><strong>Email has invalid value!</strong></span>");
            $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").addClass("error");
            $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").focus();
            return false;
        }else
        {
            $("#spEmail").attr("style","display:none");
            $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").removeClass("error");
        }
        return true;
    }
    
    function validateRegisterCandidate()
    {
        var email = $("#ctl00_UscCandidateSignInUp1_txtRe_Email").val();
        var pass = $("#ctl00_UscCandidateSignInUp1_txtRe_Password").val();
        var message ="";
        
        if(email == "")
            message += "Email is invalid";
        if(pass == "")
            message += "Password is invalid";
        var files = $("#LogoUpload").get(0).files;
        if (files.length > 0 && files[0].size > 2048000) {
           message += "Size of upload logo should be less than 2048 KB";
        }
        if(message != "")
        {
            Alert.notify("", "<span style='color:red;'><strong>"+message+"</strong></span>");
            return false;
        }
        else
            return true;
    }
    
    function forgotpasswordCandidate()
    {
        var email = $("#ctl00_UscCandidateSignInUp1_txtEmailaddress").val();
        if(ValidateForgotPassword())
        {
         $.ajax({
                type: "POST",
                url: "/Services/wsJobAlert.asmx/CandForgotPassword",
                data: "{email:'"+ email +"'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                beforeSend: function(data){
                    $("#cmdForgotPassword").prop('disabled', true);
                    $("#cmdForgotPassword").val("Sending ...");
                    $(".ajax-loading").show();
                },
                success: function (data){
                    $('.ajax-loading').css('display','none');
                    if(data.Code == "1")
                    {
                        Alert.notify("", "<span style='color:blue;'>"+data.Result+".</strong></span>");
                        $("#txtEmailAlert_1").val("");
                        $("#cmdForgotPassword").prop('disabled', false);
                        $("#cmdForgotPassword").val("Register");                    
                    }
                    else
                        Alert.notify("", "<span style='color:red;'><strong>"+data.Result+"</strong></span>");
                    
                },
                error: (function(xhr) {
                    console.log(xhr.responseText);
                    $('.ajax-loading').css('display','none');
                })
            });
        }
    }
    
    function registerCandidate()
    {
        var captcha = $("#re_captcha_register_cand").find(".g-recaptcha-response").val();
        var email = $("#ctl00_UscCandidateSignInUp1_txtRe_Email").val();
        var pass = $("#ctl00_UscCandidateSignInUp1_txtRe_Password").val();
        var fname = $("#ctl00_UscCandidateSignInUp1_txtRe_FirstName").val();
        var lname = $("#ctl00_UscCandidateSignInUp1_txtRe_LastName").val();
        var choice = $("#ctl00_UscCandidateSignInUp1_cboHearAboutUs").val();
        var title = $('input[name=radUserTitle]:checked').val();
        var isFit2WorkChecked = $('input[name=radIsFit2WorkChecked]:checked').val();
        var isWantReceiveMail = $('input[name=radIsWantReceiveMail]:checked').val();
        var jobAlert = $('input[name=radJobAlertSignUp]:checked').val();
        var offers = $('input[name=radOffersSignUp]:checked').val();
        var newsletters = $('input[name=radNewslettersSignUp]:checked').val();   
        var jtype = $("#ctl00_UscCandidateSignInUp1_ddlJobTypeJSK").val()
        var jsubtype = $("#ctl00_UscCandidateSignInUp1_ddlSubTypeJSK").val();
        var state = $("#ctl00_UscCandidateSignInUp1_ddlStateJSK").val();
        var city = $("#ctl00_UscCandidateSignInUp1_ddlCityJSK").val();      
        if(validateRegisterCandidate())
        {
            var data = new FormData();
            var files = $("#LogoUpload").get(0).files;
            if (files.length > 0) {
               data.append("image", files[0]);
            }
            data.append('email', email);
            data.append('password', pass);
            data.append('firstName', fname);
            data.append('lastName', lname);
            data.append('headabout', choice);
            data.append('captcha', captcha);
            data.append('title', title);
            data.append('isFit2WorkChecked', isFit2WorkChecked);
            data.append('isWantReceiveMail', isWantReceiveMail);
            data.append('jobAlert', jobAlert);
            data.append('offers', offers);
            data.append('newsletters', newsletters);
            data.append('jobtype', jtype);
            data.append('jobsubtype', jsubtype);
            data.append('state', state);
            data.append('city', city);
            
            // before send
            $("#candidateRegister").prop('disabled', true);
            $("#candidateRegister").val("Sending ...");
            $(".ajax-loading").show();
                
            var xhr = new XMLHttpRequest();
            xhr.open('POST', '/AJAXServer.ashx?ajaxAct=CandidateSignUp');
            xhr.send(data);
            xhr.onreadystatechange = function (resp) {
                if (xhr.readyState == 4) {
                    var msg = xhr.responseText.replace(/"/g, "");
                    if (xhr.status == 200) {
                        if(msg == "Successfully create.")
                        {
                            Alert.notify("", "<span style='color:green;'><strong>You have successfully registerd!</strong></span>");
                            $("#txtEmailAlert_1").val("");
                            $("#candidateRegister").prop('disabled', false);
                            $("#candidateRegister").val("Register");
                            $(location).attr('href',"/CandidateDashboard.html");
                        }
                        else
                        {
                            Alert.notify("", "<span style='color:red;'><strong>"+msg+"</strong></span>");
                        }
                    } 
                    else {
                        console.log(xhr.responseText);
                        Alert.notify("", "<span style='color:red;'><strong>Register failed.</strong></span>");
                    }
                }
                $('.ajax-loading').css('display','none');
            }
            
        }
    }
    function showLogoPreview(input) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#candidate-logo').attr('src', e.target.result);
            }
            reader.readAsDataURL(input.files[0]);
        }
    }
