function throttle(f, delay){
    var timer = null;
    return function(){
        var context = this, args = arguments;
        clearTimeout(timer);
        timer = window.setTimeout(function(){
            f.apply(context, args);
        },
        delay || 500);
    };
}

$('#weather_input').keyup(throttle(function(){

   var weathervalue = $( "#weather_input" ).val();
   loadWeather(weathervalue, '');    

}));

$( "#weather_input" ).keypress(function() {

  //var weathervalue = $( "#weather_input" ).val();
  //loadWeather(weathervalue, '');
  //console.log( "Handler for .keypress() called." );
