function loadWeather(location, woeid) {
  $.simpleWeather({
    location: location,
    woeid: woeid,
    unit: 'c',
    success: function(weather) {
	  
	   html = '<i class="icon iconw-' + weather.code + '"></i>';
	   html += '<h3><span class="glyphicon glyphicon-map-marker"></span> <span class="location">' + weather.city + '</span> <span class="temp">' + weather.temp + '&deg;</span></h3>';
	   
	   
	   
	   var monthNames = [ "January", "February", "March", "April", "May", "June",
						"July", "August", "September", "October", "November", "December" ];
	   var dayNames= ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"]

	   var newDate = new Date();
	   //newDate.setDate(newDate.getDate());
	   
	   html +='<span class="date">' + dayNames[newDate.getDay()] + " " + ordinal_suffix_of(newDate.getDate()) + ', ' + monthNames[newDate.getMonth()] + ' ' + newDate.getFullYear() +' - ' + weather.currently +'</span>';	   
	 
	  
      $("#weather_app").html(html);
	  
	  current_location = '<h2>' + weather.city + ', '+ weather.country + '</h2>';
	
	  
	  $("#location").html(current_location);
	  
	  gettoday = '<h2>NOW</h2>';
	  gettoday += '<h3><i class="iconw-' + weather.code + '"></i> </h3>';
	  gettoday += '<div id="weather-detail">';
	  gettoday += '<h4>' + weather.temp + '&deg;' + weather.units.temp + '</h4>';
         gettoday += '<h4> High ' + weather.high + '&deg;C Low ' + weather.low +'&deg;C</h4>';
         gettoday += '<h4>' + weather.currently + ' - '+weather.wind.direction+' '+weather.wind.speed+' '+weather.units.speed+'</h4></div>';
         $("#today").html(gettoday);
	  
	  
	  
	  forcast = "";
	  weather.forecast.length = 5 ;
	  
	  for(var i=1;i<weather.forecast.length;i++) {
	  
	    forcast +='<div class="visible-md visible-lg col-md-3">';
		forcast += '<h2>'+ weather.forecast[i].day +'</h2>';
        forcast += '<h2><i class="iconw-' + weather.forecast[i].code + '"></i></h2>'
		forcast += '<h2>'+ weather.forecast[i].text + ' </h2>';		
		
		forcast += '<h3> High ' + weather.forecast[i].high + '&deg;C Low ' + weather.forecast[i].low +'&deg;C</h3></div>';
		//forcast += '<div id="line"></div>';
              //forcast += '<hr>';
		
	 }
	  
	  $("#weather_forcast").html(forcast);
	  
    },
    error: function(error) {
      $("#weather_app").html('<p>' + error + '</p>');
    }
  });
}



function ordinal_suffix_of(i) {
    var j = i % 10,
        k = i % 100;
    if (j == 1 && k != 11) {
        return i + "st";
    }
    if (j == 2 && k != 12) {
        return i + "nd";
    }
    if (j == 3 && k != 13) {
        return i + "rd";
    }
    return i + "th";
}

