<script type="text/ng-template" id="quickBop.html">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12" analytics-product sku="{{ bopController.state.sku }}" list="QuickBOP">
                









                <section class="preview__title">
                    





<h1 class="display-1 product__title" itemprop="name"
    ng-bind="bopController.state.name || ''"
    data-test="product-title">
    
</h1>

                </section>

                







                <section class="util-margin-bottom-extra-small">
    <div id="BVRRSummaryContainer" class="product-reviews__ratings [ v2 ]">
        <div class="product-reviews__ratings-placeholder"></div>
    </div>
</section>


                <section class="util-margin-bottom-medium">
                    <div class="owl--fade"
     ds-carousel="bopController.state.images.thumbnailImages || bopController.page.images.thumbnailImages"
     carousel-config="bopController.carouselBopConfig"
     carousel-item-alias="image">
    <div>
        <ds-icon ng-if="image.defaultImage" name="placeholder" version="2"></ds-icon>
        <img ng-if="!image.defaultImage" class="image--partial-fill" ng-src="{{ image.largeUrl }}" ds-image-on-load="autoSizeContainer">
    </div>
</div>

                </section>

                <section class="util-margin-bottom-medium">
                    <ds-bop-price ng-if="bopController.state.price"
              sku="bopController.getSelectedSku()"
              price="bopController.state.price"
              promotion="bopController.state.promotion"
              free-gift-promotions="bopController.state.freeGiftPromotions"
              sellable="bopController.page.sellable"
              discountable="bopController.page.discountable">
</ds-bop-price>


                </section>

                <section class="box-group util-margin-bottom-medium">
                    

<div class="box-group__box" ng-if="bopController.page.dimensionNames.length">
    <div ng-repeat="(dimensionIndex, dimensionName) in bopController.page.dimensionNames">
        <span ng-if="dimensionName !== 'Colour'" class="util-margin-bottom-extra-small">
            {{dimensionName}}: <strong ng-bind="bopController.state.dimensions[dimensionIndex][dimensionName].description"></strong>
        </span>
        <div>
            <ol class="list-group util-margin-bottom-small [ v3 ]">
                <li class="list-group list-group--spaced box box--rounded box--condensed box--hover [ v3 ]"
                    ng-if="dimensionName === 'Colour'"
                    ng-repeat="attribute in bopController.page.attributesByDimensionName[dimensionName]"
                    ng-class="{
                        'box--rounded-selected': bopController.isAttributeSelected(dimensionIndex, dimensionName, attribute.value),
                        'box--rounded-disabled': !bopController.isAttributeEnabled(dimensionIndex, dimensionName, attribute.value),
                        'box--rounded-unavailable': !bopController.isAttributeSelectable(dimensionIndex, dimensionName, attribute.value),
                        'promotion-gift__bop-child': bopController.doesAttributeHaveGift(dimensionIndex, dimensionName, attribute.value)
                    }"
                    ng-click="bopController.handleAttributeSelection(dimensionIndex, dimensionName, attribute.value, attribute.description).setChild()"
                >
                    {{dimensionIndex}}
                    <button class="button button--fill button--rounded [ v3 ]"
                            ng-style="{'background': bopController.getBackgroundForAttribute(attribute)}"
                    ></button>
                </li>
                <li class="list-group--spaced box box--condensed box--fixed box--hover [ v3 ]"
                    ng-if="dimensionName !== 'Colour'"
                    ng-repeat="attribute in bopController.page.attributesByDimensionName[dimensionName]"
                    ng-class="{
                        'box--disabled': !bopController.isAttributeSelectable(dimensionIndex, dimensionName, attribute.value),
                        'box--selected': bopController.isAttributeSelected(dimensionIndex, dimensionName, attribute.value),
                        'promotion-gift__bop-child': bopController.doesAttributeHaveGift(dimensionIndex, dimensionName, attribute.value)
                    }"
                    ng-click="bopController.handleAttributeSelection(dimensionIndex, dimensionName, attribute.value, attribute.description).setChild()"
                >
                    <button class="button button--light button--no-decor button--dimension [ v3 ]"
                            ng-bind="attribute.description"></button>
                </li>
            </ol>
            <div class="form__field form__field--select form__field--ellipsis util-margin-bottom-extra-small [ v2 v3 ]" ng-if="dimensionName === 'Colour'">
                <span ng-bind="bopController.getColourDescription() || 'bop.dimensions.colourSelection' | translate" class="ellipsis__viewport"></span>
                <select
                    title="{{ bopController.getColourDescription() || 'bop.dimensions.colourSelection' | translate }}"
                    ng-model="bopController.selectedColour"
                    ng-change="bopController.handleColorDropdownSelection()"
                    ng-options="attribute as attribute.description for attribute in bopController.page.attributesByDimensionName[dimensionName] track by attribute.value"
                >
                    <option value="" selected disabled>{{ 'bop.dimensions.colourSelection' | translate }}</option>
                </select>
            </div>
        </div>

    </div>

</div>

                    











<div class="box-group__box" ng-controller="BasketProductController as basketProductController">
    
        
            <div class="media [ v2 ]">
                <div class="media__gallery fill--xs util-margin-right-small [ v2 ]">
                    








<div class="form--table fill--xs [ v3 ]" ng-init="basketProductController.questions = bopController.state.child.productQuestions">
    <div class="form__field--table form__field--quantity [ v3 ]">
        <input class="form__field form__field--subtle util-margin-right-extra-small util-margin-bottom-small [ v2 ]"
               name="quantity"
               ng-keyup="basketProductController.handleKeyPress($event, bopController.state.child.sku || '',
                   bopController.state.child.ageRestriction.ageRestricted || false,
                   bopController.state.child.productQuestions,
                   'FOP')"
               disabled
               ng-disabled="!bopController.canAddToBasket(false)"
               ng-model="basketProductController.quantity.value"
               type="number"
               min="{{ ::basketProductController.quantity.min }}"
               max="{{ ::basketProductController.quantity.max }}"
               value="1"
               data-test="add-to-basket-quantity-input">
    </div>

    <div class="form__field--table [ v3 ]">
        <button class="button button--primary button--add-to-basket button--action util-margin-bottom-small fill--xs [ v2 v3 ]"
                ng-click="basketProductController.handleAddToBasket(bopController.state.child.sku || '',
                    bopController.state.child.ageRestriction.ageRestricted || false,
                    bopController.state.child.productQuestions,
                    'FOP')"
                disabled
                ng-disabled="!bopController.canAddToBasket(false)"
                sku="{{ bopController.state.child.sku || '' }}" data-test="add-to-basket-button">
            Add To Basket
        </button>
    </div>
</div>

                </div>
                <div data-test="stock-availability-message" class="media__gallery fill--xs util-margin-right-small [ v2 ] bop--availability--container">
                    <div ng-if="basketProductController.errorMessage">
                        <div role="alert" class="alert alert--error util-margin-bottom-extra-small [ v3 ]" ng-cloak>
                            <p><strong ng-bind-html="basketProductController.errorMessage"></strong></p>
                        </div>
                    </div>
                    <div class="bop--availability--component" ng-if="!basketProductController.errorMessage">
                        








<small class="caption display--block [ v3 ]" ng-if="bopController.state.availability">
    <ds-bop-availability availability="bopController.state.availability || bopController.page.availability"></ds-bop-availability>
</small>

<small class="caption display--block [ v3 ]" ng-if="!bopController.state.availability">
    
        
        
        
        
        
            
            <mark class="highlight--subtle">Out of stock</mark>
            <link itemprop="availability" href="http://schema.org/OutOfStock" />
        
    
</small>

                    </div>
                </div>
            </div>
            




        
        
    
</div>

                    







    
    
        <div class="box-group__box box-group__box--highlight" ng-if="bopController.getQuantityInBasket('')" ng-cloak>
            <a class="default-link" href="/basket">
                <span><strong data-test="bop-basket-quantity">{{ bopController.getQuantityInBasket('') }}</strong> of these items in your basket</span>
            </a>
            <a class="default-link util-align-inline-float--right hidden-xxs" href="/basket" data-test="view-basket-link">
                View basket
            </a>
        </div>
    


                </section>

                


<strong ng-if="bopController.page.supplier && bopController.page.supplier.name"
        ng-bind="'bop.dropship.supplier' | translate: { title: bopController.page.supplier.name }">
</strong>


                





<section class="util-margin-bottom-medium" ng-if="bopController.page.productQuestionsRequired || false" ng-cloak>
    <div class="media [ v2 ]">
        <div class="media__gallery fill--xs util-margin-right-small [ v2 ]">
            <i class="ico ico-pharmacy" data-test="pharmacy-icon"></i>
        </div>
        <div class="media__body">
            <p class="content-body">
                This is a specialist veterinary product. We will ask you a simple set of questions to confirm that you are buying the correct medicine for your pet and know how to use it safely
            </p>
        </div>
    </div>
</section>

<section class="util-margin-bottom-medium" ng-if="bopController.state.child.ageRestriction && bopController.state.child.ageRestriction.ageRestricted || false" ng-cloak>
    <div class="restricted-message content-body">
        <i data-test="shared-restricted-age-icon" class="icon-shared icon-shared-restricted-age-small"></i>
        <p><em>Warning:</em> By law we cannot sell this product to anyone under 18, by placing an order with us you are confirming that you are over the age of 18.</p>
    </div>
</section>


                





<section class="util-margin-bottom-medium" ng-if="bopController.state.child.showCourierMessage || false" ng-cloak>
    <div class="courier-row">
        <div class="shelf-row shelf-row--free">
            <i class="icon-shared icon-shared-courier-van"></i>
        </div>
        <p>
            Order by 3pm for Express Courier Delivery&nbsp;
            <span class="courier-information">
            <i class="icon-shared icon-shared-info"></i>
            <span class="tooltip tooltip--courier [ v2 ]">
                <span class="tooltip__hotspot [ v2 ]"></span>
                Some of the items we sell are a bit too bulky to fit on our delivery vans so we offer our customers <strong class="highlight--courier">Next Day Delivery via courier</strong>.
                <span class="tooltip--courier__delivery-info [ v2 ]">
                    <strong>Deliveries are made from</strong>
                    8am - 8pm Monday to Saturday and 9am - 5pm on Sundays. Your delivery slot will only be confirmed once your order has been placed.
                </span>
            </span>
        </span>
        </p>
    </div>
</section>


                




<section ng-if="bopController.state.content">
    <div ng-repeat="content in bopController.state.content  | limitTo:1">
        <h3 class="headline util-margin-bottom-extra-small" ng-bind-html="content.title"></h3>
        <div class="bop-content-body content-body util-margin-bottom-small" ng-bind-html="content.content"></div>
    </div>
</section>


                <a class="button button--secondary button--stretch [ v2 ]" href="{{bopController.state.url}}">
                    See more details
                </a>
            </div>
        </div>
    </div>
</script>

