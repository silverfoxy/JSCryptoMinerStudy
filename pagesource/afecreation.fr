<script type="text/javascript">
    function onSuggestLoad() {
    // Example: Custom pattern with handler
    // 1) create a AFSSuggest object with the input element, the
    //  suggest URL and the service ID. The input element must exist
    //and be a "text"-type input. It will be used to popup the
    //suggestion list at the right place and its value as the
    //suggestion query. 
      //var suggest = new AFSSuggest(document.getElementById('s_pattern'),'https://afecreation.afs-antidot.net/afs.suggest', 7085) ;
    if(document.getElementById('s_pattern')){
    var suggest = new AFSSuggest(document.getElementById('s_pattern'),'https://afecreation.afs-antidot.net/acp?afs%3Astatus=stable', 7085, 'collocFull_fr') ;// 2) You can use the "setPattern" fonction to change the way
      //each suggestion is displayed
      suggest.setPattern("{afs:suggestion}{afs:distance,choice,0# <em>Exact</em>|1# <em>Flou</em>}");
      // 3) You can also add a suggestion handler. It's a JavaScript
      //object with a "onSuggestion" function. This function is
      //called each time a suggestion is selected and each time the
      //suggestion popup is updated. When a suggestion is selected
      //the first argument is the suggestion, the second the feed and
      //the last the metadata associated with it.
      suggest.addSuggestionHandler({
          onSuggestion: function(suggestion, feed, metadata) {
      var text = "";
      if (suggestion) {
          text = "Suggestion: " + suggestion + " Feed: "+ (feed ? feed : "<aucun>");
      }      
          var elm = document.getElementById('s_info');
          while (elm.hasChildNodes()) {
          elm.removeChild(elm.firstChild);
          }
          elm.appendChild(document.createTextNode(text));
          if (suggestion) {
          document.getElementById('s_antidot_form').acceptCharset="UTF-8";
          document.getElementById('s_antidot_form').submit();
          }
      }
      });
      }
      }
