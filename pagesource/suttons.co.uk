$(window).scroll(function() {
if ($(window).width() > 1007) {
if ($(this).scrollTop() > 0){  
    $('header').addClass("sticky");
    $('body').addClass("stuck");
  }
  else{
    $('header').removeClass("sticky");
    $('body').removeClass("stuck");
    $("#ProductBuyHead").hide();
  }
}
});
    


