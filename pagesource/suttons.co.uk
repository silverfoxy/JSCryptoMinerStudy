// Add class to navigation elements with sublevel 
function ApplyCSS () {
	$(".SIDENAV li").each(function (index, element) {
		if ($(this).find(".tag").length > 0) {
			$(this).addClass('mega');
			$(this).find(' > span > a').addClass("mega");
		}
	});
	$(".SIDENAV li.mega li.mega").each(function (index, element) {
	   var link = $(this).find(' > span.PANELTITLE');
	   var name = link.text();
	   if ($(this).find(".tag > ul > li.mega").length > 1) {
		    console.log(name + " has more than one set");
		    $(this).removeClass("mega");
		    $(this).find('> .PANELTITLE').remove();
		    $(this).find(".mega").removeClass("mega");
		    $(this).find(".tag").removeClass("tag").addClass("not");
		    $(this).find("li.level2 > .not").remove(); 
	    }
	});
}
$( document ).ready(function() {
    ApplyCSS ();
	// Add Close Button to sublevel 
	$( ".SIDENAV .tag" ).prepend( "<a href='' class='close'>Back</a>" );	
	// Toggle sublevel on click
	$(".SIDENAV a.mega").click(function( event ) {
		event.preventDefault();
		var sub = $(this).parent().next(".tag");
		if ( $( sub ).children(".linkBox").length > 0 ) {
			// Do nothing
		}
		else {
            // Copy top level link and add to sublevel if not done already
			sub.prepend("<div class='linkBox'></div>")
			var toplink = $(this).clone();
			sub.children('.linkBox').prepend( toplink );
		};
		sub.toggle("slide");
        $("html, body").animate({ scrollTop: 0 }, "slow");
	});
	$(".SIDENAV .PANELTITLE").click(function( event ) {
		var sub = $(this).next(".tag");
		if ( $( sub ).children(".linkBox").length > 0 ) {
		}
		else {
            sub.prepend("<div class='linkBox'></div>")
            var toplink = $(this).clone();
	        sub.children('.linkBox').prepend( toplink );
		};
		sub.toggle("slide");
        $("html, body").animate({ scrollTop: 0 }, "slow");
	});
	// Close sublevel 
	$(".SIDENAV .tag .close").click(function( event ) {
		event.preventDefault();
		$(this).parent().hide("slide");
	});
	// Close all sublevels when navigation is hidden
	$(".exit-off-canvas").click(function( event ) {
		$(".SIDENAV .tag").hide("fade", 500);
	});
});
</script>
