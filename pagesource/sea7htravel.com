-webkit-transition: all .15s ease-in-out;
-moz-transition: all .15s ease-in-out;
-ms-transition: all .15s ease-in-out;
-o-transition: all .15s ease-in-out;
transition: all .15s ease-in-out;
}
.contact-notifications {
display: block;
overflow: hidden;
}
.contact-notifications > p.contact-form-error-message-with-border {
display: block;
font-size: 12px;
padding: 10px;
background-color: #ffd5d2;
color: #c1524a;
}
.contact-notifications > p.contact-form-success-message-with-border {
display: block;
font-size: 12px;
padding: 10px;
background-color: #daffdb;
color: #548856;
}
.contact-notifications p img {
float: left;
margin: 5px;
}
/* Search Widget */
.search-widget {
display: block;
width: 100%;
height: 40px;
line-height: 40px;
margin: 0;
padding: 0 8px;
font-size: 12px;
color: #666;
border: 0;
border: 1px solid #ebebeb;
}
.search-widget .search-icon {
float: left;
background-color: #dedede;
color: #6b6b6b;
font-size: 12px;
width: 25px;
height: 25px;
line-height: 26px;
text-align: center;
border-radius: 50%;
margin-top: 7px;
}
.search-widget input[type="text"] {
border: 0;
background: none;
outline: 0;
min-width: 90%;
height: 40px;
}
/* Featured Post */
.FeaturedPost h3 {
margin: 15px 0;
}
.FeaturedPost p {
font-size: 12px;
opacity: .7;
margin: 0;
}
/* Newsletter */
.FollowByEmail .widget-content {
background: #fff;
border: 1px solid #ebebeb;
border-top: 0;
padding: 20px;
}
.FollowByEmail .title {
margin: 0 !important;
border: 0 !important;
}
.follow-by-email-inner span {
font-size: 12px;
opacity: .5;
margin-bottom: 10px;
display: block;
}
.follow-by-email-address {
width: 100%;
height: 35px;
margin: 0;
padding: 0 10px;
font-size: 12px;
color: #666;
border: 1px solid #ebebeb;
outline: 0;
text-align: center;
}
.follow-by-email-submit {
display: block;
background-color: #21b6ea;
color: #fff;
border: 0;
width: 100%;
height: 35px;
margin-top: 8px;
padding: 0;
font-size: 12px;
font-weight: 700;
text-shadow: none;
outline: 0;
border-radius: 2px;
}
.follow-by-email-inner form {
display: block;
position: relative;
overflow: hidden;
}
/* Category List */
.list-label-widget-content li {
float: right;
display: block;
line-height: 19px;
margin-bottom: 16px;
overflow: hidden;
position: relative;
width: 50%;
}
.list-label-widget-content li:nth-child(odd) .label-count {
left: 20px;
}
.list-label-widget-content li a {
padding: 0;
}
.list-label-widget-content li span.label-count {
display: inline-block;
margin-top: 0;
font-size: 11px;
padding: 4px 6px;
min-width: 22px;
line-height: 1;
position: absolute;
left: 0;
text-align: center;
}
.list-label-widget-content li span.label-count:before {
content: '';
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 100%;
border: 1px solid #000;
-webkit-border-radius: 30px;
-moz-border-radius: 30px;
border-radius: 30px;
opacity: .2;
}
.list-label-widget-content li span:not(.label-count) {
color: #21b6ea;
}
/* Tags Widget */
.cloud-label-widget-content {
display: block;
overflow: hidden;
position: relative;
clear: both;
}
.cloud-label-widget-content span {
display: block;
float: right;
}
.cloud-label-widget-content span a,
.cloud-label-widget-content span span {
display: block;
float: right;
margin-left: 5px;
margin-bottom: 5px;
font-size: 12px;
padding: 0 10px;
background: #ebebeb;
border-radius: 1px;
text-decoration: none;
-webkit-transition: all .35s ease-in-out;
-moz-transition: all .35s ease-in-out;
-ms-transition: all .35s ease-in-out;
-o-transition: all .35s ease-in-out;
transition: all .35s ease-in-out;
}
.cloud-label-widget-content span a:hover,
.cloud-label-widget-content span span {
background: #78C2A8;
color: #fff;
}
/* Footer
----------------------------------------------- */
footer.footer {
display: block;
padding: 50px 0;
color: #fff;
position: relative;
}
footer.footer:before {
content: '';
display: block;
position: absolute;
background-color: rgba(0, 0, 0, 0.9);
top: 0;
left: 0;
right: 0;
bottom: 0;
width: 100%;
height: 100%;
}
footer.footer a {
color: #fff;
}
.footer-column {
display: block;
overflow: hidden;
}
.footer-column .widget > h2 {
display: block;
position: relative;
font-size: 15px;
color: #fff;
border-bottom: 1px solid rgba(255, 255, 255, 0.08);
margin-bottom: 30px;
}
/* PageList */
.footer-column .PageList ul li {
display: block;
width: 50%;
float: right;
margin-bottom: 10px;
}
.footer-column .PageList ul li a {
font-size: 12px;
color: #fff;
}
/* ListLabel */
.footer-column .list-label-widget-content li span.label-count:before {
border-color: #fff;
}
/* PopularPosts */
.footer-column .popular-posts ul li {
border-color: rgba(255, 255, 255, 0.1);
}
.footer-column .popular-posts ul li .item-thumbnail {
width: 70px;
height: 60px;
border-radius: 3px;
}
/* CloudLabel*/
.footer-column .cloud-label-widget-content span a,
.footer-column .cloud-label-widget-content span span {
background-color: rgba(255, 255, 255, 0.17);
color: #fff;
}
.footer-column .cloud-label-widget-content span a:hover {
background-color: #78C2A8;
}
/* CopyRights */
footer.footer .copyrights {
display: block;
overflow: hidden;
position: relative;
font-size: 12px;
clear: both;
margin-top: 40px;
text-align: center;
padding-top: 30px;
