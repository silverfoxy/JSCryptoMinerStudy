<script type="text/javascript">
	var _default_action_button = "_affiliate_store_default_button_action_text";
	var _domain_url = "http://www.harveynorman.co.nz/index.php?dispatch=buy_from_store.getfile&amp;affiliate_id=";

	
	$(function () {
		$('#buy_from_store_affiliate_form').dialog({
			autoOpen: false,
			modal: true,
			draggable: false,
			resizable: false,
			title: "Buy from store",
			width: 600,
			position: 'center',
			height: 'auto'
		});

		$("body")
			.delegate('.btn-buy-from-store-affiliate', 'click', function(event) {
				event.preventDefault();
				$("div.buy-from-store-wrapper-dialog #buy_from_store_logo_wrapper").html("").hide();

				var affiliate_details = $(event.target).data('bfsDetails');
				if(typeof affiliate_details === 'object'){

					$("#buy_from_store_affiliate_form").dialog({
						title: affiliate_details.lightbox_header
					});

					$("div.buy-from-store-wrapper-dialog #buy_from_store_body").html(affiliate_details.lightbox_body);
					$("div.buy-from-store-wrapper-dialog #buy_from_store_proceed").html(affiliate_details.lightbox_call_to_action||_default_action_button);
					$("div.buy-from-store-wrapper-dialog #buy_from_store_proceed").attr('href', affiliate_details.partner_url || "#");

					$("div.buy-from-store-wrapper-dialog #buy_from_store_t_and_c").html(affiliate_details.lightbox_t_and_c);

					if(typeof affiliate_details.logo == 'string' && affiliate_details.logo.trim() != '') {
						$("div.buy-from-store-wrapper-dialog #buy_from_store_logo_wrapper").html("<img src=" + _domain_url + affiliate_details.affiliate_id + " class='col-md-6'/>");
						$("div.buy-from-store-wrapper-dialog #buy_from_store_logo_wrapper").show();
					}

					$("#buy_from_store_affiliate_form").dialog("open");
			}
		});

		$("#buy_from_store_action_btn_close").off('click').on('click', function(event) {
			event.preventDefault();
			$("#buy_from_store_affiliate_form").dialog("close");
		});
	});
	
</script>
