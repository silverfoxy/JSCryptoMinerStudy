                    html.push("<p>");
                    for (key in storeHours) {
                        var schedule = storeHours[key];
                        var day = key.charAt(0).toUpperCase() + key.slice(1);

                        if (schedule.closed === 'Y') {
                            continue;
                        }

                        var opening = [ pad(schedule.from_hour, 2, 0, 1), ":", pad(schedule.from_minute, 2, 0, 2), ' AM'].join("");
                        var closing = [ pad(schedule.to_hour, 2, 0, 1), ":", pad(schedule.to_minute, 2, 0, 2), ' PM'].join("");

                        html.push([day, ": ", opening, " - ", closing].join("") + "<br/>")
