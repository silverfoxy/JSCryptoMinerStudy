<script type="text/javascript">
  jQuery(function($) {
    var isMobile = navigator.userAgent.match(/Android|iPhone|iPad|iPod/ig);
  	$(document).ready(function() {
      if (!window.sessionStorage.noapp && window.location.pathname === '/') {
        if (isMobile) {
          $('body').prepend('<div class="lts-modal-wrapper"><div class="lts-modal"><span class="lts-modal-dismiss">âœ–</span></div>');

          var userAgent = isMobile[0];
          var appUrl = userAgent === 'Android' ? 'https://play.google.com/store/apps/details?id=com.sunny.lts&hl=en' : 'https://itunes.apple.com/us/app/lts-app/id1082027077';
          $('.lts-modal').prepend('<a href="' + appUrl + '" target="_blank"><img src="image_uploads/LTS-PopUp4.jpg"></a>');

          // hide modal when dismiss
          $('.lts-modal-wrapper, .lts-modal-dismiss').click(function(e) {
            window.sessionStorage.noapp = true;
            $(this).animate({
              opacity: 0,
              'z-index': -2333,
            }, 200);
          });

          // show wrapper
          $('.lts-modal-wrapper').animate({
            opacity: 1,
            'z-index': 23333,
          }, 200);

          // show popup img then show dismiss button
          $('.lts-modal').animate({
            opacity: 1,
            top: 0,
          }, 500, function() {
            $('.lts-modal-dismiss').animate({
              opacity: 1,
            }, 250);
          });
        }
      }
  	});
  });
</script>
