/* Responsive embed container YouTube (2018-01-17) */
.embed-container { 
	position: relative; 
	padding-bottom: 56.25%; 
	height: 0; 
	overflow: 
	hidden; 
	max-width: 100%; 
} 
.embed-container iframe, 
.embed-container object, 
.embed-container embed { 
	position: absolute; 
	top: 0; 
	left: 0; 
	width: 100%; 
	height: 100%; 
}

/* green orderd list bullets */
ol.bullets-green {
  counter-reset: li;
  margin-left: 0;
  padding-left: 0;
  font-size: 15px;
  color: #686868;
}
ol.bullets-green > li {
  position: relative;
  margin: 12px 0 4px 2em;
  padding: 2px 0 0 8px;
  list-style: none;
}
ol.bullets-green > li::before {
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  border-radius: 100%;
  content: counter(li);
  counter-increment: li;
  position: absolute;
  top: -2px;
  left: -2em;
  width: 2em;
  margin-right: 8px;
  padding: 4px;
  color: #fff;
  background: #8CD600;
  text-align: center;
  font-size: 16px;
  font-weight: 600;
}

/* table styling */
table.table {
  border-collapse: collapse;
  margin-bottom: 0;
  width: 100%;
  max-width: 100%;
  border: none;
  background: transparent;
  border-spacing: 0;
  box-sizing: border-box;
}
table.table thead > tr > th {
  border-bottom: 2px solid #8CD600;
  vertical-align: bottom;
  padding: 15px 10px;
  font-weight: bold;
  text-transform: uppercase;
  background: white;
  color: #333;
  text-align: left;
}
table.table > tbody > tr > td {
  padding: 15px 10px;
  border-bottom: 1px solid #F2F2F2;
  text-align: left;
}
.table-striped > tbody > tr:nth-of-type(odd) {
  background: #fafafa;
}

