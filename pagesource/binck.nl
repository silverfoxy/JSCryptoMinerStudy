}
.hex1 {
  display: inline-block;
  text-align: center;
  line-height: 100px;
  position: relative;
  margin: 20px 0 40px;
  border-radius: 2px;
  background-color: #8cd600;
  height: 43px;
  width: 75px;
}
.hex1:before,
.hex1:after {
  position: absolute;
  top: 0;
  left: 0px;
  z-index: -1;
  content: "";
  border-radius: 2px;
  background-color: #8cd600;
  height: 43px;
  width: 75px;
}
.hex1:before {
  -webkit-transform: rotate(60deg);
  -moz-transform: rotate(60deg);
  -ms-transform: rotate(60deg);
  -o-transform: rotate(60deg);
  transform: rotate(60deg);
}
.hex1:after {
  -webkit-transform: rotate(-60deg);
  -moz-transform: rotate(-60deg);
  -ms-transform: rotate(-60deg);
  -o-transform: rotate(-60deg);
  transform: rotate(-60deg);
}
.hex1 em {
  color: #fff;
  font-size: 41px;
  position: relative;
  top: -19px;
}
.hex1 em.fa-location-arrow {
  left: -2px;
}
.hex1 em.fa-forward {
  font-size: 39px;
  left: 4px;
}
.hex1.blue {
  background-color: #468ccb;
}
.hex1.blue:before,
.hex1.blue:after {
  background-color: #468ccb;
}
