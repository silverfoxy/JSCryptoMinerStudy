    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="description" content="">
    <meta name="keywords" content="">

    <script src="/Content/javascript/jquery-2.1.0.min.js"></script>

    <style type="text/css">
        body {
            margin: 0;
            padding: 0;
            font-family: arial, sans-serif;
        }

        h1 {
        }

        h2 {
            margin: 0;
            padding: 0 10px 10px 10px;
            font-size: 18px;
        }

        h3 {
            margin: 0;
            padding: 0 10px 10px 10px;
            font-size: 16px;
        }

        p, ul, li, a {
            font-size: 14px;
            line-height: 19px;
        }

        p, ul {
            margin: 0;
            padding: 0 10px 20px 10px;
        }

        li {
            padding-bottom: 5px;
        }

        a {
            font-weight: bold;
            /*Orginal farve*/
            /*color: #0c71a6;*/
            color: #00588D;
            text-decoration: none;
        }

        .dntSite {
            max-width: 940px;
            margin: 0 auto;
            padding: 10px 20px 20px 20px;
        }

        .dntHeader {
            float: left;
            width: 100%;
            margin-bottom: 15px;
            border-bottom: 3px solid #000 /*#087683*/;
            /*background:#087683;*/
            background: transparent;
        }

            .dntHeader h1 {
                display: inline-block;
                margin: 65px 0 0 0;
                padding: 0;
                font-size: 24px;
                /*color:#fff;*/
            }

        .uvmLogo {
            float: right;
            margin-bottom: 10px;
        }

        .left {
            float: left;
            width: 70%;
            max-width: 640px;
        }

        .right {
            float: right;
            width: 30%;
            max-width: 320px;
            margin: 0 0 10px 0;
        }

        .box {
        }

        .w50 {
            float: left;
            width: 48%;
            max-width: 320px;
            min-height: 380px;
            margin-top: 20px;
        }

            .w50 + .w50 {
                float: right;
                margin-left: 10px;
            }

        .elev {
            display: table-cell;
            vertical-align: bottom;
            width: 640px;
            height: 300px;
            background-image: url('/Content/img/forsidebg.jpg');
            background-size: cover;
            border-radius: 3px;
        }

        .elevStart {
            float: left;
            max-height: 150px;
            width: 100%;
            padding: 10px;
            background: rgba(255,255,255,0.8);
        }

            .elevStart p {
                width: auto;
                padding-bottom: 0;
            }

            .elevStart h2 {
                margin: 0;
                padding: 0;
                font-size: 30px;
            }

        .personale, .kommune {
            padding: 10px 0;
            background: #f5f5f5;
            border: 1px solid #ddd;
            border-radius: 3px;
        }

        .menuList {
            list-style: none;
        }

        .basicbutton {
            width: 93%;
            height: auto;
            margin: 0 10px 10px 10px;
            padding: 5px 10px;
            font-size: 14px;
            font-weight: bold;
            color: #ffffff;
            text-align: center;
            cursor: pointer;
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            border-radius: 5px;
            border: 0 none;
        }

            .basicbutton:hover {
                -webkit-box-shadow: 1px 1px 2px 0px rgba(0,0,0,0.50);
                -moz-box-shadow: 1px 1px 2px 0px rgba(0,0,0,0.50);
                box-shadow: 1px 1px 2px 0px rgba(0,0,0,0.50);
            }

        footer {
            clear: both;
            float: left;
            width: 100%;
            margin-top: 20px;
            padding: 20px 0;
            border-top: 3px solid #000 /*#087683*/;
        }

        div.footerblock {
            width: 33.333%;
            margin: 0px;
            float: left;
        }

        .green {
            background-color: #297906;
        }

        .blue {
            background-color: #0c71a6;
        }

        .gray {
            background-color: #707070;
        }

        .start {
            width: auto;
            margin: 10px 0 20px 0;
            padding: 10px 20px 10px 20px;
            color: #ffffff;
            font-size: 20px;
            text-decoration: none;
            background: #297906;
            /*background-image: -webkit-linear-gradient(top, #d93442, #b82b47);
        background-image: -moz-linear-gradient(top, #d93442, #b82b47);
        background-image: -ms-linear-gradient(top, #d93442, #b82b47);
        background-image: -o-linear-gradient(top, #d93442, #b82b47);
        background-image: linear-gradient(to bottom, #d93442, #b82b47);*/
            -webkit-border-radius: 13;
            -moz-border-radius: 13;
            border-radius: 13px;
        }

        @media only screen and (max-width: 768px) {
            .dntSite {
                width: auto;
            }

            .dntHeader h1 {
                max-width: 50%;
                margin-top: 50px;
                font-size: 20px;
            }

            .elev {
                max-width: 640px;
            }

            .w50 {
                width: 47.5%;
                margin-right: 0;
            }
        }

