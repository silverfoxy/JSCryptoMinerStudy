        <div class="container">
            <div class="row" style="margin-bottom:40px;">
                <div class="col-lg-6" id="div-noticia" style="margin-bottom:20px;">
                    <h2 class="container-title">
                        <div class="col-noticia" >
                            Noticias
                        </div>
                        <button v-if="cantidadgruponoticias > 1" class="btn btn-mas"  sytle=""  v-on:click="refrescarNoticias(counternoticias)">Ms noticias</button>
                    </h2>
                    <div v-if="noticias">
                        <div v-for="( not, key ) in noticias" >
                            <div v-if="key == counternoticias" >
                                <div id="noticia" class="row" v-for="noticia in not">
                                    <div class="col-sm-4">
                                        <img class="img-responsive" :src="noticia.thumbnail">
                                    </div>
                                    <div class="col-sm-8">
                                        {{ noticia.date }}
                                        <br>
                                        <span style="font-family:Raleway; font-weight:bold;">
                                        {{ decodeHtml(noticia.title) }}
                                    </span>
                                        <p>
                                            {{ decodeHtml(noticia.excerpt) }}
                                        </p>
                                        <p>
                                            <a :href="noticia.url">
                                                Seguir Leyendo...
                                            </a>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div v-else>
                        <p>No hay noticias disponibles.</p>
                    </div>
                </div>
                <div class="col-lg-6">
                    <h2 class="container-title">
                        <div class="col-noticia" >
                            Eventos
                        </div>
                        <button v-if="cantidadgrupoeventos > 1" class="btn btn-mas" v-on:click="refrescarEventos(countereventos)">Ms eventos</button>
                    </h2>
                    <div v-if="eventos">
                        <div v-for="( ev, key ) in eventos">
                            <div v-if="key == countereventos">
                                <div id="noticia" class="row" v-for="evento in ev">
                                    <div class="col-sm-4" v-if="evento.thumbnail">
                                        <img class="img-responsive" :src="evento.thumbnail">
                                    </div>
                                    <div class="col-sm-8">
                                        {{ evento.fecha }}
                                        <br>
                                        <span style="font-family:Raleway; font-weight:bold;">
                                        {{ decodeHtml(evento.title) }}
                                    </span>
                                        <p>
                                            {{ decodeHtml(evento.excerpt) }}
                                        </p>
                                        <p>
                                            <a :href="evento.url">
                                                Seguir Leyendo...
                                            </a>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div v-else>
                        <p>No hay eventos disponibles.</p>
                    </div>
                </div>
            </div>
        </div>
        <!-- Novedades -->
    </div>
