<script type="text/javascript">
    $(function () {
        $('#facultades').waypoint(function () {
            $(".facultad").addClass("animate");
        }, {offset: 800});
    });
</script>

<script>
    var $app = new Vue({
        el: '#app',
        data: {
            formulario: {
                prospecto: {
                    nombre: null,
                    apellido: null,
                    email: null,
                    telefono: null,
                    anexo: null,
                    campania: null,
                    carreras: [],
                    modalidad: null,
                    nivel: null,

                },
                cargandoenvio: false,
                cargando: false,
                errores: null,
                enviado: null,
                anexos: null,
                campanias: null,
                carreras: null,
            },
            noticias: null,
            eventos: null,
            facultades: null,
            countereventos:0,
            counternoticias:0,
            cargandoeventos:false,
            cargandonoticias:false,
            cantidadgruponoticias:null,
            cantidadgrupoeventos:null,
        },
        created: function () {
            this.noticias = laravel.noticias;
            this.eventos = laravel.eventos;
            this.facultades = laravel.facultades;
            this.formulario.anexos = laravel.anexos;
            this.formulario.prospecto.nivel = 'grado';
            this.formulario.prospecto.modalidad = 'presencial';
            this.formulario.prospecto.anexo = this.formulario.anexos[0];

            this.cantidadgruponoticias  = laravel.cantidadgruponoticias;
            this.cantidadgrupoeventos   = laravel.cantidadgrupoeventos;

            this.traerCampanias();
            this.traerCarreras();
        },
        methods:{
            decodeHtml: function (html) {
                var txt = document.createElement("textarea");
                txt.innerHTML = html;
                return txt.value;
            },
            scrollTo: function(elemento){
                $('html, body').animate({
                    scrollTop: $("#"+ elemento).offset().top - 70
                }, 2000);
            },
            traerCampanias: function () {

                this.formulario.prospecto.campania = null;
                this.formulario.campanias = null;

                if (this.formulario.prospecto.anexo !== null) {

                    var url = '/api/crm/campanias/anexos/' +
                        this.formulario.prospecto.anexo.idCrm;

                    this.$http.get(url,{
                      params: {
                        nivel: this.formulario.prospecto.nivel,
                        modalidad: this.formulario.prospecto.modalidad,
                        facultad: null,
                      }
                    }).then(
                        function (response) {
                            this.formulario.campanias = response.data;
                            this.formulario.prospecto.campania = this.formulario.campanias[0];
                        }, function (error) {

                        });
                } else {
                    this.formulario.prospecto.campania = null;
                    this.formulario.campanias = null;
                }
            },
            traerCarreras: function () {

                this.formulario.prospecto.carreras = [];
                this.formulario.carreras = null;
                this.formulario.cargando = true;

                if (this.formulario.prospecto.anexo !== null) {

                    var url = '/api/carreras/anexos/' + this.formulario.prospecto.anexo.id;

                    this.$http.get(url, {
                        params: {
                          tipo: this.formulario.prospecto.nivel,
                          modalidad: this.formulario.prospecto.modalidad
                        }
                    }).then(
                        function (response) {
                            this.formulario.carreras = response.data;
                            this.formulario.cargando = false;
                        }, function (error) {
                            this.formulario.prospecto.carreras = [];
                            this.formulario.carreras = null;
                            this.formulario.cargando = false;
                        });
                } else {
                    this.formulario.prospecto.carreras = [];
                    this.formulario.carreras = null;
                    this.formulario.cargando = false;
                }
            },
            llenarCombos:function(){
                this.traerCampanias();
                this.traerCarreras();
            },
            submit: function () {
                this.formulario.cargandoenvio = true;
                this.$http.post('/api/crm/prospecto', this.formulario.prospecto, {
                    headers: {
                        'X-CSRF-TOKEN': document.querySelector('input[name="_token"]').getAttribute('value')
                    }
                }).then(
                    function (response) {
                        this.formulario.errores = null;
                        this.formulario.enviado = true;
                        location.href = "/solicitud-informacion/gracias";
                    }, function (error) {
                        if(error.data !== null && typeof error.data === 'object'){
                            this.formulario.errores = error.data;
                            this.formulario.cargandoenvio = false;
                            this.scrollTo('formulario');
                        }
                    }
                );
            },
            obtenerIconoConFilete: function (array) {
                if(array!==undefined){
                    for (i = 0; i < array.length; i++) {
                        if(array[i].match(/\b-filete\b/) !== null){
                            return array[i];
                        }
                    }
                }
            },
            obtenerIconoSinFilete: function (array) {
                if(array!==undefined){
                    for (i = 0; i < array.length; i++) {
                        if(array[i].match(/\b-filete\b/) === null){
                            return array[i];
                        }
                    }
                }
            },
            refrescarNoticias: function (counter){
                this.cargandonoticias = true
                counter = counter+1;
                if( counter < this.cantidadgruponoticias ){
                    this.counternoticias = counter;
                    this.cargandonoticias = false;
                }else if( counter == this.cantidadgruponoticias ){
                    this.counternoticias = 0;
                }

            },
            refrescarEventos: function(key){
                this.cargandoeventos = true
                key = key+1;
                if( key < this.cantidadgrupoeventos ){
                    this.countereventos = key;
                    this.cargandoeventos = false;
                }else if( key == this.cantidadgrupoeventos ){
                    this.countereventos = 0;
                }
            },

        }
    });

    var grupos = [];
    var anexos = [];

    $app.formulario.anexos.forEach(function (h) {
        anexos.push(Number(h.agrupar));
    });

    var agrupacionSinRepetidos = anexos.reduce(function(a,b){
        if (a.indexOf(b) < 0 ) a.push(b);
        return a;
    },[]);

    //Armo el array con los anexos que son los agrupadores.
    for (i = 0; i < agrupacionSinRepetidos.length; i++) {
        for (a = 0; a < $app.formulario.anexos.length; a++) {
            var h = $app.formulario.anexos[a];
            if(agrupacionSinRepetidos[i] === h.id){
                grupos.push({
                    idgrupo:    h.agrupar,
                    provincia:  h.provincia,
                    direccion:  h.direccion,
                    email:      h.email,
                    url:        h.url,
                    coords:
                        [
                            h.latitud,
                            h.longitud
                        ]
                });
                break;
            }
        }
    }

    $(function(){
        var grupoSeleccionadoAlCargar = 0;

        var map = new jvm.Map({
            container: $('#mapa'),
            map: 'ar_merc',
            zoomOnScroll: false,
            panOnDrag: true,
            zoomMax: 50,
            zoomButtons: false,
            backgroundColor:'#f3f4f4',
            markers: grupos.map(function (h) {
                return {name: h.provincia, latLng: h.coords}
            }),
            regionStyle: {
                initial: {
                    fill: '#fff',
                    "fill-opacity": 1,
                    stroke: '#000000',
                    "stroke-width": 1,
                    "stroke-opacity": 1
                },
                hover: {
                    "fill-opacity": 1,
                    cursor: 'cursor'
                }
            },                        
            markerStyle:{
                initial: {
                    fill: '#7bc9b8',
                    "fill-opacity": 1,
                    stroke: '#fff',
                    "stroke-width": 10,
                    "stroke-opacity": 0.0,
                    r: 10
                },
                selected: {
                    fill: '#0E6A5B'
                }
            },
            onRegionTipShow: function (e, el, code) {
                e.preventDefault();
            },
            onRegionClick: function (e, el, code) {
                e.preventDefault();
            }, 
            onMarkerTipShow: function (e, el, code) {
                e.preventDefault();
            },
            onMarkerClick: function (e, el, code) {
                map.clearSelectedMarkers();
                map.setSelectedMarkers(el);

                var anexo = grupos[el];
                mostrarInfoAnexosEnMapa($('#info-anexo'), anexo);
            }    

        });

        function mostrarInfoAnexosEnMapa(elemento, anexo){
            // recorro todos los anexos el item agrupar con el anexo agrupador seleccionado
            elemento.html("");
            $app.formulario.anexos.map(function (h) {
                if( h.agrupar == anexo['idgrupo'] )
                {
                    elemento.append("<div class='bloque-info'>"
                        +"<div class='anexo_titulo' style='font-weight:bold'>"+h.titulo+"</div>"
                        +"<div>"+h.direccion+"</div>"
                        +"<div><a href='" + h.url + "'>Conocer sede</a></div>"
                        +"</div>"
                    );
                }
            })
        }

        map.setSelectedMarkers(grupoSeleccionadoAlCargar);
        var anexo = grupos[grupoSeleccionadoAlCargar];
        mostrarInfoAnexosEnMapa($('#info-anexo'), anexo);
    }); 

</script>

