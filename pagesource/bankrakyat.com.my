            data = $.parseJSON(data);
            if(data.status == 'success') window.location.href = data.page;
        });
    });

    /* online banking picklist at header */
    $('.select-banking').change(function() {
        if ($('.select-banking').val() != '') {
            var target = $('option:selected', this).attr('data-open');
            var link = $('.select-banking').val();
            if(target == 'blank') window.open(link,'_blank');
            else window.location = link;
        }
    });

    /* article rightbar pick type/section */
    $('.article-select-type').change(function() {
        if ($('.article-select-type').val() != '') {
            var link = $('.article-select-type').val();
            window.location = link;
        }
    });

    /* apply now button */
    $("#btnApplyNow").click(function(event) {
        event.preventDefault();

        var $ac = "";
        var $pg = "";

        $.post("/t/applynow_ajax", {tpl:"applynow_ajax", action:"track", id:$ac, code:$pg})
        .done(function(data) {});
    });
    
    /* detect mobile */
    function detectmob() { 
        if( navigator.userAgent.match(/Android/i)
            || navigator.userAgent.match(/webOS/i)
            || navigator.userAgent.match(/iPhone/i)
            || navigator.userAgent.match(/iPad/i)
            || navigator.userAgent.match(/iPod/i)
            || navigator.userAgent.match(/BlackBerry/i)
            || navigator.userAgent.match(/Windows Phone/i)
        ) {
            return true;
        } else { return false;
        }
    }

    var ismobile = detectmob();
    if(ismobile === true) {
        $("[wa-type='share_wa_web']").hide();
        $("[wa-type='share_wa_mob']").show();
    } else { $("[wa-type='share_wa_web']").show();
        $("[wa-type='share_wa_mob']").hide();
    }
</script>

