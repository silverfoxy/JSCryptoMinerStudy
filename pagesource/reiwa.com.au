    function configureAutoComplete() {

        var searchBox = $("#ctl00_uxSearchProperty_ucSearchPropertyCriteriaResidential_uxSearchLocation_txtListingSearch");
        searchBox.addClass("clearable");
        searchBox.autocomplete({
            source: function (request, response) {
                var searchType = $("#ctl00_uxSearchProperty_ucSearchPropertyCriteriaResidential_uxSearchLocation_hidSearchType").val();
                var searchString = extractLast($("#ctl00_uxSearchProperty_ucSearchPropertyCriteriaResidential_uxSearchLocation_txtListingSearch").val());

                // delegate back to autocomplete, but extract the last term
                $.ajax({
                    url: 'https://ac.reiwa.com.au/api/AutoComplete',
                    type: 'GET',
                    dataType: 'json',
                    data: {
                        listingSearchTypeId: searchType,
                        searchString: searchString
                    },
                    contentType: "text/plain; charset=utf-8",
                    traditional: true,
                    success: function (data, textStatus, xhr) {
                        var list = [];

                        for (var i = 0; i < data.results.length; i++) {
                            list.push({
                                field: data.results[i].category,
                                value: data.results[i].description,
                                label: data.results[i].displayName,
                                category: data.results[i].category
                            });
                        }

                        response(list);
                    },
                    error: function (xhr, textStatus, errorThrown) {
                    }
                });
            },

            focus: function () {
                // prevent value inserted on focus
                return false;
            },

            // Called when user selects an Autocomplete menu option
            select: function (event, ui) {
                // Terms, displayed in the search input textbox
                var terms = splitTerms(this.value);
                // remove the current input
                terms.pop();
                // add the capitalised selected option
                var selectedOption = ui.item.value.replace(/\b[a-z]/g, function (letter) {
                    return letter.toUpperCase();
                });

                switch (ui.item.category.toLowerCase()) {
                    case 'region':
                        selectedOption += '-region';
                        break;

                    case 'sub-region':
                        selectedOption += '-sub-region';
                        break;

                    case 'inner-region':
                        selectedOption += '-inner-region';
                        break;

                    case 'postcode':
                        selectedOption = selectedOption.split(' ').slice(1).join(' ');
                        break;

                    case 'listingno':
                        selectedOption = selectedOption.split(' ')[0];
                        break;
                }

                terms.push(selectedOption);
                // add placeholder to get the comma-and-space at the end
                terms.push("");
                this.value = terms.join("; ");

                //if (onAutoCompleteTextChange != null) {
                //    onAutoCompleteTextChange(this.value, $(this));
                //}

                return false;
            }
        })

       // Changes this from a flat to a categorised list
      .data("ui-autocomplete")._renderMenu = function (ul, items) {
          var that = this,
              currentCategory = "";
          $.each(items, function (index, item) {
              if (item.category != currentCategory) {
                  ul.append('<li class=\'ui-autocomplete-category\'>' + item.category + '</li>');
                  currentCategory = item.category;
              }
              that._renderItemData(ul, item);
          });
      };
    }

    function extractLast(term) {
        return splitTerms(term).pop();
    }

    function splitTerms(val) {
        return val.split(/;\s*/);
    }
</script>

