<script>
    $(document).ready(function () {
        // bind list items to the responsive mobile version
        bindMobileMenu();

        var menu = $(".navbar-global .nav.navbar-nav.container"),
            subMenu = $(".navbar-global .moreDropDown"),
            more = $("#moreNavItem"),
            parent = $(".navbar-global"),
            ww = $(window).width(),
            smw = more.outerWidth();        

        more.css('width', smw);

        function contract() {
            menu.children("li").each(function () {
                var w = $(this).outerWidth();
                var elementWidth = $(this).find("span").outerWidth() + 20;
                $(this).data("outerwidth", elementWidth);
                if (w > smw) smw = w + 20;
                return smw;
            });
            var w = 0,
                outerWidth = parent.width() - smw - 20;
            for (i = 0; i < menu.children("li").size() ; i++) {
                w += menu.children("li").eq(i).data("outerwidth");
                if (w > outerWidth) {
                    menu.children("li").eq(i).nextAll().not("#moreNavItem")
                        .detach()
                        .css('opacity', 0)
                        .prependTo(".moreDropDown")
                        .stop().animate({
                            'opacity': 1
                        }, 300);
                    break;
                }
            }
            showOrHideMore();
        }

        function expand() {
            menu.children("li").each(function () {
                var w = $(this).outerWidth();
                var elementWidth = $(this).find("span").outerWidth()+20;
                $(this).data("outerwidth", elementWidth);
                if (w > smw) smw = w + 20;
                return smw;
            });
            var w = 0,
                outerWidth = parent.width() - smw - 20;
                menu.children("li").each(function () {
                var elementWidth = $(this).outerWidth()+20;
                w += elementWidth;
                return w;
                });
            var numberOfItemsInDropdown = subMenu.children("li").size();
            for (i = 0; i < numberOfItemsInDropdown ; i++) {
                w += subMenu.children("li").eq(0).data("outerwidth");
                if (w > outerWidth) {
                    subMenu.children("li").eq(0)
                        .detach()
                        .appendTo(".navbar-global .nav.navbar-nav.container");
                }
            }

            showOrHideMore();
        }

        function showOrHideMore() {
            more.detach();
            more.appendTo(".navbar-global .nav.navbar-nav.container");
            if (subMenu.children("li").length == 0) {
                more.hide();
            } else {
                more.show();
            }
        }
        contract();

        $(window).on("resize", function (e) {
            if ($(window).width() < 768) {
                return;
            }
            ($(window).width() > ww) ? expand() : contract();
            ww = $(window).width();
            
        });

        function bindMobileMenu() {
            var lis = $('ul.nav.navbar-nav.container').children("li");
            lis.eq(0).nextAll().andSelf().not("#moreNavItem").clone().appendTo('#ulMobile');
        }

    });
</script>

