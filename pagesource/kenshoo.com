<script type='text/javascript'>
var themeCurrentLang = (function(){
	if ( 'undefined' == typeof themeLocalizations ) 
		return {};

	var current;
	var hostname = window.location.hostname;

	// hostname = 'www.kenshoo.mx';
	// hostname = 'www.kenshoo.com.sg';

	themeLocalizations.forEach( function( lang, i ) {
		if ( hostname != themeLocalizations[ i ].host ) 
			return;

		current = themeLocalizations[ i ];
	});

	if ( !current ) 
		current = themeLocalizations[0];

	createCss();

	return current;

	function createCss( css ) {
		var css = getCss();

		if ( !css )
			return;

		var head  = document.head || document.getElementsByTagName( 'head' )[0];
		var style = document.createElement( 'style' );

		style.type = 'text/css';
		style.id   = 'theme-localization-styles';

		if ( style.styleSheet ) {
			style.styleSheet.cssText = css;
		} else {
			style.appendChild( document.createTextNode( css ) );
		}

		head.appendChild( style );
	}

	function getCss() {
		if ( !current.host ) 
			return;

		var css  = '';
		var hide = ' { display:none !important; }';
		var tags = [];

        if ('en-us' == current.code) {
            tags.push('en');
            tags.push('en-us');
        } else {
			tags.push( 'intl' );

			if ( 'en-gb' == current.code ) {
				tags.push( 'en' );
				tags.push( 'en-gb' );
			} else {
				tags.push( current.class );
			}
		}

		tags.forEach( function( code, i ) {
			css += '.hide-' + code + hide;
		});

		return css;
	}
})();
</script>
