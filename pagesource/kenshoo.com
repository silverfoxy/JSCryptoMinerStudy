<script type='text/javascript'>
;(function() {
	if ( 'undefined' == typeof themeFrontLatestPosts )
		return;

	document.addEventListener( 'DOMContentLoaded', ready );

	var isDocReady  = false;
	var isAjaxReady = false;

	var xhr  = new XMLHttpRequest();
	var data = '';
	var url  = window.location.origin + '/' + themeFrontLatestPosts.url + '&lang-code=' + themeCurrentLang.code;

	xhr.open( 'GET', url, true );
	xhr.send();

	xhr.onreadystatechange = function() {
		if ( 4 != xhr.readyState ) 
			return;

		isAjaxReady = true;

		if ( xhr.status != 200 ) {
			// alert(xhr.status + ': ' + xhr.statusText);
		} else {
			data = xhr.responseText;
		}

		if ( isDocReady ) 
			showContent();
	}

	function ready() {
		isDocReady = true;

		if ( isAjaxReady ) 
			showContent();
	}

	function showContent() {
		var block = document.getElementById( 'js-front-latest-posts' );

		block.innerHTML = data;
		block.classList.remove( 'loading-block' );
	}
})();
</script>
