<script type="text/javascript">
function enquiryForm(){
	var enquiry_name = document.getElementById('enquiry_name').value;
	var enquiry_number = document.getElementById('enquiry_number').value;
	var enquiry_email = document.getElementById('enquiry_email').value;
	var enquiry_pincode = document.getElementById('enquiry_pincode').value;
	
