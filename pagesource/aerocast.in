	var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	
	if( enquiry_name.trim() == ''){
		alert('Please Enter Your Name!');
		document.getElementById('enquiry_name').focus();
		return false;
	}else if( charreg.test( enquiry_name.trim() ) == false){
		alert('Please Enter Valid Name');
            document.getElementById('enquiry_name').focus();
            return false;
	}else if( enquiry_number.trim() == '' ){
	 alert('Please Enter Your mobile Number!');
	 document.getElementById( 'enquiry_number' ).focus();
	 return false;
 }else if( isNaN (enquiry_number.trim()) ){
	 alert('Please Enter Number Only!!');
	 document.getElementById( 'enquiry_number' ).focus();
	 return false;
 }else if( enquiry_number.length != 10 ){
	 alert('Please Enter Your 10 Digit mobile Number!');
	 document.getElementById( 'enquiry_number' ).focus();
	 return false;
 }else if( enquiry_number.trim().charAt(0) == '0'){
	 alert( 'Please Do Not put zero!' );
	  document.getElementById( 'enquiry_number' ).focus();
	 return false;
 }else if( enquiry_number.trim().charAt(+91) == '+91'){
	 alert( 'Please Do Not put +91!' );
	  document.getElementById( 'enquiry_number' ).focus();
	 return false;
 }else if( enquiry_number.trim().charAt(+91) == '+91'){
	 alert( 'Please Do Not put +91!' );
	  document.getElementById( 'enquiry_number' ).focus();
	 return false;
 }else if( enquiry_number.trim() == '0123456789' || enquiry_number.trim() == '9876543210' || enquiry_number.trim() == '1234567890'){
	 alert( 'Please Enter Valid Mobile Number!' );
	  document.getElementById( 'enquiry_number' ).focus();
	 return false;
 }else if( enquiry_email.trim() == ''){
		alert('Please Enter Your Email!');
		document.getElementById('enquiry_email').focus();
		return false;
	}else if( reg.test( enquiry_email.trim() ) == false){
		alert('Please Enter Valid Email!');
			document.getElementById('enquiry_email').focus();
			return false;		
	}
	else if( enquiry_pincode.trim()== '' ){
		alert('Please Enter Valid Pincode!');
		document.getElementById('enquiry_pincode').focus();
		return false;		
	}
	else if( isNaN(enquiry_pincode.trim())){
		alert('Please Enter Pincode Only Number!');
		document.getElementById('enquiry_pincode').focus();
		return false;		
	}
	else{
		document.enquiryFormSubmit.submit();
	}
}
