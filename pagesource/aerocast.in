<script type="text/javascript">
function newConnForm(){
	var new_name = document.getElementById('new_name').value;
	var new_num = document.getElementById('new_num').value;
	var new_email = document.getElementById('new_email').value;
	var new_city = document.getElementById('new_city').value;
	var new_area = document.getElementById('new_area').value;
	var new_pincode = document.getElementById('new_pincode').value;
	var new_remark = document.getElementById('new_remark').value;
	var new_add = document.getElementById('new_add').value;
	
	var charreg = /^[a-zA-Z\_ ]+$/g;
	var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	
	if( new_name.trim() == ''){
		alert('Please Enter Your Name!');
		document.getElementById('new_name').focus();
		return false;
		
	}else if( charreg.test( new_name.trim() ) == false){
		alert('Please Enter Valid Name');
            document.getElementById('new_name').focus();
            return false;
			
	}else if( new_num.trim() == '' ){
	 alert('Please Enter Your mobile Number!');
	 document.getElementById( 'new_num' ).focus();
	 return false;
	 
	 }else if( isNaN (new_num.trim()) ){
		 alert('Please Enter Number Only!!');
		 document.getElementById( 'new_num' ).focus();
		 return false;
		 
	 }else if( new_num.length != 10 ){
		 alert('Please Enter Your 10 Digit mobile Number!');
		 document.getElementById( 'new_num' ).focus();
		 return false;
		 
	 }else if( new_num.trim().charAt(0) == '0'){
		 alert( 'Please Do Not put zero!' );
		  document.getElementById( 'new_num' ).focus();
		 return false;
		 
	 }else if( new_num.trim() == '1234567890' || new_num.trim() == '9876543210' ){
		 alert( 'Please Enter Valid Mobile Number!' );
		  document.getElementById( 'new_num' ).focus();
		 return false;
		 
	 }else if( new_email.trim() == ''){
			alert('Please Enter Your Email!');
			document.getElementById('new_email').focus();
			return false;
			
	}else if( reg.test( new_email.trim() ) == false){
		alert('Please Enter Valid Email!');
			document.getElementById('new_email').focus();
			return false;	
			
	}else if( new_city.trim() == ''){
			alert('Please Enter Your City/Twon!');
			document.getElementById('new_city').focus();
			return false;
			
	}else if( new_area.trim() == ''){
			alert('Please Enter Your Area/Sector!');
			document.getElementById('new_area').focus();
			return false;
			
	}else if( new_pincode.trim() == ''){
			alert('Please Enter Your Pin Code!');
			document.getElementById('new_pincode').focus();
			return false;
			
	}else if( new_remark.trim() == ''){
			alert('Please Enter Remarks!');
			document.getElementById('new_remark').focus();
