<!-- Enquiry Form Starts Here-->
<script type="text/javascript">
function popupEnquiry(){
	var popup_name = document.getElementById('popup_name').value;
	var popup_number = document.getElementById('popup_number').value;
	var popup_email = document.getElementById('popup_email').value;
	var popup_pincode = document.getElementById('popup_pincode').value;
	
	var charreg = /^[a-zA-Z\_ ]+$/g;
	var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	
	if( popup_name.trim() == ''){
		alert('Please Enter Your Name!');
		document.getElementById('popup_name').focus();
		return false;
	}else if( charreg.test( popup_name.trim() ) == false){
		alert('Please Enter Valid Name');
            document.getElementById('popup_name').focus();
            return false;
	}else if( popup_number.trim() == '' ){
	 alert('Please Enter Your mobile Number!');
	 document.getElementById( 'popup_number' ).focus();
	 return false;
 }else if( isNaN (popup_number.trim()) ){
	 alert('Please Enter Number Only!!');
	 document.getElementById( 'popup_number' ).focus();
	 return false;
 }else if( popup_number.length != 10 ){
	 alert('Please Enter Your 10 Digit mobile Number!');
	 document.getElementById( 'popup_number' ).focus();
	 return false;
 }else if( popup_number.trim().charAt(0) == '0'){
	 alert( 'Please Do Not put zero!' );
	  document.getElementById( 'popup_number' ).focus();
	 return false;
 }else if( popup_number.trim() == '1234567890' || popup_number.trim() == '9876543210' ){
	 alert( 'Please Enter Valid Mobile Number!' );
	  document.getElementById( 'popup_number' ).focus();
	 return false;
 }else if( popup_email.trim() == ''){
		alert('Please Enter Your Email!');
		document.getElementById('popup_email').focus();
		return false;
	}else if( reg.test( popup_email.trim() ) == false){
		alert('Please Enter Valid Email!');
			document.getElementById('popup_email').focus();
			return false;		
	}else if( popup_pincode.trim() == ''){
		alert('Please Enter Pin Code!');
			document.getElementById('popup_pincode').focus();
			return false;		
	}
	else if( popup_pincode.length != 6){
		alert('Please Enter Valid Pin Code!');
			document.getElementById('popup_pincode').focus();
			return false;		
	}
	else{
		document.popenquiry.submit();
	}
}
</script>

