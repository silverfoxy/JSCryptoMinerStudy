	/*champ recherche*/
	var InputRechercheInitial = 'Saisissez ici le titre que vous cherchez';
	$('#InputRecherche')
		.focusin(function(){
			if($(this).val()==InputRechercheInitial) {$(this).val('');}
			$(this).css('color','#000000');
		})
		.focusout(function(){
			if($(this).val()=='') {
				$(this).val(InputRechercheInitial);
				$(this).css('color','#b3b3b3');
			}			
		});	
	if($('#InputRecherche').val()!=InputRechercheInitial) {$('#InputRecherche').css('color','#000000');}
	$('#FormRechercheSubmit').click(function() {
		$('#FormRecherche').submit();
	});
	$('#FormRecherche').submit(function() {
		if($('#InputRecherche').val()!=InputRechercheInitial) {
			var titre = $('#InputRecherche').val();
			location.href="catalogue.php?recherche=oui&titre="+titre+"#moteur";
		} else {
			location.href="catalogue.php";
		}
		return false;
