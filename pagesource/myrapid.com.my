function fn_planner_go(){
    var str_page_name;
	str_page_name = "plan-my-journey";

    var s_from = $("#txt_from").val();
    var s_to = $("#txt_to").val();

    if (s_from.length === 0 && s_to.length === 0) {
            return;
    }

    //william - get time
    var $time = '';
    if ($('#select_depart_hour').val() != '' && $('#select_depart_min').val() != '') {
        if ($('#select_depart_tt').val() == 'PM' && $('#select_depart_hour').val() != '12') {
            var int_hour = Number($('#select_depart_hour').val()) + 12
            $time = int_hour + ':' + $('#select_depart_min').val() + ":00";
        } else {
            $time = $('#select_depart_hour').val() + ':' + $('#select_depart_min').val() + ":00";
        }
    }

	if (s_from == 'Your Location'){
		location.href = str_page_name +
                "?from=" + encodeURIComponent(s_from) +
                "&to=" + encodeURIComponent(s_to) +
                "&frlat=" + $("#hf_from_lat").val() +
                "&frlng=" + $("#hf_from_lng").val() +
                //william - pass time and options in querystring
                "&time=" + $time +
                "&routing=" + $("input[name=route-option]:checked").val() +
                "&transport=" + $("input[name=route-type]:checked").val();
	}else {
		location.href = str_page_name +
                "?from=" + encodeURIComponent(s_from) +
                "&to=" + encodeURIComponent(s_to) +
                //william - pass time and options in querystring
                "&time=" + $time +
                "&routing=" + $("input[name=route-option]:checked").val() +
                "&transport=" + $("input[name=route-type]:checked").val();
	}
}



