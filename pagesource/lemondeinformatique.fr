                {
                    $(".popup-content").remove();
                    $(".popup").prepend(msg);
                }
           }
         });
    
    });

	$('body').on( 'click', '.popup .close-popup', function() {       
        $(".overlay").remove();
        $("body").removeClass("overflow-hidden");
    });


