				$('sl-check-id').set('class','updates_pulldown');
			}
			if($('sl-check-update-id') != undefined && $('sl-check-update-id').hasClass('updates_pulldown_active')){
				$('sl-check-update-id').set('class','updates_pulldown');
			}
		
		
