<script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/elasticsearch/14.0.0/elasticsearch.jquery.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/elasticsearch/14.0.0/elasticsearch.js"></script>
	<script type="text/javascript">

    var query_array;
    var autocomplete_html = '';
    var active_id;
    var results_length;
	var search_text_input;
    /* Instantiate Elasticsearch Client */
    var client = new $.es.Client({
      host: 'https://public:public@9d7f05d785e8d69665252463d0dd2f89.us-west-2.aws.found.io:9243',
      log: 'trace'
    });
    
    /* Activate search bar and call ES client search */
    $(document).ready(function() {
      $('#txtSearch').keyup(function(){
        search(this.value);
		search_text_input = this.value;
		if (this.value.match(/^[A-Z]/i)) {
			$('#auto_contents').css("visibility","visible");
		}
		else {
			$('#auto_contents').css("visibility","hidden");
		}
      });
    });

    //$(document).keydown(function(e) {
    //  if (e.keyCode === 8) {
    //    document.getElementById("txtSearch").focus();
    //  }
    //});

    $(document).on('click', function(e) {
      if( e.target.id != 'auto_contents') {
        $("#auto_contents").hide();
      }
    });

	$("#auto_contents").mouseout(function() {
		document.getElementById("txtSearch").focus();
	});
	
    // Change focus to dropdown menu on Down Arrow
    $("#txtSearch").keydown(function(event) {
	  if (event.keyCode == 40) {
	  event.preventDefault();
        document.getElementById("0").focus();  
        active_id = 0;
        document.getElementById("txtSearch").value = document.getElementById("0").children[0].text;
      }
    });

    $('#auto_contents').click(function(e) {
	  e.preventDefault();
	  e.stopPropagation();
      if (e.isTrigger) {
        return;  
      } else {

		var select_html;

		var clicked = e.target.parentElement.innerHTML.toString();
		if (clicked.startsWith("<li")) {
			select_html = e.target.childNodes[0].innerHTML;
		}
		else if (clicked.startsWith("<a")) {
			select_html = e.target.innerHTML;
		}
		else {
			select_html = clicked;
		}


		var substring = select_html.replace("<b>","");
		var substring2 = substring.replace("</b>","");
        selectPlace(substring2);

        $("#auto_contents").hide();
		$("#btnGo").trigger('click');
		
      }
    })

    $('#auto_contents').keydown(function(e) {
      e.preventDefault();
      results_length = query_array.length - 1;

      if (e.keyCode === 40) {
        if (active_id >= results_length) {
          active_id = 0;
        } else {
          active_id++;
        } 
        document.getElementById("txtSearch").value = document.getElementById(active_id).children[0].text;
      }
      else if (e.keyCode === 13) {
        document.getElementById("txtSearch").value = document.getElementById(active_id).children[0].text;
        $("#auto_contents").html(autocomplete_html).hide();
		$("#btnGo").trigger('click');
      }
      else if (e.keyCode === 38) {
        if (active_id <= 0) {
          active_id = results_length;
        }
        else {
          active_id--;
        }
        document.getElementById("txtSearch").value = document.getElementById(active_id).children[0].text;
      }
      enableArrows(query_array, active_id);
    });


    function selectPlace(selection) {
      document.getElementById("txtSearch").value = selection;
    };

    // save array Callback function
    function saveArray(input_array) {
      return input_array;
    };


    // add array to HTML 
    function addArrayToHTML(query_array,input_text) {

      autocomplete_html = '';
      for (var i = 0; i <= query_array.length - 1; i++) {
        autocomplete_html += '<li tabindex="-1" id="'+ [i] +'"><a class="placeslist" href="">' + query_array[i][0].substr(0,input_text.length) + '<b>' + query_array[i][0].substr(input_text.length,query_array[i][0].length) + '</b></a></li>';
      }
      autocomplete_html += '';
      $("#auto_contents").html(autocomplete_html).show();
    }

    // enable arrow functionality
    function enableArrows(query_array, id) {
      var num_records = query_array.length -1 

      if (id < 0) {
        id += num_records + 1;
      }
      if (id > num_records) { 
        id -= num_records + 1;
      }
      document.getElementById([id]).focus();
    }; 


     /* ES Autocomplete (JSON format) */
     function search(input_text) {
      client.search({
        index: 'mainsearch_places',
        body: {
          suggest: {
            name_suggest: {
              //prefix: "fort w",
              prefix: input_text,
              completion: {
                field: "Name",
                size: 5000,
                fuzzy: {
                  fuzziness: 0
                }
              }
            }
          }
        }
      }).then(function (resp) {

        /* Retrieve hits, save to output array */
        var hits = resp.suggest.name_suggest[0].options;
        var out_array = []
        for (i = 0; i < hits.length; i++) {
          out_array.push([hits[i]._source.Name, hits[i]._source.OneCode, hits[i]._source.Population]);
        };
        
        /* Sort Array by Integer */
        out_array.sort(function(a,b){
          return a[2] - b[2];
        });
        
        // Remove all but top 10 results and sort Descending by Population
		var num_of_results = 5
        if (out_array.length >= num_of_results) {
          out_array = out_array.slice(out_array.length - num_of_results,out_array.length);
        } 
        
        /* Reverse array sorting */
        out_array.reverse();

        /* Create Display Array for Search UI */
        query_array = [];
        for (var i=0; i < out_array.length; i ++) {
          query_array.push([out_array[i][0],out_array[i][1]]);
        }   

        // create HTML array for dropdown
        addArrayToHTML(saveArray(query_array),search_text_input);

        // if the input field is empty, then hide it
        var input_length = $('#txtSearch').val().length;
        if (input_length < 1) {
          $("#autocomplete").children().hide();
          var active_id = '0';
        }
        else {
          $("#autocomplete").children().show();
        }

      }, function (err) {
        console.trace(err.message);
      }); 
    }
  </script>
	
