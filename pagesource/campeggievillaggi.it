$("#search-submit").click(function()
{
    var data        = new Object();
    data.star       = 0;
    data.location   = "";
    data.service    = "";
    data.zone       = "";
    data.idRegion   = "";

    // Recupero numero di stelle selezionate
    for(var i = 5; i > 0; i--)
    {
        if ($(".bk-search-star[bk-data-id=" + i + "]").hasClass("bk-search-star-selected") && data.star==0) data.star = i;
    }

    $(".search-zone").each(function()
    {
        if ($(this).is(":checked")) data.zone += $(this).attr("bk-data-id") + "-";
    });

    $(".search-location").each(function()
    {
        if ($(this).is(":checked")) data.location += $(this).attr("bk-data-id") + "-";
    });

    $(".search-service").each(function()
    {
        if ($(this).is(":checked")) data.service += $(this).attr("bk-data-id") + "-";
    });


    if (data.zone.length == 0 && $("#search-region").val() == "-1")
    {
        $("#search-result").removeClass("bk-hided");
        return;
    }

    $("#search-submit").html("ricerca in corso&nbsp;&nbsp;<i class='fa fa-refresh fa-spin'></i>");
    //$("#search-submit").attr("disabled", "disabled");

    data.name       = $("#search-name").val();
    data.idRegion	= $("#search-region").val();
    data.idProv	    = $("#search-prov").val();
    data.location   = data.location.length > 0?data.location.slice(0,-1):"";
    data.service    = data.service.length > 0?data.service.slice(0,-1):"";
    data.zone       = data.zone.length > 0?data.zone.slice(0,-1):"";

    jsonData	= JSON.stringify(data);
    $.ajax(
        {
            type: 'POST',
            url: "http://www.campeggievillaggi.it/api/get/url/search/",
            dataType: "json",
            data: {data:jsonData},
            async:false,
            success: function(response)
            {
                //alert(response.url);
                clearSearchFields();
                location.href = response.url;
            }
        });
});

