var getRegion = function(id, elementId, showAllOptions)
{
    showAllOptions = (typeof showAllOptions !== 'undefined'?showAllOptions:true);
    var allRegionOption = "<option value='-1'>-- Tutte le regioni --</option>";

    if (id == "-1")
    {
        $("#" + elementId + "region").attr("disabled", "disabled");
        $("#" + elementId + "region").html(showAllOptions?allRegionOption:"");

        $("#" + elementId + "region option[value='-1']").prop("selected", true);
        getProv($("#" + elementId + "region").val(), elementId);
    }
    else
    {
        $.ajax(
            {
                type: 'GET',
                url: "http://www.campeggievillaggi.it/api/get/region/" + id + "/",
                dataType:"json",
                async: false,
                success: function (res) {
                    var str = showAllOptions?allRegionOption:"";

                    $.each(res.data, function (key, value) {
                        str += '<option value="' + value.id + '">' + value.regione + '</option>';
                    });

                    $("#" + elementId + "region").html(str);
                    $("#" + elementId + "region").removeAttr("disabled");
                    getProv($("#" + elementId + "region").val(), elementId);
                }
            });
    }
}

var getProv = function(id, elementId)
{
    var allProvOption = "<option value='-1'>-- Tutte le province --</option>";

    if (id == "-1")
    {
        $("#" + elementId + "prov").attr("disabled", "disabled");
        $("#" + elementId + "prov").html(allProvOption);
        getCity($("#" + elementId + "prov").val(), elementId);
    }
    else
    {
        $.ajax(
            {
                type: 'GET',
                url: "http://www.campeggievillaggi.it/api/get/prov/" + id + "/",
                dataType:"json",
                async: false,
                success: function (res) {
                    var str = allProvOption;

                    $.each(res.data, function (key, value) {
                        str += '<option value="' + value.id + '">' + value.provincia + '</option>';
                    });

                    $("#" + elementId + "prov").html(str);
                    $("#" + elementId + "prov").removeAttr("disabled");
                    getCity($("#" + elementId + "prov").val(), elementId);
                }
            });
    }
}

var getCity = function(id, elementId)
{
    var allCityOption = "<option value='-1'>-- Tutte le località --</option>";

    if (id == "-1")
    {
        $("#" + elementId + "city").attr("disabled", "disabled");
        $("#" + elementId + "city").html(allCityOption);
    }
    else
    {
        $.ajax(
            {
                type: 'GET',
                url: "http://www.campeggievillaggi.it/api/get/city/" + id + "/",
                dataType:"json",
                async: false,
                success: function (res) {
                    var str = allCityOption;

                    $.each(res.data, function (key, value) {
