    if ($(this).attr("bk-data-id") == currentStar) // Disattivazione di tutte le stelle
    {
        for(var i = 5; i > 0; i--)
        {
            $(".bk-search-star[bk-data-id='" + i + "']").removeClass("bk-search-star-selected");
            $(".bk-search-star[bk-data-id='" + i + "']").removeClass("fa-star");
            $(".bk-search-star[bk-data-id='" + i + "']").addClass("fa-star-o");
