var bkValidate = function(idPanelToValidate, excludeDisabled)
{
    idPanelToValidate = (typeof idPanelToValidate !== 'undefined'?idPanelToValidate:"");
    excludeDisabled = (typeof excludeDisabled!== 'undefined'?excludeDisabled:true);

    // Controllo coerenza campi
    var validateStatus = true;
    var isToExclude = false;

    idPanelToValidate = idPanelToValidate != ""?"#" + idPanelToValidate + " ":"";

    $(idPanelToValidate + "[bk-data-validator]").each(function()
    {
        $("[bk-data-validator-for='" + $(this).attr("id") + "']").removeClass("has-success");
        $("[bk-data-validator-for='" + $(this).attr("id") + "']").removeClass("has-error");

        $("[bk-data-validator-for='" + $(this).attr("id") + "'] .bk-label").removeClass("has-success");
        $("[bk-data-validator-for='" + $(this).attr("id") + "'] .bk-label").removeClass("has-error");

        isToExclude = excludeDisabled && $(this).is(":disabled")?true:false;

        if (!isToExclude)
        {
            if ($(this).attr("type") == "checkbox")
            {
                if ($(this).prop("checked")) $("[bk-data-validator-for='" + $(this).attr("id") + "']").addClass("has-success");
                else
                {
                    $("[bk-data-validator-for='" + $(this).attr("id") + "']").addClass("has-error");
                    validateStatus = false;
                }
            }
            else
            {
                if ($(this).val().length < $(this).attr("bk-data-validator-min") || $(this).val().length > $(this).attr("bk-data-validator-max"))
                {
                    validateStatus = false;
                    $("[bk-data-validator-for='" + $(this).attr("id") + "']").addClass("has-error");
                    $("[bk-data-validator-for='" + $(this).attr("id") + "'] .bk-label").addClass("has-error");
                }
                else if (!$(this).val().match(bkValidatorExpr[$(this).attr("bk-data-validator-match")]))
                {
                    validateStatus = false;
                    $("[bk-data-validator-for='" + $(this).attr("id") + "']").addClass("has-error");
                    $("[bk-data-validator-for='" + $(this).attr("id") + "'] .bk-label").addClass("has-error");
                }
                else
                {
                    $("[bk-data-validator-for='" + $(this).attr("id") + "']").addClass("has-success");
                    $("[bk-data-validator-for='" + $(this).attr("id") + "'] .bk-label").addClass("has-success");
                }
            }
        }
    });

    return validateStatus;
};

var bkValidateClear = function(idPanelToValidate)
{
    idPanelToValidate = (typeof idPanelToValidate !== 'undefined'?idPanelToValidate:"");
    idPanelToValidate = idPanelToValidate != ""?"#" + idPanelToValidate + " ":"";

    $(idPanelToValidate + "[bk-data-validator]").each(function()
    {
        $("[bk-data-validator-for='" + $(this).attr("id") + "']").removeClass("has-success");
        $("[bk-data-validator-for='" + $(this).attr("id") + "']").removeClass("has-error");
    });
};

