			console.log("dataURL is invalid.");
			return;
		}
		if (typeof dataLabel == 'undefined' || !dataLabel || typeof dataValue == 'undefined' || !dataValue) {
			console.log("dataLabel or dataValue is not defined.");
		}
		
		
		var $thisSelector = $('#' + elementID);
		var $thisSelectorOptions = $thisSelector.find("option");
		if ($thisSelectorOptions.length > 0 && $thisSelectorOptions[0].value == "") {
			var $dummyOption = $thisSelectorOptions.first().clone();
			$thisSelector.empty();
			$thisSelector.append($dummyOption);
		} else {
			$thisSelectorOptions.empty();
		}
		
		$.get(dataURL, function(data){
			for (var index in data) {
				var item = data[index];
				var itemText = item[dataLabel];
				var itemCode = item[dataValue];
				var $newOption = $("<option>").val(itemCode).text(itemText);
				$thisSelector.append($newOption);
