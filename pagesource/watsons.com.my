	function changeCountryStateSelectorOptions(stateSelectorId, countryCode, easyDropDownOrSelectpicker, stateWithLabelContainerId) {
		if (typeof stateSelectorId == 'undefined' && typeof countryCode == 'undefined') {
			console.debug("changeStateOptions(): parms is not found");
			return;
		}
		var $stateSelector = $("#" + stateSelectorId);
		if ($stateSelector.length < 1) {
			console.debug("changeStateOptions(): stateSelector not found");
			return;
		}
		var statesData = [{"MY":[{"code":"JH","description":"Johor","countryIso":"MY"},{"code":"KH","description":"Kedah","countryIso":"MY"},{"code":"KN","description":"Kelantan","countryIso":"MY"},{"code":"KL","description":"Kuala Lumpur","countryIso":"MY"},{"code":"ME","description":"Melaka","countryIso":"MY"},{"code":"NS","description":"Negeri Sembilan","countryIso":"MY"},{"code":"PH","description":"Pahang","countryIso":"MY"},{"code":"PK","description":"Perak","countryIso":"MY"},{"code":"PL","description":"Perlis","countryIso":"MY"},{"code":"PG","description":"Pulau Pinang","countryIso":"MY"},{"code":"PU","description":"Putrajaya","countryIso":"MY"},{"code":"SL","description":"Selangor","countryIso":"MY"},{"code":"SE","description":"Terengganu","countryIso":"MY"},{"code":"SA","description":"Sabah","countryIso":"MY"},{"code":"SK","description":"Sarawak","countryIso":"MY"}]}];
		if (typeof statesData == 'undefined') {
			console.debug("changeStateOptions(): statesData is not found");
			return;
		}
		$stateSelector.prop('disabled', false);
		// easyDropDownOrSelectPicker 
		if (easyDropDownOrSelectpicker == "easyDropDown" && $stateSelector.parent().hasClass('old')) {
			$stateSelector.easyDropDown('destroy');
		}
		
		// vary options
		// clear all options
		var $dummyOption = $stateSelector.find("option").first();
		if ($dummyOption.val() != "") {
			$dummyOption = $('<option>', {
				value: "",
				text: "Please select"
			});
		}
		$stateSelector.empty();
		// put a dummy option
		$stateSelector.append($dummyOption);
		$stateSelector.val('');
		// input proper options, source data format: [ {"MY": [{"code":"MY_100", "description":"Malaysia 100"}, {...}] }, {...} ]
		for (var i in statesData) {
			for (var countryIso in statesData[i]) {
				// if countryCode is found
				if (countryIso == countryCode) {
					for (var k in statesData[i][countryIso]) {
						var stateData = statesData[i][countryIso][k];
						$stateSelector.append($('<option>', {
						    value: stateData.code,
						    text: stateData.description
						}));
					}
				}
			}
		}
		
		// easyDropDownOrSelectPicker
		if(easyDropDownOrSelectpicker == "easyDropDown" && !$stateSelector.parent().hasClass('old')){
			$stateSelector.easyDropDown();
		} else if (easyDropDownOrSelectpicker == "selectpicker") {
			$stateSelector.selectpicker("refresh");
		}
		
		// display state selector or not, enable/disable
		if (typeof stateWithLabelContainerId != 'undefined') {
			if ($stateSelector.children().length > 1) {
				$('#' + stateWithLabelContainerId).show();
				$stateSelector.prop('disabled', false);
			} else {
				$('#' + stateWithLabelContainerId).hide();
				$stateSelector.prop('disabled', 'disabled');
			}
		}
	}
	
