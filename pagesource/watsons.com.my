function callback(data, currentMessage) {
	if(data > 0) {
		openFreeItemsPopup(data, currentMessage);
	} else {
		updateAndShowMiniCart(data);
	}
}

function cdoGet(form_data, url,myCallback,methodType,dataType,errorCallBack) {
	showLoadingDiv();
	responseData = null;
	if(methodType==undefined||methodType==null)methodType="get";
	if(dataType==undefined||dataType==null)dataType="html";
	$.ajax({
		type: methodType,
		url: url,
		data: form_data,
		cache: false,
		dataType : dataType,
		success: function(responseData) {
			var hasError = isErrorPopup(responseData);
			if (hasError) {
				openContentPopup(responseData);
				closeLoadingDiv();
			}else {
				if(errorCallBack!=undefined&&errorCallBack!=null&&typeof(errorCallBack)=='function'
					&&$(responseData).find("input[name='errors']").length>0){
					errorCallBack.call(this,responseData);																
				}else{
					if(myCallback!=undefined&&myCallback!=null&&typeof(myCallback)== 'function' ){
						myCallback.call(this,responseData);
						//callback(responseData, 0);
						openContentPopup(responseData);
					}else {
						//callback(responseData, 0);
						openContentPopup(responseData);
					}
						
				}
				closeLoadingDiv();
			}
		},
		error:function(responseData){
			//alert("error:"+responseData.status);
			if(errorCallBack!=undefined && errorCallBack!=null && typeof(errorCallBack)== 'function') {
				errorCallBack.call(this, responseData);
			}
		}
	});
	return false;
}
function doGet(form_data, url,myCallback,methodType,dataType,errorCallBack) {
	responseData = null;
	if(methodType==undefined||methodType==null)methodType="get";
	if(dataType==undefined||dataType==null)dataType="html";
	$.ajax({
		type: methodType,
		url: url,
		data: form_data,
		cache: false,
		dataType : dataType,
		success: function(responseData) {
			var hasError = isErrorPopup(responseData);
			if (hasError) {
				openContentPopup(responseData);
			}else {
				if(errorCallBack!=undefined&&errorCallBack!=null&&typeof(errorCallBack)=='function'
					&&$(responseData).find("input[name='errors']").length>0){
					errorCallBack.call(this,responseData);																
				}else{
					if(myCallback!=undefined&&myCallback!=null&&typeof(myCallback)== 'function' ){
						myCallback.call(this,responseData);
					}else
						openContentPopup(responseData);
						//callback(responseData, 0);
				}	
			}
			/* $(".branchBankCodeSelectId-menu").show().css({"z-index":"-1"});
			$(".branchBankCodeSelectId-menu").mCustomScrollbar("update"); */
		},
		error:function(responseData){
			//alert("error:"+responseData.status);
		}
	});
	return false;
}

function doGetAsync(form_data, url,myCallback,methodType,dataType,errorCallBack,isAsync) {
	responseData = null;
	if(methodType==undefined||methodType==null)methodType="get";
	if(dataType==undefined||dataType==null)dataType="html";
	$.ajax({
		type: methodType,
		url: url,
		data: form_data,
		cache: false,
		async:isAsync,
		dataType : dataType,
		success: function(responseData) {
			var hasError = isErrorPopup(responseData);
			if (hasError) {
				openContentPopup(responseData);
			}else {
				if(errorCallBack!=undefined&&errorCallBack!=null&&typeof(errorCallBack)=='function'
					&&$(responseData).find("input[name='errors']").length>0){
					errorCallBack.call(this,responseData);																
				}else{
					if(myCallback!=undefined&&myCallback!=null&&typeof(myCallback)== 'function' ){
						myCallback.call(this,responseData);
					}else
						callback(responseData, 0);
				}	
			}
		},
		error:function(responseData){
			//alert("error:"+responseData.status);
		}
	});
	return false;
}

function isErrorPopup(responseData) {

	var openErrorPopup = $(responseData).addBack().find("#serviceUnavailable");
	var minicartShippableToPRCErrorPopup = $(responseData).find("#minicartShippableToPRCErrorPopup");

	var hasError = $(openErrorPopup).length > 0;
	var hasMinicartError = $(minicartShippableToPRCErrorPopup).length > 0;
	
	var errorFlag=false;
	if(hasError || hasMinicartError){
		errorFlag=true;
	}
	return errorFlag;
}

function showLoadingDiv(){
	$.isLoading({
		'text': "",
		'tpl': '<span style="display:block;margin:300px auto;width:100%;text-align:center;">%text%<img src="/_ui/desktop/theme-igc/images/loader.gif" /></span>'
	}); 
}

function closeLoadingDiv(){
	$.isLoading("hide");
	$('.isloading-overlay').hide();
}

function openContentPopup(content) {
	$("body").css("overflow-x", "hidden");
	$("#overlaypopup").html(content);
	$("#overlaypopup").fadeIn(function(){
		$("#overlaypopup").css("height", $(document).height());
		$("#overlaypopup").children(".popupQuickview").css("top", $(document).scrollTop());
	});
}
//wiggins ,simple method
function _openContentPopup(pid) {
	$("body").css("overflow-x", "hidden");
	var id = pid || 'overlaypopup';
	$("#"+id).fadeIn(function(){
		$("#"+id).css("height", $(document).height());
	});
}
function _closePopOver(pid) {
	var id = pid || 'overlaypopup';
	$('#'+id).stop().fadeOut(function(){
		$("body").css("overflow-x", "auto");
	});
}


function addToBasket(form_data,url, isFreeItem){
	showLoadingDiv();
	var addFreeItem = isFreeItem || false;
	var tempData = null;
	
	$.ajax({
		url: url,
		data: form_data,
		type: 'GET',
		dataType: 'html',
		success: function(data) {
			try {
				var gaData = $(data).find("#gaCartPopUpData").val();
				var gaProductDataObj = jQuery.parseJSON(gaData);
				var qty = null;
				
				if (typeof gtmProductAdd2Cart !== 'undefined' && $.isFunction(gtmProductAdd2Cart)) {
					var str = form_data.split("&");
					for(i=0; i<str.length; i++) {
						if (str[i].indexOf("qty") >= 0) {
							qty = str[i].substr(str[i].indexOf("qty")+4); 
						}
					}
					
					if (qty == null) {
						qty = 1;
					}
					
					gtmProductAdd2Cart(gaProductDataObj['id'], parseInt(qty));
				}
			} catch(e) {
				// do nth here.
			}
			try {
				//Check if any free gift for the added product
		        if(data.indexOf("free*true")!=-1)
		        {
	        	    cdoGet(null, '/freeItemsPopup?message=0', function(freeItems){
	        	    	$('#overlaypopup').empty();
	        	    	$('#overlaypopup').html(freeItems);
	        	        _openContentPopup('overlaypopup');
	        	        
	        	    },'GET', 'html');
		        } 
		    } catch(e) {
		        // do nth here.
		    }
		}
	}).done(function(data) {
		$("#myBasket").empty().html($('<div>').html(data).find("#myBasket").html());
				
		if (addFreeItem) {
			_closePopOver();
			if ($('#mybasketCheckoutButton').length)
        	{
        		location.reload();
        	}
		} 
		closeLoadingDiv();
		//why we need to close the pop up after open? This cause issue in free item pop up.
		//    setTimeout(function() {$('#popupClose').trigger('click');closeLoadingDiv();},3000);
		$("#overlaypopup").hide();
		setTimeout(function() {$('.myBasket.fr').trigger('mouseenter')},1000);
		setTimeout(function() {$('.myBasket.fr').trigger('mouseleave')},3000);
		
	});
	
}

	function changeRegion(code, subSelectId, url, autoOpen, selectValue) {
		var subSelector = $("#" + subSelectId);
		var success = function(list) {
			subSelector.empty();
			subSelector.selectpicker('destroy');
			subSelector.append($('<option></option>').val("").prop('selected', true).html("Please select</option>"));
			$.each(list, function(index, item) {
				
				if (selectValue != undefined && selectValue == item.code) {
					subSelector.append($('<option></option>').val(item.code).prop('selected', true)
						.html(item.name + ' ' + item.postcode));
					} else {
						subSelector.append($('<option></option>').val(item.code).html(item.name + ' ' + item.postcode));
					}
				});
			subSelector.selectpicker('val');

			//Added by HPE-Ricky, to cater "select" tags with "title" attribute, have to rebuild selectpicker once more
			if (selectValue != undefined)
			{
				subSelector.val(selectValue);
				subSelector.selectpicker("refresh");
			}

			$("#cityCodeSelectId").val(code);
			$("#townCityId").val(code);
			$("#districtInputId").val('');
		}
		
		if ($.trim(code) == "") {
			success.call(this, new Array());
			return;
		}

		doGet(null, url, success, "get", "json");
	}
	

	function changeRegionOpen(code, subSelectId, url, isAsync, districtCode) {
		var subSelector = $("#" + subSelectId);
		var success = function(list) {
			subSelector.easyDropDown('destroy');
			subSelector.empty();

			$("<option value=''>Please select</option>").appendTo(subSelector);
			$.each(list, function(index, item) {
				if (districtCode != undefined && districtCode == item.code) {
					subSelector.append($('<option></option>').val(item.code).prop('selected', true)
						.html(item.name + ' ' + item.postcode));
				} else {
					subSelector.append($('<option></option>').val(item.code).html(item.name + ' ' + item.postcode));
				}
			});

			subSelector.easyDropDown({
		        cutOff: 20			      
		    });
			$(".dropdown .carat").addClass("sprite sprite-P01B29ExpandSmStatic");
		}

		if ($.trim(code) == "") {
			success.call(this, new Array());
			return;
		}

		doGetAsync(null, url, success, "get", "json", null, false);
	}
	
	function processProductReview() {
		$('.productReview').each(function(){
			var productRaty=Math.ceil($(this).find('.averageRating').val());
			if(productRaty>=3){
				$(this).find('.reviewStar').html('');
				$(this).find('.reviewText').show();
				for(i=0;i<5;i++){
					if(i+1<=productRaty){
						$(this).find('.reviewStar').append('<i class="sprite sprite-P01T06StarPress fl"></i>');
					}else{
						$(this).find('.reviewStar').append('<i class="sprite sprite-P01T06StarStatic fl"></i>');
					}
				}
			}else{
				$(this).find('.reviewStar').html('');
				$(this).find('.reviewText').hide();
			}
		})
		
	}
	
