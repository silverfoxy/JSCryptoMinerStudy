    $('.partnerFinder .results .reset').on('click', resetForm);</script><script>var lang = CookieHelper.getItem(CookieHelper.KEYS.LANG);
    function populateBlogSlide(postData) {
        $('.blog-indicator').attr('data-slide-to', $('.carousel-indicators li').length - 1);
        $('.blog-slide .fill').css('background-image', 'url("' + postData.featured_image.source + '")');
        $('.blog-slide .blog-title').html(postData.title);
        $('.blog-slide .blog-more').attr('href', postData.link);
    }
    function digestWPData(data) {
        var i;
        var tmp;
        data.sort(function (a, b) {
            return a.menu_order - b.menu_order;
        });
        for (i = 0; i < data.length; i += 1) {
            var imgUrl = data[i].featured_image.source;
            tmp = $($('.posts a.post-link')[i]);
            tmp.attr('href', data[i].link);
            if (imgUrl) {
                tmp.find('.post').css('background-image', 'url("' + imgUrl + '")');
            }
            tmp.find('.post-title').html(data[i].title);
        }

        populateBlogSlide(data[0]);
    }

    $.get('https://blog.tugo.com/' + lang + '/wp-json/posts?filter[category_name]=featured-post', digestWPData)</script><script>$(document).ready(function () {
