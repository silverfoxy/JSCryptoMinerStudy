<script type="text/javascript">
	var brontoCart = null;
	var BrontoCartGenerating = false;

    function isNullOrWhitespace( input ) {
        if (typeof input === 'undefined' || input == null) return true;
        return String(input).replace(/\s/g, '').length < 1;
    }
	
	function BuildBrontoCartJSON() {
		var BrontoCartJSONReq = {};
		var TotalTax = 0.00;
		var GrandTotal = 0.00;
		if ($("#TaxTotal").length && !isNullOrWhitespace($("#TaxTotal").text()) && parseFloat($("#TaxTotal").text().trim().replace("$","").replace(",","")) > 0) {
			TotalTax = parseFloat($("#TaxTotal").text().trim().replace("$","").replace(",",""));
			if ($("#dollar.OrderTotal").length) {
			GrandTotal = parseFloat($("#dollar.OrderTotal").text().trim().replace("$","").replace(",",""));
			}
		}
		BrontoCartJSONReq.url = "/typeservice/BrontoCart.cfc?method=GetBrontoCartJSON&urlcs=HOM&TotalTax=" + TotalTax + "&GrandTotal=" + GrandTotal;
		BrontoCartJSONReq.type = "GET";
		BrontoCartJSONReq.dataType = "jsonp";
		BrontoCartJSONReq.contentType = "application/json; charset=utf-8";
		BrontoCartJSONReq.success = function (data) {
			if (!isNullOrWhitespace(brontoCart)) {brontoCart = null};
			if (data[0] !== null && data[0] !== 0) {
				brontoCart = data[0];
			}
		}
		BrontoCartJSONReq.error = function (jqXHR, exception, errorThrown) {
			brontoCart = null;
			console.log(jqXHR);
			console.log(exception);
			console.log(errorThrown);
		}
		BrontoCartJSONReq.complete = function () {
				console.log('brontoCart = ');
				console.log(brontoCart);
				BrontoCartGenerating = false;
		}
		BrontoCartJSONReq.jsonpCallback = "brontocallback";		
		$.ajax(BrontoCartJSONReq);
	}
	
	function InitBrontoCartJSON() {
			if (BrontoCartGenerating === true) {
				CheckBrontoCartGeneratingInterval = setInterval(CheckBrontoCartGenerating, 1000);				
			} else {
				BrontoCartGenerating = true;
				BuildBrontoCartJSON();
			}
	}
	function CheckBrontoCartGenerating() {
		if (BrontoCartGenerating === false) {
			clearInterval(CheckBrontoCartGeneratingInterval);
			BrontoCartGenerating = true;
			BuildBrontoCartJSON();
		}
	}
	function CheckTax() {
		if ($("#TaxTotal").length && !isNullOrWhitespace($("#TaxTotal").text()) && parseFloat($("#TaxTotal").text().trim().replace("$","").replace(",","")) !== 'NaN' && parseFloat($("#TaxTotal").text().trim().replace("$","").replace(",","")) > 0) {
			InitBrontoCartJSON();
		}
	}
	
	var observer = new MutationObserver(BuildBrontoCartJSON),
		elTarget = document.querySelector("#MiniCartTotal"),
		objConfig = {
			childList: true,
			subtree: true,
			attributes: true,
			characterData: true
		};
	observer.observe(elTarget, objConfig);
	
	var observer2 = new MutationObserver(CheckTax),
		elTarget2 = document.querySelector("#TaxTotal"),
		objConfig2 = {
			childList: true,
			subtree: true,
			attributes: true,
			characterData: true
		};
	if ($("#TaxTotal").length) {
		observer2.observe(elTarget2, objConfig2);	
	}
	
	InitBrontoCartJSON();

</script>
	
	

