<script src="https://maps.google.com/maps/api/js?key=AIzaSyDndgTbyPcF5hfDtw_yKl2oCcLVOfnRVak&v=3.24&language=en"></script>
<script>
		
		var map;
	  	var latLngControl;
	  	var mapBounds;
	  	
var markers = new Array();
var marker_icons = new Array();
var marker_icons_over = new Array();
function addMarker(bubble, image_path, image_over_path, bubbleSize, bubblePoint, latitude, longitude, title, url){
var lat_lang = new google.maps.LatLng(latitude, longitude);
if(!marker_icons[bubble]){
marker_icons[bubble] = new google.maps.MarkerImage(image_path,new google.maps.Size(bubbleSize, bubbleSize), new google.maps.Point(0,0), new google.maps.Point(bubblePoint,bubblePoint), new google.maps.Size(bubbleSize,bubbleSize));
marker_icons_over[bubble] = new google.maps.MarkerImage(image_over_path ,new google.maps.Size(bubbleSize, bubbleSize), new google.maps.Point(0,0) , new google.maps.Point(bubblePoint,bubblePoint), new google.maps.Size(bubbleSize,bubbleSize));
}
var marker = new google.maps.Marker({
position: lat_lang,
map: map,
icon: marker_icons[bubble],
zIndex: 5
});
markers[markers.length] = marker;
//MouseOver
google.maps.event.addListener(marker, "mouseover", function(event){
console.log("hover!")
//actualizamos la posicion del HOVER
latLngControl.updatePosition(lat_lang);
latLngControl.set('visible', true);
latLngControl.node_.innerHTML = "<b>" + title + "</b>";
marker.setIcon(marker_icons_over[bubble]);
});
//MouseOut
google.maps.event.addListener(marker, "mouseout", function(event){
console.log("out!")
latLngControl.set('visible', false);
marker.setIcon(marker_icons[bubble]);
});
if (url) {
google.maps.event.addListener(marker, "click", function(){
statsClick(new Array('facet','sidebar','mapLocation'));
window.location.href = url;
});
}else{
marker.setCursor('default');
}
mapBounds = fit_position(mapBounds, lat_lang);
}
function addCurrentSearchMarker(latitude, longitude, title){
var lat_lang = new google.maps.LatLng(latitude, longitude);
var marker = new google.maps.Marker({
position: lat_lang,
map: map,
zIndex: 5
});
//MouseOver
google.maps.event.addListener(marker, "mouseover", function(event){
//actualizamos la posicion del HOVER
latLngControl.updatePosition(lat_lang);
latLngControl.set('visible', true);
latLngControl.node_.innerHTML = "<b>" + title + "</b>";
});
//MouseOut
google.maps.event.addListener(marker, "mouseout", function(event){
latLngControl.set('visible', false);
});
mapBounds = fit_position(mapBounds, lat_lang);
}
function fit_position(bounds, pos){
if(bounds == undefined || bounds== null){
//bounds are undefined -> create from position
pad = 0.049;//10km
var sw = new google.maps.LatLng(pos.lat() - pad, pos.lng() - pad);
var ne = new google.maps.LatLng(pos.lat() + pad, pos.lng() + pad);
bounds = new google.maps.LatLngBounds(sw, ne);
}else if(!bounds.contains(pos)){
bounds.extend(pos);
}else{}
return bounds;
}
LatLngControl.prototype = new google.maps.OverlayView();
LatLngControl.prototype.draw = function() {};
LatLngControl.prototype.createHtmlNode_ = function() {
var divNode = document.createElement('div');
divNode.id = 'latlng-control';
divNode.index = 100;
return divNode;
};
LatLngControl.prototype.visible_changed = function() {
this.node_.style.display = this.get('visible') ? '' : 'none';
};
LatLngControl.prototype.updatePosition = function(latLng) {
var projection = this.getProjection();
var point = projection.fromLatLngToContainerPixel(latLng);
var center = projection.fromLatLngToContainerPixel(this.map.getCenter());
if(point.x < center.x){
offset_X = 20;
}else{
offset_X = -30;
}
if(point.x < center.x){
offset_Y = 1;
}else{
if(point.y < center.y){
offset_Y = 20;
}else{
offset_Y = -35;
}
}
this.node_.style.left = point.x + offset_X + 'px';
this.node_.style.top = point.y + offset_Y + 'px';
};
function LatLngControl(map) {
this.node_ = this.createHtmlNode_();
map.controls[google.maps.ControlPosition.TOP].push(this.node_);
this.setMap(map);
this.set('visible', false);
}
function markerMouseOver(index){
var marker = markers[index];
latLngControl.updatePosition(marker.getPosition());
google.maps.event.trigger(marker, 'mouseover');
}
function markerMouseOut(index){
var marker = markers[index];
google.maps.event.trigger(marker, 'mouseout');
}
			
