	var onload_elements = [];
	
	function addLoadElement(scripts, myFunction) {
		onload_elements.push({
			'scripts': scripts,
			'function': myFunction
		});
	}
	
	function get_script(url, success){
        var script = document.createElement('script');
        script.src = url;
        var head = document.getElementsByTagName('head')[0];
        var done = false;
        script.onload = script.onreadystatechange = function(){
			if(!done && (!this.readyState || this.readyState == 'loaded' || this.readyState == 'complete') ) {
				done = true;
				success();
				script.onload = script.onreadystatechange = null;
			}
        };
        head.appendChild(script);
	}
	
	function load_multiple_scripts(element) {
		
		if(element.scripts && element.scripts.length > 0){
			get_script(element.scripts[0], function(){
				element.scripts.shift();
				load_multiple_scripts(element);
			});
		}else if (element.function) {
			element.function();
		}

	}
	
	function load_deferred_scripts() {
		for (var i=0; i < onload_elements.length; i++) {
			var element = onload_elements[i];
			load_multiple_scripts(element);
		}
	}
	
	
	function defer_images(){
		var images = document.getElementsByTagName('img');
		for (var i=0; i < images.length; i++) {
			if(images[i].getAttribute('data-src')) {
				images[i].setAttribute('src',images[i].getAttribute('data-src'));
				images[i].removeAttribute('data-src');
			}
		}	
	}	

	if (window.addEventListener){
		window.addEventListener("load", load_deferred_scripts, false);
	}else if(window.attachEvent){
		window.attachEvent("onload", load_deferred_scripts);
	}else{
		addLoadEvent(load_deferred_scripts);
	}
	
