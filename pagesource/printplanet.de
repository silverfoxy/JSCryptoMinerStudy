<script type="text/javascript">

    var PP = PP || {};

    $(function(){

        PP.MainCtrl = (function() {

            return {

                init : function() {
                    PP.SlideshowCtrl.init();
                    return true;
                }

            };

        })();


        PP.SlideshowCtrl = (function() {

            var oContainer = $("#slideshow")

            var oNavi = $(".slidetabs", oContainer ).tabs(".images > a", {

                effect: 'fade',

                fadeOutSpeed: "slow",

                rotate: true

            }).slideshow( {

                interval: 4000,

                autoplay : true

            });

            var oSlideshow = oNavi.data("slideshow");
            var oTabs = oNavi.data("tabs");
            var oNaviExt = $(".mask > a.navi");
            var oHelper = $("#content a.helper");

            return {

                init : function() {

                    var bNaviEnter = false;
                    var coords = oHelper.offset();
                    coords.width = oHelper.width();
                    coords.height = oHelper.height();

                    $("#content").delegate( "a.helper", "click", function() {
                        window.location = oNavi.data('tabs').getCurrentPane().attr("href");
                    }).delegate ( "a.helper", "mouseenter", function(e) {
                        oNaviExt.show();
                        oSlideshow.stop();
                    }).delegate ( "a.helper", "mouseleave", function(e) {

                        if(
                            e.clientX < coords.left
                            || e.clientX > coords.left + coords.width
                            || e.clientY < coords.top
                            || e.clientY > coords.top + coords.height
                        ) {
                            oNaviExt.hide();
                            oSlideshow.play();
                        }

                    });

                    oContainer.delegate( "a.next", "click", function() {
                        oTabs.next();
                    }).delegate( "a.prev", "click", function() {
                        oTabs.prev();
                    });

                    return true;
                }

            };

        })();


        PP.MainCtrl.init();

    });

</script>
