<script type="text/javascript">
	$(function() {

		var selItem = $("#nav-themen > ul > li"); // Links der Hauptnavigation
		var selFade = $("#fadeNav");
		var position, fadeHide, fadeTimeOut, navTimeOut;
		var fadeActive = false;
		var navControlItem = null;

		// Unterkategorie ausblenden
		fadeHide = function() {
			selFade.hide();
			fadeActive = false;
			if (navControlItem != null) {
				navControlItem.removeClass('control');
			}
		};

		// Kategorie Maus rein
		selItem.bind("mouseover", function() {

			clearTimeout(navTimeOut);
			clearTimeout(fadeTimeOut);

			if ($(this).hasClass('control')) {

				$(this).removeClass('control');

			} else if (!$(this).hasClass('open')) {

				if (navControlItem != null) {
					navControlItem.removeClass('control');
				}

				if ($("ul li", this).length > 0) {

					$("ul li", selFade).remove();
					$("ul li", this).clone().appendTo($("ul", selFade));

					navControlItem = $(this);

					position = $(this).position();
					selFade.css('margin-top', position.top + 40);
					selFade.show();
				} else {
					fadeHide();
				}
			} else {
				fadeHide();
			}
		});

		// Kategorie Maus verlassen
		selItem.bind("mouseleave", function() {
			if (!$(this).hasClass('open')) {
				fadeTimeOut = setTimeout(fadeHide, 10);
			}
		});

		// Unterkategorie Maus rein
		selFade.bind("mouseover", function() {
			clearTimeout(fadeTimeOut);
			fadeActive = true;
			navControlItem.addClass('control');
		});

		// Unterkategorie Maus verlassen
		selFade.bind("mouseleave", function() {
			navTimeOut = setTimeout(fadeHide, 10);
		});

	});
</script>

