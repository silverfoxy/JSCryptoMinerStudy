                                            player.playbackRate = 1;
                                            setTimeout(function () {
                                                var x = new Date().getTime();
                                                setTimeout(function(){showAfterImage(false)}, (player.duration - player.currentTime - 0.6) * 1000);
                                            }, 1000);
                                        }
                                    } else if (playCountIncremented && remaining >= 5) {
