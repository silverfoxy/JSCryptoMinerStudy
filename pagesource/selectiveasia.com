                                        if(viewedVideos.hasOwnProperty(videoId)){
                                            // 43200000 = 1000 * 3600 * 12
                                            // Only play once every 12 hours...
                                            if( (now-viewedVideos[videoId]) < 43200000){
                                                alreadyPlayed = true;
                                            }else{
