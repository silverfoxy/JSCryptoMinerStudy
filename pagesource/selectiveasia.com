                                            }
                                        }
                                        if(!alreadyPlayed){
                                            viewedVideos[videoId] = now;
                                            Cookies.set('viewedVideos',viewedVideos, { expires: 7 });
                                        }
                                    }
                                    if (!playCountIncremented && remaining < 5) {
                                        playCountIncremented = true;
                                        played++;
                                        if (played >= loops) {
