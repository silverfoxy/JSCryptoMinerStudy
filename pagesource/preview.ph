        $(document).ready(function(){
            var
                    metaUrl = $('meta[property="og:url"]').attr('content'),
                    metaTitle = $('meta[property="og:title"]').attr('content'),
                    metaDescription = $('meta[property="og:description"]').attr('content'),
                    metaCaption = $('meta[property="og:site_name"]').attr('content'),
                    blurb = '',
                    descriptionSns = '';

            if($('.article-story').length > 0){
                $('.article-story blockquote, .article-story img:not([data-shoppable-image])').each(function(){
                    if($(this).parents().hasClass('gallery-list-photos')) return false;
                    if($(this).prop('tagName') == 'BLOCKQUOTE') {
                    $(this).wrap('<p></p>');
                    blurb = $(this).text();
                    }
                    if($(this).prop('tagName') == 'IMG' && !$(this).parents().hasClass('gallery-collapsed') && ($(this).width() != 16 && $(this).height() != 16)) {
                        $(this).wrap('<p></p>');
                        img = $(this).attr('src');
                        var imgWidth = $(this).width();
                        var imgHeight = $(this).height();
                    }
                    if (blurb) {
                        descriptionSns = blurb;
                    }else{
                        descriptionSns = metaDescription;
                    }
                     var condition = $(this).attr('itemprop') === undefined  && $(this).parentsUntil('.article-story')
                    && !$(this).parents().hasClass('widget-recommend') && !$(this).parents().hasClass('instagram-media') && !$(this).parents().hasClass('gallery-collapsed')
                    && !$(this).parents().hasClass('sns-embed')  && $(this).attr('class') === undefined
                    && !$(this).parents().hasClass('widget-recommend') && ($(this).width() != 16 && $(this).height() != 16);
                    if (condition)  {
                        var html = '<div class="sns-feed-article">';
                            html += '<div class="sns-feed-container">';
                            html += '<div class="sns-hex">';
                            html += '<div class="sns-count-container">';
                            html += '<span class="sns-count">';
                            html += '<svg class="svg-share-box btn-icon icon icon-share-box icon-share" style="margin-top: 5px;">';
                            html += '<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#share-article"></use>';
                            html += '</svg>';
                            html += '</span>';
                            html += '</div>';
                            html += '</div>';
                            html += '<div class="sns-off">';
                            html += '<div class="sns-count-container">';
                            html += '<span class="sns-count">&nbsp;</span>';
                            html += '</div>';
                            html += '</div>';
                            html += '<div class="sns-cancel">';
                            html += '<div class="sns-count-container">';
                            html += '<span class="sns-count">';
                            html += '<svg class="svg-share-box btn-icon icon icon-share-box icon-share" style="margin-top: 5px;">';
                            html += '<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#close-feed-article"></use>';
                            html += '</svg>';
                            html += '</span>';
                            html += '</div>';
                            html += '</div>';
                            html += '</div>';
                            html += '</div>';
                            html += '<div class="sns-backdrop">';
                            html += '<div class="sns-content">';
                            html += '<div class="sns-sharethis">';
                            html += '<div class="gtm-articlePhotoFacebook sns-item share-button" data-sns="facebook" data-caption="'+metaCaption+'" data-desc="'+descriptionSns+'" data-title="'+metaTitle+'" data-url="'+metaUrl+'" data-image="'+img+'">';
                            html += '<span class="gtm-articlePhotoFacebook btn btn-fb">';
                            html += '<svg class=" svg-facebook btn-icon icon icon-share icon-fb" style="margin: -5px 0 10px 0!important";>';
                            html += '<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#facebook-article"></use>';
                            html += '</svg>';
                            html += '</span>';
                            html += '</div>';
                            html += '<div class="gtm-articlePhotoTwitter sns-item share-button" data-sns="twitter" data-desc="'+descriptionSns+'" data-title="'+metaTitle+'" data-url="'+metaUrl+'" data-image="'+img+'">';
                            html += '<span class="gtm-articlePhotoTwitter btn btn-twitter">';
                            html += '<svg class="gtm-articlePhotoTwitter svg-twitter btn-icon icon icon-share icon-twitter" style="margin: -5px 0 10px 0!important;">';
                            html += '<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#twitter-article"></use>';
                            html += '</span>';
                            html += '</div>';
                            html += '<div class="gtm-articlePhotoPinterest sns-item share-button" data-sns="pinterest" data-desc="'+descriptionSns+'" data-title="'+metaTitle+'" data-url="'+metaUrl+'" data-image="'+img+'">';
                            html += '<span class="gtm-articlePhotoPinterest btn btn-pin">';
                            html += '<svg class="gtm-articlePhotoPinterest svg-pinterest btn-icon icon icon-share icon-pinterest" style="margin: -5px 0 10px 0!important";>';
