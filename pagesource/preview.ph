                        }else{
                            $(this).parent().css('padding-bottom', '1px');
                        }

                        $(this).parent().css('position', 'relative');
                        $(this).parent().css('margin', '0 auto');
                        $(this).parent().addClass('feed');
                        $(this).parent().append(html);
                    }
                });
