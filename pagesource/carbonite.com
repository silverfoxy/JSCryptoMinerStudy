  <!-- ready Shim -->
  function ready(fn) {
    if (document.attachEvent ? document.readyState === "complete" : document.readyState !== "loading"){
      fn();
    } else {
      document.addEventListener('DOMContentLoaded', fn);
    }
  };

  <!-- Animate -->

  ready(function () {
    
    var lillustration = $('#illustration-left'),
      lbase = lillustration.find('#base'),
      ldome = lillustration.find('#dome'),
      lglow = lillustration.find('#outer-glow'),
      llaptop = lillustration.find('#laptop'),
      lserver = lillustration.find('#server'),
      lfolders = lillustration.find('#folders'),
      lfolderstand = lillustration.find('#folderstand'),
      lfolder1 = lillustration.find('#whitefolder1'),
      lfolder2 = lillustration.find('#whitefolder2'),
      lfolder3 = lillustration.find('#whitefolder3'),
      lphoto = lillustration.find('#photo'),
      lmail = lillustration.find('#mail'),
      lmusic = lillustration.find('#music'),
      lpowerbutton = lillustration.find('#powerbutton');
    
    var rillustration = $('#illustration-right'),
      rbase = rillustration.find('#base2'),
      rdome = rillustration.find('#dome2'),
      rglow = rillustration.find('#outer-glow2'),
      rlaptop = rillustration.find('#laptop2'),
      rserver1 = rillustration.find('#server2-1'),
      rserver2 = rillustration.find('#server2-2'),
      rserver3 = rillustration.find('#server2-3'),
      rfolders = rillustration.find('#folders2'),
      rfolder1 = rillustration.find('#bluefolder1'),
      rfolder2 = rillustration.find('#bluefolder2'),
      rfolder3 = rillustration.find('#bluefolder3'),
      rgears = rillustration.find('#gears2'),
      rgear1 = rillustration.find('#gear2-1'),
      rgear2 = rillustration.find('#gear2-2'),
      rgear3 = rillustration.find('#gear2-3'),
      rarrow1 = rillustration.find('#arrow2-1'),
      rarrow2 = rillustration.find('#arrow2-2'),
      serverlight1 = rillustration.find("#serverlight1"),
      serverlight2 = rillustration.find("#serverlight2"),
      serverlight3 = rillustration.find("#serverlight3");
    
    var ease = Elastic.easeOut.config(1, 1);
    
    
    /* LEFT SIDE TIMELINES */
    
    var lintroTl = new TimelineMax();
    lintroTl
      .from(lbase, 1, {y: 50, autoAlpha: 0})
      .from(llaptop, .5, {y: -50, autoAlpha: 0, ease: ease }, '-=.15')
      .from(lfolderstand, .5, {y: -50, autoAlpha: 0, ease: ease}, 'folderstand-=.15')
      .from(lfolder3, .5, {y: -50, autoAlpha: 0, ease: ease}, '-=.2')
      .from(lfolder2, .5, {y: -50, autoAlpha: 0, ease: ease}, '-=.2')
      .from(lfolder1, .5, {y: -50, autoAlpha: 0, ease: ease}, '-=.2')
      .from(lserver, .5, {y: -50, autoAlpha: 0, ease: ease}, '-=.15')
      .from(ldome, 1, {autoAlpha: 0, y: -50, ease: ease, delay: .25})
      .from(lglow, 1, {autoAlpha: 0 });
      
    
    var lfolderbounceTl = new TimelineMax({ repeat: -1, repeatDelay: 3, delay: 4 });
    lfolderbounceTl
      .to(lfolder3, .2, { y: -5, yoyo: true, repeat: 1 })
      .to(lfolder2, .2, { y: -5, yoyo: true, repeat: 1 })
      .to(lfolder1, .2, { y: -5, yoyo: true, repeat: 1 });
    
    var lfilesTl = new TimelineMax({ repeat: -1, repeatDelay: 2 });
    lfilesTl
      .to(lphoto, .5, {scale: 0, transformOrigin: 'center center', delay: 4 })
      .from(lmail, .5, {scale: 0, transformOrigin: 'center center', delay: .1 })
      .to(lmail, .5, {scale: 0, transformOrigin: 'center center', delay: 4 })
      .from(lmusic, .5, {scale: 0, transformOrigin: 'center center', delay: .1 })
      .to(lmusic, .5, {scale: 0, transformOrigin: 'center center', delay: 4 })
      .to(lphoto, .5, { scale: 1, transformOrigin: 'center center', delay: .1 });
    
    var gray1 = '#323B3B',
      green1 = '#A2C14A',
      frequency1 = 6;
    lightFlicker(lpowerbutton, gray1, green1, frequency1, .2);
  
  
    /* RIGHT SIDE TIMELINES */
  
    var rgearsTl = new TimelineMax();
    rgearsTl
    .timeScale(.2)
    .from(rgear1, 3, {rotation: 360, transformOrigin: "center center", ease: Linear.easeNone, repeat: -1}, 0)
    .from(rgear2, 4, {rotation: -360, transformOrigin: "center center", ease: Linear.easeNone, repeat: -1}, 0)
    .from(rgear3, 4, {rotation: -360, transformOrigin: "center center", ease: Linear.easeNone, repeat: -1}, 0);
  
    var rintroTl = new TimelineMax();
    rintroTl
    .from(rbase, 1, {y: 50, autoAlpha: 0, delay: .75})
    .add('servers')
    .from(rserver2, .5, {y: -50, autoAlpha: 0, ease: ease}, 'servers')
    .from(rserver3, .5, {y: -50, autoAlpha: 0, ease: ease}, 'servers+=.1')
    .from(rserver1, .5, {y: -50, autoAlpha: 0, ease: ease}, 'servers')
    .from(rlaptop, .5, {y: -50, autoAlpha: 0, ease: ease}, '-=.15')
    .from(rfolders, .5, {scale: 0, transformOrigin: "center center", autoAlpha: 0}, 'elements')
    .from(rgears, .5, {scale: 0, transformOrigin: "center center", autoAlpha: 0}, 'elements')
    .from(rarrow1, .5, {scale: 0, transformOrigin: "50% 50%", autoAlpha: 0}, 'elements')
    .from(rarrow2, .5, {scale: 0, transformOrigin: "50% 50%", autoAlpha: 0}, 'elements')
    .from(rdome, 1, {autoAlpha: 0, y: -50, ease: ease}, 'startArrows')
    .from(rglow, 1, {autoAlpha: 0});
  
    
    var rfoldersTl = new TimelineMax({repeat: -1, repeatDelay: 4});
    rfoldersTl
    .timeScale(.8)
    .to(rfolder3, .2, {scale: 0, transformOrigin: 'center, center', delay: .5})
    .to(rfolder2, .2, {y: '+=4', x: '-=4', fill: '#67CBED', transformOrigin: 'center, center'}, '-=.1')
    .to(rfolder1, .2, {y: '+=4', x: '-=4', fill: '#009DBE', transformOrigin: 'center, center'}, '-=.1')
    .to(rfolder3, .2, {scale: 1, transformOrigin: 'center, center'}, '-=.2')
  
    .set(rfolder3, {scale: 1, transformOrigin: 'center, center'})
    .set(rfolder2, {y: '-=4', x: '+=4', transformOrigin: 'center, center', fill: '#009DBE'})
    .set(rfolder1, {y: '-=4', x: '+=4', scale: 0, transformOrigin: 'center, center', fill: '#145974'})
    .to(rfolder1, .2, {scale: 1, transformOrigin: 'center, center', delay: .1});
  
    var gray2 = '#323B3B',
      blue2 = '#67CBED';
    lightFlicker(serverlight1, gray2, blue2, 4, 1.7);
    lightFlicker(serverlight2, gray2, blue2, 7, 1.7);
    lightFlicker(serverlight3, gray2, blue2, 10, 1.7);
  
  
    /* SHARED TIMELINES */
    var glowPulseTl = new TimelineMax({ pause: true });
    glowPulseTl
      .fromTo(lglow, 2, {opacity: 0}, {opacity: 1, yoyo: true, repeat: -1, delay: 4}, 'pulse-start')
      .fromTo(rglow, 2, {opacity: 0}, {opacity: .3, yoyo: true, repeat: -1, delay: 4}, 'pulse-start');
  
    
    /* SHARED FUNCTIONS */
  
    function lightFlicker(object, baseColor, hiliteColor, frequency, rate) {
      var delay = Math.floor(Math.random() * frequency),
        flickerRate = rate;
    
      var flickerTl = new TimelineMax({onComplete: lightFlicker, onCompleteParams: [object, baseColor, hiliteColor, frequency, rate]});
      flickerTl
      .set(object, {css: {fill: baseColor}})
      .set(object, {css: {fill: hiliteColor}, delay: delay})
      .set(object, {css: {fill: baseColor}, delay: flickerRate})
      .set(object, {css: {fill: hiliteColor}, delay: flickerRate})
      .set(object, {css: {fill: baseColor}, delay: flickerRate});
    }
  });
