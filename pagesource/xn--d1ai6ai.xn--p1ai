		/* шаг 1 */
		/*отмеченные чекбоксы парфюмерии */
		$(""+ selector +" .body-parfumeria input:checkbox").each(function (index, value) {
			if ($(this).is(":checked")) {
				index=index+1;
				op['type['+index+']'] = $(this).data("type");
			}
			else {}
		});
		/*отмеченные чекбоксы косметологии */
		$(""+ selector +" .body-kosmet .withch "+ kosmet +" .children_kosmet input:checkbox").each(function (index, value) {
			if ($(this).is(":checked")) {
				index=index+1;
				op['type['+index+']'] = $(this).data("type");
			}
			else {}
		});
		/* шаг 2. отмеченные бренды */
		if ($(""+ selector +" .body-proizvoditel .btn-all-pr").hasClass("active")) {
			/*  дефаулт  */
		}
		else if ($(""+ selector +" .body-proizvoditel .btn-pop-pr").hasClass("active")) {
			$(""+ selector +" .body-proizvoditel .popular input:checkbox").each(function (index, value) {
				if ($(this).is(":checked")) {
					index=index+1;
					op['brand_p['+index+']'] = $(this).data("brand");
				}
				else {}
			});
		}
		else if ($(""+ selector +" .body-proizvoditel .btn-alp-pr").hasClass("active")) {
			$(""+ selector +" .body-proizvoditel .alpha input:checkbox").each(function (index, value) {
				if ($(this).is(":checked")) {
					index=index+1;
					op['brand_p['+index+']'] = $(this).data("brand");
				}
				else {}
			});
		}
		else {
			/*  дефаулт  */
		}
		/* шаг 3. мужчины-женщины */
		if ($(""+ selector +" .body-category .btn-woman").hasClass("active")) {
			op['category[1]'] = 11;
		}
		if ($(""+ selector +" .body-category .btn-man").hasClass("active")) {
			if ($(""+ selector +" .body-category .btn-woman").hasClass("active")) {
				op['category[2]'] = 12;
			}
			else {
				op['category[1]'] = 12;
			}
		}
		/* шаг 4. тестеры. */
		var tester = $(""+ selector +" .body-tester").find(".checked").attr("data-type");
		//console.log(tester);
		if (tester == "select") {
			op['filter[7]'] = "select";
		}
		else if (tester == "exclude") {
			op['filter[7]'] = "exclude";
		}
		else if (tester == "unimport") {
			op['filter[7]'] = "unimport";
		}
		else {
			op['filter[7]'] = "unimport";
		}
		/* шаг 5. наборы. */
		var nabor = $(""+ selector +" .body-nabor").find(".checked").attr("data-type");
		//console.log(tester);
		if (nabor == "select") {
			op['filter[17]'] = "select";
		}
		else if (nabor == "exclude") {
			op['filter[17]'] = "exclude";
		}
		else if (nabor == "unimport") {
			op['filter[17]'] = "unimport";
		}
		else {
			op['filter[17]'] = "unimport";
		}
		/* шаг 6. цена. */
		var price_ot = $(""+ selector +" .body-price").find("input[name='price_ot']").val();
		var price_do = $(""+ selector +" .body-price").find("input[name='price_do']").val();
		if (price_ot.length === 0) {
			op['price_from'] = "";
		}
		else {
			op['price_from'] = price_ot;
		}
		if (price_do.length === 0) {
			op['price_to'] = "";
		}
		else {
			op['price_to'] = price_do;
		}
		/* шаг 7. объем. */
		var volume_ot = $(""+ selector +" .body-objem").find("input[name='f-volume-a']").val();
		var volume_do = $(""+ selector +" .body-objem").find("input[name='f-volume-b']").val();
		if (volume_ot.length === 0) {
			op['volume_from'] = "";
		}
		else {
			op['volume_from'] = volume_ot;
		}
		if (volume_do.length === 0) {
			op['volume_to'] = "";
		}
		else {
			op['volume_to'] = volume_do;
		}
		/* шаг 8. только в наличии. */
		if ($(''+ selector +' .body-navi .available input').is(':checked')) {
			op['available'] = "1";
		}
		else {
			op['available'] = "16";
		}
		/* шаг 9. только в распрадаже. */
		if ($(''+ selector +' .body-navi .sales input').is(':checked')) {
			op['sale'] = "3";
		}
		else {
			op['sale'] = "";
		}
		/* шаг 10. Тип брендов - все, популярные или алфавит. */
		if ($(""+ selector +" .body-proizvoditel .btn-all-pr").hasClass("active")) {
			/*  дефаулт  */
		}
		else if ($(""+ selector +" .body-proizvoditel .btn-pop-pr").hasClass("active")) {
			op['brands_type'] = "popular";
		}
		else if ($(""+ selector +" .body-proizvoditel .btn-alp-pr").hasClass("active")) {
			op['brands_type'] = "list";
		}
		else {
		/*  дефаулт  */
		}
		/* шаг 11. количество элементов на странице. */
		var element_page  = $(''+ selector +' .body-navi select.form-control').val();
		if (element_page == "20") {
			op['items_pp'] = "20";
		}
		else if (element_page == "50") {
			op['items_pp'] = "50";
		}
		else if (element_page == "100") {
			op['items_pp'] = "100";
		}
		else {
			op['items_pp'] = "20";
		}
		return op;
	}
	/* Функция для вывода результата */
	function load_new_offers(search_type) {
		var search_t = search_type;
		//console.log(search_t);
		var op ={};
		op.params = $.toJSON(to_array(search_t));
		op.cmd = 'count_offers';
		var post_json = $.toJSON(op);
		$.post('/search', op, function(d){
			eval('data = '+d);
			if(data.result == 'OK') {
				console.log(data.count);
				show_new_offers(data.count, search_t);
			}
		});
	}
	/* Выводим результат */
	function show_new_offers(d, search_t) {
		if (search_t == "modal") {
			var selector = "#modal_advsearch";
		}
		else {
			var selector = "#search_sidebar";
		}
		console.log(search_t);
		console.log(d);
		$(""+ selector +" .buttonsearch a.searchbut").text("Показать ("+d+")");
		if ($(""+ selector +" .search_found").hasClass("opened")) {
		}
		else {
			$(""+ selector +" .search_found").slideToggle();
			$(""+ selector +" .search_found").show();
			$(""+ selector +" .search_found").addClass("opened");
		}
		if(d>0) {
			$( document ).ready(function() {
				if ($(""+ selector +" .search_found").hasClass("active")) {
					$(""+ selector +" .search_found .founded").html("Найдено предложений: "+d+"");
					$(""+ selector +" .search_found .col-sm-12").show();
				}
				else {
					$(""+ selector +" .search_found .founded").html("Найдено предложений: "+d+"");
					$(''+ selector +' .search_found').addClass("active");
					$(""+ selector +" .search_found .col-sm-12").show();
				}
			});
		}
		else {
			$(''+ selector +' .search_found').removeClass("active");
			$(""+ selector +" .search_found .founded").html("Ничего не найдено");
			$(""+ selector +" .search_found").css("height", "60px");
			$(""+ selector +" .search_found .col-sm-12").hide();
		}
	}
