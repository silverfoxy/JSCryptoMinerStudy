	function load_search(url, search_t) {
		var op =[];
		op = to_array(search_t);
		console.log(op);
		op['cmd'] = 'show_items';
		var uri_str = '';
		var emp ='';
		for(var p in op) {
			if (!op.hasOwnProperty(p)) continue;
			var item = op[p];
			if(p.indexOf('function')!=-1) continue;
			uri_str = uri_str +emp+p+'='+item;
			emp = '&'
		}
		if(url.indexOf('?')==-1) {
			emp = '?';
		}
		else {
			emp ='&';
		}
		var addr = url+emp+uri_str;
		document.location.href = addr;
	}
	
