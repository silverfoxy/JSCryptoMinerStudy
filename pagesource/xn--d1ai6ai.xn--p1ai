				var param = {};
				var f = $(this).parents('.mess_form:first');
				param.cmd = $(f).find(".c_msg").val();
				param.main_email_name = $(f).find(".main_email_name").val(); 
				param.main_email_contact = $(f).find(".main_email_contact").val();
				param.main_email_text = $(f).find(".main_email_text").val();
				param.abuse = $(f).find(".abuse").val(); 
				if (isEmpty(param.main_email_name) ) {     
					alert("Укажите ваше имя!");
					return false; 
				}
				if (isEmpty(param.main_email_contact)) {
					alert("Укажите E-mail или телефон!");
					return false; 
				}
				if (isEmpty(param.abuse)) {
					alert("Введите текст сообщения!");
					return false; 
				}
         
				var ab_cat =$(f).find(".ab_cat:checked").val();
				if(ab_cat) {
					param.category = ab_cat;
				} 

				param[nm] =  vl;
				$(f).find('.info-box').hide();
				$.post('', param, function(d){
				
                  $(f).find('.info-box').html(d);
