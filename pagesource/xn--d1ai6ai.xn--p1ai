						noUiSlider.create(skeypressSlider, {
							start: [0, 500],
							connect: true,
							range: {
								'min': 0,
								'max': 500
							},
							pips: {
								mode: 'values',
								values: [0, 250, 500],
								density: 4
							}
						});
						skeypressSlider.noUiSlider.on('update', function( values, handle ) {
							sinputs[handle].value = Math.round(values[handle]);
						});
						function ssetSliderHandle(i, value) {
							var r = [null,null];
							r[i] = value;
							skeypressSlider.noUiSlider.set(r);
						}
						sinputs.forEach(function(input, handle) {
							input.addEventListener('change', function(){
							ssetSliderHandle(handle, this.value);
						});
						input.addEventListener('keydown', function( e ) {
							var values = skeypressSlider.noUiSlider.get();
							var value = Number(values[handle]);
							var steps = skeypressSlider.noUiSlider.steps();
							var step = steps[handle];
							var position;
							switch ( e.which ) {
								case 13:
										ssetSliderHandle(handle, this.value);
								break;
								case 38:
									position = step[1];
									if ( position === false ) {
										position = 1;
									}
									if ( position !== null ) {
										ssetSliderHandle(handle, value + position);
									}
								break;
								case 40:
									position = step[0];
									if ( position === false ) {
										position = 1;
									}
									if ( position !== null ) {
										ssetSliderHandle(handle, value - position);
									}
								break;
							}
						});
					});
					skeypressSlider.noUiSlider.on('end', function(){
						var search_type = "sidebar";
						load_new_offers(search_type);
					});
				</script>	
			
		</div>
