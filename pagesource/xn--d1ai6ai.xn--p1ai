	function showBrands() {
		var sel_m = true;
		var sel_w = true;
		var set_hidden  = true;
		$('.sym_cat').addClass('g-hidden');
		if(!$('#ch-category-'+11).attr('checked')) {
			sel_w = false;
		}
		if(!$('#ch-category-'+12).attr('checked')){
			sel_m = false;
		}
		if(!sel_w && !sel_m) {
			sel_m = true;
			sel_w = true;
			set_hidden = false;
		}
		var letter = $.trim(this.innerHTML);
		var sel = [];
		for(i in data_brands) {
			var hidden_item = set_hidden;
			var cl = 'sym_cat';
			var b =  data_brands[i];
			if (b.ct_m > 0) {
				cl = cl+" "+"b_men";
				if(set_hidden && sel_m) {
					hidden_item = false;
				}
			}
			if (b.ct_w > 0) {
				cl = cl+" "+"b_women";
				if(set_hidden && sel_w) {
					hidden_item = false;
				}
			}
			if(hidden_item) cl = cl+" g-hidden";
				if((letter == '0-9') && (i !='0')) {
					if (i.search(/[а-я]/i) >= 0) continue;
						var d = 0;
						for (d=0;d<=9;d++) {
							if (i.substr(0,1)== d) {
								sel.push('<a class="'+cl+'" id="cat_b_'+b.id+'" href="/catalog/all/'+b.url+'">'+i+'</a><span class="'+cl+'" > | </span>');
							}
						}
				}
				else if (i.substr(0,1).toUpperCase()==letter)
					sel.push('<a class="'+cl+'" id="cat_b_'+b.id+'" href="/catalog/all/'+b.url+'">'+i+'</a><span class="'+cl+'" > | </span>');
