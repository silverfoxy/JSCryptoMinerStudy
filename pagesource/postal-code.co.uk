	function geocodePosition(pos) 
		{
			geocoder = new google.maps.Geocoder();
				
			geocoder.geocode
			({
				latLng: pos
			}, 
			function(results, status) 
			{
									       jQuery("#distance2").html("<strong>"+latMes+"</strong>: ");  
		
		jQuery("#timetravel").html("<strong>"+lngMes+"</strong>: ");
				if (status == google.maps.GeocoderStatus.OK) 
				{

				    //address2="#"+pos.lat()+","+pos.lng();
					full_address=results[0].formatted_address;
					postal_code=extractFromAdress(results[0].address_components, "postal_code");
					showResults();
					setTimeout(function(){placeMarker(pos,results[0].formatted_address);},200);
					setTimeout(function(){setInfoWindow(pos,results[0].formatted_address);},300);
				} 
				else 
				{
				if(marker!=null)
				{

				   	marker.setTitle(promptMessage);
					setInfoWindow(pos,addressMessage);


				}	
                //$("#mapErrorMsg").html('Cannot determine address at this location.'+status).show(100);
				}	
			}
		);
		}
