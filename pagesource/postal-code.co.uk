function extractFromAdress(components, type){
    var temp="";
    for (var i=0; i<components.length; i++)
        for (var j=0; j<components[i].types.length; j++)
		{
		    //temp=temp+" "+components[i].long_name;
            if (components[i].types[j]==type && components[i].long_name!=null && components[i].long_name!=undefined) {/*alert(temp);*/return components[i].long_name;}
		}	
    return "";
}
