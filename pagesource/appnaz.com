<script>function loadmore(){var next_page=$('#next_page').val();getdata(next_page);}function getdata(page){var type='deal';$.ajax({url:'http://appnaz.com/api/ios_api/deal_list_language?check_price=1519986850&all_ratings=100&drop_percent=50&drop_value=0&size=30&order_by=check_price&gl=US&page='+page,type:'GET',dataType:'json',success:function(data){console.log(data);$('#next_page').val(parseInt(data.page)+1);var list_app=data.result;var array_time=new Array;var html="";for(i=0;i<list_app.length;i++){var string_tmp="";var time=timeSince(new Date(parseInt(list_app[i].check_price)*1000));array_time.push(time);string_tmp+='<div class="content-time">';if(array_time.indexOf(time)){string_tmp+=time;}string_tmp+='</div>';var os='ios';var category;var category_url;var start="";if(os=='ios'){category=$.parseJSON(list_app[i].category);category_url=category[0].toLowerCase();}else{category=list_app[i].category;category_url=category.toLowerCase();start=' <div class="app-rate"><i class="fa fa-star"></i>'+list_app[i].score+'</div>';start+='<div class="app-down"><i class="fa fa-download"></i>'+list_app[i].installs+'+</div>';}if(category_url.indexOf('&')!=-1){category_url=category_url.replace(' & ','_and_');}if(category_url.indexOf(' ')!=-1){category_url=category_url.replace(' ','_');}var red="";if(list_app[i].price==0){red="red";}var myvar='<div class="content-app">'+'                <div class="app-left">'+'                  <div class="app-img"><a itemprop="url" href="/ios/'+ChangeToSlug(list_app[i].title)+'-'+list_app[i].appid+'"><img src="'+list_app[i].cover+'"></a></div>'+'                  <div class="app-name"><a itemprop="url" href="/ios/'+ChangeToSlug(list_app[i].title)+'-'+list_app[i].appid+'">'+list_app[i].title_translate+'</a></div>'+'                  <div class="app-type"><a itemprop="url" href="/ios/dev?id='+list_app[i].id_dev+'">'+list_app[i].offerby+'</a> - <a itemprop="url" href="/ios/category/'+category_url+'">'+list_app[i].category_translate+'</a></div>'+''+'                  <div class="rate-down">'+start+'                                      '+'                    '+'                                         '+'                  </div>'+''+''+'                </div>'+'                <div class="app-right">'+'                                    <div class="app-old-price">'+list_app[i].recent_price+'</div>'+'                                    <div class="app-new-price '+red+'">'+list_app[i].price_translate+'</div>'+'                                    <div class="app-discount  '+red+'">-'+list_app[i].drop_percent+'%</div>'+'                                  </div>'+'              </div>';string_tmp+=myvar;html+=string_tmp;}$('.content-form').append(html);}})}function timeSince(date){var seconds=Math.floor((new Date()-date)/1000);var interval=Math.round(seconds/31536000);if(interval>1){return interval+" years ago";}interval=Math.round(seconds/2592000);if(interval>1){return interval+" months ago";}interval=Math.round(seconds/86400);if(interval>1){return interval+" days ago";}interval=Math.round(seconds/3600);if(interval>1){return interval+" hours ago";}interval=Math.round(seconds/60);if(interval>1){return interval+" minutes ago";}return Math.round(seconds)+" seconds ago";}function ChangeToSlug(title){var slug;slug=title.toLowerCase();slug=slug.replace(/\`|\~|\!|\@|\#|\||\$|\%|\^|\&|\*|\(|\)|\+|\=|\,|\.|\/|\?|\>|\<|\'|\"|\:|\;|_/gi,'');slug=slug.replace(/ /gi,'-');slug=slug.replace('/','');return slug;}function translate_category(){}</script>