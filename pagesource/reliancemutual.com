<script type="text/javascript">
    function saveTextBoxes() {
        var regMob = /^[789]\d{9}$/;
        var regName = /^[A-Za-z0-9 ]{3,20}$/;
        var regEmail = /^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/;
        var isValid = true;
        var username = $('#elssusername').val();
        var mobileno = $('#elssmobileno').val();
        var Email = $('#elssEmail').val();
        if (username == "") {
            $('#elssusernamemessage').css('display', 'block');
            isValid = false;
        }
        else {
            $('#elssusernamemessage').css('display', 'none');
            if (!regName.test(username)) {
                $('#elssusername').siblings('span.errorName').text('Please enter valid name.');
                $('#elssusername').siblings('span.errorName').css('display', 'block');
                isValid = false;
            }

        }

        if (mobileno == "") {
            $('#elssmobileno').siblings('span.errorMobile').text('Please enter your mobile number.');
            $('#elssmobileno').siblings('span.errorMobile').css('display', 'block');
            isValid = false;
        }
        else {
            $('#elssmobileno').siblings('span.errorMobile').css('display', 'none');
            if (!regMob.test(mobileno)) {
                $('#elssmobileno').siblings('span.errorMobile').text('Please enter valid mobile number.');
                $('#elssmobileno').siblings('span.errorMobile').css('display', 'block');
                isValid = false;
            }
        }

        if (Email == "") {
            $('#elssEmail').siblings('span.errorEmail').text('Please enter your email ID.');
            $('#elssEmail').siblings('span.errorEmail').css('display', 'block');
            isValid = false;
        }
        else {
            $('#elssEmail').siblings('span.errorEmail').css('display', 'none');
            if (!regEmail.test(Email)) {
                $('#elssEmail').siblings('span.errorEmail').text('Please enter valid email ID.');
                $('#elssEmail').siblings('span.errorEmail').css('display', 'block');
                isValid = false;
            }
        }
        if (isValid) {
            SaveCallLead(username, Email, mobileno);
            document.getElementById("main2").style.display = "block";
            document.getElementById("main1").style.display = "none";
        }
    }

    function SaveCallLead(Name, EmailID, MobileNo) {
        var Url = window.location.origin;
        $.ajax({
            type: "POST",
            url: Url + '/_layouts/15/Reliance.MF.ApplicationPages/SchemeSearch.aspx/HaveUsCallYou',
            data: JSON.stringify({ Name: Name, EmailId: EmailID, MobileNo: parseInt(MobileNo) }),
            datatype: 'json',
            contentType: 'application/json; charset=utf-8',

            success: function (data) {
                if (data.d) {


                    resetHaveCallUFields();
                }
                else {
                    alert("Unable to send your request. Please try again later");
                }
            },
            failure: function (data) {
                alert(data.d);
            }
        });
    }


    (function () {
        'use strict';
        function classReg(cl) {
            return new RegExp('(\\s|^)' + cl + '(\\s|$)');
        }
        function hasClass(el, cl) {
            return el.className.match(classReg(cl));
        }
        function addClass(el, cl) {
            if (!hasClass(el, cl)) {
                el.className += ' ' + cl;
            }
        }
        function removeClass(el, cl) {
            if (hasClass(el, cl)) {
                el.className = el.className.replace(classReg(cl), '');
            }
        }
        function extend(obj1, obj2) {
            var obj = {};
            for (var key in obj1) {
                obj[key] = obj2[key] === undefined ? obj1[key] : obj2[key];
            }
            return obj;
        }
        var Modal = function (el, opts) {

            this.modal = el,
            this.triggerBtn = document.querySelectorAll('[data-trigger-modal="' + this.modal.id + '"]'),
            this.closeBtn = this.modal.querySelectorAll('[data-close-modal]'),
            this.body = document.body,
            this.defaults = {
                activeClass: 'modal--active', // active class set to modal when it opens
                bodyClass: 'modal-is-active', // active class set to body when modal opens
                overlay: true, // adds overlay to modal
                overlayClass: 'modal__overlay', // class for overlay of modal
                openCallback: null, // callback for when modal opens
                closeCallback: null // callack for when modal closes
            },
            this.opts = extend(this.defaults, opts);

        };
        Modal.prototype = {

            init: function () {
                this.modalTriggering();

            },

            modalTriggering: function () {

                var self = this;
                for (var i = 0; i < self.triggerBtn.length; i++) {
                    self.triggerBtn[i].addEventListener('click', self.openModal.bind(this));
                }

            },

            openModal: function () {
                document.getElementById("main2").style.display = "none";
                document.getElementById("main1").style.display = "block";
                var self = this;
                if (self.opts.overlay) {
                    self.appendOverlay();
                }
                addClass(self.modal, self.opts.activeClass);
                addClass(self.body, self.opts.bodyClass);
                for (var i = 0; i < self.closeBtn.length; i++) {
                    self.closeBtn[i].addEventListener('click', this.closeModal.bind(this));
                }
                if (self.opts.openCallback) {
                    self.opts.openCallback.call(self);
                }
                setTimeout(function () {
                    self.closeBtn[0].focus();
                }, 500);

            },
            closeModal: function () {
                var self = this;
                removeClass(self.modal, self.opts.activeClass);
                removeClass(self.body, self.opts.bodyClass);
                for (var i = 0; i < self.closeBtn.length; i++) {
                    self.closeBtn[i].removeEventListener('click', this.closeModal.bind(this));
                }
                if (self.opts.overlay) {
                    self.removeOverlay();
                }
                if (self.opts.closeCallback) {
                    self.opts.closeCallback.call(self);
                }
            },
            appendOverlay: function () {
                var self = this;
                self.overlay = document.createElement('div');
                self.overlay.className = self.opts.overlayClass;
                self.modal.appendChild(self.overlay);

            },
            removeOverlay: function () {
                var self = this;
                setTimeout(function () {
                    self.overlay.remove();
                }, 500);

            }

        };

        window.Modal = Modal;

    })();
</script>
