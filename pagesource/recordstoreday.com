<script>
 $( "#myCarousel li" ).first().addClass( "active" );
 $( ".item" ).first().addClass( "active" )

 var news_video_playing = 0;
 var news_videos = $('[id^="news_video_"]');
 var $myCarousel = $("#myCarousel_video");


 function pause_all_news_videos (){
     // The videos will set this to false after pausing
     // But not fast enough for the carousel's before-slide event
     news_video_playing = 0;
     for (var i=0; i < news_videos.length; i++ ){
         $f(news_videos[i]).api("pause");
     }
 }

 function set_news_video_playing (val){
     news_video_playing = val;
 }

 function slide_next(){
     pause_all_news_videos();
     $myCarousel.carousel("next");
 }

 function slide_prev(){
     pause_all_news_videos();
     $myCarousel.carousel("prev");
 }

 function set_up_news_video (vid){
     var $vid = $f(vid);
     $vid.addEvent('play', function(){set_news_video_playing(1);});
     $vid.addEvent('pause', function(){set_news_video_playing(0);});
     $vid.addEvent('finish', function(){set_news_video_playing(0);});
 }

 for (var i=0; i < news_videos.length; i++ ){
     $f(news_videos[i]).addEvent('ready',set_up_news_video);
 }

 $myCarousel.on("slide.bs.carousel", function (event) {
     return !news_video_playing;
 });

 $myCarousel.carousel({pause:'hover'});

 $("#carousel-next").on('click',slide_next);
 $("#carousel-prev").on('click',slide_prev);
 $('.item h3').on('click', function(){
     $myCarousel.carousel('pause');
	});
</script>
