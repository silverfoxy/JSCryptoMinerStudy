 <div class=" text-center"><h3>RECORD STORE DAY BEGINS IN</h3></div>
        <div class="col-xs-3"><h4 style="padding:0px; margin:0px">Days</h4><h4 style="padding:0px; margin:0px"><div id="rsd_countdown_days" class="number"></div></h4></div>
        <div class="col-xs-3"><h4 style="padding:0px; margin:0px">Hours</h4><h4 style="padding:0px; margin:0px"><div id="rsd_countdown_hours" class="number"></div></h4></div>
        <div class="col-xs-3"><h4 style="padding:0px; margin:0px">Minutes</h4><h4 style="padding:0px; margin:0px"><div id="rsd_countdown_minutes" class="number"></div></h4></div>
        <div class="col-xs-3"><h4 style="padding:0px; margin:0px">Seconds</h4><h4 style="padding:0px; margin:0px"><div id="rsd_countdown_seconds" class="number"></div></h4></div>
    </div>
</div>
<div class="col-xs-2 col-md-4"></div>


<script type="text/javascript">
 window.rsd_countdown = (function(){
     var rc         = this;
     var root_el    = document.getElementById("rsd_countdown");
     var days_el    = document.getElementById("rsd_countdown_days");
     var hours_el   = document.getElementById("rsd_countdown_hours");
     var minutes_el = document.getElementById("rsd_countdown_minutes");
     var seconds_el = document.getElementById("rsd_countdown_seconds");
     var test_now   = 0;
     // RSD starts April 21st 2018 @ 8am (12pm UTC).
     // Date.UTC counts months starting at 0
     var rsd_starts = new Date(Date.UTC(2018,3,21,12,0,0));

     rc.test_date = function(date_string){
         test_now = new Date(date_string);
         rc.update_vals();
     };

     rc.update_vals = function(){
         var update_freq_ms = 100;
         var diff_ms        = 0;
         var days_til       = 0;
         var hours_til      = 0;
         var minutes_til    = 0;
         var seconds_til    = 0;
         var now;

         if(test_now){
             now = test_now;
         }else{
             now = new Date();
         }

         diff_ms = rsd_starts.getTime() - now.getTime();

         if(diff_ms > 0){
             var t           = Math.floor(diff_ms/1000);
             seconds_til     = t % 60;
             t               = Math.floor(t/60);
             minutes_til     = t % 60;
             t               = Math.floor(t/60);
             hours_til       = t % 24;
             t               = Math.floor(t/24);
             days_til        = t;
         }

         days_el.innerHTML    = days_til;
         hours_el.innerHTML   = hours_til;
         minutes_el.innerHTML = minutes_til;
         seconds_el.innerHTML = seconds_til;

         setTimeout(rc.update_vals, update_freq_ms);
     };

     rc.update_vals();

     return rc;
 })();
</script>


