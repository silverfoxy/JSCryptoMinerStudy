		url: '/quotes.xml',
		success: populator
	});
});

var showing;
var current_quote = 0;
var max_quotes = 0;

function populator(xml, status, request) {
	$("quote",xml).each(function(id) {  
 		var quote = $("quote",xml).get(id);
		$("#quotes").append("<div class='quote'><div class='quotes_content'>" + $('contents',quote).text() +  " </div> " + "<div class='quotes_name'>" + $('author',quote).text() + "</div></div>\n");
	});
	max_quotes = $("div.quote").length;
	showing = $('#quotes div.quote:first');
	showing.siblings().hide();
	if (max_quotes > 0) {
		setInterval('show_next_rotating_item(showing)', 5000);
	}
}
 
// The below function repeatedly gets called, to do the rotating
function show_next_rotating_item(t){
	t.fadeOut();
	var target = current_quote;
	while (target == current_quote) {
		target = Math.floor(Math.random() * max_quotes) + 1;
	}
	//console.log("Showing quote " + target + " / " + max_quotes);
	
	var next_rotating_item = $("#quotes div.quote:nth-child("+target+")");
	if(!next_rotating_item.attr('class')){
		next_rotating_item = $('#quotes div.quote:first');
	}	
	showing = next_rotating_item;
	showing.fadeIn();
	current_quote = target;
}
</script>
