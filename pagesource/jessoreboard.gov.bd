            function view_result()
            {
                var s_id = prompt("Enter Student ID");
                var year = prompt("Enter Year");
                var url = "http://student.jessoreboard.gov.bd/academic_transcript.php?id=" + s_id + "&year=" + year;
                var win = window.open(url, '_blank');
                win.focus();
                return false;
            }

