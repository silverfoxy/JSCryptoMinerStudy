<script>
    window.branded = window.branded || {};
    window.branded.cordial = window.branded.cordial || 
        {   
            debug: true,
            legacyNewsletter: false,
            onloader: function(){
                onload = window.branded.cordial.onload;
                for(var i = 0; i < onload.length; i++){
                    if(typeof onload[i] == 'function'){
                        onload[i]();
                    }
                }
            }, 
            onload: [],
            after: function(object, method, fn) {
                var originalMethod = object[method];
                object[method] = function () {
                    originalMethod.apply(object, arguments);
                    fn.call(object);
                };
            },
            dynamicTracking: function(){
                (function($){ 
                    $('.cordial').each(function(){
                        var eventData = {};
                        var eventName = $(this).data('event-name');
                        if(typeof eventName != 'undefined' && eventName.length > 0){
                            var eventPoints = $(this).data('event-points');
                            if(typeof eventPoints != 'undefined' && eventPoints.length > 0){
                                eventPoints = eventPoints.split(',');
                                for(var i=0; i < eventPoints.length; i++){
                                    var eventAttribute = eventPoints[i];
                                    if(eventAttribute.length > 0){
                                        eventData[eventAttribute] = $(this).data('event-' + eventAttribute);
                                    }
                                }
                            }
                            cordial.event(eventName, eventData);
                        }
                    });
                })(jQuery);
            },
            isEmail: function (email){
                var p = '^[-!#$%&\'*+\\./0-9=?A-Z^_`a-z{|}~]+@[-!#$%&\'*+\\/0-9=?A-Z^_`a-z{|}~]+\.[-!#$%&\'*+\\./0-9=?A-Z^_`a-z{|}~]+$';
                return email == '' || new RegExp(p).test(email);
            },
            convertFormData: function(data){
                // data is in the output format of serializeArray();
                var contactInfo = {};
                (function($){
                    var formattedData = {};
                    var email = false;

                    $.each(data, function(){

                        var value = this.value;
                        if(value == 'true' || value == 'false') value = Boolean(value);
                        var key = this.name;

                        if(value.length > 0){
                            contactInfo[key] = value;
                        }
                    });
                })(jQuery);
                return contactInfo;
            },
            legacyGenerateMap: function(data){
                var map = {};
                (function($){
                    $.each(data.split(","), function(index, element){
                        var parts = element.split(":");
                        var key = parts[0];
                        var value = parts[1];
                        map[key] = value;
                    });
                })(jQuery);
                return map;
            },
            legacyGetEmail: function (data, map){
                var email = false;
                (function($){
                    $.each(data, function(){
                        if(this.name in map) {
                            var newKey = map[this.name];
                            if(newKey == 'email') {
                                email = this.value;
                            }
                        }
                    });
                })(jQuery);
                return email;
            },
            legacyGetContactInfo: function(data, map){
                // data is in the output format of serializeArray();
                var contactInfo = {};
                (function($){
                    var formattedData = {};
                    var email = false;

                    $.each(data, function(){
                        formattedData[this.name] = this.value;
                        if(this.name in map) {
                            var newKey = map[this.name];
                            var theValue = this.value;
                            if(theValue == 'true' || theValue == 'false') theValue = Boolean(theValue);
                            if(newKey == 'email') {
                                email = this.value;
                            } else {
                                contactInfo[newKey] = theValue;
                            }
                        }
                    });
                })(jQuery);
                return contactInfo;
            },
            captureEmail: function(email){
                cordial.identify(email);
            },
            log: function (message){
                if(window.branded.cordial.debug){
                    console.log(message);
                }
            }
        };
    window.branded.cordial.onload.push(window.branded.cordial.dynamicTracking);
</script>

