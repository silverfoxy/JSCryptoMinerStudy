		// Added by Jeremy K 20150122
		//The key will be the text displayed in the menu ( "Key": "Value" ) 
		//The value should be the URL the menu item should go to. 
		var add = { 
			//"St. Helens Online Academy": "http://shoa.sthelensk12.org" - Removed by Marie - Case #00127585
			"St. Helens-Yankton Arthur Academy": "http://shyaa.arthuracademy.org" //Added by Chris Case 00193064
		} 
		
		var more = ""; 
		
		$.each(add, function(k, v){  //build the new schools to add to the list
			more += '<li><a href="' + v + '">' + k + '</a></li>'; 
		}); 
		
		//Add them to the current list
		$('.sw-mystart-dropdown.schoollist .sw-dropdown .sw-dropdown-list').append(more);    
		
		//Sorting Function
		$.fn.sortList = function() {
			var mylist = $(this);
			var listitems = $('li', mylist).get();
			listitems.sort(function(a, b) {
			    var compA = $(a).text().toUpperCase();
			    var compB = $(b).text().toUpperCase();
			    return (compA < compB) ? -1 : 1;
			});
			$.each(listitems, function(i, itm) {
			    mylist.append(itm);
			});
		}
		
		//sort the list A-Z
		$('.sw-mystart-dropdown.schoollist .sw-dropdown .sw-dropdown-list').sortList();
	});
	
