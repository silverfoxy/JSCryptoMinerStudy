		function addtocartajax(deliverableid){
			// alert("adding: (did=" + deliverableid);
			$('#addToCartIcon' + deliverableid).removeClass("fa-plus-circle");
			$('#addToCartIcon' + deliverableid).addClass("fa-cog fa-spin");
			// alert($('#addToCartLink' + deliverableid).attr('onclick'));
			$('#addToCartLink' + deliverableid).off( "click" );
			$('#addToCartLicenseLink' + deliverableid).off( "click" );
			$.ajax({
					url: '/cart/addtocartajax?deliverableid=' + deliverableid,
					type: 'get',
					success: function(response) {
						responseAry = response.split(":");
						var data = responseAry[0];
						var newcartitemid = responseAry[1];
						if (data == 'added') {
							$('#addToCartNote').html('Item added to cart');
							$("#addToCartNote").fadeIn(500);
							window.setTimeout('$("#addToCartNote").fadeOut(3000);' , 5000);
							$('#addToCartIcon' + deliverableid).removeClass("fa-cog fa-spin addToCartIconGray");
							$('#addToCartIcon' + deliverableid).addClass("fa-check-circle addToCartIconGreen");
							$('#addToCartLink' + deliverableid).attr('onclick', '');
							$('#addToCartLicenseLink' + deliverableid).attr('onclick', '');
							$('#addToCartLink' + deliverableid).off( "click" );
							$('#addToCartLicenseLink' + deliverableid).off( "click" );
							$('#addToCartLink' + deliverableid).click(function(){
								removefromcartajax(deliverableid,newcartitemid);
							});
							$('#addToCartLicenseLink' + deliverableid).click(function(){
								removefromcartajax(deliverableid,newcartitemid);
							});
							// update cartcount in header
							var cartCount = $('#cartCount').text();
							var newCartCount = parseInt(cartCount) + 1;
							if (newCartCount < 1) {
								newCartCount = 1;
							}
							$('#cartCount').html(newCartCount);

						} else if (data == 'incart') {
							$('#addToCartNote').html('This item is already in your cart');
							$("#addToCartNote").fadeIn(500);
							window.setTimeout('$("#addToCartNote").fadeOut(3000);' , 5000);
							$('#addToCartIcon' + deliverableid).removeClass("fa-cog fa-spin addToCartIconGray");
							$('#addToCartIcon' + deliverableid).addClass("fa-check-circle addToCartIconGreen");
							$('#addToCartLink' + deliverableid).attr('onclick', '');
							$('#addToCartLicenseLink' + deliverableid).attr('onclick', '');
							$('#addToCartLink' + deliverableid).off( "click" );
							$('#addToCartLicenseLink' + deliverableid).off( "click" );
							$('#addToCartLink' + deliverableid).click(function(){
								removefromcartajax(deliverableid,newcartitemid);
							});
							$('#addToCartLicenseLink' + deliverableid).click(function(){
								removefromcartajax(deliverableid,newcartitemid);
							});
						} else if (data == 'notavail') {
							$('#addToCartNote').html('This product is not currently available with this licensing option.');
							$("#addToCartNote").fadeIn(500);
							window.setTimeout('$("#addToCartNote").fadeOut(3000);' , 5000);
							$('#addToCartIcon' + deliverableid).removeClass("fa-cog fa-spin");
							$('#addToCartIcon' + deliverableid).addClass("fa-exclamation-circle");
							$('#addToCartLink' + deliverableid).attr('onclick', '');
							$('#addToCartLicenseLink' + deliverableid).attr('onclick', '');
							$('#addToCartLink' + deliverableid).off( "click" );
							$('#addToCartLicenseLink' + deliverableid).off( "click" );
						} else if (data == 'cookie') {
							$('#addToCartNote').html('You must have cookies enabled to shop on our site. Sorry.');
							$("#addToCartNote").fadeIn(500);
							window.setTimeout('$("#addToCartNote").fadeOut(3000);' , 5000);
							$('#addToCartIcon' + deliverableid).removeClass("fa-cog fa-spin");
							$('#addToCartIcon' + deliverableid).addClass("fa-exclamation-circle");
							$('#addToCartLink' + deliverableid).attr('onclick', '');
							$('#addToCartLicenseLink' + deliverableid).attr('onclick', '');
							$('#addToCartLink' + deliverableid).off( "click" );
							$('#addToCartLicenseLink' + deliverableid).off( "click" );
						} else {
							$('#addToCartIcon' + deliverableid).removeClass("fa-cog fa-spin");
							$('#addToCartIcon' + deliverableid).addClass("fa-exclamation-circle");
							alert('An unknown error occurred while adding the item to your cart. Please contact us if this problem persists: ' + data);
							$('#addToCartLink' + deliverableid).attr('onclick', '');
							$('#addToCartLicenseLink' + deliverableid).attr('onclick', '');
							$('#addToCartLink' + deliverableid).off( "click" );
							$('#addToCartLicenseLink' + deliverableid).off( "click" );
						}
					}
			});
		}

		function removefromcartajax(deliverableid,cartitemid){
			// alert("removing: (did=" + deliverableid + " | ciid=" + cartitemid);
			$('#addToCartIcon' + deliverableid).removeClass("fa-check-circle addToCartIconGreen");
			$('#addToCartIcon' + deliverableid).addClass("fa-cog fa-spin");
			$('#addToCartLink' + deliverableid).attr('onclick', '');
			$('#addToCartLicenseLink' + deliverableid).attr('onclick', '');
			$('#addToCartLink' + deliverableid).off( "click" );
			$('#addToCartLicenseLink' + deliverableid).off( "click" );
			$.ajax({
					url: '/cart/removefromcartajax?id=' + cartitemid,
					type: 'get',
					success: function(data) {
						if (data == 'removed') {
							$('#addToCartNote').html('Item removed from cart');
							$("#addToCartNote").fadeIn(500);
							window.setTimeout('$("#addToCartNote").fadeOut(3000);' , 5000);
							$('#addToCartIcon' + deliverableid).removeClass("fa-cog fa-spin addToCartIconGreen");
							$('#addToCartIcon' + deliverableid).addClass("fa-plus-circle addToCartIconGray");
							// update cartcount in header
							var cartCount = $('#cartCount').text();
							var newCartCount = parseInt(cartCount) - 1;
							if (newCartCount < 0) {
								newCartCount = 0;
							}
							$('#cartCount').html(newCartCount);
							$('#addToCartLink' + deliverableid).attr('onclick', '');
							$('#addToCartLicenseLink' + deliverableid).attr('onclick', '');
							$('#addToCartLink' + deliverableid).off( "click" );
							$('#addToCartLicenseLink' + deliverableid).off( "click" );
							$('#addToCartLink' + deliverableid).click(function(){
								addtocartajax(deliverableid);
							});
							$('#addToCartLicenseLink' + deliverableid).click(function(){
								addtocartajax(deliverableid);
							});
						} else {
							$('#addToCartIcon' + deliverableid).removeClass("fa-cog fa-spin");
							$('#addToCartIcon' + deliverableid).addClass("fa-exclamation-circle");
							alert('An unknown error occurred while adding the item to your cart. Please contact us if this problem persists: ' + data);
						}
					}
			});
		}
		function showModal(header,content) {
			$('#largeGenericModalTitle').html(header);
			$('#largeGenericModalBody').html(content);
			$('#largeGenericModal').modal();
		}
		function toggleFavorite(id) {
		
			showModal('Please Sign In','<p><strong>You must <a href="/secure/login/">sign in</a> to use favorites.</strong></p>');
		
	}
