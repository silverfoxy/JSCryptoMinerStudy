jQuery(window).load(function(){
  jQuery('#newsslider-4592 .news-content:first-child()').addClass('top-content');
  jQuery('#newsslider-4592 .nh-anim:first-child()').addClass('selected first');
  jQuery('#newsslider-4592 .g-newsslider-pagination li:first-child()').addClass('selected');
});

(function ($) {
  "use strict";

  var          hl,
  newsList = $('#newsslider-4592 .news-headlines'),
  navigation_next = $('#newsslider-4592 .g-newsslider-navigation .next'),
  navigation_prev = $('#newsslider-4592 .g-newsslider-navigation .prev'),
  pagination = $('#newsslider-4592 .g-newsslider-pagination li'),
  newsListItems = $('#newsslider-4592 .news-headlines li'),
  firstNewsItem = $('#newsslider-4592 .news-headlines li:nth-child(1)'),
  newsPreview = $('#newsslider-4592 .news-preview'),
  elCount = $('#newsslider-4592 ul.news-headlines').children().length - 1,
  vPadding = (parseInt(firstNewsItem.css('padding-top').replace('px', ''), 10)) +
  (parseInt(firstNewsItem.css('padding-bottom').replace('px', ''), 10)),
  vMargin = (parseInt(firstNewsItem.css('margin-top').replace('px', ''), 10)) +
  (parseInt(firstNewsItem.css('margin-bottom').replace('px', ''), 10)),
  cPadding = (parseInt($('.news-content').css('padding-top').replace('px', ''), 10)) +
  (parseInt($('.news-content').css('padding-bottom').replace('px', ''), 10)),
  speed = 6000, // this is the speed of the switch
  myTimer = null,
  siblings = null,
  totalHeight = null,
  indexEl = 0,
  i = null;
  newsListItems.addClass('nh-anim');

  function doTimedSwitch() {
    myTimer = setInterval(function () {
      if (($('#newsslider-4592 .selected').prev().index() + 1) === elCount) {
        firstNewsItem.trigger('click');
      } else {
        $('#newsslider-4592 .selected').next(':not(.first)').trigger('click');
      }
    }, speed);
  }

  clearInterval(myTimer);
      doTimedSwitch();
  
  function doClickItem() {
    navigation_next.on('click', function () {
      $('#newsslider-4592 .g-newsslider-headlines li.selected').next().trigger('click');
    });
    navigation_prev.on('click', function () {
      $('#newsslider-4592 .g-newsslider-headlines li.selected').prev().trigger('click');
    });

    newsListItems.on('click', function () {
      newsListItems.removeClass('selected');
      $(this).addClass('selected');

      siblings = $(this).prevAll();
      totalHeight = 0;

      // this loop calculates the height of individual elements, including margins/padding
      for (i = 0; i < siblings.length; i += 1) {
        totalHeight += $(siblings[i]).height();
        totalHeight += vPadding;
        totalHeight += vMargin;
      }

      indexEl = $(this).index() + 1;

      $('#newsslider-4592 .g-newsslider-pagination li').removeClass('selected');
      $('#newsslider-4592 .g-newsslider-pagination li:nth-child(' + indexEl + ')').addClass('selected');

      $('#newsslider-4592 .news-content:nth-child(' + indexEl + ')').siblings().removeClass('top-content');
      $('#newsslider-4592 .news-content:nth-child(' + indexEl + ')').addClass('top-content');

      clearInterval(myTimer);
      // comment out the line below if you don't
      // want it to rotate automatically
              doTimedSwitch();
          });

    pagination.on('click', function () {

      pagination.removeClass('selected');
      $(this).addClass('selected');

      siblings = $(this).prevAll();
      totalHeight = 0;

      // this loop calculates the height of individual elements, including margins/padding
      for (i = 0; i < siblings.length; i += 1) {
        totalHeight += $(siblings[i]).height();
        totalHeight += vPadding;
        totalHeight += vMargin;
      }

      indexEl = $(this).index() + 1;
      $('#newsslider-4592 .g-newsslider-headlines .nh-anim').removeClass('selected');
      $('#newsslider-4592 .g-newsslider-headlines .nh-anim:nth-child(' + indexEl + ')').addClass('selected');

      $('#newsslider-4592 .news-content:nth-child(' + indexEl + ')').siblings().removeClass('top-content');
      $('#newsslider-4592 .news-content:nth-child(' + indexEl + ')').addClass('top-content');

      clearInterval(myTimer);
      // comment out the line below if you don't
      // want it to rotate automatically
              doTimedSwitch();
          });
  }

  function doWindowResize() {
    $(window).resize(function () {
      clearInterval(myTimer);
      $('#newsslider-4592 .selected').trigger('click');
    });
  }

  // this is the poor man's 'init' section
  doClickItem();
  doWindowResize();
  $('#newsslider-4592 .selected').trigger('click');

})(jQuery);

</script>
