                border-style: solid;
                border-width: 1em;
                box-shadow: 1px -2px 1px 0 rgba(0, 0, 0, 0.4);
                box-sizing: border-box;
                content: "";
                display: block;
                height: 0;
                left: 139px;
                position: absolute;
                top: 1px;
                transform: rotate(-46deg);
                transform-origin: 0 0 0;
                width: 0;
