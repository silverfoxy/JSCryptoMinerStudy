var re = new RegExp('^http(?:s)?://((?:(?:[a-zA-Z]|[a-zA-Z][a-zA-Z0-9\\-]*[a-zA-Z0\\-9])\\.)*(?:[A-Za-z]|[A-Za-z][A-Za-z0-9\\-]*[A-Za-z0-9]))(?:/|$)', 'i');
return getFirstGroup(re, httpUrl);
}
function trackVirtual(gaq, href, prefix) {
var urlWithoutProtocol = getFirstGroup(/^(?:(?:http(?:s)?)|ftp):\/(\/.+)$/i, href) || href;
gaq.push(['_trackPageview', '/virtual' + (prefix || '') + urlWithoutProtocol]);
}
jQuery('a:not(.no-google-analytics)').click(function(e) {
var protocol, hostName, tracker, recipient, delay,
isFileLink = false,
fileTypes = /\.(pdf|doc|docx|ppt|pptx|pps|ppsx|xls|xlsx|exe|vcf|eps|jpg|png|zip|txt)$/i,
jThis = jQuery(this),
href = jQuery.trim(jThis.attr("href")),
target = jThis.attr("target");
if (href) {
isFileLink = fileTypes.test(href);
protocol = getFirstGroup(/^(http|https|mailto|ftp):.+$/, href);
switch(protocol) {	
case "ftp":
isFileLink = true;
break;
case "http":
case "https":
hostName = getHostname(href);
if (hostName && hostName.indexOf("uu.se") !== -1) {
if (_gat &&
getHostname(location.href) !== hostName
&& !(new RegExp('m\.teknat\.uu\.se|rektorsbloggen\.uu\.se|mp\.uu\.se|studentportalen\.uu\.se|www\.alumnnatverk\.uu\.se|ub\.uu\.se\.libanswers\.com|libanswers\.ub\.uu\.se|doit\.medfarm\.uu\.se|media\.medfarm\.uu\.se').test(hostName))) {
tracker = _gat._getTrackerByName();
if (typeof tracker._getLinkerUrl(href, true) == 'string') {
jQuery(this).attr("href", tracker._getLinkerUrl(href, true));
}
}	    
} else if (_gaq) { 
trackVirtual(_gaq, href, '/external');
if (!target) {
delay = 100;
e.preventDefault();
}
}
break;
case "mailto":
recipient = getFirstGroup(/^[^:]+:(.+)$/, href);
if (_gaq && recipient) {
_gaq.push(['_trackPageview', '/virtual/email/' + recipient]);
}
break;
}
if (isFileLink) {
trackVirtual(_gaq, href, '/documents');
if (!target) {
delay = 100;
e.preventDefault();
}
}
if (delay) {
setTimeout('document.location = "' + href + '"', delay);
}				
}
});
});
} catch (e) {
if (typeof UU !== 'undefined' && UU.logError) {
UU.logError(e);
}
}
