<script type="text/javascript">
    jQuery(document).ready(function () {

        jQuery('#ctl00_ctl15_SIASearch').click(function () {
            var returnvalue = true;
            if (jQuery.trim(jQuery('#ctl00_ctl15_SIASearchKeyword').val()).length == 0) {
                returnvalue = false;
            }
            var selKeyword = jQuery('#ctl00_ctl15_selKeyword');
            if (selKeyword != null) {
                selKeyword.val(jQuery.trim(jQuery('#ctl00_ctl15_SIASearchKeyword').val()));
                if (typeof _gaq !== "undefined" && _gaq !== null) {
                    var searchedTerm = '';
                    searchedTerm = selKeyword.val().replace("'", "\\'");
                    _gaq.push(['_trackEvent', 'Search', 'Term', searchedTerm]);
                }
            }
            return returnvalue;
        });

        jQuery('#ctl00_ctl15_SIASearchKeyword').autocomplete({
            source: function (request, response) {
                jQuery.ajax({
                    url: "/SIA_Services/SIA_Services.svc/GetCourses",
                    dataType: "json",
                    data: { val: JSON.stringify(data = { PartialCourseCode: request.term }) },
                    success: function (data) {
                        if (data.d.Data) {
                            response(jQuery.map(data.d.Data.Courses, function (item) {
                                return {
                                    label: item.CourseName,
                                    value: item.CourseName
                                }
                            }));
                        }
                        else {
                            return {
                                label: '',
                                value: ''
                            }
                        }
                    }
                });
            },
            minLength: 2,
            open: function () {
                jQuery(this).removeClass("ui-corner-all").addClass("ui-corner-top");
                return false;
            },
            close: function () {
                jQuery(this).removeClass("ui-corner-top").addClass("ui-corner-all");
                return false;
            }
        });

        jQuery("#ctl00_ctl15_SIASearchKeyword").keydown(function (event) {
            if (event.keyCode == 13) {
                var selKeyword = jQuery('#ctl00_ctl15_selKeyword');
                if (selKeyword != null) {
                    selKeyword.val(jQuery.trim(jQuery('#ctl00_ctl15_SIASearchKeyword').val()));
                }
               jQuery("#ctl00_ctl15_SIASearch").click();
            }
        });

    });
</script>
        
    


