        function validadados(){
            var vouenviar = true;
            $('.limpavel').each(function(){
                if($(this).val() == ''){					
                    $(this).prev().prev().show().delay(5000).fadeOut('fast');
                    vouenviar = false;
                    return false;
                }else
                    vouenviar = true
            });
            if(vouenviar)
                jqForm('newsletterForm','refreshNewsletter','divaguardenews');
        }
