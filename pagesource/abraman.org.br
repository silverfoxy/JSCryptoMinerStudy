            <script type="text/javascript">
                $(document).ready(function(){
                    
                     $("a.showimgmodal").fancybox();
                    
                    //Para igualar as alturas da sidebar e do conteudo principal					   
                    var altura1 = $("#sidebar").height();
                    var altura2 = $("#principal").height();
                    if (altura1>=altura2){
                        $("#principal").css('height',altura1);
                    } else{
                        $("#sidebar").css('height',altura2);
                    }

                    //Colocar margens
                    $('.noticias-home li:even').css('margin-right','15px');
                   // $('.publicidade li:even').css('margin-right','15px');

                    //Banner		
                    $('#imgcycle').cycle({
                        fx:'fade',
                        speed:'slow',
                        timeout:5000,
                        pager:'#navslide',
                        pagerEvent:'mouseover'
                    });
					
					$('#cycle').cycle({
                        fx:'fade',
                        speed:'slow',
                        timeout:5000,
                        pager:'#navslide_hotsite',
                        pagerEvent:'mouseover'
                    });
                        
                     $('.cycleEventos').cycle({
                        fx:'fade',
                        speed:'slow',
                        timeout:5000
                    });

                    $('#cycleParceiros').cycle({
                        fx:'fade',
                        speed:'slow',
                        timeout:5000
                    });

                    //PlaceHolder
                    $('.limpavel').live('focusin',function(){
                        $(this).prev('label').hide();
                    }).live('focusout',function(){
                        var valor = $(this).val();
                        if(valor==''){
                            $(this).prev('label').show();
                        }
                    }).live('keyup',function(){
                        var valor = $(this).val();
                        if(valor==''){
                            $(this).prev('label').show();
                        }else{
                            $(this).prev('label').hide();
                        }
                    });	

                    $('#formpesquisa').submit(function(){
                        //se o campo nde pesquisa estiver vazio
                        if($('#search').val() == ''){
                            // exibe a mensagem de erro
                            $("#vazio").show().delay(2000).fadeOut('fast');
                                return false;
                        }else{
                                                        return true;
                                                }
                    });

                    //Escritorios
                    $('#Map area').mouseover(function(){
                        var uf = $(this).attr('lang');
                        $('#mapas img').hide();
                        $('#mapas img[lang='+uf+']').show();
                        $('.endereco').hide();
                        $('.endereco[lang='+uf+']').show();
                    });	

                    $(".tel").mask("(99)9999-9999");

                    //Agenda
                    $('#cycleagenda').cycle({
                        fx:'fade',
                        speed:'slow',
                        timeout:8000,
                        next:'.prox-agenda',
                        prev:'.voltar-agenda'
                    });	

                    //Submenu
                    $("#menu li a").click(function(){
                             var menu = $(this).attr("lang");
                            if($('.'+menu).is(':visible')){
                                    $('.'+menu).hide();
                                    return false;
                            }
                            $('.submenu-principal').hide();    
                            $('.'+menu).show();              
                    });
                    
                    var myOpen = function(hash){hash.w.fadeIn('2000').show();};
                    var myClose=function(hash) { hash.w.fadeOut('2000',function(){ hash.o.remove(); }); };
                    
                    $('#popup').jqm({
                            onShow: myOpen,
                            onHide: myClose
                    }).jqmShow();
                    
                    $(".sub-1").mouseover(function(){
                        $("#sub-1").css('visibility','visible');
                    }).mouseout(function(){
                        $("#sub-1").css('visibility','hidden');
                    });					
                
                    $(".sub-2").mouseover(function(){
                        $("#sub-2").css('visibility','visible');
                    }).mouseout(function(){
                        $("#sub-2").css('visibility','hidden');
                    });					
                
                    $(".sub-3").mouseover(function(){
                        $("#sub-3").css('visibility','visible');
                    }).mouseout(function(){
                        $("#sub-3").css('visibility','hidden');
                    });					
                
                    $(".sub-4").mouseover(function(){
                        $("#sub-4").css('visibility','visible');
                    }).mouseout(function(){
                        $("#sub-4").css('visibility','hidden');
                    });					                   
					
					$(".submenu").mouseover(function(){
                        $(this).css('visibility','visible');
                    }).mouseout(function(){
                        $(this).css('visibility','hidden');
                    });
					
					 //Caixa de Login
                    $("#indicacao").fancybox({
                        'scrolling'		: 'no',
                        'titleShow'		: false,
                        'onClosed'		: function() {
                        $("#login_error").hide();
                        }
                    });
                    
                    
                });
                    
                function jqForm(nomeForm,divResult,aguarde,after){
                    jqPost(nomeForm,divResult,null,null,aguarde,after);
                }	
            </script> 
        

