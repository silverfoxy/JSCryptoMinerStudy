function getposOffset( obj )
{
	var addOffSet = 10;
	var totaloffset = obj.offsetTop;
	var parentEl = obj.offsetParent;

	while (parentEl!=null)
	{
		totaloffset = totaloffset + parentEl.offsetTop;
		parentEl=parentEl.offsetParent;
	}

	return totaloffset + addOffSet;
}

function mySrhJongmok_move( action, opt)
{
	if(opt == '1'){
		var page = document.mysrh_form.cur_page.value;
	}else{
