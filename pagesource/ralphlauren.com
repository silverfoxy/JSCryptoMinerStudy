function getCookie(key) {
var cookies = document.cookie.split(';');
for (var i = 0; i < cookies.length; i++) {
var tokens = cookies[i].split('=');
var cookieKey = tokens[0].trim();
if (cookieKey === key) {
return tokens[1];
}
}
return '';
}
if (getCookie('pageNameDuplicate') != null) {
