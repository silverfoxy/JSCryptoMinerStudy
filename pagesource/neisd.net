	function dynamicContent() {
		
		// ********** MYSTART ********** //
		$(".ac-dropdown-container .selector").on('click keypress', function(event){
	        if(globalClick(event) === true){
				//DONT LET THE PAGE JUMP ON KEYPRESS
	            event.preventDefault();
	            controlDropdowns(event);
	        }
	    });

		//GLOBALLY CLOSE DROPDOWNS WHEN HOVERED AWAY FROM
	    $(".ac-dropdown-container").hover(function(e) {
			e.preventDefault();
		}, function() {

			$(".ac-dropdown",this).attr("aria-hidden", "true").slideUp(300, "swing"); 
			$(this).removeClass("open"); 
		});

		//SHOW USER OPTIONS IF THEY ARE LOGGED IN
		if($("#sw-mystart-account").length){
        	$("#gb-page").addClass("logged-in");
        	$(".ac-mystart-button.user.logged").show();
            $("#sw-mystart-mypasskey").removeClass("sw-mystart-button").addClass("ac-mystart-button").insertBefore(".ac-mystart-button.manage");
        } else {
        	
        	 //HIDE REGISTER BUTTON IF HIDDEN IN SITE MANAGER
              if(!$("#ui-btn-register").length){
                  $(".ac-mystart-button.signin").show();
              } else {
              	$(".ac-mystart-button.user").not(".logged").show();
              }
              
        }

        $(".ac-mystart-button:visible:last").addClass("last-visible");
		
		// ********** HEADER ********** //

    	//SOCIAL ICON HOVERS
    	$(".social-icon").hover(function() {
			$(".gb-social-icons").addClass("hovering");
		}, function() {
			$(".gb-social-icons").removeClass("hovering");
		});

		$(".social-icon").focus(function() {
			$(".gb-social-icons").addClass("hovering");
		});

		$(".social-icon").focusout(function() {
			$(".gb-social-icons").removeClass("hovering");
		});

		// ADD LOGO
		var logoCheck = "true";
		var logoSrc = jQuery.trim("/cms/lib/TX02215002/Centricity/Template/GlobalAssets/images///logos/ne-isd-logo.png");
		if(logoCheck == "true") {
			var srcSplit = logoSrc.split("/");
			var srcSplitLen = srcSplit.length;
			if((logoSrc != "") && (srcSplit[srcSplitLen - 1] != "default-man.jpg")) {
				$("#gb-logo").append('<a href="/northeastisd" title="North East Independent School District"><img src="' + logoSrc + '" alt="North East Independent School District" /></a>').removeClass("hidden");
			} else {
				$("#gb-logo").append('<a href="/northeastisd" title="North East Independent School District"><img src="/cms/lib3/SW00000036/Centricity/template/234/logos/fairbanks-logo.png" alt="North East Independent School District" /></a>').removeClass("hidden");
			}
		}
		
		// ADD SITENAME
		var siteNameOne = jQuery.trim('North East');
		var siteNameTwo = jQuery.trim('Independent School District');
		var siteTagline = jQuery.trim('Tradition of Excellence');
		
		if((siteNameOne == "") && (siteNameTwo == "")) {
			var splitLen = 2;
			var siteName = "North East Independent School District";
			siteName = siteName.split(" ");
			var siteNameLength = siteName.length;
			if(siteNameLength > 2){
				siteNameEnd = jQuery.trim(siteName.splice(-splitLen, siteName.length).toString().replace(/,/g, " "));
			} else {
				siteNameEnd = jQuery.trim(siteName.splice(-1, siteName.length).toString().replace(/,/g, " "));
			}
			siteNameBegin = jQuery.trim(siteName.toString().replace(/,/g, " "));
			$("#gb-sitename").prepend("<h1><span>" + siteNameBegin + "</span><br>" + siteNameEnd + "</h1>");
		} else if((siteNameOne != "") && (siteNameTwo == "")) {
			$("#gb-sitename").prepend("<h1><span>" + siteNameOne + "</span></h1>");
		} else if((siteNameOne == "") && (siteNameTwo != "")) {
			$("#gb-sitename").prepend("<h2>" + siteNameTwo + "</h2>");
		} else if((siteNameOne != "") && (siteNameTwo != "")) {
			$("#gb-sitename").prepend("<h1>" + siteNameOne + "<br/><span>" + siteNameTwo + "</span></h1><h2>"+siteTagline+"</h2>");
		}
		
		// ********** CHANNEL BAR ********** //
		$(".sw-channel-item").unbind('hover');
		$(".sw-channel-item").hover(function(){
			$(".sw-channel-item ul").stop(true, true);
			var subList = $(this).children('ul');
			if ($.trim(subList.html()) !== "") {
				subList.slideDown(300, "swing");
			}
			$(this).addClass("hover");
		}, function(){
			$(".sw-channel-dropdown").slideUp(300, "swing");
			$(this).removeClass("hover");
		});
        
        //MyNEISD
        $(".sw-channel-item").each(function(){
        	if($("> a span", this).text() == "MyNEISD"){
            	$(this).addClass("my-neisd");
            }
        })
		
		// ********** BODY ********** //

		//PHOTO ELEMENT HOVERS
		$(".element-txt a").focus(function() {
			$(this).parent().addClass("hovering");
		});

		$(".element-txt a").focusout(function() {
