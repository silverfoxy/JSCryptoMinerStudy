    function buscarCalendario(strPath,anio,strPathDsi) {
        var impuesto = $('#Impuesto').val();
        var mes = $('#Mes').val();

        if ((impuesto == "S") || (mes == "S")) {
            alert("Debe seleccionar impuesto y mes");
            return false;
        }
        
        switch (mes) {
            case "01":                              
            $('[id=mesCalendario]').html("Enero");   
            break;
            case "02":
            $('[id=mesCalendario]').html("Febrero")              
            break;
            case "03":
            $('[id=mesCalendario]').html("Marzo")                             
            break;
            case "04":
            $('[id=mesCalendario]').html("Abril")                 
            break;
            case "05":
            $('[id=mesCalendario]').html("Mayo")                 
            break;
            case "06":
            $('[id=mesCalendario]').html("Junio")                 
            break;
            case "07":
            $('[id=mesCalendario]').html("Julio")                 
            break;
            case "08":
            $('[id=mesCalendario]').html("Agosto")                 
            break;
            case "09":
            $('[id=mesCalendario]').html("Septiembre")                 
            break;
            case "10":
            $('[id=mesCalendario]').html("Octubre")                 
            break;
            case "11":
            $('[id=mesCalendario]').html("Noviembre")                 
            break;
            case "12":
            $('[id=mesCalendario]').html("Diciembre")                 
            break;          
        }

        switch (impuesto) {
            case "Inmobiliario":
                //Cambio img del calendario segn impuesto-mes
                $("#imgCalendario").attr("src", strPath + "inmobiliario" + "-" + mes + ".gif");             
                //Busco en la base de datos referencias de vencimientos del mes seleccionado
                referenciasVencimientos($('#Mes').val(), anio,0);
                break;
                case "InmoComplementario":
                //Cambio img del calendario segn impuesto-mes
                $("#imgCalendario").attr("src", strPath + "inmoComplementario" + "-" + mes + ".gif");
                //Busco en la base de datos referencias de vencimientos del mes seleccionado
                referenciasVencimientos($('#Mes').val(), anio, 10);
                break;
                case "Automotor":
                //Cambio img del calendario segn impuesto-mes
                $("#imgCalendario").attr("src", strPath + "automotores" + "-" + mes + ".gif");
                //Busco en la base de datos referencias de vencimientos del mes seleccionado
                referenciasVencimientos($('#Mes').val(), anio, 1);
                break;
                case "Embarcaciones":
                //Cambio img del calendario segn impuesto-mes
                $("#imgCalendario").attr("src", strPath + "embarcaciones" + "-" + mes + ".gif");
                //Busco en la base de datos referencias de vencimientos del mes seleccionado
                referenciasVencimientos($('#Mes').val(), anio, 3);
                break;
                case "IIBB":
                document.location.href = strPathDsi + "Informacion/InfoGeneral/Vencimientos/caMensual.asp?anio=" + anio;
                break;
                case "IIBBconvenio":            
                document.location.href = strPathDsi + "Informacion/InfoGeneral/Vencimientos/caConvenio.asp?anio=" + anio;
                break;
                case "AgentesRecaudacion":
                switch (mes) {
                    case "01":
                    mes = "ENERO"
                    break;
                    case "02":
                    mes = "FEBRERO"
                    break;
                    case "03":
                    mes = "MARZO"
                    break;
                    case "04":
                    mes = "ABRIL"
                    break;
                    case "05":
                    mes = "MAYO"
                    break;
                    case "06":
                    mes = "JUNIO"
                    break;
                    case "07":
                    mes = "JULIO"
                    break;
                    case "08":
                    mes = "AGOSTO"
                    break;
                    case "09":
                    mes = "SEPTIEMBRE"
                    break;
                    case "10":
                    mes = "OCTUBRE"
                    break;
                    case "11":
                    mes = "NOVIEMBRE"
                    break;
                    case "12":
                    mes = "DICIEMBRE"
                    break;
                    case "2018":
                    mes = "ENERO"
                    anio = "2018";
                    break;
                }
                document.location.href = strPathDsi + "Informacion/InfoGeneral/Vencimientos/caRecaudacionMes.asp?mes=" + mes + "&anio=" + anio;
                break;
                case "AgentesInformacion":                
                document.location.href = strPath + "calendarioagentes"+anio+".pdf";
                break;
            }

        }

        function referenciasVencimientos(mes, anio,impuesto) {
            $.get("Include/referenciasVencimientos.asp", { anio: anio, mes: mes, impuesto:impuesto }, 
                function (data) {
                    $("#referencias").html(data);
                });    
        }
    </script>
