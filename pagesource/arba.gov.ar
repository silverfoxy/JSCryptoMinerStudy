});
$( window ).resize(function() {
	CambioResol();
});
function CambioResol(){
	$(".btn-menu").val(   $(window).width()<550 ? "":  ValorMenu      );
}

//Funcin de delay
function delay(milis) {
	ahora=new Date();
	var antes=ahora.getTime();
	var despues=ahora.getTime();
	while (despues-antes < milis){
		ahora=new Date();
		var despues=ahora.getTime();
	}
}

