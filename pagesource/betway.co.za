    function ShowFilter(e, isMobileFilter) {
        if (isMobileFilter)
            showMobileFiltersOnly();

        e.stopPropagation();
        onFilterDropdownClick();
        $('#filterleagues-button').dropdown("toggle");
    }

    function ResetBreadCrumbToDefaultSport(e, marketTypeCategoryId) {
        try {
            ResetBreadCrumbToSport(e, marketTypeCategoryId, '00000000-0000-0000-da7a-000000550001');
        } catch (err) {
            console.log(err.message);
        }
    }

    function ResetBreadCrumbToSport(e, marketTypeCategoryId, sportId) {
        try {
            if (typeof showOverlay == "function") showOverlay();
            e.stopPropagation();

            var allEvents = "/";
            if (marketTypeCategoryId == "00000000-0000-0000-da7a-000000580003")
                allEvents = "/Event/InPlay";

            if (typeof pat == "object" && typeof pat.post == "function") {
                pat.post("/Event/ClearLeagueFilter", { marketTypeCategoryId: marketTypeCategoryId, sportId : sportId })
                    .done(function(data) {
window.location.reload();
                    });
            } else {

                if (window.runningAjax != null && window.runningAjax === true) {
                    console.log('preventing additional query');
                    return false;
                }

                window.runningAjax = true;
                ajax.post("/Event/ClearLeagueFilter",
                    { marketTypeCategoryId: marketTypeCategoryId, sportId: sportId },
                    function() {
window.location.reload();
                    }
                );
            }

        } catch (err) {
            console.log(err.message);
        }
    }

    function SetRegionFilterAndReload(regionId, isMultiMarket) {
        filterLeaguesByRegionId(regionId, isMultiMarket, '00000000-0000-0000-da7a-000000580001');
    }

    function SetLeagueFilterAndReload(leagueid, isMultiMarket) {
       
        if (typeof filterLeaguesByLeagueId == "function") {
            filterLeaguesByLeagueId(leagueid, isMultiMarket, '00000000-0000-0000-da7a-000000580001');
        } else {
            var allEvents = "/";
            if ('00000000-0000-0000-da7a-000000580001' == "00000000-0000-0000-da7a-000000580003")
                allEvents = "/Event/InPlay";
            var leagueids = [];
            leagueids.push(leagueid);
            filterLeaguesLite(leagueids, allEvents);
        }
    }
</script>

