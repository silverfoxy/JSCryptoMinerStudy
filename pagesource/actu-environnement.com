        function delete_ERR_cookie(cookieName)
        {
            var err_warn = document.cookie.substr(document.cookie.indexOf("ERR_WARN_ME=") + "ERR_WARN_ME=".length);
            err_warn = err_warn.substr(0, err_warn.indexOf(";"));
            t_err_warn = err_warn.split("%7C");
            
            var index = t_err_warn.indexOf(cookieName);
            if(index != -1)
            {
                t_err_warn.splice(index, 1);
            
                var new_err_warn = "ERR_WARN_ME=" + t_err_warn.join("%7C");
                document.cookie = new_err_warn + ';path=/';
                window.location.reload();
            }
            return false;
        }
        
        function delete_cookie(cookieName)
        {
            document.cookie = cookieName + "=;path=/";
            window.location.reload();
            return false;            
        }
