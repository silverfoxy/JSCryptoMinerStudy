   $(document).ready(function() {
      loadWeather('29926',''); //@params location, woeid
   });

   function loadWeather(location, woeid) {
      $.simpleWeather({
         location: location,
         woeid: woeid,
         unit: 'f',
         success: function(weather) {
            html = '<hr><h2>Local Weather<br><i class="icon-'+weather.code+'"></i><br> '+weather.temp+' °'+weather.units.temp+'<br>';
            html += '' +weather.currently+ '<br></h2><p style="color: white; font-size: 75%;">' +weather.updated+ '</p>';
      
            $("#weather").html(html);
         },
         error: function(error) {
            $("#weather").html('<p>'+error+'</p>');
         }
   });
}
