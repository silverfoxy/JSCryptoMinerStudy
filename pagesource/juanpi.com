    <script type="text/javascript">
        $.ajax({
            type: "get",
            dataType:'jsonp',
            jsonpCallback:'callback_time',
            url: '//upload.juancdn.com/time?ts='+new Date(),
            success: function (time) {
                var servertime = time/1000;
                $(".brand_time").each(function() {
                    var endtime = $(this).attr("data-etime");
                    if(endtime && (endtime-servertime > 0 || endtime != 'undefined')){
                        $(this).css('display','block');
                        runInterval(endtime, servertime, this, true);
                    }
                });

            }
        });
        function runInterval(endtime, servtime, _self, flag) {
            var _tmp = endtime-new Date(servtime);
            if ( _tmp > 0 ) {
                (function(endtime, servtime, _self, flag) {
                    setInterval(function() {
                        timestamp=endtime-new Date(servtime);
                        //计算出相差天数
                        days=Math.floor(timestamp/(24*3600));
                        //计算出小时数
                        leave1=timestamp%(24*3600);    //计算天数后剩余的毫秒数
                        hours=Math.floor(leave1/(3600));
                        //计算相差分钟数
                        leave2=leave1%(3600) ;       //计算小时数后剩余的毫秒数
                        minutes=Math.floor(leave2/(60));
                        //计算相差秒数
                        leave3=leave2%(60);      //计算分钟数后剩余的毫秒数
                        seconds=Math.round(leave3);
                        days=days<0?0:days;
                        hours=hours<0?0:hours;
                        minutes=minutes<0?0:minutes;
                        seconds=seconds<0?0:seconds;
                        if ( flag ) {
                            $(_self).children().children('span[class=brand-days]').children('em').html(days);
                            if(0 <= hours && hours < 10) {hours ='0'+hours;}
                            $(_self).children().children('span[class=brand-hours]').children('em').html(hours);
                        }
                        if(0 <= minutes  && minutes < 10) {minutes ='0'+minutes;}
                        if(0 <= seconds  && seconds < 10) {seconds ='0'+seconds;}
                        $(_self).children().children('span[class=brand-minutes]').children('em').html(minutes);
                        $(_self).children().children('span[class=brand-seconds]').children('em').html(seconds);
                        servtime++;
                    }, 1000);
                })(endtime, servtime, _self, flag);
            }
        }
        $('body').ready(function(){
            //埋点
            var parm=  $.extend({}, BURIED_POINT.class_paqName,{'is_home':true});
            BURIED_POINT.init({class_attr:['.banner a','.banner_l a','.banner_r a','.brand-status a','.brand-icons .fl a','.side-ad a','.full_column a'],class_paqName:parm});
