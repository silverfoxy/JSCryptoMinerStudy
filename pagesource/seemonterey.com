<script>
	require(["jquery", "plugins_common_custom_mmenuall", "domReady!"], function($) {
		var root = $('[data-guid=1e94cb8f-ae86-4790-a791-adec3daf6685]');
		var nav = root.find('.secondary-nav-container');
		var searchBox = root.find('.desktop-search');

		var closeAllPrimary = function() {
			// nav.find('> ul > li li.open ul:visible').hide();
			nav.find('li.open').removeClass('open');

			var industryNav = $('header').find('ul.open');
			if (industryNav.length) {
				industryNav.removeClass('open');
			}
			if (searchBox.hasClass('open')) {
				searchBox.removeClass('open');
			}
		};

		var closeAllPrimaryMod = function() {
			// nav.find('> ul > li li.open ul:visible').hide();
			nav.find('li.open:not(:hover)').removeClass('open');

			var industryNav = $('header').find('ul.open');
			if (industryNav.length) {
				industryNav.removeClass('open');
			}
			if (searchBox.hasClass('open')) {
				searchBox.removeClass('open');
			}
		};

		searchBox.find()

		if($('.main-navbar-container').hasClass('meetings')) {
			var visitorsLink = '<li class="item-topnav"><a href="/" target="_self" title="Visitors">Visitors</a></li>';
			nav.find('.topNav').append(visitorsLink);
		}

		nav.find('ul li.item-topnav').hover(function(e) {
			if ($(this).closest('.dropdown').length === 0) {
				var isOpen = $(this).closest('li').hasClass('open');
				closeAllPrimaryMod();
				if (!isOpen) {
					$(this).closest('li').toggleClass('open');
				}
			}
		}, function(e) { closeAllPrimary(); });

		nav.find('ul li a').on('click', function(e) {
			/* root drop down item */
			if ($(this).closest('.dropdown').length === 0) {
				var isOpen = $(this).closest('li').hasClass('open');
				closeAllPrimary();

				if (!isOpen) {
					$(this).closest('li').toggleClass('open');
					e.preventDefault();
				}
			}
			else if (e.target != this && e.target.tagName === 'SPAN') {
				/* open up a sub menu if the user clicked the arrow span */
				var parent = $(this).closest('li');
				var sub = parent.find('>ul');

				if (sub.length > 0) {
					if(sub.is(':visible')) {
						sub.slideUp('fast');
						parent.removeClass('open');
					} else {
						sub.slideDown('fast');
						parent.addClass('open');
					}
				}
				e.preventDefault();
			}
			else {
				closeAllPrimary();
			}
		});

		$(window).on('scroll', function() {
			closeAllPrimary();
		});
	});
</script>
