 	function isAlphanumeric(elem){
		var alphaExp = /^[0-9a-zA-Z,]+$/;
		if(elem.match(alphaExp)){
			return true;
		}else{
			return false;
		}
	} 
	function trim(str){
		if(!str || typeof str != 'string')
			return null; 
			return str.replace(/^[\s]+/,'').replace(/[\s]+$/,'').replace(/[\s]{2,}/,' ');
	
	}
  
 
