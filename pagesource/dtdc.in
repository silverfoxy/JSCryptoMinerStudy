<script type="text/javascript">
		
		function Initialize()
		{
		  req=null;
		  try
		  {
			  req=new XMLHttpRequest();
		  }
		  catch (e)
		  {
			  try
			  {
				req=new ActiveXObject("Msxml2.XMLHTTP");
			  }
			  catch (e)
			  {
				req=new ActiveXObject("Microsoft.XMLHTTP");
			  }
		  }
		  return req;
		}
				
		function Process()
		{
			//alert("process");
			var city_list;
			(req.readyState == 4)?(req.status == 200)?(req.responseText=="")?city_list = ""	:city_list = req.responseText	: false	:false;
		}
		
		req = new Initialize();
		function getval(obj,flag,divid,hobj,crtyobjval,strCond,cityCond)
		{
			//alert("getval");	
			var url="getcity.asp";
//			url = url +"?flag="+flag;
//			url = url +"&strCond="+strCond;
//			url = url +"&cityCond="+cityCond;	
//			url = url +"&code="+obj.value;
//			url = url +"&crty="+crtyobjval;	
//			url=url+"&sid="+Math.random();
			var city_list;
			if(req != null)
			{
				req.onreadystatechange = Process;
				req.open("GET",url,false); //Synchronous Connection
				req.send(null);
				if(req.status==200)
				{
					xmlDoc=req.responseXML;	
					cNodes=xmlDoc.documentElement.childNodes;
					city_list = "";
					//alert(cNodes.length);
					for(i=0;i<cNodes.length;i++)
					{
						city_list = city_list + cNodes[i].getAttribute("NAME")+'';// + '=' + cNodes[i].getAttribute("CODE") + '';
					    citys = cNodes[i].getAttribute("CODE");
						//city_list = city_list + cNodes[i].getAttribute("NAME")+ '-'+ cNodes[i].getAttribute("CODE") + '';
						if (city_list !="") city_list = city_list + '**'
					}
				}
				else
				{city_list = '';}
			}
			 var clist;
			try
			{
				//alert("try");
				clist = city_list.split('**');
			}
			catch(err)
			{
				//alert("err");	
				//getval(document.getElementById("txtCity"),0,"autoCity",document.getElementById("hideCity"),"","GETCITY",document.frmLocation.frCD.value);

			}
			new AutoSuggest(obj,clist,divid,hobj,flag);
		}	
		getval(document.getElementById("txtCity"),0,"autoCity",document.getElementById("hideCity"),"","GETCITY",document.frmLocation.frCD.value);
		
	function chkKeyword()
	{
		var chkVal = document.getElementById("txtKeyWord").value;
		if (chkVal=="")
