			{return false;}
		}
		else
		{
			if(count>23)			
			{return false;}
		}
		alfValues="ABCDEFGHIJKLMNOPQRTSUVWXYZ";							
		cnno=cnno.toUpperCase();
		var alf = cnno.charAt(0);
		//alert(alf)
		var i;
		//alert(cnno);	
