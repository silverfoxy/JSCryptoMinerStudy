     }
}	

function removeDuplicates(cnNumbers) {
 
 //alert(cnNumbers);
	var cnNumberArry = cnNumbers.split(",");
	var UniqCnNumber = [];
	$.each(cnNumberArry, function(i, e) {
		if ($.inArray(e, UniqCnNumber) == -1)
			UniqCnNumber.push(e);
	});
	
//alert(UniqCnNumber);
	return UniqCnNumber;
} 


