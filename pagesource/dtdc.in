			}	
		}
		
			if(isInvalid==true){alert("The Consignment Number is invalid!!");}
			if(valid==false){return false;}


		if(valid==true)
		{
			strconno=strconno+cnno+",";
			//alert("strconno="+strconno);
			count=count+1;
			//alert("count="+count);
		}	
