                    <div class="fldr-res-title ng-binding" data-bind="text:Restaurant.Name">&nbsp;</div>
                    <div class="fldr-res-address ng-binding" data-bind="text:Restaurant.Address">&nbsp;</div>
                </div>
            </div>

