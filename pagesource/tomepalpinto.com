			$('.varThemestoTop').fadeIn();
		} else {
			$('.varThemestoTop').fadeOut();
		}
	});
	$('.varThemestoTop').click(function(){
		$('html, body').animate({scrollTop : 0},800);
		return false;
	});
	
});
</script>
