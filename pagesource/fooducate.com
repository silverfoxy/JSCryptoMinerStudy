		<div class="nutrients panel panel-default">
			<div class="panel-heading">
				<h2 class="panel-title">Nutrition</h2>
			</div>
			<div class="panel-body">
				<ul class="item-list">
					{{if servingSize}}
						<li class="item"><em>Serving Size:</em> ${servingSize}</li>
					{{/if}}
					<li class="item">
						<div class="row">
							<span class="col-xs-2 col-xs-offset-7 text-center">Amount per Serving</span>
							<span class="col-xs-2 text-center">My Daily Value</span>
						</div>
					</li>
					{{each nutrients}}
						<li class="item">
							<div class="row">
								<span class="col-xs-7">
									<span class="nutrient-level-${$value.getIndent()}">${$value.getDisplayName()}</span>
								</span>
								<span class="col-xs-2 text-center">${$value.getValue()}${$value.getUom()}</span>
								<span class="col-xs-2 text-center">${$value.getDailyValue()}</span>
								<span class="col-xs-1 daily-value-indication">
									<i class="type ${FDCT.presenter.Product.getTypeIconClass($value.getDailyValueIndication())}"></i>
								</span>
							</div>
						</li>
					{{/each}}
					{{if !FDCT.model.AuthDao.isSignedIn()}}
						<li class="item">
							<a class="login btn btn-success">Login / signup to see personalized values</a>
						</li>
					{{/if}}
				</ul>
			</div>
		</div>
	{{/if}}
</div>
</script>

