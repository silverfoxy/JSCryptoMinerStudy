<script type="text/javascript">

  function checkRadioBySelect(idSelect) {
	switch (idSelect) {
		case 'srcLiberacoes': document.getElementById('rcLiberacoes').checked = true; break;

		case 'srcExecucao':	  document.getElementById('rcExecucao').checked = true; break;
		case 'srgTipoSancao': document.getElementById('rgTipoSancao').checked = true; break;
		case 'trgNomeCnpj':
			document.getElementById('rgNomeCnpj').checked = true;
			document.getElementById('trgNomeCnpj').select();
		break;
	}
  }

  function mostrarGrupo(id) {
    var opcoes;
    var grupos = new Array;
    var i;

    opcoes = document.getElementById("menu-grupos").getElementsByTagName("li");
    grupos.push(document.getElementById("grupo-despesas"));
    grupos.push(document.getElementById("grupo-receitas"));
    grupos.push(document.getElementById("grupo-convenios"));
    grupos.push(document.getElementById("grupo-sancoes"));
    grupos.push(document.getElementById("grupo-servidores"));
	grupos.push(document.getElementById("grupo-mais"));

    for (i = 0; i < opcoes.length; i++) {
      if (opcoes[i].id == "opt-" + id) {
        opcoes[i].className = "selecionado sem-borda-" + id;
        grupos[i].style.display = "block";
      }
      else {
        opcoes[i].className = "borda-" + id;
        grupos[i].style.display = "none";
      }
    }
  }
</script>

