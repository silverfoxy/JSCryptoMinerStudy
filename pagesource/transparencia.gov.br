					TEM: "tem"
				},
				COMBO_CARTAOPGF_TIPOS: {
					POR_ORGAO: "por Órgão",
					POR_PORTADOR: "por Portador"
				},
				ANO_MINIMO_BOLSA_FAMILIA: 2004,
				ANO_MINIMO_DIARIAS_PAGAS: 2004
			};
		
			function getAnoExercicioSelecionado() {
				return $(CT.SELECTORS.COMBO_EXERCICIO).val();
			}
			function isAnoExercicioSelecionado() {
				return getAnoExercicioSelecionado() !== "(Selecione)";
			}
			
			function abrirBolsaFamilia() {
				if (!isAnoExercicioSelecionado()) {
					exibirMensagemSelecioneExercicio();
					return;
				}
				var codAcao = '8442';
				var anoSelecionado = getAnoExercicioSelecionado();
				if (anoSelecionado === '2007' || anoSelecionado === '2006' || anoSelecionado === '2005' || anoSelecionado === '2004') {
					codAcao = '006O';
				}
				redirecionarParaPagina('PortalTransparenciaPesquisaAcaoUF.asp?codigoAcao=' + codAcao + 
										'&codigoFuncao=08' + 
										'&NomeAcao=Transfer%EAncia+de+Renda+Diretamente+%E0s+Fam%EDlias+em+Condi%E7%E3o+de+Pobreza+e+Extrema+Pobreza+%28Lei+n%BA+10%2E836%2C+de+2004%29'+
										'&Exercicio=' + anoSelecionado);
			}
			function abrirCartaoDePagamentoGF() {
				if (!isAnoExercicioSelecionado()) {
					exibirMensagemSelecioneExercicio();
					return;
				}
				
				var tipoCartaoDePagamentoSelecionado = $(CT.SELECTORS.COMBO_CARTAOPGF).val();
				switch (tipoCartaoDePagamentoSelecionado) {
					case CT.COMBO_CARTAOPGF_TIPOS.POR_ORGAO:
						redirecionarParaPagina('PortalCartaoOrgaoSuperior.asp?Ano=' + getAnoExercicioSelecionado());
						break;
					case CT.COMBO_CARTAOPGF_TIPOS.POR_PORTADOR:
						redirecionarParaPagina('PortalCartaoCorporativoPortador.asp?Ano=' + getAnoExercicioSelecionado());
						break;
					default: // "(Selecione)"
						exibirMensagemDeErro('- Selecione um tipo de consulta de cartão para consultar');
				}
			}
			function abrirDiariasPagas() {
				if (!isAnoExercicioSelecionado()) {
					exibirMensagemSelecioneExercicio();
					return;
				}
				redirecionarParaPagina('PortalComprasDiretasFavorecidosDiarias.asp?Ano=' + getAnoExercicioSelecionado());
			}
			function abrirTem() {
				
				redirecionarParaPagina('http://br.transparencia.gov.br/tem/');
			}

			function abrirConsultaTematicaSelecionada() {
				var consultaTematicaSelecionada = $(CT.SELECTORS.RADIO_SELECIONADA).val();
				switch (consultaTematicaSelecionada) {
					case CT.RADIOS.BOLSAFAMILIA: abrirBolsaFamilia(); break;
					case CT.RADIOS.DIARIASPAGAS: abrirDiariasPagas(); break;
					case CT.RADIOS.CARTAOPGF: abrirCartaoDePagamentoGF(); break;
					case CT.RADIOS.TEM: abrirTem(); break;
					default:
						exibirMensagemDeErro('- Selecione a Consulta Temática que deseja realizar');
				}
			}
			
			function atualizarComboExercicioConsultasTematicas() {
				$(CT.SELECTORS.COMBO_EXERCICIO).show();
				$(CT.SELECTORS.COMBO_EXERCICIO_LABEL).show();
				
				var consultaTematicaSelecionada = $(CT.SELECTORS.RADIO_SELECIONADA).val();
				switch (consultaTematicaSelecionada) {
					case CT.RADIOS.BOLSAFAMILIA:
						atualizarSelectAnosExercicio(CT.SELECTORS.COMBO_EXERCICIO, CT.ANO_MINIMO_BOLSA_FAMILIA);
						break;
					case CT.RADIOS.DIARIASPAGAS:
						atualizarSelectAnosExercicio(CT.SELECTORS.COMBO_EXERCICIO, CT.ANO_MINIMO_DIARIAS_PAGAS);
						break;
					case CT.RADIOS.CARTAOPGF:
						atualizarSelectAnosExercicio(CT.SELECTORS.COMBO_EXERCICIO);
						break;
					case CT.RADIOS.TEM:
						$(CT.SELECTORS.COMBO_EXERCICIO).hide();
						$(CT.SELECTORS.COMBO_EXERCICIO_LABEL).hide();
						break;
					default:
						atualizarSelectAnosExercicio(CT.SELECTORS.COMBO_EXERCICIO);
				}
			}
			
			$(function () {
				atualizarComboExercicioConsultasTematicas();
				
				$(CT.SELECTORS.TODAS_RADIOS).click(function () {
					atualizarComboExercicioConsultasTematicas();
				});
				
				$(CT.SELECTORS.COMBO_CARTAOPGF).change(function () {
					// Qualquer alteracao no combo marca, automaticamente, como selecionada a radio de "Cartão de Pagamento do Governo Federal"
					$(CT.SELECTORS.RADIO_CARTAOPGF).prop("checked", true);
					atualizarComboExercicioConsultasTematicas();
				});
				
				$(CT.SELECTORS.BOTAO_CONSULTAR).click(function ()  { 
					abrirConsultaTematicaSelecionada();
				});
			});
		
		}());
		
		(function () {
