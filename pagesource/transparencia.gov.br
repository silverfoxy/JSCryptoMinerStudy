<script type="text/javascript">
	(function () { // INICIO: IIFE/JavaScript Consultas Despesas
		"use strict";
		
		
		function atualizarSelectAnosExercicio(selector, anoMinimo, anoMaximo) {
			$(selector).empty().append($("<option>(Selecione)</option>"));
			var selectAnoExercicio = $(selector);
			
			var anosExercicioPortal = [2018,2017,2016,2015,2014,2013,2012,2011,2010,2009,2008,2007,2006,2005,2004,2003,2002];
			$.each(anosExercicioPortal, function (i, ano) {
				if (typeof anoMinimo !== "undefined" && ano < anoMinimo) { return; } // somente a partir de anoMinimo
				if (typeof anoMaximo !== "undefined" && ano > anoMaximo) { return; } // somente ate de anoMaximo
				selectAnoExercicio.append($('<option>', { value: ano, text: ano }));
			});
		}
		
		function redirecionarParaPagina(url) {
			window.location.href = url;
		}
		
		function exibirMensagemDeErro(mensagem) {
			alert('Atenção:\n' + mensagem);
		}
		function exibirMensagemSelecioneExercicio() {
			exibirMensagemDeErro('- Selecione um exercício para consultar');
		}
		
		
		(function () {
			var TR = {
				SELECTORS: {
					BOTAO_CONSULTAR: "#botaoConsultarTransferenciasDeRecursos",
					COMBO_EXERCICIO: "#comboAnoExercicioTransferenciasDeRecursos",
					RADIO_SELECIONADA: "input:radio[name=consultasTransferenciasDeRecursos]:checked",
					COMBO_TIPOFAVORECIDO: "#comboTRTipoFavorecido",
					RADIO_PORFAVORECIDO: "#radioTRPorFavorecido"
				},
				RADIOS: {
					POR_ESTADO_MUNICIPIO: "porEstadoMunicipio",
					POR_PROGRAMA: "porPrograma",
					POR_ACAO: "porAcao",
					POR_FAVORECIDO: "porFavorecido"
				},
				COMBO_TIPOFAVORECIDO_TIPOS: {
					POR_ENTES_GOVERNAMENTAIS: "Entes governamentais",
					POR_ENTIDADES_SEM_FINS_LUCRATIVOS: "Entidades sem fins lucrativos",
					POR_DEMAIS_PJ: "Demais pessoas jurídicas",
					POR_PF: "Pessoas físicas",
					POR_PJ_POR_ATIVIDADE_ECONOMICA: "Pessoas jurídicas por atividade econômica"
				},
				TR_ANO_MINIMO: 2004
			};
			
			function getAnoExercicioSelecionado() {
				return $(TR.SELECTORS.COMBO_EXERCICIO).val();
			}
			function isAnoExercicioSelecionado() {
				return getAnoExercicioSelecionado() !== "(Selecione)";
			}
			
			function abrirConsultaTRPorEstadoMunicipio() {
				redirecionarParaPagina('PortalTransparenciaListaUFs.asp?Exercicio=' + getAnoExercicioSelecionado());
			}
			function abrirConsultaTRPorPrograma() {
				redirecionarParaPagina('PortalTransparenciaTRProgramaPesquisaPrograma.asp?Exercicio=' + getAnoExercicioSelecionado());
			}
			function abrirConsultaTRPorAcao() {
				redirecionarParaPagina('PortalTransparenciaPesquisaAcao.asp?Exercicio=' + getAnoExercicioSelecionado());
			}
			function abrirConsultaTRPorFavorecido() {
				var tipoFavorecidoSelecionado = $(TR.SELECTORS.COMBO_TIPOFAVORECIDO).val();
				switch (tipoFavorecidoSelecionado) {
					case TR.COMBO_TIPOFAVORECIDO_TIPOS.POR_ENTES_GOVERNAMENTAIS:
						redirecionarParaPagina('PortalTransparenciaPesquisaFavorecido.asp?hidIdTipoFavorecido=2&hidNumCodigoTipoNaturezaJuridica=1&Exercicio=' + getAnoExercicioSelecionado());
						break;
					case TR.COMBO_TIPOFAVORECIDO_TIPOS.POR_ENTIDADES_SEM_FINS_LUCRATIVOS:
						redirecionarParaPagina('PortalTransparenciaPesquisaFavorecido.asp?hidIdTipoFavorecido=2&hidNumCodigoTipoNaturezaJuridica=3&Exercicio=' + getAnoExercicioSelecionado());
						break;
					case TR.COMBO_TIPOFAVORECIDO_TIPOS.POR_DEMAIS_PJ:
						redirecionarParaPagina('PortalTransparenciaPesquisaFavorecido.asp?hidIdTipoFavorecido=2&hidNumCodigoTipoNaturezaJuridica=2&Exercicio=' + getAnoExercicioSelecionado());
						break;
					case TR.COMBO_TIPOFAVORECIDO_TIPOS.POR_PF:
						redirecionarParaPagina('PortalTransparenciaPesquisaFavorecidoPF.asp?Exercicio=' + getAnoExercicioSelecionado());
						break;
					case TR.COMBO_TIPOFAVORECIDO_TIPOS.POR_PJ_POR_ATIVIDADE_ECONOMICA:
						redirecionarParaPagina('PortalTransparenciaAtividadeEconomica_1.asp?Exercicio=' + getAnoExercicioSelecionado());
						break;
					default: // "(Selecione)"
						exibirMensagemDeErro('- Selecione um tipo de Favorecido para consultar');
				}
			}
			
			function abrirConsultaTRSelecionada() {
				// como todos TR exigem ano exercicio, verificamos logo aqui
				if (!isAnoExercicioSelecionado()) {
					exibirMensagemSelecioneExercicio();
					return;
				}
				var consultaTRSelecionada = $(TR.SELECTORS.RADIO_SELECIONADA).val();
				switch (consultaTRSelecionada) {
					case TR.RADIOS.POR_ESTADO_MUNICIPIO: abrirConsultaTRPorEstadoMunicipio(); break;
					case TR.RADIOS.POR_PROGRAMA: abrirConsultaTRPorPrograma(); break;
					case TR.RADIOS.POR_ACAO: abrirConsultaTRPorAcao(); break;
					case TR.RADIOS.POR_FAVORECIDO: abrirConsultaTRPorFavorecido(); break;
					default:
						exibirMensagemDeErro('- Selecione a consulta de Transferências de Recursos que deseja realizar');
				}
			}
			
			$(function () {
				atualizarSelectAnosExercicio(TR.SELECTORS.COMBO_EXERCICIO, TR.TR_ANO_MINIMO);
				
				$(TR.SELECTORS.BOTAO_CONSULTAR).click(function () {
					abrirConsultaTRSelecionada();
				});
				
				// seleciona automaticamente o radio de favorecido caso o valor na combo seja alterado
				$(TR.SELECTORS.COMBO_TIPOFAVORECIDO).change(function () {
					$(TR.SELECTORS.RADIO_PORFAVORECIDO).prop("checked", true);
				});
			});
			
		}());
		
		(function () {
			var GD = {
				SELECTORS: {
					BOTAO_CONSULTAR: "#botaoConsultarGastosDiretos",
					COMBO_EXERCICIO: "#comboAnoExercicioGastosDiretos",
					RADIO_SELECIONADA: "input:radio[name=consultasGastosDiretos]:checked",
					COMBO_TIPOFAVORECIDO: "#comboGDTipoFavorecido",
					RADIO_PORFAVORECIDO: "#radioGDPorFavorecido"
				},
				RADIOS: {
					POR_TIPO_DE_DESPESA: "porTipoDeDespesa",
					POR_ORGAO_EXECUTOR: "porOrgaoExecutor",
					POR_PROGRAMA: "porPrograma",
					POR_ACAO: "porAcao",
					POR_FAVORECIDO: "porFavorecido"
				},
				COMBO_TIPOFAVORECIDO_TIPOS: {
					POR_ENTES_GOVERNAMENTAIS: "Entes governamentais",
					POR_ENTIDADES_SEM_FINS_LUCRATIVOS: "Entidades sem fins lucrativos",
					POR_PF_EMPRESAS_E_OUTROS: "Pessoas físicas, empresas e outros",
					POR_PJ_POR_ATIVIDADE_ECONOMICA: "Pessoas jurídicas por atividade econômica"
				},
				GD_ANO_MINIMO: 2004
			};

			function getAnoExercicioSelecionado() {
				return $(GD.SELECTORS.COMBO_EXERCICIO).val();
			}
			function isAnoExercicioSelecionado() {
				return getAnoExercicioSelecionado() !== "(Selecione)";
			}
			
			function abrirConsultaGDPorTipoDeDespesa() {
				redirecionarParaPagina('PortalComprasDiretasEDDespesas.asp?Ano=' + getAnoExercicioSelecionado());
			}
			function abrirConsultaGDPorOrgaoExecutor() {
				redirecionarParaPagina('PortalComprasDiretasOEOrgaoSuperior.asp?Ano=' + getAnoExercicioSelecionado());
			}
			function abrirConsultaGDPorPrograma() {
				redirecionarParaPagina('PortalTransparenciaGDProgramaPesquisaPrograma.asp?Desastre=0&Ano=' + getAnoExercicioSelecionado());
			}
			function abrirConsultaGDPorAcao() {
				redirecionarParaPagina('PortalComprasDiretasPesquisaAcao.asp?Ano=' + getAnoExercicioSelecionado());
			}
			function abrirConsultaGDPorFavorecido() {
				var tipoFavorecidoSelecionado = $(GD.SELECTORS.COMBO_TIPOFAVORECIDO).val();
				switch (tipoFavorecidoSelecionado) {
					case GD.COMBO_TIPOFAVORECIDO_TIPOS.POR_ENTES_GOVERNAMENTAIS:
						redirecionarParaPagina('PortalComprasDiretasFavorecido.asp?TipoPesquisa=1&Ano=' + getAnoExercicioSelecionado());
						break;
					case GD.COMBO_TIPOFAVORECIDO_TIPOS.POR_ENTIDADES_SEM_FINS_LUCRATIVOS:
						redirecionarParaPagina('PortalComprasDiretasFavorecido.asp?TipoPesquisa=3&Ano=' + getAnoExercicioSelecionado());
						break;
					case GD.COMBO_TIPOFAVORECIDO_TIPOS.POR_PF_EMPRESAS_E_OUTROS:
						redirecionarParaPagina('PortalComprasDiretasFavorecido.asp?TipoPesquisa=2&Ano=' + getAnoExercicioSelecionado());
						break;
					case GD.COMBO_TIPOFAVORECIDO_TIPOS.POR_PJ_POR_ATIVIDADE_ECONOMICA:
						redirecionarParaPagina('PortalComprasDiretasAtividadeEconomica.asp?Ano=' + getAnoExercicioSelecionado());
						break;
					default: // "(Selecione)"
						exibirMensagemDeErro('- Selecione um tipo de Favorecido para consultar');
				}
			}
			
			function abrirConsultaGDSelecionada() {
				// como todos GD exigem ano exercicio, verificamos logo aqui
				if (!isAnoExercicioSelecionado()) {
					exibirMensagemSelecioneExercicio();
					return;
				}
				var consultaGDSelecionada = $(GD.SELECTORS.RADIO_SELECIONADA).val();
				switch (consultaGDSelecionada) {
					case GD.RADIOS.POR_TIPO_DE_DESPESA: abrirConsultaGDPorTipoDeDespesa(); break;
					case GD.RADIOS.POR_ORGAO_EXECUTOR: abrirConsultaGDPorOrgaoExecutor(); break;
					case GD.RADIOS.POR_PROGRAMA: abrirConsultaGDPorPrograma(); break;
					case GD.RADIOS.POR_ACAO: abrirConsultaGDPorAcao(); break;
					case GD.RADIOS.POR_FAVORECIDO: abrirConsultaGDPorFavorecido(); break;
					default:
						exibirMensagemDeErro('- Selecione a consulta de Gastos Diretos que deseja realizar');
				}
			}
			
			$(function () {
				atualizarSelectAnosExercicio(GD.SELECTORS.COMBO_EXERCICIO, GD.GD_ANO_MINIMO);
				
				$(GD.SELECTORS.BOTAO_CONSULTAR).click(function () {
					abrirConsultaGDSelecionada();
				});

				// seleciona automaticamente o radio de favorecido caso o valor na combo seja alterado
				$(GD.SELECTORS.COMBO_TIPOFAVORECIDO).change(function () {
					$(GD.SELECTORS.RADIO_PORFAVORECIDO).prop("checked", true);
				});
			});
			
		}());
		
