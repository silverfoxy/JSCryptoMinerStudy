<script id="HitTemplate" type="text/template">
  {{#hits}}
    <div itemscope itemtype="http://schema.org/Product" class="grid__item large--one-fifth medium--one-quarter small--one-half collection-product product-hit" id="hit-{{ handle }}">

      <div class="img_wrapper" style="">
       <a class="image cover" href="/products/{{ handle }}">
         <img itemprop="image" src="{{ image_url }}{{^image_url}}//cdn.shopify.com/s/files/1/0318/2649/t/9/assets/imageless.jpg?36014{{/image_url}}" alt="{{ name_en }}" onload="imgLoaded(this)" />
        </a>
      </div>

      <div class="grid__item one-half price-caption new-price-caption {{left_caption_class}} no-font-smoothing">
        Brand New
      </div>
      <div class="grid__item one-half right price-caption used-price-caption {{right_caption_class}} no-font-smoothing">
        Pre Owned
      </div>

      <div class="grid__item one-half-spaced new-collection-price {{left_field_class}} no-font-smoothing">
        <span class="hit-new-price"></span>
      </div>
      <div class="grid__item one-half-spaced right used-collection-price {{right_field_class}} no-font-smoothing">
        <span class="hit-used-price"></span>
      </div>

      <span style="display:none" class="handle">{{ handle }}</span>
      <p class="h6">
        <a href="/products/{{ handle }}">{{ name_en }}</a>
      </p>

    </div>
  {{/hits}}
</script>



