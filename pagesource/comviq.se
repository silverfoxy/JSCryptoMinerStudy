    <script type="text/ng-template" id="instantSearchModal">
    <section class="wrapper wrapper--canvas">
        <form ng-submit="instantSearchModalCtrl.redirectToSearch($event)"
                class="instant-search-modal__form"
                autocomplete="off"
                action="#">
            <div class="input-group input-group--search">
                <input type="search" 
                       class="input input--search"
                       id="js-instant-search-modal-input"
                       placeholder="Sök på Comviq.se"
                       ng-model="instantSearchModalCtrl.instantSearchQuery"
                       ng-change="instantSearchModalCtrl.instantSearch()"
                       ng-model-options="{ debounce: instantSearchModalCtrl.debounce }"
                       autofocus>
                <span class="input-group__icon"></span>
                <a href="" 
                   class="input-group__clear" 
                   ng-if="instantSearchModalCtrl.instantSearchQuery" 
                   ng-click="instantSearchModalCtrl.clearQuery()"></a>
                <div class="spinner input-group__spinner" 
                     ng-if="instantSearchModalCtrl.loading">
                    <div class="spinner__spin"></div>
                </div>
            </div>
        </form>

        <div class="instant-search-modal__results-container" 
             ng-if="instantSearchModalCtrl.results.total" 
             ng-cloak>
            <div class="category category--canvas-dark"
                 ng-if="instantSearchModalCtrl.results && instantSearchModalCtrl.results.productResult.length">
                Produkter
                <a ng-href="{{'/sok?searchinput=' + instantSearchModalCtrl.instantSearchQuery + '&tab=comviqproduct'}}"
                   href="" 
                   target="_self" 
                   class="u-pull-right">Fler</a>
            </div>
            <div ng-repeat="result in instantSearchModalCtrl.results.promotedProductResult | limitTo:3 track by result.id">
                <cq-search-result result="result" 
                                  is-compact="true"
                                  classes="search-result--canvas"
                                  query="instantSearchModalCtrl.instantSearchQuery"
                                  nr-of-results="instantSearchModalCtrl.results.total"></cq-search-result>
            </div>
            <div ng-repeat="result in instantSearchModalCtrl.results.productResult | limitTo:resultsLeft.product track by result.id">
                <cq-search-result result="result" 
                                  is-compact="true"
                                  classes="search-result--canvas"
                                  query="instantSearchModalCtrl.instantSearchQuery"
                                  nr-of-results="instantSearchModalCtrl.results.total"></cq-search-result>
            </div>
            <div class="category category--canvas-dark"
                 ng-if="instantSearchModalCtrl.results && instantSearchModalCtrl.results.helpResult.length">
                Hjälpartiklar
                <a ng-href="{{'/sok?searchinput=' + instantSearchModalCtrl.instantSearchQuery + '&tab=comviqhelp'}}"
                    href="" target="_self" class="u-pull-right">Fler</a>
            </div>
            <div ng-repeat="result in instantSearchModalCtrl.results.promotedHelpResult | limitTo:3 track by result.id">
                <cq-search-result result="result"
                                  is-compact="true"
                                  classes="search-result--canvas"
                                  query="instantSearchModalCtrl.instantSearchQuery"
                                  nr-of-results="instantSearchModalCtrl.results.total"></cq-search-result>
            </div>
            <div ng-repeat="result in instantSearchModalCtrl.results.helpResult | limitTo:resultsLeft.help track by result.id">
                <cq-search-result result="result"
                                  is-compact="true"
                                  classes="search-result--canvas"
                                  query="instantSearchModalCtrl.instantSearchQuery"
                                  nr-of-results="instantSearchModalCtrl.results.total"></cq-search-result>
            </div>
            <div class="category category--canvas-dark"
                 ng-if="instantSearchModalCtrl.results && instantSearchModalCtrl.results.webResult.length">
                Övrigt
                <a ng-href="{{'/sok?searchinput=' + instantSearchModalCtrl.instantSearchQuery + '&tab=comviqweb'}}"
                    href="" 
                    target="_self" 
                    class="u-pull-right">Fler</a>
            </div>
            <div ng-repeat="result in instantSearchModalCtrl.results.promotedWebResult | limitTo:3 track by result.id">
                <cq-search-result result="result"
                                  is-compact="true"
                                  classes="search-result--canvas"
                                  query="instantSearchModalCtrl.instantSearchQuery"
                                  nr-of-results="instantSearchModalCtrl.results.total"></cq-search-result>
            </div>
            <div ng-repeat="result in instantSearchModalCtrl.results.webResult | limitTo:resultsLeft.web track by result.id">
                <cq-search-result result="result"
                                  is-compact="true"
                                  classes="search-result--canvas"
                                  query="instantSearchModalCtrl.instantSearchQuery"
                                  nr-of-results="instantSearchModalCtrl.results.total"></cq-search-result>
            </div>
        </div>
    </section>
</script>


