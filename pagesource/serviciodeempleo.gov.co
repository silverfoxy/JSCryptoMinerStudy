        <script>
          $(document).ready(function() {         
          var total = 0;
          var url = "http://buscadordeempleo.gov.co/home/getvacantesVigentes?callback=?"
        
          $.getJSON(url, function(data) {
        
              console.log(data);
              total = data.cantidadvacantes;
              console.log(total);
              var clock;
              total -= 222;
              var arreglo = total.toString().split("");
              var num = [0, 0, 0, total];
               
              for (n=2; n>=0; n--){
                  var arreglostr = "";
                  for(digito=0; digito < arreglo.length; digito++){
                      if(arreglo[digito]>0){
                          if(arreglo[digito]!=1)
                              arreglo[digito]=arreglo[digito]-1;
                      }
                      else{
                          arreglo[digito]=9;
                      }
                      arreglostr += arreglo[digito]+"";
                  }
                  num[n]=parseInt(arreglostr)
              }
               
              var i = 0;
              setTimeout(function () {
                  setInterval(function () {
                      
                      if (i<4) {
                          $(document).ready(function () {
                              clock = new FlipClock($('.clock'), num[i], {
                                  clockFace: 'Counter',
                              });    
                          });      
                      }
                      else if(i<6){
                          for(y=0; y<100;y++)
                              clock.increment();
                      }
                      else if(i<8){
                          for(y=0; y<10;y++)
                              clock.increment();
                      }
                      else if(i<10){
                              clock.increment();
                      }
                          i++; 
                      }, 400);
                  }
              );
              
          }).success(function() {    
                    //alert("Successfully retrieved data.");
          }).error(function() {
                    //alert("Service Unavailable.");
          }); 
          });
        </script>
   
