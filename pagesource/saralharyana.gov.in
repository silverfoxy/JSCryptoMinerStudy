		 function MD5Authnew(){
			var seed = -1184070423;
			var seedUI=-2060313683;
			
			var pwd = document.loginform.password.value;
			var usrName=document.loginform.userName.value;			
			
			if(seed!=0 && seedUI!=0){				
				
				var seed1 = MD5(MD5(pwd)+seed);
				var seedUIMD5 = MD5(usrName+seedUI);
				
				document.loginform.loginnameEncrpt.value=usrName;
				document.loginform.randomNo.value = seed1;
				document.loginform.loginname.value=usrName;
				document.loginform.loginNameHidden.value=seedUIMD5;					
			}
			document.loginform.userName.value="";
			document.loginform.password.value = "";
			
			document.loginform.method = "post";
			document.loginform.action = "login.do?OWASP_CSRFTOKEN=GWSZ-07GB-E50A-N66R-OIF5-T6V8-E33F-M20L" ;
			document.loginform.submit();
			
			//return true;
		}
		
		function getPopupWindow(urlText){
	 
		 var url = urlText.split('~')[1];
		 var titleText = urlText.split('~')[0];
		 var urlStr = url+"?OWASP_CSRFTOKEN=GWSZ-07GB-E50A-N66R-OIF5-T6V8-E33F-M20L";
		 var width  = 960;
		 var height = 600;
		 if(url.search("forgotPassword") != -1){
			width  = 800;
			height = 400;
			}
		 var left = (window.screen.width / 2) - ((width / 2) + 10);
		 var top = (window.screen.height / 2) - ((height / 2) + 50);
	 
		  window.open(urlStr,titleText,
					"status=no,height=" + height + ",width=" + width + ",resizable=yes,left="
					+ left + ",top=" + top + ",screenX=" + left + ",screenY="
					+ top + ",toolbar=no,menubar=no,scrollbars=yes,location=no,directories=no");
	   
		 
	 }
	 
