                    schools += '<h1 class="special-school">Special Services</h1><ul class="special-school border-box"></ul></div>';
                    
                    $('.sw-channel-item.mega-schools .sw-channel-dropdown .mega-columns').prepend(schools);
					
                    //ADD IN THE SCHOOLS
                    for(var i in schoolList) {
                        if(schoolList[i].schoolVisible.toLowerCase() == 'true')  {
                        	
                        	if(schoolList[i].schoolType.toLowerCase() == 'high') {
                        		highSchools += '<li><a href=\'' + schoolList[i].schoolUrl + '\' tabindex=\'0\'>' + schoolList[i].schoolName + '</a></li>';
                        	}
                            if(schoolList[i].schoolType.toLowerCase() == 'middle') {
                        		middleSchools += '<li><a href=\'' + schoolList[i].schoolUrl + '\' tabindex=\'0\'>' + schoolList[i].schoolName + '</a></li>'
                        	}
                            if(schoolList[i].schoolType.toLowerCase() == 'elementary') {
                        		elemSchools += '<li><a href=\'' + schoolList[i].schoolUrl + '\' tabindex=\'0\'>' + schoolList[i].schoolName + '</a></li>'
                        	}
                            if(schoolList[i].schoolType.toLowerCase() == 'alternative') {
                        		altSchools += '<li><a href=\'' + schoolList[i].schoolUrl + '\' tabindex=\'0\'>' + schoolList[i].schoolName + '</a></li>'
                        	}
                            if(schoolList[i].schoolType.toLowerCase() == 'special') {
                        		specialSchools += '<li><a href=\'' + schoolList[i].schoolUrl + '\' tabindex=\'0\'>' + schoolList[i].schoolName + '</a></li>'
                        	}
                            if(schoolList[i].schoolType.toLowerCase() == 'k-8') {
                        		k8Schools += '<li><a href=\'' + schoolList[i].schoolUrl + '\' tabindex=\'0\'>' + schoolList[i].schoolName + '</a></li>'
                        	}
                        }
                        
                    }

                    $('.sw-channel-item.mega-schools .sw-channel-dropdown .mega-columns .mega-column.two ul.high-school').prepend(highSchools);
                    $('.sw-channel-item.mega-schools .sw-channel-dropdown .mega-columns .mega-column.two ul.middle-school').prepend(middleSchools);
                    $('.sw-channel-item.mega-schools .sw-channel-dropdown .mega-columns .mega-column.three ul.elem-school').prepend(elemSchools);
                    $('.sw-channel-item.mega-schools .sw-channel-dropdown .mega-columns .mega-column.four ul.alt-school').prepend(altSchools);
                    $('.sw-channel-item.mega-schools .sw-channel-dropdown .mega-columns .mega-column.four ul.special-school').prepend(specialSchools);
                    $('.sw-channel-item.mega-schools .sw-channel-dropdown .mega-columns .mega-column.four ul.k-8-school').prepend(k8Schools);
                    
                    // ADD IMAGE
                    var imgColumn = '';
                    var imgSrc = jQuery.trim('/cms/lib/MI01808718/Centricity/Template/GlobalAssets/images///Channel Images/schoolsdropdown.png');
                    var srcSplit = imgSrc.split("/");
                    var srcSplitLen = srcSplit.length;
