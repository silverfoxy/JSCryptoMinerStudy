	function eventsByDay(container) {
		//MOVE ALL EVENTS WITH THE SAME DATES INTO THE SAME CONTAINER
		var countLi = 0;
		$(".ui-article", container).each(function(){
			if (!$(this).find("h1.ui-article-title.sw-calendar-block-date").size()){
				var moveArticle = $(this).html();
				$(this).parent().prev().children().children().next().append(moveArticle);
				$(this).parent().remove();
				
			};
		});
		
		//ADD CLASSES TO EACH UPCOMING EVENT LI IF LIMITING THEM - REMOVE IF NOT NECESSARY
		if($(".hp").length) {
        
        	//LIMIT EVENTS ON HOMEPAGE - REMOVE IF NOT NEEDED
		$("#hp-region-two .upcomingevents li:gt(4)").remove();
			
			// ADD CLASSES TO ARTICLES
			$("#hp-region-two .upcomingevents .ui-articles").wrap("<div class='ui-article-wrapper'></div>");
			$("#hp-region-two .upcomingevents li").first().addClass("first");
			$("#hp-region-two .upcomingevents li").last().addClass("last");
			var eventClass = ["one", "two", "three", "four", "five"];
			$("#hp-region-two .upcomingevents li").each(function() {
				var thisIndex = $(this).index();
				$(this).addClass("event-" + eventClass[thisIndex]);
			});
			
		}
        
        $('.ui-widget.app.upcomingevents .ui-widget-detail .ui-articles').each(function(){
				$(this).addClass("ui-clear");
			});
            
            
        
        /*
        This should be placed within the eventsByDay function right after that functions loop.
      */

      $(".ui-article", container).each(function(){
			var newDateTime = $('.upcoming-column.left h1', this).html().toLowerCase().split("</span>");
			var newDate = '';
			var dateIndex = ((newDateTime.length > 2) ? 2 : 1); //if the dayWeek is set to yes we need to account for that in our array indexing
			
			//add a zero to the date if it is a single digit
			if( newDateTime[dateIndex].length == 1 ) newDateTime[dateIndex] = "0" + newDateTime[dateIndex];
			
			//if we have the day of the week make sure to include it.
			if( dateIndex > 1 ){ 
				newDate += newDateTime[0] + "</span>";
			}
			
			//add in the month
			newDate += newDateTime[dateIndex - 1] + '</span>'; //the month is always the in the left array position to the date
			
			//wrap the date in a new tag
			newDate += '<span class="jeremy-date">' + newDateTime[dateIndex] + '</span>';
			
			//append the date and month back into their columns
			$('.upcoming-column.left h1', this).html(newDate);	
			
            $('div.upcoming-column.left h1.ui-article-title', this).each(function(){
                $(" span.joel-month", this).next('.jeremy-date').andSelf().wrapAll('<div class="date-outer"/>');
            });
		});
        
        $(".ui-widget.app.upcomingevents li .ui-article-description").each(function() {
          		// CHECK FOR TIME
            if($(".sw-calendar-block-time", this).length) {
            	$(this).append($(".sw-calendar-block-time", this));
            } else {
            	$(this).append('<span class="sw-calendar-block-time">ALL DAY</span>');
            }
          });
        
	}
	
