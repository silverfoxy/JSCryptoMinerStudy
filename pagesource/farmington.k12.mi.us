                    $(".sw-channel-item.mega-schools .sw-channel-dropdown").removeAttr("class").wrap('<ul class="sw-channel-dropdown schools outer ui-clear border-box"><li class="mega-columns ui-clear"></li></ul>');
                    
                    var schools = '', elemSchools = '', middleSchools = '', highSchools = '', altSchools = '', specialSchools = '', k8Schools = '';
                    
                    //BUILD THE STRUCTURE
                    schools += '<div class="mega-column two ui-clear border-box">'
                    schools += '<h1 class="high-school">High Schools</h1><ul class="high-school border-box"></ul>';
                    schools += '<h1 class="middle-school">Middle Schools</h1><ul class="middle-school border-box"></ul></div>';
                    schools += '<div class="mega-column three ui-clear border-box">'
