	function appView(){
		if(!appViewSet) {
			//DOC make homepage apps collapsible for small mobile devices
			if(!$(".hp-content.top div.ui-widget.app.active").length && !$(".hp-content.bottom div.ui-widget.app.active").length){
				$(".hp.column.two div.ui-widget.app:first").addClass("active");
			}
			
			//DOC dont add app into accordion if it doesnt have a header
			$(".hp-content.top div.ui-widget.app, .hp-content.bottom div.ui-widget.app").each(function() {
				if($(".ui-widget-header", this).hasClass("ui-helper-hidden") || jQuery.trim($(".ui-widget-header h1", this).text()) == "") {
					$(this).addClass("no-header");
				}
			});
			
			//DOC accordion header click
			$(".hp-content.top div.ui-widget-header, .hp-content.bottom div.ui-widget-header").click(function(){
				if($(this).parent().hasClass("active")) {
					$(this).parent().removeClass("active");
				} else {
					var clickedApp = $(this).parent().parent().attr("id");
					
					$(".hp-content.top div.ui-widget.app, .hp-content.bottom div.ui-widget.app").removeClass("active");
					$(this).parent().addClass("active");
					
					$("html, body").animate({
						scrollTop: $("#" + clickedApp).offset().top
					}, 500, function(){
						$("html, body").stop(true, false);
					});
				}
			});
			
			// OPEN PAGE NAVIGATION
			$(".sp.column.one div.ui-widget.app").addClass("active");
			
			// SUBPAGE NAVIGATION HEADER CLICK
			$(".sp.column.one div.ui-widget-header").click(function(){
				if($(this).parent().hasClass("active")) {
					$(this).parent().removeClass("active")
				} else {
					$(this).parent().addClass("active");
				}
			});
		
			appViewSet = 1;
		}
	}
	
	function appViewReset() {
		if(appViewSet) {
			$(".hp-content.top div.ui-widget-header, .hp-content.bottom div.ui-widget-header").unbind("click");
			$(".sp.column.one div.ui-widget-header").unbind("click");
			
			appViewSet = 0;
		}
	}	
	
