	function hexToRgb(h){
	        var r = parseInt((cutHex(h)).substring(0,2),16);
	        var g = parseInt((cutHex(h)).substring(2,4),16);
	        var b = parseInt((cutHex(h)).substring(4,6),16);
	        return r + ', ' + g + ', ' + b;
	}
	 
	function cutHex(h) {
	        return (h.charAt(0)=="#") ? h.substring(1,7):h;
	}
	
	function stickyHeader() {
		switch(csGetBreakPoint()) {
			case "desktop":
				// SITE NAME
				var gbHeaderOffset = $(".gb-header.left").offset().top;
				if ($(window).scrollTop() <= gbHeaderOffset) {
					if(csHeaderIsSticky) {
						$("#gb-header-left-sticky").removeClass("sticky");
						$(".gb-header.left").css("height", "auto");
						
						csHeaderIsSticky = 0;
					}
				} else {
					$(".gb-header.left").css("height", $("#gb-header-left-inner").height() + "px");
					
					if(!csHeaderIsSticky) {
						$("#gb-header-left-sticky").addClass("sticky");
						
						csHeaderIsSticky = 1;
					}
				}
				
				// TABS AND CHANNEL BAR
				var gbNavOffSet = $("nav").offset().top - $("#gb-header-left-sticky").height();
				if ($(window).scrollTop() <= gbNavOffSet) {
					if(csNavIsSticky) {
						$("#gb-header-left-sticky").removeClass("tabs");
						$("#gb-channel-list-outer").removeClass("sticky");
						$("nav").css("height", "auto");
						$("#gb-channel-list-outer").css("top", "auto");
						
						csNavIsSticky = 0;
					}
				} else {
					$("nav").css("height", $("#gb-channel-list-outer").height() + "px");
					$("#gb-channel-list-outer").css("top", $("#gb-header-left-sticky").height() + "px");
					
					if(!csNavIsSticky) {
						$("#gb-header-left-sticky").addClass("tabs");
						$("#gb-channel-list-outer").addClass("sticky");
						
						csNavIsSticky = 1;
					}
				}
			break;
			case "768": case "640": case "480": case "320":
				if(csHeaderIsSticky == 1) {
					$("#gb-header-left-sticky").removeClass("sticky tabs");
					$(".gb-header-left").css("height", "auto");
					
					csHeaderIsSticky = 0;
				}
				if(csNavIsSticky == 1) {
					$("#gb-header-left-sticky").removeClass("tabs");
					$("#gb-channel-list-outer").removeClass("sticky");
					$("nav").css("height", "auto");
					$("#gb-channel-list-outer").css("top", "auto");
					
					csNavIsSticky = 0;
				}
			break;
		}
	}
	
	function buildSchoolList(csElemSchools, csMiddleSchools, csAdditionalSchools) {
		// BUILD ELEMENTARY SCHOOL LIST
		var fullSchoolList = '<div class="gb-school-list-section elem"><h2>Elementary Schools</h2><div class="ui-clear"><ul class="gb-school-list-column elem">';
		
		var n = 1;
		for(var i = 0; i < csElemSchools.length; i++) {
			fullSchoolList += '<li><a href="' + csElemSchools[i][1] + '">' + csElemSchools[i][0] + '</a></li>';
			
			n++;
			if(n == 8) {
				fullSchoolList += '</ul><ul class="gb-school-list-column elem">';
				
				n = 1;
			}
		}
		
		fullSchoolList += '</ul></div></div>';
		
		// BUILD MIDDLE SCHOOL LIST
		fullSchoolList += '<div class="gb-school-list-section middle"><h2>Middle Schools</h2><ul class="gb-school-list-column middle">';
		for(var m in csMiddleSchools) {
			fullSchoolList += '<li><a href="' + csMiddleSchools[m][1] + '">' + csMiddleSchools[m][0] + '</a></li>';
		}
		fullSchoolList += '</ul></div>';
		
		// BUILD ADDITIONAL SCHOOL LIST
		fullSchoolList += '<div class="gb-school-list-section additional"><h2>Additional Campuses</h2><ul class="gb-school-list-column additional">';
		for(var a in csAdditionalSchools) {
			fullSchoolList += '<li><a href="' + csAdditionalSchools[a][1] + '">' + csAdditionalSchools[a][0] + '</a></li>';
		}
		fullSchoolList += '</ul></div>';
		
		// ADD EVERYTHING TO THE DOM
		$("#gb-school-list").html(fullSchoolList);
		
		// BIND THE LIST CLICK EVENT
		$("#gb-schools-btn").click(function(e) {
			e.preventDefault();
			$("#gb-school-list-outer").slideToggle();
		});
	}
	
