<style type="text/css">

.wrap--hero {
  padding-left: 0;
  padding-right: 0;
}

.bg-dotted.bg-dotted--green:before {
  /*background-color: #1DCC7F;*/
  background-image: url("data:image/svg+xml,%3Csvg width='20' height='20' viewBox='0 0 20 20' xmlns='http://www.w3.org/2000/svg'%3E%3Cg fill='%23F3F3F3' fill-rule='evenodd'%3E%3Ccircle cx='3' cy='3' r='3'/%3E%3Ccircle cx='13' cy='13' r='3'/%3E%3C/g%3E%3C/svg%3E");
  left: 1px;
}

.card--hero {
  padding-bottom: 32px;
  padding-left: 8px;
}

.fm .card.card--hero .image {
  position: relative;
}

.fm .card.card--hero .image:before {
  background-color: #FFFFFF;
  content: '';
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 8px;
  z-index: 1;
}

.fm .card.card--hero .image img {
  left: -100%;
  max-width: none;
  width: 200%;
}


.fm .card.card--hero .image.hidden-small + h3 {
  font-size: 30px;
  line-height: 40px;
  margin-top: -64px;
  margin-right: 32px;
  padding: 16px 128px 0 8px;
}

.card--hero p {
  margin-bottom: 0;
  margin-top: 0;
  padding: 16px 16px 64px 8px;
}

.card--hero__link {
  background-color: #166D66;
  color: white;
  display: block;
  margin: -32px 64px 0 8px;
  padding: 16px;
  position: relative;
}

.card--hero__link:hover {
  border-color: transparent;
  color: white;
  text-decoration: none;
}

.card--hero__link span {
  float: right;
}

.card--hero__link span.icon svg {
  width: 20px;
}

@media (min-width: 460px) {
  .fm .card.card--hero .image.hidden-small + h3 { padding-right: 0px; }
}

@media (min-width: 750px) {
  .bg-dotted.bg-dotted--green { margin-bottom: 64px; }
  .bg-dotted.bg-dotted--green:before { bottom: 48px; left: 32px; }
  .card--hero { padding-left: 46px; padding-bottom: 64px; }
  .fm .card.card--hero .image:before { display: none; }
  .fm .card.card--hero .image img { left: 0px; width: 150%; }
  .fm .card.card--hero .image.hidden-small + h3 { font-size: 46px; line-height: 60px; }
  .card--hero p { padding-right: 128px; }
  .card--hero__link { width: 30%; }

}

@media (min-width: 960px) {
  .bg-dotted.bg-dotted--green { margin-bottom: 64px; }
  .bg-dotted.bg-dotted--green:before { display: none; }
  .card--hero { height: 600px; margin-left: 46px; overflow-y: hidden; padding: 64px 0 0; }
  .fm .card.card--hero .image { height: 100%; left: 0; padding-bottom: 0; position: absolute; top: 0; width: 100%; }
  .fm .card.card--hero .image.hidden-small + h3, .card--hero p { width: 470px; }
  .fm .card.card--hero .image.hidden-small + h3 { margin-top: 0; padding-bottom: 16px; padding-left: 16px; }
  .fm .card.card--hero p { padding-bottom: 64px; }
  .card--hero__link { margin-top: -32px; width: 300px; }
  .card--hero__link:hover { text-decoration: underline; }
  .fm .card.card--hero .image img { width: 135%; }
}

@media (min-width: 1300px) {
  .bg-dotted.bg-dotted--green:before { display: none; }
  .fm .card.card--hero .image { padding-bottom: 0; }
  .fm .card.card--hero .image img { width: 105%; }
  .fm .card.card--hero .image.hidden-small + h3 { padding-right: 128px; }
  .fm .card.card--hero .image.hidden-small + h3, .fm .card.card--hero p { padding-left: 32px; width: 50%; }
  .card--hero__link { margin-left: 32px; }
}

</style>

