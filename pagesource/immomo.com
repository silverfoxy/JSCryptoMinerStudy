(function() {
    $('.wall-pace').delegate('li', 'mouseenter', function() {
        var className = $(this)[0].className;

        if (className == "rect-x" || className.indexOf("sq-lg") > -1){
            var dlMove = className == "rect-x" ? "0" : "128px";

            $(this).find('dl').stop().animate({
                top: dlMove
            }, 200)
            $(this).find('img').stop().animate({
                top: "-5%"
            }, 200)
        }

    }).delegate('li', 'mouseleave', function() {
        var className = $(this)[0].className;
        if (className == "rect-x" || className.indexOf("sq-lg") > -1){
            $(this).find('dl').stop().animate({
                top: '100%'
            }, 200)
            $(this).find('img').stop().animate({
                top: '0'
            }, 200)
        }
    });
    showImages(IDS,OBJECT);
})();

function showImages(arr,obg){
    var url = 'http://s.momocdn.com/static/w5/img/website/photowall/'
        l = arr.length,
        imgId = [''],
        liArr = $('#photo-wall').find('li');
        
    liArr.each(function (index,c){
        var o = searchKind($(this).attr('class'));
        if (o.intr){
            setImgAndIntr(this,o.kind);
        } else {
            setImg(this,o.kind);
        }
        l--;
    });

    function removeElement(index){
        arr = arr.slice(0,index).concat(arr.slice(index+1,arr.length));
    }

    function getNum(){
        return Math.floor(Math.random()*(l-1)) + 1;
    }

    function searchKind(className){
        var o = {};
        if (className == 'sq-lg' || className == 'sq-lg wall-last-4'){
            o.kind = '1';
            o.intr = true;
        }else if (className == 'sq-sm'){
            o.kind = '1';
            o.intr = false;
        } else if (className == 'rect-x'){
            o.kind = '2';
            o.intr = true;
        } else if (className == 'rect-y' || className == 'rect-y wall-last-3'){
            o.kind = '3';
            o.intr = false;
        }
        return o;
    }

    function setImg(c,kind){
        var index = getNum(),
            path = url+kind+'/'+arr[index]+'.jpg';

        $(c).find('img').attr('src',path);
        removeElement(index);
    }

    function setImgAndIntr(c,kind){
        var index = getNum(),
            path = '',
            _html = '';
        if (OBJECT[arr[index]].n != '' && OBJECT[arr[index]].n != undefined){
            var o = OBJECT[arr[index]];
            path = url+kind+'/'+arr[index]+'.jpg';
            $(c).find('img').attr('src',path);
            _html = '<dt>'+o['n']+'</dt><dd>'+o['a']+'</dd><dd>城市：'+o['h']+'</dd><dd>宣言：'+o['w']+'</dd>';
            $(c).find('dl').html(_html);
            removeElement(index);
        } else {
            setImgAndIntr(c,kind);
        }
    }
}
