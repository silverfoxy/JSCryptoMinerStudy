        <script>
        var hasEmarsysCartData = false;
        require(['jquery', 'Magento_Customer/js/customer-data', 'Magento_Checkout/js/checkout-data'], function ($, customerData, checkoutData) {
            var cartData = customerData.get('cart');
            cartData.subscribe(function (updatedCart) {
                if (!hasEmarsysCartData) {
                    var cartItems = [];
                    $.each(updatedCart.items, function (idx, item) {
                        cartItems.push({
                            'item'    : item.product_sku,
                            'price'   : parseFloat(item.product_price_value_with_ew) * parseInt(item.qty),
                            'quantity': parseInt(item.qty)
                        });
                    });
                    // Passing on visitor's cart contents to feed cart abandonment campaigns
                    ScarabQueue.push(['cart', cartItems]);
                    console.info('emarsys: cart. Data: ' + JSON.stringify(cartItems));
                    var email = updatedCart.cart_email ? updatedCart.cart_email : checkoutData.getValidatedEmailValue();
                    if (email) {
                        ScarabQueue.push(['setEmail', email]);
                        console.info('emarsys: setEmail. Data: ' + email);
                    }
                    hasEmarsysCartData = true;
                }
            });
        });
    </script>
    <script>
        window.onerror = function(msg, url, line) {
            var key = "collected_errors";
            var errors = {};
            if (sessionStorage.getItem(key)) {
                errors = JSON.parse(sessionStorage.getItem(key));
            }
            if (!(window.location.href in errors)) {
                errors[window.location.href] = [];
            }
            errors[window.location.href].push("error: \'" + msg + "\' " + "file: " + url + " " + "line: " + line);
            sessionStorage.setItem(key, JSON.stringify(errors));
        };
    </script>
