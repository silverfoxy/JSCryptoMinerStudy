					<script>
						function compteur() {
							var divH = document.getElementById('nb_heures') ;
							var divM = document.getElementById('nb_minutes') ;
							var divS = document.getElementById('nb_secondes') ;
							var nb_heures = parseInt(divH.textContent) ;
							var nb_min = parseInt(divM.textContent) ;
							var nb_sec = parseInt(divS.textContent) ;
							nb_sec-- ;
							if (nb_sec == -1) {
								nb_sec = 59 ;
								nb_min-- ;
								if (nb_min == -1){
									nb_min = 59 ;
									nb_heures-- ;
								}
							}
							nb_heures = nb_heures.toString() ;
							if(nb_heures.length==1)nb_heures="0"+nb_heures ;
							nb_min = nb_min.toString() ;
							if(nb_min.length==1)nb_min="0"+nb_min ;
							nb_sec = nb_sec.toString() ;
							if(nb_sec.length==1)nb_sec="0"+nb_sec ;

							divH.textContent = nb_heures+"H" ;
							divM.textContent = nb_min+"M" ;
							divS.textContent = nb_sec+"S" ;	

							setTimeout("compteur();", 1000) ;	
						}
						compteur() ;
