//		}
//
//	}) ;


	//ajax pour le suivre ce jeu : 
	$('body').on('click','#btn_follow',function(e){
		e.preventDefault() ;
		var $this = $(this) ;
		var idArticle = $this.attr('rel') ;
		$.ajax({
			url : "../ajax_follow.php",
			dataType : "json",
			data : {id : idArticle},
			success : function(json) {
//				console.log(json) ;
				if (json.success == "OK"){
					if (json.type == "followed") {
						$this.text('Ne plus suivre ce jeu') ;
					} else {
						$this.text('Suivre ce jeu') ;
					}
					$('#account_followed').text(json.total_suivi) ;
				}
			}

		})
	})
})

</script>
