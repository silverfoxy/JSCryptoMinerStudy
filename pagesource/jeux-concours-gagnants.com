$(document).ready(function(){
	//definition des variables utiles 
	var screenW = $(window).width() ;
	var screenH = $(window).height() ;
	var bodyH = $('body').css('height') ;
	$(window).scrollTop(0) ;
		//script des popups :
/*	if ($('.voile_noir').css('display') != 'none') {
		$('body').css({
			'height' : screenH+'px',
			'overflow' : 'hidden'
		}) ;
		
	}*/
	$(window).on('scroll',function(){
		if($(window).scrollTop()> 350){
			$('#backToTop').show() ;
		} else {
			$('#backToTop').hide() ;
		}
	})

	$('#backToTop').on('click',function(e){
		e.preventDefault() ;
		$("html, body").stop().animate({scrollTop:0},400) ;
	})

	$('.btn_close').on('click',function(e){
		e.preventDefault() ;
		$(this).parents('.container_popup').fadeOut() ;
		$(this).parents('.voile_noir').fadeOut() ;
		$('body').css({
			'height' : bodyH+'px',
			'overflow' : 'auto'
		}) ;
	})

	$('#classementMb').on('click',function(e){
		e.preventDefault() ;
		window.open('classement.php','Mot de passe oublié',"menubar=no, status=no, scrollbars=no, menubar=no, width=230, height=390") ;
	})

	//on aligne la hauteur des deux colonnes  :
