<script type="text/javascript">
    function changeSrc2(){
        $('#homeQuoteCaptcha').attr('src','index.php?route=common/captcha/getHomeQuote');
    }
    $(function(){
        $('.quote-btn').click(function () {
            var _this = $(this);
            var main = _this.parents('.item').find('.main-product').data('product-name');
            var nub = _this.parents('.osn-exp').prev('.product-gird').find('.qoute-part').size()+1;
            var table = "<h4 style='display: none'>"+main+"</h4><table style='border-collapse: collapse; width: 100%; border-top: 1px solid #DDDDDD; border-left: 1px solid #DDDDDD; margin-bottom: 20px;'><tr><th style='font-size: 12px; border-right: 1px solid #DDDDDD; border-bottom: 1px solid #DDDDDD; background-color: #EFEFEF; font-weight: bold; text-align: left; padding: 7px; color: #222222;'>Product</th><th style='font-size: 12px; border-right: 1px solid #DDDDDD; border-bottom: 1px solid #DDDDDD; background-color: #EFEFEF; font-weight: bold; text-align: left; padding: 7px; color: #222222;'>Configure</th></tr>";
            table+='<tr><td style="font-size: 12px;	border-right: 1px solid #DDDDDD; border-bottom: 1px solid #DDDDDD; text-align: left; padding: 7px;" rowspan="'+nub+'">'+main+'</td></tr>';
            _this.parents('.osn-exp').prev('.product-gird').find('.qoute-part').each(function(){
                table +='<tr><td style="font-size: 12px;	border-right: 1px solid #DDDDDD; border-bottom: 1px solid #DDDDDD; text-align: left; padding: 7px;"><strong style="color:#008000">'+$(this).data('product-nub')+'x</strong>&nbsp;'+$(this).data('product-name')+'</td></tr>';
            });
            table +="</table>";
            $('#productInfo').empty().html(table);
            $('#quoteModel').modal('show');
        });

        $('#sendQuote').click(function(){
            var $btn = $(this);
            $btn.button('loading');
            if(checkform()){
                var data = {
                    "FristName":$('input[name=FristName]').val(),
                    "LastName":$('input[name=LastName]').val(),
                    "Email": $.trim($('input[name=Email]').val()),
                    "CountryRegion":$('select[name=CountryRegion]').val(),
                    "Content":$('textarea[name=Content]').val(),
                    "ProductInfo":$('#productInfo').html(),
                    "MainProductName":$('#productInfo').find('h4').text(),
                    "homeVer":$('input[name=homeVer]').val()
                };
                $.post('index.php?route=module/home_quote/sendQuote',data,function(r){
                    if(r.status == 'success'){
                        $('#success-alert').removeClass('alert-danger').removeClass('hide').addClass('alert-success').text("Your Quote Send Success");
                        $btn.button('reset');
                        $btn.hide();
                         $('#quoteModel').modal('hide');
                         $('#successModal').modal('show');
                         setTimeout(function(){
                            $('#successModal').modal('hide');
                        },3000);
                    }else{
                        $('#success-alert').removeClass('alert-success').removeClass('hide').addClass('alert-danger').text(r.info);
                        $btn.button('reset').text("Try Again");
                    }
                },'json');
            }else{
                $btn.button('reset');
            }
        });

        $('#quoteModel').on('hide.bs.modal',function(){
            $('.error-text').text("");
            $('input[name=FristName]').val("");
            $('input[name=LastName]').val("");
            $('input[name=Email]').val("");
            $('select[name=CountryRegion]').val("");
            $('textarea[name=Content]').val("");
            $('input[name=homeVer]').val();
            changeSrc2();
            $('#sendQuote').show();
            $('#error-alert').removeClass('hide').text("");
            $("#quoteModel .form-group").removeClass('has-warning');
        })
    });

    function checkform(){
        $('.error-text').css('color','#f00').text("");
        $("#quoteModel .form-group").removeClass('has-error');
        if($('input[name=homeVer]').val() == ""){
            $('input[name=homeVer]').parent().addClass('has-error').find('.error-text').text("Please Enter Verify code");
            return false;
        }


        if($('input[name=FristName]').val() == ""){
            $('input[name=FristName]').parent().addClass('has-error').find('.error-text').text("Please Enter Your FristName");
            return false;
        }
        if($('input[name=LastName]').val() == ""){
            $('input[name=LastName]').parent().addClass('has-error').find('.error-text').text("Please Enter Your LastName");
            return false;
        }
        if($('input[name=Email]').val() == ""){
            $('input[name=Email]').parent().addClass('has-error').find('.error-text').text("Please Enter Your Email");
            return false;
        }
        var pattern = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/;
        if(!pattern.test($('input[name=Email]').val())){
            $('input[name=Email]').parent().addClass('has-error').find('.error-text').text("Please Check Your Email");
            return false;
        }
        if($('select[name=CountryRegion]').val() == ""){
            $('select[name=CountryRegion]').parent().addClass('has-error').find('.error-text').text("Please Choose Your CountryRegion");
            return false;
        }

        if($('textarea[name=Content]').val() == ""){
            $('textarea[name=Content]').parent().addClass('has-error').find('.error-text').text("Please Enter Quote Content");
            return false;
        }

        return true;
    }
