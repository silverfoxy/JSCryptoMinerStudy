                });
                $('#erroMsgContact').addClass('alert-warning').show().text("Please Check Your Email");
                $('#ContactUsBtn').button('reset');
                return false;
            }

            if (uname.val() == "") {
                uname.css({
                    border: "1px #f00 solid"
                });
                $('#erroMsgContact').addClass('alert-warning').show().text("Please Enter Your Name");
                $('#ContactUsBtn').button('reset');
                return false;
            }

            if (contact.val() == "") {
                contact.css({
                    border: "1px #f00 solid"
                });
                $('#erroMsgContact').addClass('alert-warning').show().text("Please Enter Quote Content");
                $('#ContactUsBtn').button('reset');
                return false;
            }


            if (ver.val() == "") {
                ver.css({
                    border: "1px #f00 solid"
                });
                $('#erroMsgContact').addClass('alert-warning').show().text("Please Enter Validation Code");
                $('#ContactUsBtn').button('reset');
                return false;
            }

            var data = {
                email: email.val(),
                name: uname.val(),
                contact: contact.val(),
                ver: ver.val()
            };

            $.post('/index.php?route=module/quote/sendContact', data, function(r) {
                if (r.status == 'success') {
                    $('#erroMsgContact').removeClass('alert-warning').addClass('alert-success').hide();
                    $("#liveChart").modal('hide');
                    $('#successModal2').modal('show');
                    setTimeout(function() {
                        $('#ContactUsBtn').button('reset');
                        $('#successModal2').modal('hide');
                    }, 3000);
                } else {
                    $('#erroMsgContact').addClass('alert-warning').show().text(r.info);
                    $('#ContactUsBtn').button('reset').text("Try Again");
                    $('#ContactUsBtn').button('reset');
                }
            }, 'json');
        })
    })
</script>
</body>

</html>