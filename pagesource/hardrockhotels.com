            for (var i = 0; i < Math.min(data.query.results.item.length, 3); i++)
            {
                var entry = data.query.results.item[i];
                var li = document.createElement("li");   
                var a = document.createElement("a");   
                a.href = entry.link;
                a.innerHTML = '<span class="rssArrow"></span>';          
                a.innerHTML += '<span class="rssTitle">' + entry.title + '</span>';     
                var oDatePieces = entry.pubDate.split(" ")
                a.innerHTML += '<span class="rssDescription">' + oDatePieces[0] + ' ' + oDatePieces[2] + ' ' + oDatePieces[1] + ' ' + oDatePieces[3] + '</span>';   
                li.appendChild(a);
                container.appendChild(li);  
                $('.rssTitle').each(function()
                {
                    textLength = $(this).text().length;
                    if( textLength > 70)
                    {
                        $(this).text($(this).text().substr(0,70)+'...');
                    }     
                });   
            }        
        }   
    });
});
</script>
