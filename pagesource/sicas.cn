    <script type="text/javascript">
        function checkpopmenu() {
            if ($("#pop_menu").is(":hidden"))
                $("#pop_menu").slideDown("fast");
            else
                $("#pop_menu").hide();
        }
        var _chattable = "";
        function chatdialog()
        {
            var chatname = window.localStorage.getItem("chatname");
            if (chatname == null || chatname == "")
            {
                _chattable = document.getElementById("chatc").innerHTML;
                var pageii = layer.open({
                    type: 1,
                    content: _chattable,
                    style: 'width:96%; height:236px; border: none;',
                    success: function (elem) { document.getElementById("chatc").innerHTML = ""; },
                    end: function () { document.getElementById("chatc").innerHTML = _chattable;}
                });
            }
            else
            {
                var layerid=layer.open({ type: 2, content: 'Loading...' });
                window.location.href = "/Chat/Index";
                layer.close(layerid);
            }
        }
        function checkchat()
        {
            var chatName = $("#chatName").val();
            var chatEmail = $("#chatEmail").val();
            if ($.trim(chatName) == "")
            {
                layer.open({ content: 'The full name cannot be empty.', btn: 'Ok' });
                return false;
            }
            if ($.trim(chatEmail) == "") {
                layer.open({ content: 'The E-mail cannot be empty.', btn: 'Ok' });
                return false;
            }
            if (!(/^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/.test(chatEmail))) {
                layer.open({ content: 'Invalid e-mail address. Please enter your valid e-mail address.', btn: 'Ok' });
                return false;
            }
            var chatPhone = $("#chatPhone").val();
            if ($.trim(chatPhone) == "") {
                layer.open({ content: 'The telephone cannot be empty.', btn: 'Ok' });
                return false;
            }
            localStorage.setItem("chatname", chatName);
            localStorage.setItem("chatemail", chatEmail);
            localStorage.setItem("chatphone", chatPhone);
            var layerid=layer.open({ type: 2, content: 'Loading...'});
            window.location.href = "/Chat/Index";
            layer.close(layerid);
        }
        $(document).ready(function () {$('nav#menu').mmenu();});
    </script>
