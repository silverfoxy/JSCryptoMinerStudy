        function checkindexreg() {
            var fullname = $("#textFullName").val();
            if ($.trim(fullname) === "") {
                $("#tipp").html("Full name cannot be empty.").show();
                $("#textFullName").focus();
                return false;
            }
            var firstname = "";
            var lastname = "";
            if (fullname.indexOf(" ") > -1)
            {
                firstname = fullname.split(" ")[0];
                lastname = fullname.split(" ")[1];
            }
            else {
                firstname = fullname;
            }
            var nationality = $("#SelectNationality").val();
            if (nationality === "-1") {
                $("#tipp").html("Please select your nationality.").show();
                $("#SelectNationality").focus();
                return false;
            }
            var uEmail = $("#textEmail").val();
            if (!(/^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/.test(uEmail))) {
                $("#tipp").html("Invalid e-mail address. Please enter your valid e-mail address.").show();
                $("#textEmail").focus();
                return false;
            }
            var telpone = $("#textPhone").val();
            if (!isDigit(telpone)) {
                $("#tipp").html("Please enter valid Cell phone.").show();
                $("#textCode").focus();
                return false;
            }
            var upassword = $("#textPassword").val();
            if ($.trim(upassword) === "") {
                $("#tipp").html("The password cannot be empty.").show();
                $("#textPassword").focus();
                return false;
            }
            if (upassword.length < 6) {
                $("#tipp").html("The password can not be less than 6 letters.");
                $("#textPassword").focus();
                return false;
            }
           
            var _nationality = $("#SelectNationality").find("option:selected").text();
            var codeid = _nationality.match(/\+\d+/);
            codeid = codeid.toString().replace("+", "");
            var layerid = layer.open({ type: 2, content: 'Verify...' });
            $.ajax({
                type: 'POST',
                url: "/City/Register",
                data: { firstName: firstname, lastName: lastname, rpassword: upassword, textPhone: telpone, nationality: nationality, textEmail: uEmail, textCode: codeid },
                dataType: 'json',
                success: function (data) {
                    layer.close(layerid);
                    if (data.status == "1") {
                        var rurl = GetQueryString("ReturnUrl");
                        if (rurl != null)
                            window.location.href = rurl;
                        else
                            window.location.href = "/Apply/Index";
                    }
                    else {
                        layer.open({ content: data.message, skin: 'msg', time: 3 });
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    layer.close(layerid);
                }
            });
            return true;
        }
        function isDigit(s) {
            var patrn = /^[0-9]{6,12}$/;
            if (!patrn.exec(s)) return false;
            return true;
        }
        function GetQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }
        $(function () {
            var swidth = $(window).width();
            var movex = swidth - 20;
            $("#mySwipe").width(movex);
            $(".swipe-wrap").width(movex * 6);
            $("#mySwipe").find("img").width(movex);
           
        });
    </script>
