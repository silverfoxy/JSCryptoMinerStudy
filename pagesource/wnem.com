		}
		$target.find('dl').append(toAdd);	
	}

	CSWxManager.setOption('iconSize', 40);
	// CSWxManager configuration is in custom.js
	CSWxManager.ready(function(wx) {
    	if (wx.length === 0) {
      			return;
    	}
		
		function rotate() {
			// Specific to the MyWeather XML format
    		var hi = wx.fetch('high');
    		var lo = wx.fetch('low');
    		$target = $wn('#csCol4wx .wxInfo');
			$target.find('.temp').html(wx.fetch('temperature') + '&deg;');
			$target.find('.wxIcon').css('background-image', 'url("' + wx.fetch('iconurl') + '")');
			$target.find('.loc').html('<strong>' + wx.fetch('name') + '</strong>');
			$target.find('.condition').html(wx.fetch('sky'));
      		$target.find('.lo').html('Lo&nbsp;' + lo + '&deg;');
			$target.find('.high').html('Hi&nbsp;' + hi + '&deg;');
      		wx.next();
    	}

		rotate();
    	setInterval(rotate, 5000);
  	});  	
}
CScol4Wx();
