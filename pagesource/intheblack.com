function checkIfDisplay() {

	if(
		getCookie("intheblackNewsletterSubscriber") != "true" &&
		getCookie("displayedPopup") != "true" &&
		pageviewCount > 0 &&
		secondCount > 110
	) {
		displayedPopup = true;
			setCookie("displayedPopup", displayedPopup, 0);
		displayPopup();
	}
}


function setCookie(n,v,e) {
    var d = new Date();
    d.setTime(d.getTime() + e);
	var expires = "";
		if( e != 0) { expires = ";expires="+ d.toUTCString(); };
    document.cookie = n+"="+v + expires + ";path=/";
//    console.log("Cookie set: " + getCookie(n) );
}

function getCookie(n) {
    var name = n+"=";
    var ca = document.cookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length,c.length);
        }
    }
    return "";
}

function displayPopup() {
	$.fancybox({
