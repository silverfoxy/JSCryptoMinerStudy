	});
});

$('#search_submit').on('mouseup',function(){
	$('#loader').show();
})

$(document).on("click", ".lang", function(e){
	var id=$(this).attr('id');
	var rel=$(this).attr('rel');
	$('.lang').removeClass('active');
	$(this).addClass('active');	
	if($(this).attr('rel')=='ENIT' || $(this).attr('rel')=='ITEN'){
		$('#en_lng').addClass('active');	
		$('#wdg_srchbtn3').show();
	}else{
		$('#wdg_srchbtn3').hide();
	}
	
	if($(this).attr('rel')=='FRIT' || $(this).attr('rel')=='ITFR'){
		$('#fr_lng').addClass('active');	
		$('#wdg_srchbtn5').show();
	}else{
		$('#wdg_srchbtn5').hide();
	}
	$('#search_word').focus();

	$.ajax({
		type: "POST",
		url: "http://www.garzantilinguistica.it/wp-content/themes/garzantilinguistica_new/services/svr-search-lang.php",
		data: { lang: $(this).attr('rel') },
		beforeSend: function ( xhr ) {
			if(id=='wdg_srchbtn3'){
				if(rel=='ENIT'){
					$('#wdg_srchbtn3').attr('rel','ITEN').removeClass('it-en');
				}else if(rel=='ITEN'){
					$('#wdg_srchbtn3').attr('rel','ENIT').addClass('it-en');
				}
			}
			
			if(id=='wdg_srchbtn5'){
				if(rel=='FRIT'){
					$('#wdg_srchbtn5').attr('rel','ITFR').removeClass('it-fr');
				}else if(rel=='ITFR'){
					$('#wdg_srchbtn5').attr('rel','FRIT').addClass('it-fr');
				}
			}
		}
	})
})
</script>		

