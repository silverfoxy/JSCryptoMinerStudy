<script>
(function(){  // –∞–Ω–æ–Ω–∏–º–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è (function(){ })(), —á—Ç–æ–±—ã –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ "a" –∏ "b" –Ω–µ —Å—Ç–∞–ª–∏ –≥–ª–æ–±–∞–ª—å–Ω—ã–º–∏
var a = document.querySelector('#mvideo'), b = null;  // —Å–µ–ª–µ–∫—Ç–æ—Ä –±–ª–æ–∫–∞, –∫–æ—Ç–æ—Ä—ã–π –Ω—É–∂–Ω–æ –∑–∞–∫—Ä–µ–ø–∏—Ç—å
window.addEventListener('scroll', Ascroll, false);
document.body.addEventListener('scroll', Ascroll, false);  // –µ—Å–ª–∏ —É html –∏ body –≤—ã—Å–æ—Ç–∞ —Ä–∞–≤–Ω–∞ 100%
function Ascroll() {
  if (b == null) {  // –¥–æ–±–∞–≤–∏—Ç—å –ø–æ—Ç–æ–º–∫–∞-–æ–±—ë—Ä—Ç–∫—É, —á—Ç–æ–±—ã —É–±—Ä–∞—Ç—å –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç—å —Å —Å–æ—Å–µ–¥—è–º–∏
    var Sa = getComputedStyle(a, ''), s = '';
    for (var i = 0; i < Sa.length; i++) {  // –ø–µ—Ä–µ—á–∏—Å–ª–∏—Ç—å —Å—Ç–∏–ª–∏ CSS, –∫–æ—Ç–æ—Ä—ã–µ –Ω—É–∂–Ω–æ —Å–∫–æ–ø–∏—Ä–æ–≤–∞—Ç—å —Å —Ä–æ–¥–∏—Ç–µ–ª—è
      if (Sa[i].indexOf('overflow') == 0 || Sa[i].indexOf('padding') == 0 || Sa[i].indexOf('border') == 0 || Sa[i].indexOf('outline') == 0 || Sa[i].indexOf('box-shadow') == 0 || Sa[i].indexOf('background') == 0) {
        s += Sa[i] + ': ' +Sa.getPropertyValue(Sa[i]) + '; '
      }
    }
    b = document.createElement('div');  // —Å–æ–∑–¥–∞—Ç—å –ø–æ—Ç–æ–º–∫–∞
    b.style.cssText = s + ' box-sizing: border-box; width: ' + a.offsetWidth + 'px;';
    a.insertBefore(b, a.firstChild);  // –ø–æ–º–µ—Å—Ç–∏—Ç—å –ø–æ—Ç–æ–º–∫–∞ –≤ —Ü–µ–ø–ª—è—é—â–∏–π—Å—è –±–ª–æ–∫ –ø–µ—Ä–≤—ã–º
    var l = a.childNodes.length;
    for (var i = 1; i < l; i++) {  // –ø–µ—Ä–µ–º–µ—Å—Ç–∏—Ç—å –≤–æ –≤–Ω–æ–≤—å —Å–æ–∑–¥–∞–Ω–Ω–æ–≥–æ –ø–æ—Ç–æ–º–∫–∞ –≤—Å–µ—Ö –æ—Å—Ç–∞–ª—å–Ω—ã—Ö –ø–æ—Ç–æ–º–∫–æ–≤ (–∏—Ç–æ–≥–æ: —Å–æ–∑–¥–∞–Ω –ø–æ—Ç–æ–º–æ–∫-–æ–±—ë—Ä—Ç–∫–∞, –≤–Ω—É—Ç—Ä–∏ –∫–æ—Ç–æ—Ä–æ–≥–æ –ø–æ –ø—Ä–µ–∂–Ω–µ–º—É —Ä–∞–±–æ—Ç–∞—é—Ç —Å–∫—Ä–∏–ø—Ç—ã)
      b.appendChild(a.childNodes[1]);
    }
    a.style.height = b.getBoundingClientRect().height + 'px';  // –µ—Å–ª–∏ –ø–æ–¥ —Å–∫–æ–ª—å–∑—è—â–∏–º —ç–ª–µ–º–µ–Ω—Ç–æ–º –µ—Å—Ç—å –¥—Ä—É–≥–∏–µ –±–ª–æ–∫–∏, –º–æ–∂–Ω–æ —Å–≤–æ—ë –∑–Ω–∞—á–µ–Ω–∏–µ
    a.style.padding = '0';
    a.style.border = '0';  // –µ—Å–ª–∏ —ç–ª–µ–º–µ–Ω—Ç—É –ø—Ä–∏—Å–≤–æ–µ–Ω padding –∏–ª–∏ border
  }
  if (a.getBoundingClientRect().top <= 0) { // elem.getBoundingClientRect() –≤–æ–∑–≤—Ä–∞—â–∞–µ—Ç –≤ px –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã —ç–ª–µ–º–µ–Ω—Ç–∞ –æ—Ç–Ω–æ—Å–∏—Ç–µ–ª—å–Ω–æ –≤–µ—Ä—Ö–Ω–µ–≥–æ –ª–µ–≤–æ–≥–æ —É–≥–ª–∞ –æ–±–ª–∞—Å—Ç–∏ –ø—Ä–æ—Å–º–æ—Ç—Ä–∞ –æ–∫–Ω–∞ –±—Ä–∞—É–∑–µ—Ä–∞
    b.className = 'sticky';
  } else {
    b.className = '';
  }
  window.addEventListener('resize', function() {
    a.children[0].style.width = getComputedStyle(a, '').width
  }, false);  // –µ—Å–ª–∏ –∏–∑–º–µ–Ω–∏—Ç—å —Ä–∞–∑–º–µ—Ä –æ–∫–Ω–∞ –±—Ä–∞—É–∑–µ—Ä–∞, –∏–∑–º–µ–Ω–∏—Ç—å—Å—è —à–∏—Ä–∏–Ω–∞ —ç–ª–µ–º–µ–Ω—Ç–∞
}
})()
