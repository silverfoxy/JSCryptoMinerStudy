        $('body').trigger("main.geocode", geocode);
    });
    /* General removal of elements based on geocode*/
    $('body').on("main.geocode", function (e, geo) {
        var geocodes = ['gb', 'us', 'ca', 'roi', 'aus'];

        if (geocodes.indexOf(geo) < 0)
            geo = 'int';

        $('.geocode:not(.' + geo + ')').remove();
        $('body').trigger('main.geocode.done');
    });
</script>
 
