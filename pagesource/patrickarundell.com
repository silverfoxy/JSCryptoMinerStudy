<script type="text/javascript">
    $(document).ready(function () {
        // responsive navigation
        $toggle = $("<a id='nav-toggle' class='nav-toggle'>Menu</a>").click(function () {
            $("#nav").toggleClass("open");
        })
        $("#nav").prepend($toggle);

        /* GEOCODING */
