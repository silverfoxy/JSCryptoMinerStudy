                                <textarea class="textarea long-field" cols="30" rows="5" id="description-bad" name="description-bad"></textarea>
                            </div>
                            <div class="desc-group field-group">
                                <label for="feedback_name">Name:</label>
                                <input id="feedback_name" type="text" />
                            </div>
                            <div class="desc-group field-group">
                                <label for="feedback_email">Email:</label>
                                <input id="feedback_email" type="email" />
                            </div>
                            <div class="desc-group field-group">
                                <fieldset class="group">
                                    <div class="checkbox web-info-consent-container">
                                        <input id="web-info-consent" type="checkbox" name="web-info-consent" />
                                        <label for="web-info-consent">Include data about your current environment, like the browser and version. This helps us understand your feedback better.</label>
                                    </div>
                                </fieldset>
                                <div class="field-group">
                                    <a href="#" id="show-recorded-web-info">What additional information is included about my current environment?</a>
                                    <div id="additional-collected-info" style="display:none;">
                                        <table>
                                            <tr>
                                                <th scope="row">Referring URL:</th>
                                                <td id="collected-info-referrer"></td>
                                            </tr>                                
                                            <tr>
                                                <th scope="row">Browser:</th>
                                                <td id="collected-info-browser"></td>
                                            </tr>                   
                                            <tr>
                                                <th scope="row">Screen Resolution:</th>
                                                <td id="collected-info-resolution"></td>
                                            </tr>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="dialog-button-panel">
                        <input id="feedback-submit" type="button" onclick="submitFeedback()" class="aui-button aui-button-primary submit-button" value="Submit">
                        <a href="#" onclick="hideFeedbackDialog()" class="cancel">Close</a>
                    </div>
                </div>
            </div>
        </div>
    </body>

