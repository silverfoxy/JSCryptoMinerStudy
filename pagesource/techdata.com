											}
											
											function getFormattedDate(input) {
												var fDate = new Date(input);
												var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
												var formattedDate = monthNames[fDate.getMonth()] + " " + fDate.getDate() + ", " + fDate.getFullYear();
												return formattedDate;
											}

											function handleResponse(response) {
												var itemsToDisplay = 4; //response.query.results.feed.entry.length;
												//console.log(response);
												var rssoutput = "";		
												var feed = response.query.results.feed;
												//rssoutput += "<div><div style='font-size:14px;margin-left:15px;'><a href='" + feed.link.href + "'>" + feed.title + "</a></div><br>";
												for (var i=0; i<itemsToDisplay; i++){
													rssoutput += "<div><div><a href='" + feed.entry[i].link.href + "'>" + feed.entry[i].title + "</a></div><div>" + getFormattedDate(feed.entry[i].published) + "</div></div></br>";
												}		
												document.getElementById('news').innerHTML = rssoutput;
											}
												
											document.addEventListener("DOMContentLoaded", function() {	    
												var feedUrl = "http://apps.shareholder.com/rss/rss.aspx?channels=7689&companyid=TECD&sh_auth=2623357869.0.0.41494.0e35baedfa795dd86cbd85b085931b62"
