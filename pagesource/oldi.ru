		$('#city_select_input').on("keyup", function(e) {
			// console.log('city_select_input');
			
			if(e.which == 38) { //up
				highlight_city('up');
			} else if(e.which == 40) { //down
				highlight_city('down');
			} else if(e.which == 13) { // enter
				if(can_select) {
					var location_link = $('#search_city_select_btn').attr('href');
					window.location = location_link;
				}
			}else {
				$('#city_select_autocomplete').empty();
				
				var search_str = $(this).val(),
					word = $(this).val().toUpperCase(),
					matched_word = false,
					cur_city_found = false;
					
				if(search_str.length > 0) {
					
					if(search_str.length>0){
						
						var regex = new RegExp(search_str+'|'+eng_rus(search_str), "gi");
						
						$.ajax({
							type:'get',
