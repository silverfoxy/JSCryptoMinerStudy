							success:function(data){
								city_html = '';
								city_list = $.parseJSON(data);
								console.log(city_list.length, city_list);
								
								if(city_list.length){
									for(i=0;i<city_list.length;i++) {
										
										var city_id = city_list[i][0],
											city_name = city_list[i][1],
											city_url = '',
											city_region = city_list[i][2] || '',
											city_domain = city_list[i][3] || '',
											highlighted_text = city_name.replace(regex, function(match) {
												return '<span>' + match + '</span>';
											});
										
										if(city_domain.length)
											city_url = "https://"+city_domain+".oldi.ru/";
										else {
																							city_url = "https://www.oldi.ru/?selectCityID=#city_id#";
												city_url = city_url.replace('#city_id#', city_id);
																					}
										
										city_html += '<p id="ac_city_' + city_id + '"';
										city_html += '><a style="float:left;" href="' + city_url + '">' + highlighted_text + '</a><span style="margin-left: 5px;font-style: italic;color: rgb(204, 204, 204);">'+city_region+'</span></p>';
										
										if(word==city_name.toUpperCase()) {
											matched_word = true;
											can_select = true;
											$('#search_city_select_btn').css('opacity', '1');
											$('#search_city_select_btn').attr('href', current_page.replace('#city_id#', city_id));
										}
									}
									
									// console.log(city_html);
									$('#city_select_autocomplete').html(city_html);
									$('#city_select_autocomplete').show();
								}else
									$('#city_select_autocomplete').hide();
							}
						});
					}					
				}
				else
					$('#city_select_autocomplete').hide();
				
				current_city = 0;
				
				if(!matched_word) {
					can_select = false;
					$('#search_city_select_btn').css('opacity', '0.2');
					$('#search_city_select_btn').attr('href', '');
				}				
			}
		});
		
		function highlight_city(direction) {
			$('#city_select_autocomplete p').removeClass('active');
			if(direction == 'up') {
				if(current_city) {
					var cur_city_ob = $('#city_select_autocomplete p[id=ac_city_' + current_city + ']');
					var prev_city = cur_city_ob.prev();
					if(prev_city.length) {
						prev_city.addClass('active');
					}
					else {
						$('#city_select_autocomplete p:last').addClass('active');
					}
				}
				else {
					$('#city_select_autocomplete p:last').addClass('active');
				}
			}
			else if(direction == 'down') {
				if(current_city) {
					var cur_city_ob = $('#city_select_autocomplete p[id=ac_city_' + current_city + ']');
					var next_city = cur_city_ob.next();
					if(next_city.length) {
						next_city.addClass('active');
					}
					else {
						$('#city_select_autocomplete p:first').addClass('active');
					}
				}
				else {
					$('#city_select_autocomplete p:first').addClass('active');
				}
			}
			var city_id = $('#city_select_autocomplete p.active').attr('id');
			$('#city_select_input').val($('#city_select_autocomplete p.active').text())
			current_city = city_id.substring(8);
			if(current_city) {
				$('#search_city_select_btn').css('opacity', '1');
				$('#search_city_select_btn').attr('href', current_page.replace('#city_id#',current_city));
				can_select = true;
			}
			else {
				$('#search_city_select_btn').css('opacity', '0.2');
				$('#search_city_select_btn').attr('href', '');
				can_select = false;
			}
		}
		
		$('#search_city_select_btn').click(function(e) {
			if(!can_select) {
				e.preventDefault();
			}
		});
		
