						else{
							$("div.dec a[rel]",cplace).removeClass("disabled")
						}
						if (v > 100){
							v = 100;
							$(this).val(v);
						}
						else if (v == 100){
							$("div.dec a[rel]",cplace).addClass("disabled");
						}
						else{
							$("div.dec a[rel]",cplace).removeClass("disabled")
						}
				adv_recountSum(v,$("div.inc a[rel]",cplace).attr("rel"));


			});


			if (adv_getIntValue($(count)) < 2) {
				$("div.dec a[rel]",cplace).addClass("disabled");
			}

			
		});

	}
	

	function adv_getIntValue(el) {
		var v = parseInt($(el).val());
		v = (!isNaN(v))? v : 1;
		return	v;
	}

	function adv_checkIsDigit(f) {
		var validchars="0123456789";
		for (var i=f.length-1; i>=0; i--) {
			var x = new String(f.charAt(i));
			var checker = validchars.lastIndexOf(x) > -1;
			if (!checker) {
				return false;
				break;
			}
		}
		return true;
	}
		
	function adv_clearNumber(v) {
		v = v.replace(/ /g, "");
		return new Number(v);
	}
	
