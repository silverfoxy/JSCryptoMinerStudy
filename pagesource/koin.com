      var divProgressEl = $('<div></div>', {"class": "progress"});
      var spanMeterEl = $('<span></span>', {"class": "meter", style: "width: " + percent + "%"});
      var spanLabelEl = $('<span></span>', {"class": "label-meter"});
      spanLabelEl.text(percent + "%");
      spanMeterEl.append(spanLabelEl);
      divProgressEl.append(spanMeterEl);
      divChoiceEl.append(labelEl);
      divChoiceEl.append(divProgressEl);
      if (pollsChoices[x].imageUrl) {
       var wrapperEl = getWrapperEl('<div></div>', pollsChoices[x].imageUrl, questionDescription);
       liEl.append(wrapperEl);
      }
      liEl.append(divChoiceEl);
      ulEl.append(liEl);
     }

     divContentEl.append(ulEl);
     var smallEl = $('<small></small>', {"class": "total"});
     smallEl.text("Total Votes: " + questionVoteCount);
     divContentEl.append(smallEl);
    };

    var renderContentResultChart = function(data) {
     var divContentEl = $('form[id="10536813896791"] div.mod-content');
     var pollsDescription = $(data.pollsVO.description).find("Description").text();
     var h4El = $('<h4></h4>', {"class": "question"});
     h4El.text(pollsDescription);
     divContentEl.append(h4El);

     var pollsChoices = data.pollsVO.pollsChoiceVOs;
     var questionVoteCount = data.pollsVO.voteCount;
     var ulEl = $('<ul></ul>', {"class": "chart-key", "data-pie-id": "chart-pie1053681389"});
     if ("pie" == "donut") {
      ulEl = $('<ul></ul>', {"class": "chart-key", "data-pie-id": "chart-dount1053681389", "data-options": '{"donut": "true"}'});
     }
     for (var x in pollsChoices) {
      var choiceCount = pollsChoices[x].voteCount;
      var percent = Math.round(choiceCount / (questionVoteCount == 0 ? 1 : questionVoteCount) * 10000) /100;
      var questionDescription = $(pollsChoices[x].description).find("Description").text();
      var liEl = $('<li></li>', {"data-value": percent});
      var spanKeyEl = $('<span></span>', {"class": "key", title: questionDescription});
      spanKeyEl.text(percent + "%");
      var spanValueEl = $('<span></span>', {"class": "value"});
      if (pollsChoices[x].imageUrl) {
       var wrapperEl = getWrapperEl('<span></span>',pollsChoices[x].imageUrl, questionDescription);
       spanValueEl.append(wrapperEl);
      }
      spanValueEl.append(questionDescription);
      liEl.append(spanKeyEl);
      liEl.append(spanValueEl);
      ulEl.append(liEl);
     }
     var chartId = "chart-pie1053681389";
     if ("pie" == "donut") {
      chartId = "chart-dount1053681389";
     }
     var divChartEl = $('<div></div>', {"class": "chart pie", id: chartId});

     divContentEl.append(ulEl);
     divContentEl.append(divChartEl);
     var smallEl = $('<small></small>', {"class": "total"});
     smallEl.text("Total Votes: " + questionVoteCount);
     divContentEl.append(smallEl);
    };

    var renderContentResult = function(data) {
     if ("pie" == "bar") {
      renderContentResultBar(data);
     } else {
      renderContentResultChart(data);
     }
    };

    var renderFooter = function(showFirst) {
     var footerEl = $('form[id="10536813896791"] footer');
     var aButtonEl = $('<a></a>', {"class": "button small"});
     var alinkEl = $('<a></a>', {"class": "link small"});

     if (!isVoted()) {
      if (showFirst == "question") {
       aButtonEl.text("Submit");
       alinkEl.text("View Results");
       aButtonEl.bind("click", submit);
       alinkEl.bind("click", "result", refreshModule);

       footerEl.append(aButtonEl);
       footerEl.append(alinkEl);
      } else if (showFirst == "result") {
       aButtonEl.text("Vote");
       aButtonEl.bind("click", "question", refreshModule);
       footerEl.append(aButtonEl);
      }
     }
     
     if (false) {
      var alinkEl2 = $('<a></a>', {"class": "link small"});
      alinkEl2.text("Share");

      footerEl.append(alinkEl2);

      if (false) {
       stWidget.addEntry({
        "service":"sharethis",
        "element":alinkEl2[0],
        "url":"http://www.koin.com/polls/1053681389",
        "title":"LAKANA",
        "type":"custom"
       });
      } else {
       addthis.button(alinkEl2[0], {}, {url: "http://www.koin.com/polls/1053681389", title: "LAKANA"}); 
      }
     }

    };

    var renderModule = function(showFirst) {
     $.ajax({
      type: 'POST',
      url: '/ep-wcm-portlet/restful/poll/find_polls_by_id',
      data: {
       'questionId': 1053681389
      },
      success: function(data) {
       if (data && data.pollsVO && data.pollsVO.id != 0) {
        if (isVoted() || showFirst == "result") {
         renderContentResult(data);
        } else {
         renderContentQuestion(data);
        }
        renderFooter(showFirst);
        if (Pizza) {
         Pizza.init();
        }
       }
      },
      dataType: 'json'
     });
    };

    renderModule("question");
   });
  })(jQuery, AUI());
