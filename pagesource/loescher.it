        if (e.keyCode == 13 && $("#do-search-new").val().length > 0) {
            DoSearch();
        }
    }

    function DoSearch() {
        var sWhat = $("#do-search-new").val();
        //var bCat = true;
        //var bAllSite = false;
        var bCat = $("#radio-1-2").prop("checked");
        var bAllSite = $("#radio-1-1").prop("checked");

        var strParams = "What=" + sWhat + "&Cat=" + bCat + "&AllSite=" + bAllSite;
        var url = "/Ajax/Catalogo/DoSearchNEW.aspx"; //"/Ajax/Catalogo/DoSearch.aspx";  //<--TODO Richiamiamo la nuova DoSearchNew che in teoria fa le stesse cose di prima, cioè ricerca sul catalogo. Bisognerà solo cambiare la procedura sottostante che alimenta il result set!

        if (bCat) {
            $('#loading').show();
            var loader = new net.ContentLoader(url, DoSearchRes, null, "POST", strParams);
        }

        //Ricerca su tutto il sito deve reindirizzare a nuova pagina!!!
        if (bAllSite) {
            //window.location = "/result-search-new.aspx?q=" + sWhat;
            $('#loading').show();
            var loader1 = new net.ContentLoader("/Ajax/Catalogo/DoSearchFT.aspx", DoSearchFTRes, null, "POST", "q="+sWhat);
        }
    }

    function DoSearchFTRes() {
        $('#loading').hide();
        document.location.href = "/result-search-new.aspx";
    }

    function DoSearchRes() {
        $('#loading').hide();
        var strText = this.req.responseText;

        if (strText == "+") {
            //Spariamo alla pagina di visualizzazione risultati (occhio che potrebbe essere una ricerca senza risultati)
            document.location.href = "/catalogo/ricerca";
        } else if (strText.indexOf("1|") > -1) {
            //Andiamo direttamente alla scheda catalogo
            document.location.href = strText.split("|")[1];
            //document.location.href = "/dettaglio-catalogo.aspx";
        }
    }

                  
</script>

