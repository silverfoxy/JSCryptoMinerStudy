                <script id="calendar-template" type="text/x-handlebars-template">
	{{#each calendar.row}}
		<tr>
			{{#eachProperty column}}
                {{#if value.cell.single}}
                    {{#if value.performances}}
				        {{#if value.today}}
                            <td class="has-events today">
                        {{else}}
                            <td class="has-events {{#if value.cell.past}} past {{/if}}">
                        {{/if}}
                    {{else}}
                        {{#if value.today}}
                            <td class="today">
                        {{else}}
                            <td>
                        {{/if}}
                    {{/if}}
                {{else}}
                    <td class="blank">
                {{/if}}
					<span class="events-date">{{value.cell.single}}</span>
					{{#if value.performances}}
						<div class="events">
                            <div class="events-day-content">
                                <div class="events-day-content-header">
                                    <span class="events-day-content-header-day">{{value.cell.full}}</span>
                                    <span class="events-day-content-header-close">Close</span>
                                </div>
                                <div class="events-day-scrollable">
                                    <ul class="events-day-content-list">
        							{{#each value.performances}}
                                        <li>
            								<span class="event-listing-title">{{title}}</span>
                                            <span class="event-listing-meta">{{time}}, {{venue}}</span>
                                            <span class="event-listing-more-info"><a href="/{{more_info}}" class="performance_book">{{more_info_text}}</a></span>
                                            {{#if book}}
                                                <span class="event-listing-book"><a href="/{{book}}" class="performance_book{{#if member_required}} member_only js-member-lightbox{{/if}}{{#if mup}} btn--green js-member-lightbox{{/if}}">{{book_text}}</a></span>
                                            {{/if}}
                                            {{#if soldout_text}}
                                                <span class="event-listing-sold-out">{{soldout_text}}</span>
                                            {{/if}}
                                            {{#if book_custom}}
                                                <span class="event-listing-book custom"><a href="/{{book_custom}}" rel="external" class="performance_book custom{{#if mup}} js-member-lightbox{{/if}}">{{book_custom_text}}</a></span>
                                            {{/if}}
                                            {{#if book_itunes}}
                                                <span class="event-listing-book itunes"><a href="/{{book_itunes}}" rel="external" class="performance_book itunes">{{book_itunes_text}}</a></span>
                                            {{/if}}
                                            {{#if book_amazon}}
                                                <span class="event-listing-book amazon"><a href="/{{book_amazon}}" rel="external" class="performance_book amazon">{{book_amazon_text}}</a></span>
                                            {{/if}}
                                        </li>
        							{{/each}}
                                    </ul>
                                </div>
                            </div>
						</div>
					{{/if}}
				</td>
			{{/eachProperty}}
		</tr>
	{{/each}}
</script>

