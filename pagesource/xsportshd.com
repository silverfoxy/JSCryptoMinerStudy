  function getParentUntil(element, parentClass){    
      var parent = element.parentNode;    
      
      while(parent && parent.nodeType !== 9 && !hasClass(parent,parentClass))
      {     
          if(parent.parentNode) { 
              parent = parent.parentNode; 
              if(parent.className && hasClass(parent,parentClass)){
                  return parent;
              }
          }  else { return ; }    
      }

      return;
  }

  function changeCategory(type){
    secId = document.getElementById('section_list')?document.getElementById('section_list').options[document.getElementById('section_list').selectedIndex].value:0;
    var catList = getElements('.category');
    var catSelected = document.getElementById('category_list').options[document.getElementById('category_list').selectedIndex].value;
    var catId = catSelected.split('-')[1] || '0';
    var catSelectList = document.getElementById('category_list').options;
    var catChosen = [];
    var reg = /cat(\d+\b|\s)/g;
 
    for (var c = 1; c < catSelectList.length; c++) {
        var separateCatId = catSelectList[c].value.split('-')[1];
        if(secId > 0){
            if(catSelectList[c].value != (secId+'-'+separateCatId)){
                catSelectList[c].style.display = 'none';
            }else{
                catSelectList[c].style.display = '';
                catChosen.push(separateCatId);
            }
        }else{
            catSelectList[c].style.display = '';
            catChosen.push(separateCatId);
        }
    }
    
    if(type == 'section'){
        document.getElementById('category_list').selectedIndex = 0;
    }

    for (var i = 0; i < catList.length; i++) {
        if(type == 'category' && catId != '0' && !hasClass(catList[i],'cat'+catId)){     
            var p = getParentUntil(catList[i],'lshpanel');
            p.style['display'] = 'none';
            addClass(p,"notactive");
        }else{
            if(catChosen.indexOf(catList[i].className.match(reg)[0].replace("cat","")) == -1){
                var p = getParentUntil(catList[i],'lshpanel');
                p.style['display'] = 'none';
                addClass(p,"notactive");
            }else{
                var p = getParentUntil(catList[i],'lshpanel');
                p.style['display'] = '';
                removeClass(p,"notactive");
            }
        }
    }

    changeShowDateBlock();
  }
  
