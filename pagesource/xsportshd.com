        /* toggle element(hide/show)__  (it is better to replace it using jQuery or different javaScript framework) */
        function getRealDisplay(elem) {
            if (elem.currentStyle) return elem.currentStyle.visibility;
            else if (window.getComputedStyle) {
                var computedStyle = window.getComputedStyle(elem, null );
                return computedStyle.getPropertyValue('visibility');
            }
        }

        function hide(el) {
            if (!el.getAttribute('displayOld')) el.setAttribute("displayOld", el.style.visibility);
            el.style.visibility = "hidden";
        }

        displayCache = {};

        function isHidden(el) {
            var width = el.offsetWidth, height = el.offsetHeight, tr = el.nodeName.toLowerCase() === "tr", visible = el.style.visibility;
            return width === 0 && height === 0 && !tr ? true : width > 0 && height > 0 && !tr && visible != 'hidden' ? false :	getRealDisplay(el);
        }

        function toggleD(el) { isHidden(el) ? show(el) : hide(el); }


        function show(el) {
            if (getRealDisplay(el) != 'hidden') return;
            var old = el.getAttribute("displayOld");

            if ( getRealDisplay(el) === "hidden" ) {
                var nodeName = el.nodeName, body = document.body, display;
                if ( displayCache[nodeName] ) display = displayCache[nodeName];
                else {
                    var testElem = document.createElement(nodeName);
                    body.appendChild(testElem);
                    display = getRealDisplay(testElem);
                    if (display === "hidden" ) display = "visible";
                    body.removeChild(testElem);
                    displayCache[nodeName] = display;
                }

                el.setAttribute('displayOld', display);
                el.style.visibility = display;
            }
        }
        /* ___toggle element(hide/show) */

