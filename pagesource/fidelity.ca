<script type="text/javascript">
	
	var login =
	{
		title: "Login",
		lockedLinkTitle: "Advisor materials",
		msgLockedLink: "You have tried to access materials that are intended for financial advisor use only.",
		msgBadInput: "You have entered insufficient or incorrect information. Please try again or contact Fidelity Client Relations for assistance at 1-800-263-4077 between 8:00 a.m. and 8:00 p.m. ET, Monday to Friday.",
		msgDeactivated: "This User Id is deactivated. Please contact your advisor to reactivate.",
		msgSfDeactivated: "Your Dealer Centre access has been deactivated. Please contact Fidelity Dealer Relations and Industry Support for assistance at 1-888-563-4778 between 8:30 a.m. and 5:00 p.m. ET, Monday to Friday.",
		msgSfNoActiveDlrs: "Your User ID is not associated with an active dealer code.  Please contact Fidelity Dealer Relations and Industry Support for assistance at 1-888-563-4778 between 8:30 a.m. and 5:00 p.m. ET, Monday to Friday.",
		msgExecutingAjax: "Logging in, please wait.",
		msgRedirecting: "Log in successful, redirecting.",
		msgSystemError: "System error occurred. If you require assistance, please contact Fidelity Client Relations at 1-800-263-4077 between 8:00am and 8:00pm ET Monday to Friday, with incident details.",
		loginUrl: "/sites/fidca/login/login.do",
		ssoLoginUrl: "https://ficlws.fidelity.ca/aiWeb/ssologin.entry?language=en",
		promptLoginUrl: null,
		badInputCheck:
			function(e)
			{
				e.preventDefault();
				
				var goodUserId = /^[-a-zA-Z0-9_.@]{6,30}$/.test($('#login_user_id').val());
				var goodPassword = /^[-a-zA-Z0-9 ~!@#$%\^&*_\+=`|\\(){}\[\]:;"'<>,.?\/]{8,50}$/.test($('#login_password').val());
				if (!goodUserId || !goodPassword)
				{
					$('#login_user_id_field').addClass('required');
					$('#login_password_field').addClass('required');
					login.showMessage(login.msgBadInput, true);
					return false;
				}
				
				login.doLogin();
				return true;
			},
		doLogin:
			function()  
			{
				var userId = $('#login_user_id').val();
				var password = $('#login_password').val();
				var rememberMe = $('#login_remember_me').prop('checked')
				
				login.disableInputs();
				login.showMessage(login.msgExecutingAjax, false);
				
				$.ajax({
					url: login.loginUrl,
					type: "POST",
					async: true,
					contentType: "application/x-www-form-urlencoded; charset=utf-8",
					timeout: 90000,
					data: {
						userId: userId,
						password: password,
						rememberMe: rememberMe
					},
					success: function(data, status, jqxhr) {
						// Request returned successfully, handle result
						login.resetInputs();
						if (data == null) {
							login.showMessage(login.msgSystemError, true);
						} else if (data.code == 0) {
							login.disableInputs();
							login.showMessage(login.msgRedirecting, false);
							var params = "";
							if (login.promptLoginUrl != null) {
								params = "&URL=" + encodeURIComponent(login.promptLoginUrl);
							}
							window.location.href = login.ssoLoginUrl + params;
							dialogs.close(); 
						} else if (data.code == 3000) {
							$('#login_user_id_field').addClass('required');
							$('#login_password_field').addClass('required');
							login.showMessage(login.msgBadInput, true);
						} else if (data.code == 3002) {
							login.showMessage(login.msgDeactivated, true);
						} else if (data.code == 3010) {
							login.showMessage(login.msgSfDeactivated, true);
						} else if (data.code == 3012) {
							login.showMessage(login.msgSfNoActiveDlrs, true);
						
						} else if (data.code == 2) {
							login.disableInputs();
							login.showMessage(login.msgRedirecting, false);
							if (login.promptLoginUrl != null) {
								window.location.href = login.promptLoginUrl;
							} else {
 								window.location.href = $('link[rel=canonical]').attr('href');
							}
							dialogs.close();
						
						} else {
							login.showMessage(login.msgSystemError, true);
						}
					},
					error: function(xhr, status, error) {
						// Http error
						login.resetInputs();
						if (xhr.status != 200) {
							login.showMessage(login.msgSystemError + "<br/>Status: [" 
								+ xhr.status + "] " + xhr.statusText, true);
						}
					}
				})
			},
		disableInputs:
			function()
			{
				$('#login_user_id').prop('disabled', true);
				$('#login_password').prop('disabled', true);
				$('#login_submit').prop('disabled', true);
				$('#login_remember_me').prop('disabled', true);
				$('#fidelityRegisterButton').prop('disabled', true);
				$('#fidelityLoginForgotPassword').prop('disabled', true);
				$('#login_user_id').val('');
				$('#login_password').val('');
				$('#login_user_id_field').removeClass('required');
				$('#login_password_field').removeClass('required');
				login.clearMessage();
			},
		resetInputs:
			function()
			{
				$('#login_user_id').prop('disabled', false);
				$('#login_password').prop('disabled', false);
				$('#login_submit').prop('disabled', false);
				$('#login_remember_me').prop('disabled', false);
				$('#fidelityRegisterButton').prop('disabled', false);
				$('#fidelityLoginForgotPassword').prop('disabled', false);
				$('#login_user_id').val('');
				$('#login_password').val('');
			},
		showMessage:
			function(message, isError)
			{
				login.clearMessage();
				if (isError) { 
					$('#login_err_msg').addClass('error');
					$('#login_err_msg').text(message);
					$('#login_err_msg.error').slideDown(400);
				} else {
					$('#login_instruction').text(message);
				}
			},
		clearMessage:
			function()
			{
				if($('#login_err_msg').is(':visible'))
					$('#login_err_msg').slideUp(400);
				$('#login_err_msg').text('').removeClass('error');
				$('#login_instruction').text('');
			},
		setDefaultTitle:
			function()
			{
				$('#auth_login_title').text(login.title);
				$('#auth_login_innertitle').hide();
				$('#login_instruction').text('');
			},
		setLockedLinkTitle:
			function()
			{
				$('#auth_login_title').text(login.lockedLinkTitle);
				$('#auth_login_innertitle').show();
				$('#login_instruction').text(login.msgLockedLink);
			},
		getCookie:
			function(cname)
			{
				var name = cname + "=";
				var ca = document.cookie.split(';');
				for (var i = 0; i < ca.length; i++) {
					var c = ca[i];
					while (c.charAt(0) == ' ') {
						c = c.substring(1);
					}
					if (c.indexOf(name) == 0) {
						return c.substring(name.length, c.length);
					}
				}
				return "";
			}
	};

	var register =
	{
		open:
			function(e)
			{
				dialogs.setup_select_account_type();
				e.preventDefault();
				notifier.notify( 'close_overlays' );
				dialogs.close();								
				dialogs.open('auth_select_acct_type');			
			}
	};
	
	var forgot_password =
	{
		open:
			function(e)
			{
				dialogs.setup_select_account_type();
				e.preventDefault();
				notifier.notify( 'close_overlays' );
				dialogs.close();								
				dialogs.open('auth_forgot_pw_type');			
			}
	};

	var btn_urls = {
		btn_register_advisor	: "https://ficlws.fidelity.ca/aiWeb/advisor/register/dealerrep.form?language=en",
		btn_register_investor	: "https://ficlws.fidelity.ca/aiWeb/investor/password/create.form?language=en",
		btn_register_dealer		: "https://ficlws.fidelity.ca/aiWeb/dealer/password/create/primary.entry?language=en",
		btn_forgot_password_investor	: "https://ficlws.fidelity.ca/aiWeb/investor/password/resetone.entry?language=en",
		btn_forgot_password_advisor		: "https://ficlws.fidelity.ca/aiWeb/advisor/password/resetone.entry?language=en",
		btn_forgot_password_dealer		: "https://ficlws.fidelity.ca/aiWeb/dealer/password/resetone.entry?language=en",
	};

	$(document).ready(function()
	{
		var userId = login.getCookie('RememberMe');
		if (userId != null && userId != "") {
			$('#login_user_id').val(userId);
			$('#login_remember_me').prop('checked', true);
		} else {
			$('#login_user_id').val('');
			$('#login_remember_me').prop('checked', false);
		}
		$('#login_submit').off();
		$('#login_submit').on('click', login.badInputCheck);
		$('#login_user_id,#login_password').keypress(function (e) {
			if ((event.keyCode || event.which) == 13) {
				$('#login_submit').click();
				return false;
			}
		});
		
		$('#register').off();		
		$('#register').on('click', register.open);

		$('#forgot_password').off();		
		$('#forgot_password').on('click', forgot_password.open);

		$('#btn_register_advisor,#btn_register_investor,#btn_register_dealer,#btn_forgot_password_investor,#btn_forgot_password_advisor,#btn_forgot_password_dealer').on('click', function(event) {
		    event.preventDefault(); 
		    window.location.href = btn_urls[this.id];
		});
	});

</script>




