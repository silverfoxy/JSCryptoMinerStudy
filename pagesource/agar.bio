function girisYap(){
    $('#girisSonuc').html('Please Wait');
    $.ajax({
    url: "login.php",
    type: "POST",
    crossDomain: true,
    data: $('#giris').serialize(),
    dataType: "json",
    success: function(gcevap){
        if (gcevap.hata){
            $("#girisSonuc").html(gcevap.hata).show();
        }else{
            setTimeout(function () {$("#girisSonuc").html(gcevap.ok).show(); }, 1500);
            setTimeout(function () { location.reload(1); }, 3000);
        }
    }
    });
}

