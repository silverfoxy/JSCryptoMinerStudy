function notificationCounter()
{
    var msisdn = 0;
    if($jq('.sortby_menu .msisdn.active').length>0) msisdn = $jq('.sortby_menu .msisdn.active').html();
    //alert('aaaaaaaaaaaaaaaaaa');
    $jq('#notification-icon .noti-list').remove();
    
    var notificatonContainer = '<div class="top-list noti-list"><div class="tl-head">Notifications</div><div class="tl-items noti-items"></div><div class="tl-foot"><div class="tlf-row noti-footer"></div></div></div>';
    $jq('#notification-icon').append(notificatonContainer);
    $jq('.notifications_list').html('<img id="loading-image" src="https://www.telenor.com.pk/skin/frontend/telenor/default/images/FME/ajax-loader.gif">');
    if(notificationsListArr[msisdn])
    {
        addNotifications(notificationsListArr[msisdn]);
    }
    else
    {
        var url = "https://www.telenor.com.pk/username/ajax/notificationsList/";
        var data = "form_key=m5VO9LrOqiXADfPC&msisdn="+msisdn;
        jQuery.ajax({
            type: "POST",
            url: url,
            data: data,
            dataType: "json",
            success: function(data,textStatus,xhr)
            {   
                $jq('.notifications_list').html('');
                if(xhr.status == 200)
                {
                    if(data.status=="1")
                    {
                        notificationsListArr[msisdn] = data;                
                        addNotifications(data);
                    }
                    else
                    {
                        $jq('#notificationCount').hide();
                    }                                    
                }
                else
                {
                    $jq('#notificationCount').hide();
                }                            
            }        
        });     
    }
}
$jq('.my_num .msisdn').live('click',function(){
    notificationDetails();
});
function notificationDetails()
{
    var msisdn = 0;
    $jq('.notifications_list').html('');
    if($jq('.sortby_menu .msisdn.active').length>0) msisdn = $jq('.sortby_menu .msisdn.active').html();
    //alert('aaaaaaaaaaaaaaaaaa');
    $jq('#notification-icon .noti-list').remove();
    
    var notificatonContainer = '<div class="top-list noti-list"><div class="tl-head">Notifications</div><div class="tl-items noti-items"></div><div class="tl-foot"><div class="tlf-row noti-footer"></div></div></div>';
    $jq('#notification-icon').append(notificatonContainer);
    $jq('.notifications_list').html('<img id="loading-image" src="https://www.telenor.com.pk/skin/frontend/telenor/default/images/FME/ajax-loader.gif">');
    if(notificationsListArr[msisdn])
    {
        addNotificationCounter(notificationsListArr[msisdn]);
        addNotifications(notificationsListArr[msisdn]);
    }
    else
    {
        var url = "https://www.telenor.com.pk/username/ajax/notificationsList/";
        var data = "form_key=m5VO9LrOqiXADfPC&msisdn="+msisdn;
        jQuery.ajax({
            type: "POST",
            url: url,
            data: data,
            dataType: "json",
            success: function(data,textStatus,xhr)
            {   
                $jq('.notifications_list').html('');
                if(xhr.status == 200)
                {
                    if(data.status=="1")
                    {
                        notificationsListArr[msisdn] = data;                
                        addNotificationCounter(data);
                        addNotifications(data);
                    }
                    else
                    {
                        $jq('#notificationCount').hide();
                    }                                 
                }
                else
                {
                    $jq('#notificationCount').hide();
                }                            
            }        
        });     
    }
}
function addNotifications(data)
{
    var viewAll = '';
    var totalNotifications = parseInt(data['total_notifications']);
    var newNotifications = parseInt(data['new_notifications']);
    if(totalNotifications>0)
    {
        if(newNotifications>0)
        {
            $jq('#notificationCount').html(newNotifications);
            $jq('#notificationCount').show();
        } 
        else
        {
            $jq('#notificationCount').hide();
        }
        viewAll = '<a class="telenor_blue" href="https://www.telenor.com.pk/customer/ajax/viewnotifications/">View All</a>';        
    }
    else
    {
        $jq('#notificationCount').hide();
    }
    $jq('.tl-item.tl-noti').remove();
    for(var i=0; i<2; i++)
    {  
        /*var notificationHtml = '<li>';
        notificationHtml += '<span class="li_text">'+data['notifications'][i]['notification']+'</span>';            
        notificationHtml += '<span class="li_text">'+data['notifications'][i]['createddate']+'</span>';  
        notificationHtml += '</li>'; 
        $jq('.notifications_list').append(notificationHtml);*/
        var notificationHtml = '<div class="tl-item tl-noti">';
        notificationHtml += '<p>'+data['notifications'][i]['notification']+'</p>'; 
        notificationHtml += '<span class="li_date">'+data['notifications'][i]['createddate']+'</span>';
        notificationHtml += '</div>'; 
        $jq('.noti-list .noti-items').append(notificationHtml);
    }
    $jq('.noti-footer').html(viewAll);
}
function addNotificationCounter(data)
{
    //alert(data['total_notifications']);
    var viewAll = '';
    var totalNotifications = parseInt(data['total_notifications']);
    var newNotifications = parseInt(data['new_notifications']);
    if(newNotifications>0)
    {
        $jq('#notificationCount').html(newNotifications);    
        $jq('#notificationCount').show();    
    }
    else
    {
        $jq('#notificationCount').hide();
    }    
}
