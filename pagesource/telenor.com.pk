<script type="text/javascript">

$jq(document).ready(function(e) {
  //msg("hello");
  var current_screen_width;
  current_screen_width = $jq(window).width();
  var child = $jq("#slider_content > div").length;
  var not_single = false, is_small = false, is_medium = false, is_large = false, showthumbs=true;
  if ($jq('div#banner_slider').length>0) {
    if(child>1)
    {
      not_single= true;
      showthumbs=true;
    }else{
      not_single= false;
      showthumbs=false;
    }
  }
  if(current_screen_width<= 480 )
  {
    is_small = true;
    is_medium = false;
    is_large = false;
  }
    else if( current_screen_width > 480 && current_screen_width<= 1024 )
  {
    is_small = false;
    is_medium = true;
    is_large = false;
  } 
    else if( current_screen_width > 1024 )
    {
    is_small = false;
    is_medium = false;
    is_large = true;
    }
  resize_window();
  $jq(window).resize(function(){
       if($jq(window).width() != current_screen_width ){
            resize_window();
            current_screen_width = $jq(window).width();
         }   
    });
    function init_slider(device_type,height) {
        if ($jq('div#slider_content').length>0) {
        $jq('#banner_slider').camera({
                deviceType: device_type,
                autoAdvance: not_single,
                mobileAutoAdvance: not_single,
                playPause: false,
                pauseOnClick: false,
                height: height,
                loader: 'none',
                thumbnails: showthumbs,
                navigation: false,
                pagination: not_single,
                opacityOnGrid: false,
                onStartTransition: function () {
                    jQuery("#header .logo_large img").fadeOut();
                    //console.log('transition start');
                },
                onEndTransition: function () {
                    if (jQuery(".cameraContents .cameracurrent").find("span").hasClass("black")) {
                        jQuery(".logo_large img#black").fadeIn();
                        //console.log("black");
                    } else {
                        jQuery(".logo_large img#white").fadeIn();
                        //console.log("white");
                    }
                    //console.log('transition end');
                    
                }
            });
      }
    } 
    
    
    function resize_window() {
        if ($jq(window).width() <= 480 && is_small==true) {
        is_small = false; is_medium = true; is_large = true;showthumbs=false;
            $jq("#banner_slider").empty().html( $jq("#slider_content").html() );
            $jq("#nav_container").addClass("smartMenu");
        init_slider('mobile',"135%");
      }
      else if ( $jq(window).width() > 480  && $jq(window).width() <= 1024 && is_medium==true) {
        is_small = true; is_medium = false; is_large = true;showthumbs=false;
            $jq("#banner_slider").empty().html( $jq("#slider_content").html() );
            $jq("#nav_container").addClass("smartMenu");
        init_slider('tab','55%');
      }
      else if ($jq(window).width() > 1024 && is_large==true) {
            is_small = true; is_medium = true; is_large = false;if(not_single)showthumbs=true;
            $jq("#banner_slider").empty().html( $jq("#slider_content").html());
            $jq("#nav_container").removeClass("smartMenu");
        init_slider('desktop','590px');
      }
    }
});

