<script type="text/javascript">
	function setCookie() {
		var cname = "ad_cookie_banner",
		cvalue = 1,
		exdays = 365,
		d = new Date();
		d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
		var expires = "expires="+d.toUTCString();
		document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
		document.getElementById("cookiebanner").style.display = 'none';
	}
	var message = 'En poursuivant votre navigation, vous acceptez l’utilisation de cookies pour réaliser des statistiques de visites et vous proposer des publicités ciblées adaptés à vos centres d’intérêts.',
	lien = 'http://www.esam-ecoles.com' + '/en-savoir-plus-sur-les-cookies/',
	content = '<div class="cb-wrapper"><span class="text-content"><em class="icon-exclamation-sign"> </em><span>' + message + '</span></span><a class="more_link" href="' + lien + '" title="En savoir plus" target="blank" rel="nofollow">En savoir plus</a><a id="valid_cookie" class="button  " href="javascript:setCookie();" title="" rel="nofollow"><span>OK</span></a></div>';
	window.onload = function() {
		if (document.cookie.indexOf('ad_cookie_banner') == -1 ) {
			var parent = document.createElement("div");
			parent.id = "cookiebanner";
			parent.innerHTML = content;
			document.body.insertBefore(parent, document.body.childNodes[0]);
			window.onscroll = function() {setCookie();};
		}
	}
</script>
