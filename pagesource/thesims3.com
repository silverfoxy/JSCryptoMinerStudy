 */
function itemInCart(productId,userID)
{
	 if(isUserLoggedIn != null && isUserLoggedIn != false)
		 {
			addToCart(productId,isUserLoggedIn);
			refreshCartItemCount();
		 }
	else
		{
		addToSCCookie(productId,"s3s_sc");
