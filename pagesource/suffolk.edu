<script>
//YOUTUBE TRACKING
// ADD EVENT LISTENERS FOR PLAYED/PAUSED/ENDED videos
      var player;
      var ytFrames = document.querySelectorAll(".ytTrackVideo");
      
      function onYouTubeIframeAPIReady() {
        if($('.mod-slider').length > 0){
          $('.mod-slider').tinyRotary({"interval" : "0"});
        }
        
        ytFrames = document.querySelectorAll(".ytTrackVideo");
       
        for (var ik = ytFrames.length - 1; ik >= 0; ik--) {
          
          player = new YT.Player(ytFrames[ik], {
            events: {
              'onStateChange': onPlayerStateChange
            }
          });
        };
        
      }

      // 4. The API will call this function when the video player is ready.
      function onPlayerReady(event) {
        //event.target.playVideo();

      }

    
      var done = false;
      function onPlayerStateChange(event) {
          var video = event.target.a.id;

          switch (event.data) {
              case YT.PlayerState.PLAYING:
                  if (cleanTime(event.target) == 0) {
                      //console.log('started ' + cleanTime(event.target));
                      ga('send', 'event', 'video', 'started', video);
                  } else {
                      //console.log('playing ' + cleanTime(event.target))
                      ga('send', 'event', 'video', 'played', 'v: ' + video + ' | t: ' + cleanTime(event.target));
                  };
                  break;
              case YT.PlayerState.PAUSED:
                  if (event.target.getDuration() - event.target.getCurrentTime() != 0) {
                      //console.log('paused' + ' @ ' + cleanTime(event.target));
                      ga('send', 'event', 'video', 'paused', 'v: ' + video + ' | t: ' + cleanTime(event.target));
                  };
                  break;
              case YT.PlayerState.ENDED:
                  //console.log('ended');
                  ga('send', 'event', 'video', 'ended', video);
                  break;
          };
      };
      //utility
      function cleanTime(player){
          return Math.round(player.getCurrentTime())
      };
      function onPlayerError (event) {
        var video = event.target.a.id;
          switch(event.data) {
              case 2:
                  //console.log('Invalid ID' + video.id)
                  ga('send', 'event', 'video', 'invalid id',video);
                  break;
              case 100:
                  //console.log('not found' + video.id)
                  ga('send', 'event', 'video', 'not found',video);
                  break;
              case 101 || 150:
                  //console.log('not allowed' + video.id)
                  ga('send', 'event', 'video', 'not allowed',video);
                  break;
              };
      };
      function stopVideo() {
        player.stopVideo();
      }
