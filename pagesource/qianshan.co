          }
          if(isSafari=navigator.userAgent.toLowerCase().indexOf("safari")!=-1) { 
              return "safari"; 
          }  
          if(isOpera=navigator.userAgent.toLowerCase().indexOf("opera")!=-1){ 
              return "opera"; 
          }
        }
      });
    </script>
