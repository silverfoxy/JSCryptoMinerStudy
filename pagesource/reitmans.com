<script>
jQuery(document).ready(function() {
  function containsWord(elementClass, word) {
    return (elementClass).indexOf(word) !== -1;
  }

  // Array of element to track
  var elementsArray = [
    'global-navigation',
    'parent-slot',
    'header-bottom',
    'header-banner',
    'header-top-2',
    'homepage-slot-1',
    'homepage-slot-2',
    'homepage-slot-3',
    'homepage-slot-4',
    'homepage-slot-5',
    'homepage-slot-6',
    'homepage-slot-7',
    'homepage-slot-8',
    'homepage-slot-9',
    'AB-testing-top',
    'AB-testing-bottom',
    'category-top-branding-slot',
    'cat-landing-slotbanner',
    'sidenav-slot'
  ];

  for (var i = 0; i <= elementsArray.length; i++) {
    $('.' + elementsArray[i] + ' a').each(function() {
      var category = elementsArray[i];

      if (containsWord(category, "global-navigation")) {
        category = 'Global Navigation Banners';
      }
      if (containsWord(category, "header-bottom")) {
        category = 'Header-Bottom Banners';
      }
      if (containsWord(category, "header-top-2")) {
        category = 'Catfish Banners';
      }
      if (containsWord(category, "homepage-slot")) {
        category = 'Homepage Banners';
      }
      if (containsWord(category, "AB-testing-top")) {
        category = 'A/B Testing Top Banners';
      }
      if (containsWord(category, "AB-testing-bottom")) {
        category = 'A/B Testing Bottom Banners';
      }
      if (containsWord(category, "category-top-branding-slot")) {
        category = 'Category-Page Banners';
      }
      if (containsWord(category, "cat-landing-slotbanner")) {
        category = 'Feature-Page Banners';
      }
      if (containsWord(category, "sidenav-slot")) {
        category = 'Side-Nav Banners';
      }
      var href = $(this).attr('href');
      // Essential for GTM
      var id = $(this).data('id');
      var name = $(this).data('name');
      var creative = $(this).attr('title');
      var position = $(this).data('position');

      // Warnings for custom data
      //if (id === undefined || id === '') {
        //id = 'Campaign ID is undefined or empty';
      //}
      //if (name === undefined || name === '') {
        //name = 'Campaign Name is undefined or empty';
      //}
      //if (creative === undefined || creative === '') {
        //creative = 'Creative is undefined or empty';
      //}
      //if (position === undefined || position === '') {
        //position = 'Position is undefined or empty';
      //}

      // Console warnings
      //console.log(category);
      //console.log(href);
      //console.log(id);
      //console.log(name);
      //console.log(creative);
      //console.log(position);

      //$(this).attr('onClick', 'dataLayer.push({"event": "eventGA","eventCategory" : "' + category + '","eventAction" : "' + position + '","eventLabel" : "' + creative + ' (' + href + ')"})');

      app.gtm.promo.addPromotion(id, name, creative, position, $(this));
    });
  }
  // fire pushPromoView only when there is a trackable element
  for (var j = 0; j <= elementsArray.length; j++) {
    if($('div').hasClass(elementsArray[j])){
      app.gtm.promo.pushPromoView();
      break;
    }
  };
});
</script>

