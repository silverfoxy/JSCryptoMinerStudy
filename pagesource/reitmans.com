  var location = window.location.pathname;
  var directory = location.substring( location.lastIndexOf('/')+1 );
  var cleandirectory = directory.replace(/\.[^/.]+$/, "").toLowerCase();

  if (cleandirectory === "" || cleandirectory === "home-page" || cleandirectory === "home-show") {
    $("body").addClass("root");
  }
  else {
    $("body").addClass(cleandirectory+"-directory");
  };
});

// Quickview
$(document).ready(function($) {
  // Quickview
  $(".quick-view").click(function(event){
    var productsetURL = $(this).attr('href');
    if(!isMobile()){
      app.quickView.show({
        url: productsetURL,
        source: 'quickview'
      });
      return false;
    }
  });
});


// New Promo Pricing Script
function appendWordApplied() {

  if($("html").attr("lang") == "en"){
    $(".item-quantity-details .coupon-error").each(function () {
      $(this).text(" ").append("Your promo code is not valid.");
    });

    $(".promotional-message").each(function () {
      var stringPromoMessage = $(this).text();
      var stringToInclude = new RegExp("% Off");
      var stringToIgnore = new RegExp("Buy 1 Get 1");
      if(!$(this).hasClass("extra-word-applied") && stringToInclude.test(stringPromoMessage) && !stringToIgnore.test(stringPromoMessage)) {
        $(this).addClass("extra-word-applied").append(" applied");
      }
    });
    $(".promotionalMessage").each(function () {
      var stringPromoMessage = $(this).text();
      var stringToInclude = new RegExp("% Off");
      var stringToIgnore = new RegExp("Buy 1 Get 1");
      if(!$(this).hasClass("extra-word-applied") && stringToInclude.test(stringPromoMessage) && !stringToIgnore.test(stringPromoMessage)) {
        $(this).addClass("extra-word-applied").append(" applied");
      }
    });
  }
  else {

    $(".item-quantity-details .coupon-error").each(function () {
      $(this).text(" ").append("Votre code de promo est invalide.");
    });

    $(".promotional-message").each(function () {
      var stringPromoMessage = $(this).text();
      var stringToInclude = new RegExp("% de rabais");
      var stringToIgnore = new RegExp("Achetez-en 1");
      stringPromoMessage = stringPromoMessage.replace(' applique','');
      if(!$(this).hasClass("extra-word-applied") && stringToInclude.test(stringPromoMessage) && !stringToIgnore.test(stringPromoMessage)) {
        $(this).addClass("extra-word-applied").text(stringPromoMessage).append(" appliqué");
      }
    });

    $(".promotionalMessage").each(function () {
      var stringPromoMessage = $(this).text();
      var stringToInclude = new RegExp("% de rabais");
      var stringToIgnore = new RegExp("Achetez-en 1");
      stringPromoMessage = stringPromoMessage.replace(' applique','');
      if(!$(this).hasClass("extra-word-applied") && stringToInclude.test(stringPromoMessage) && !stringToIgnore.test(stringPromoMessage)) {
        $(this).addClass("extra-word-applied").text(stringPromoMessage).append(" appliqué");
      }
    });
  }
}

$(document).ready(function() {
$(".refinement .toggle").click(function() {
        $(this).toggleClass('see-options').parent().find('ul').toggle();
    });

  setTimeout(function() {
    appendWordApplied();
  },1000);

  $("#main").on('DOMNodeInserted', function(e) {
        var element = e.target;
        if ($(element).is('.search-result-items')) {
          appendWordApplied();
        }
    });
});
</script>

