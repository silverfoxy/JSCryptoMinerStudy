<script>
function getValue(key) {
    var query = window.location.search.substr(1).split("&");
    for (var i = 0; i < query.length; i++) {
        var param = query[i].split("=");
        if (param.length != 2) {
            continue;
        }
        if (param[0] == key) {
            return param[1];
        }
    }
}

var eventActionName = "x";

function emailBannerPop() {
if(typeof $.fancybox === 'undefined') {
        return;
    }
    $.fancybox($("#emailbannerhopup"), {
        padding: 0,
        autoSize: false,
        autoHeight: true,
        width: 686,
        maxWidth: 686,
        titleShow: false,
        type: 'inline',
        afterClose: function() {


            if (eventActionName === 'x') {
                $.cookie('RE_USA_hop_up', 'need_hopup', { path: '/' });
            } else {
                $.cookie('RE_USA_hop_up', 'delaymonth_hopup', { expires: 30, path: '/' });
            }

            eventActionName = "x";
            return;
        }
    });

}


$(document).ready(function() {

    var checkbannerpop = $.cookie('RE_USA_hop_up');
    var checkusersession = $.cookie('RE_user_USA_session');

    if (checkbannerpop == undefined) {
        $.cookie('RE_USA_hop_up', 'init_hopup', { expires: 365, path: '/' });
        checkbannerpop = $.cookie('RE_USA_hop_up');
    }

    if (checkusersession == undefined) {
        $.cookie('RE_user_USA_session', 'session_no_hopup', { path: '/' });
        checkusersession = $.cookie('RE_user_USA_session');
    }

    if (getValue("utm_medium") !== "email") {

        if (checkbannerpop === 'init_hopup' && checkusersession === 'session_no_hopup') {
            emailBannerPop();
            $.cookie('RE_user_USA_session', 'session_hopup_done', { path: '/' });
        } else if (checkbannerpop === 'need_hopup' && checkusersession === 'session_no_hopup') {
            emailBannerPop();
        }

    }

    $(".follow-hop-up").click(function() {
        $.cookie('RE_USA_hop_up', 'nomore_hopup', { expires: 3650, path: '/' });
    });

    $(".close-hop-up").click(function() {
        eventActionName = "x";
        $.fancybox.close();
        return false;
    });

});
