<script>

// Inserting word Extra
var langAttr = $('html').attr('lang');

function insertExtraWord(percentage) {
  $("#main").on('DOMSubtreeModified', function(e) {
    if (langAttr === "en") {
      $(".promotional-message:contains('" + percentage + "% Off')").each(function () {
        if(!$(this).hasClass("extra-word") && !$(this).is(":contains('Buy 1')")) {
          $(this).addClass("extra-word").prepend("Extra ");
        };
      });
    }
    else if (langAttr === "fr") {
      $(".promotional-message:contains('" + percentage + "% de rabais')").each(function () {
        if(!$(this).hasClass("extra-word") && !$(this).is(":contains('Achetez-en 1')")) {
          $(this).addClass("extra-word").append(" additionnel");
        };
      });
    };
  });
};

