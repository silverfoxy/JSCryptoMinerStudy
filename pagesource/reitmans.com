<script>
$(document).ready(function() {

    var catfishpop = "catfish-20180120";

    //Schema for Flexslider
    function runFlexslider() {
        $('.catfish-max').flexslider({
            animation: "slide",
            controlNav: false,
            minItems: 1.15,
            maxItems: 2.25,
            itemWidth: 340,
            itemMargin: 10
        });
    }

    function toggleCatfish() {
        $("#catfish-max").slideToggle(200).toggleClass("expanded");
        $(".catfish-container").toggleClass("catfish-overlay");
        $("#oo_float").toggleClass("hide_oo");
    }

    $("#catfish_trigger").click(function() {
        toggleCatfish();
        runFlexslider();
        return false;
    });

    $('.multi-catfish').on('click', function(event) {
        if (!$(event.target).closest('#catfish-max').length) {
            toggleCatfish();
            $.cookie(catfishpop, '1', {
                path: '/'
            });
            return false;
        }
    });

    if (!isMobile() && ($.cookie(catfishpop) != '1')) {
        toggleCatfish();
        runFlexslider();
    }

    //Inject cookie when clicked
    $('.btn, #catfish_trigger, .catfish-overlay').on('click', function(event) {
        if ($(event.target).closest('#catfish-max').length) {
            $.cookie(catfishpop, '1', {
                path: '/'
            });
        }
    });
});
</script>
