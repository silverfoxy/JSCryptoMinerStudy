<script type="text/javascript">
    var ALVar = ALVar || {};
    ALVar.TrialInfo = ALVar.TrialInfo || { '2' : { 'Title': 'PAID Membership','AltTitle': '','Description': 'Discover all the ways ArcLight Membership will enhance your moviegoing experience.','ExtendedDescription': 'Enjoy member only savings, events and many more benefits for only $15 annually.','OptionalDescription': '','CheckboxDescription': 'Yes, I want to sign up for ArcLight Membership. \u003ca href=\"/membership\"\u003eLearn more\u003c/a\u003e','MembershipPageDescription': '','AutoRenewDescription': 'Auto-renew my membership annually','Disclaimer': '' } };

    function onBigCheckBoxClicked() {
        var $bigCheckBox = $("#bigCheckBox");
        if (!$bigCheckBox.hasClass("checked")) {
            $bigCheckBox.addClass("checked");

            $('#formAutoRenew').show();

            showSignUpInformation();

            document.getElementById("panelMembershipShown").style.display = "block";
            document.getElementById("membershipPurchase").checked = true;

        }
        else {
            document.getElementById("bigCheckBox").className = "bigCheckBox";
            document.getElementById("panelMembershipShown").style.display = "none";
            document.getElementById("membershipPurchase").checked = false;
            document.getElementById("communityMembership").style.display = "none";
            document.getElementById("membership").style.display = "none";
            document.getElementById("trialMembership").style.display = "none";
            document.getElementById("divAutoRenewTrial").style.display = "none";
            document.getElementById("divAutoRenewMember").style.display = "inline";

            //$('#Terms').attr('checked', true);
            document.getElementById('Terms').checked = true;
            $('.terms').hide();
            $('#pnlTrialDisclaimer').css('display', 'none');
            $('#formAutoRenew').hide();
            $('#hMembershipType').val("");

            $('#divTrialCheckText').hide();
            $('#divMemberCheckText').show();
        }
    }

    function showSignUpInformation() {
        var $trialMembership = $('#trialMembership');
        var $divAutoRenewTrial = $("#divAutoRenewTrial");
        var $divAutoRenewMember = $("#divAutoRenewMember");
        var $communityMembership = $("#communityMembership");
        var $membership = $('#membership');
        var $pnlTrialDisclaimer = $('#pnlTrialDisclaimer');
        var $divTrialCheckText = $('#divTrialCheckText');
        var $ddlPreferredComplex = $('#ddlPreferredComplex');
        var locationId = $ddlPreferredComplex.val();

        if (ALVar && ALVar.TrialInfo[locationId.toString()]) {

            $trialMembership.find('.trialDesc').html(ALVar.TrialInfo[locationId].Description);
            $trialMembership.find('.trialDescExt').html(ALVar.TrialInfo[locationId].ExtendedDescription);
            $divTrialCheckText.html(ALVar.TrialInfo[locationId].CheckboxDescription.replace('<p>', '').replace('</p>', ''));
            $pnlTrialDisclaimer.html(ALVar.TrialInfo[locationId].Disclaimer);
            $divAutoRenewTrial.html(ALVar.TrialInfo[locationId].AutoRenewDescription);

            $trialMembership.show();
            $divAutoRenewTrial.css('display', "inline");
            $divAutoRenewMember.hide();
            $communityMembership.hide();
            $membership.hide();

            $divTrialCheckText.show();
            $('#divMemberCheckText').hide();

            $('#Terms').attr('checked', false);
            $('.terms').show();
            $pnlTrialDisclaimer.show();
            $('#hMembershipType').val($ddlPreferredComplex.find(':selected').data('membershiptype'));

        } else {
            $membership.show();
            $communityMembership.hide();
            $trialMembership.hide();
            $communityMembership.hide();
            $trialMembership.hide();
            $divAutoRenewTrial.hide();
            $divAutoRenewMember.css('display', 'inline');

            $('#Terms').attr('checked', false);
            $('.terms').show();
            $('#pnlTrialDisclaimer').css('display', 'none');
            $('#hMembershipType').val("");

            $('#divTrialCheckText').hide();
            $('#divMemberCheckText').show();
        }
    }

    $(document).ready(function () {
        document.getElementById("membership").style.display = "none";
        document.getElementById("communityMembership").style.display = "block";
        document.getElementById("trialMembership").style.display = "none";
        document.getElementById("divAutoRenewTrial").style.display = "none";
        document.getElementById("divAutoRenewMember").style.display = "none";

        $('#Terms').attr('checked', true);
        $('.terms').hide();
        $('#pnlTrialDisclaimer').css('display', 'none');
        $('#hMembershipType').val("");

        $('#divTrialCheckText').hide();
        $('#divMemberCheckText').show();

        $("#ddlPreferredComplex").change(function () {
            if ($("#bigCheckBox").hasClass('checked')) {
                showSignUpInformation();
            }
        });
    });
</script>

