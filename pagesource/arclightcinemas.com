<script>
    $(function () {

        function setLocation(location) {
            setTimeout(function () {
                var url = '/httphandlers/locationnavigationhandler.ashx';
                var data = {
                    option: 'desktop',
                    locationId: location,
                    contextId: '{4B124CAC-5186-4EBC-BAB3-823DD1CC3B19}'
                }
                var changeCallback = function (data) {
                    AL.Util.ajax.unmask();
                    if (data.Status == 'OK') {
                        if (data.ReturnData)
                            window.location = data.ReturnData;
                        else
                            window.location = window.location;
                    }
                };

                var changeCallbackError = function (data) {
                    AL.Util.ajax.unmask();
                    AL.Util.dialog.alert('msgChangeLocation', "There was an error changing your location.", 320, 200);
                }

                AL.Util.ajax.mask();
                AL.Util.createAjaxRequest(url, changeCallback, changeCallbackError, 'POST', data, 'JSON');

                AL.Util.cookie.setCookie("LocationSet", "1");
                //AL.Util.cookie.setCookie("AL_DefaultLocation", $('select.ddlLocation option:selected').val());

            }, 1000);
        }

        $('select.ddlLocation').selectmenu({
            width: '100%',
            height: '100%',
            appendTo: 'body',
            offset: '8px'
        });

        var locationSet = AL.Util.cookie.getCookie("LocationSet");
		
		console.log(locationSet);
		
        function displayLocationSelector() {
            if (locationSet != null) {
                $('.main-wrapper').removeClass('blur');
                $('.location-selector-overlay').hide();
            } else {
                $('select.ddlLocation').selectmenu("close");
                $('.main-wrapper').addClass('blur');
                $('<div class="location-selector-overlay"></div>').insertAfter($('.main-wrapper'));
                $('.location-selector-overlay').show();
                $('.main-wrapper.blur, .location-selector-overlay').on('click', function (e) {
                    e.preventDefault();
                });
                setTimeout(function () {
                    $('select.ddlLocation').selectmenu("open");
                    $('.ui-selectmenu-menu').data('og-left', $('.ui-selectmenu-menu').css('left'));
                    $('.ui-selectmenu-menu').data('og-top', $('.ui-selectmenu-menu').css('top'));
                    $('.ui-selectmenu-menu').css('left', $(window).width() / 2 - $('.ui-selectmenu-menu').width() / 2);
                    $('.ui-selectmenu-menu').css('top', $(window).height() / 2 - $('.ui-selectmenu-menu').height() / 2);

                    $('.main-wrapper').addClass('blur');
                    $('.ui-selectmenu-menu').prepend('<header><h3>Select Your Location</h3></header>');
                    $('.ui-selectmenu-menu').addClass('no-location-selected');

                    $('.ui-selectmenu-menu a').on('click', function (e) {
                        e.preventDefault();

                        AL.Util.cookie.setCookie("LocationSet", "1");
                        //AL.Util.cookie.setCookie("AL_DefaultLocation", $('select.ddlLocation option:selected').val());

                        var loc = $(this);

                        $('select.ddlLocation').selectmenu("close");

                        loc.parent().addClass('selected');
                        $('.ui-selectmenu-menu').css('left', $('.ui-selectmenu-menu').data('og-left'));
                        $('.ui-selectmenu-menu').css('top', $('.ui-selectmenu-menu').data('og-top'));
                        $('.ui-selectmenu-menu').addClass('location-selected');
                        $('.ui-selectmenu-menu').removeClass('no-location-selected');
                        $('.location-selector').removeClass('no-location-selected');
                        $('.main-layout').removeClass('blur');

                        if (loc.attr('aria-selected') == "true") {
                            setTimeout(function () {
                                setLocation($('select.ddlLocation option:selected').val());
                            }, 1000);
                        }
                    });
                }, 500);
            }
        }

        displayLocationSelector();

        setInterval(function() {
            // Is AL_DefaultLocation cookie not valid
            var location = AL.Util.cookie.getCookie("LocationSet");
            if (location == null && !$('.main-wrapper').hasClass('blur')) { // check if window is already open
                locationSet = null;
                displayLocationSelector();
            }
        }, 600000);

        
        $('.location-nav').on('change', '.ddlLocation', function (e) {
            var location = $(this);
            setLocation(location.val());
        });
    });

</script>
