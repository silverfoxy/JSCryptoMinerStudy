                                addSize([0,0],[ [1,1],[300,250],[800,600],[336,280],[300,600],[120,600],[160,600] ]).
                                build();
                            break;
                    }
                }else if(typeof(size_mapping) == 'object'){
                    default_size = size_mapping;
                }
            
                var slot = googletag.defineSlot(slotCode, default_size, elId);
                
