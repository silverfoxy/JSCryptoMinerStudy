    function pg_new_ad($el, slotCode, size_mapping, targeting) {

        if(is_brand || is_sensible)
           return false; 
            
        var ad_data = $el.data();
        
        if (typeof targeting !== 'object') { 
            targeting = {}; 
        }
        
        targeting['Tipo']       = 'home';
        targeting['Brand']      =  is_brand ? 'true' : 'false';
        targeting['id']         = '';
        targeting['Secciones']  = '';
        targeting['Microsite']  = 'playgroundmagdo';
        
        if("" == 1){
            targeting['Sensible']  = 'true';
        }
        
        if((pg_is_mobile && ad_data.mobile == '1') || (!pg_is_mobile && ad_data.desktop == '1') ){
            var elId = $el.attr('id'); 
            googletag.cmd.push(function() { 
                
                var mapping      = false,
                    default_size = [1,1]
            
                if(typeof(size_mapping) == 'string'){
                    switch(size_mapping){
                        case 'invideo':
                            mapping = googletag.sizeMapping().addSize([0,0],[ [1,1],[300,250],[336,280],[300,600] ]).build();
                            break;
                            
                        case 'intext':
