$(document).ready(function() {

	var video = $('.fullscreen-bg__video');

	video.prop('muted', true);

	$(".hero_audio").click( function (){

		if ($(this).hasClass('hero_audio--muted')) {
			$(this).removeClass('hero_audio--muted');
		} else {
			$(this).addClass('hero_audio--muted');
		}

    if( video.prop('muted') ) {
    	video.prop('muted', false);
    } else {
      video.prop('muted', true);
    }



  });
});

