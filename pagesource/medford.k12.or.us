    src: url('/cms/lib/OR01915738/Centricity/template/22/font/medford-extra.eot');
}
@font-face {
    font-family: 'medford-extra';
    src: url("data:application/x-font-ttf;charset=utf-8;base64,") format('truetype');
    font-weight: normal;
    font-style: normal;
}

[class^="dev-icon-"], [class*=" dev-icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'medford-extra' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.dev-icon-minus:before {
    content: "\e900";
}
.dev-icon-plus:before {
    content: "\e901";
}
