                    $(this).parent().wrap('<div class="ui-widget app blog-wrapper"></div>'); 
            }
            $('.ui-widget-header h1', this).append('<div class="tabbed-apps-cont-close-trigger"><i class="dev-icon-plus"></i></div>');
            
            $(this).appendTo($('#tabbed-apps-cont'));
        });
        
        $('#tabbed-apps-cont > div').hide();
        
