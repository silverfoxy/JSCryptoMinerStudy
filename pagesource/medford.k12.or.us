	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(left,  rgba(0,0,0,0.6) 0%, rgba(125,185,232,0) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(0,0,0,0.6)), color-stop(100%,rgba(125,185,232,0))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(left,  rgba(0,0,0,0.6) 0%,rgba(125,185,232,0) 100%); /* Chrome10+,Safari5.1+ */
