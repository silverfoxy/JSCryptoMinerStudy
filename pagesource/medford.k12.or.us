/* GroupBegin Mystart */
#sw-mystart-outer {
	width: auto;
	display: none;
}
#sw-mystart-inner {
	width: auto;
	max-width: 960px;
}
#sw-search-input {
	width: 145px;
}
.sw-mystart-dropdown .sw-dropdown-arrow {
	height: 14px;
	width: 17px;
	top: 2px;
	right: 1px;
	background: none;
	padding: 2px 0px 0px 6px;
	color: #999;
	font-size: 11px;
}
#sw-myaccount .sw-myaccount-chevron {
	background: none;
	font-size: 10px;
	color: #FFF;
	top: 5px;
	right: 6px;
}
#sw-mystart-account.clicked-state:hover .sw-myaccount-chevron {
	background: none;
	color: #000;
}

#gb-custom-mystart-outer{
	position: relative;
	width: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background: #696970;
	z-index: 9005;
}
#gb-custom-mystart .mystart-item{
	display: inline-block;
	padding: 0 20px;
	margin: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	height: 39px;
	vertical-align: middle;
}
#gb-custom-mystart .mystart-item:not(.home){
	padding: 13px 20px;
}
#gb-custom-mystart .right .mystart-item:not(.home){
	padding: 12px 5px;
}
#gb-custom-mystart .mystart-item.home{
	padding: 0 20px 0 9px;
}
#gb-custom-mystart a.home > span.hide320{
	vertical-align: middle;
}
#gb-custom-mystart .mystart-item,
#gb-custom-mystart .mystart-item > p{
	font: 600 14px/1 'Open Sans', sans-serif;
	color: #FFF;
	float: none;
	margin: 0;
	text-decoration: none;
	text-transform: uppercase;
}
#gb-custom-mystart a.home > span.logo-icon{
	position: relative;
	background: #FFF;
	display: inline-block;
	padding: 5px 6px 7px 0px;
	margin-right: 17px;
	vertical-align: middle;
}
#gb-custom-mystart a.home > span.logo-icon:before,
#gb-custom-mystart a.home > span.logo-icon:after{
	content: '';
	position: absolute;
	top: 0;
	bottom: 0;
}
#gb-custom-mystart a[alt='District Home'] > span:before{
	left: -10px;
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 21px 10px 18px 0;
	border-color: transparent #ffffff transparent transparent;
}
#gb-custom-mystart a[alt='District Home'] > span:after{
	right: -10px;
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 18px 0 21px 10px;
	border-color: transparent transparent transparent #ffffff;
}
div#gb-custom-mystart > div span.icon-font{
	color: #FFF;
	font-size: 11px;
	margin-left: 1px;
	top: -2px;
}
#gb-custom-mystart > .right .mystart-item.mystart-button{
	background: #FFF;
}
#gb-custom-mystart > .right .mystart-item.mystart-button > a{
	display: inline-block;
	padding: 2px;
	text-decoration: none;
	color: #9AA85B;
}
/* GroupEnd */ 
 
