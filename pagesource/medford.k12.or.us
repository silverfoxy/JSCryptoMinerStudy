        $(element).prepend("<div class='tabs-container' role='tablist'><div class='clear'></div></div>");

        // CREATE TABS AND SET ATTRIBUTES
        $("div.ui-widget.app", element).not($("div.ui-widget.app div.ui-widget.app", element)).each(function(i) {
            var myHeader = $("div.ui-widget-header h1", this).html();
            $(this).parents("div.region.ui-hp").find("div.tabs-container div.clear").before("<div id='cs-tab" + (i + 1) + "' class='tab-button' aria-controls='" + $("> div[id*='pmi']", element).eq(i).attr("id") + "' aria-selected='false' role='tab' tabindex='-1' data-index='" + i + "'><span class='tab-left'></span><span class='tab-right'>" + myHeader + "</span></div>");
