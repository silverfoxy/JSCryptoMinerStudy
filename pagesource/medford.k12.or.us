	background: -ms-linear-gradient(left,  rgba(0,0,0,0.6) 0%,rgba(125,185,232,0) 100%); /* IE10+ */
	background: linear-gradient(to right,  rgba(0,0,0,0.6) 0%,rgba(125,185,232,0) 100%); /* W3C */
}
