    		$( "#b_scroll" ).slider({
      			orientation: "vertical",
      			range: "min",
      			min: 16,
      			max: itemsWidth ,
      			value: itemsWidth ,
      			handle: '.handle',
      			stop: function (event, ui) {
           		ul.animate({ 'bottom': ui.value * -1 }, 500);

      			},
      			slide: function (event, ui) {
           		ul.css('bottom', ui.value * -1);
      		}
    		});


		

        					

