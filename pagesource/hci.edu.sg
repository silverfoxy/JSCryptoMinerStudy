	//This is for the banner news 
        $("#1").css("display", "block");
 
        //Timer
        var old = 1;
        var current = 1;
       
        var hover = 0;
        var nextAction = function (direction) {
            old = current;
             
            if(direction) {
              if (++current == max+1) current = 1;
	    }
            else
            {
		if (--current == 0) current = max;
            }

            $("#" + old).toggle("fade", {}, 800, function () { $("#" + current).toggle("fade"); });


        


        };
        //Timer Function
        var timer = $.timer(function () {  //Timer Effects

            nextAction(true);
        });

        var TimerStart = function () {
            timer.set({ time: 8000, autostart: true });
        };  //TimerStart

        var TimerPause = function () {timer.pause();};
        TimerStart();


        $("#newbanner").hover(function () {TimerPause();}, function () {TimerStart();});


        $(".b_prev").click(function () {
		TimerPause();
		nextAction(false);
		TimerStart();
	});
        $(".b_next").click(function () {
		TimerPause();
		nextAction(true);
		TimerStart();
	});

                                     
</script>


