            }
            else
            {
	            if(elm.attr('id')=='menu_alerte_bas'){
	                elm.load('/include/ajax/getMenu.php', '', function(){
		                var of = z.offset();
		                $('.menu_ouvert_js').slideUp();
		                elm.addClass('menu_ouvert_js').css({
		                    top: of.top + z.height() + 5,
		                    right: 0
	                    }).stop(true,true).slideDown();
	                    z.addClass('menu_parent_ouvert');
	                });
	            }
            }
        }
