	    $('.norc-toflive-elm').animate({top:'+=310'},500);
        }
        else
        {
            $('.menu-ui-opened').fadeOut('fast', function(){
		$('#container-menu-'+menu).addClass('menu-ui-opened').delay(50).css('display','block');
