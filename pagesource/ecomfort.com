function sayt(){
    $('#searchText').devbridgeAutocomplete({
        paramName: 'q',
        serviceUrl: '/sayt.php',
        maxHeight:'525px',
        triggerSelectOnValidInput:false,
        showNoSuggestionNotice:true,
        noSuggestionNotice:'<strong>No Results</strong><br /><i>Try Refining Your Search</i>',
        onSelect: function (suggestion) {
            $('#searchForm').submit();
        }
    });
}

