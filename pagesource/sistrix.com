                for (var j = 0; j < menu_entries.length; j++) {
                    var is_selected = menu_entries[j].className.match(new RegExp(/navigation-overlay-[a-z]+/, 'i'));
                    is_selected = is_selected && match.toString().split('-')[3] == is_selected.toString().split('-')[2];
                    menu_entries[j].style.opacity = is_selected ? 1 : 0;
