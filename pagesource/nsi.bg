		],
		links : [
			'/bg/content/1961/пътувания-на-българи-в-чужбина-и-посещения-на-чужденци-в-българия', '/bg/content/1961/пътувания-на-българи-в-чужбина-и-посещения-на-чужденци-в-българия', '/bg/content/1961/пътувания-на-българи-в-чужбина-и-посещения-на-чужденци-в-българия', '/bg/content/1961/пътувания-на-българи-в-чужбина-и-посещения-на-чужденци-в-българия', '/bg/content/1961/пътувания-на-българи-в-чужбина-и-посещения-на-чужденци-в-българия'
		]
	};
	var json = false, 
		chartData = false,
		chart = false,
		options = {
			width: $( "#cssmenu .cssmenu-left" ).width(), 
			height: 190, 
			title: 'Коефициенти на безработица', 
			legend: {
				position: 'none'
			},
			chartArea:{left:'8%',top:'6%',width:'90%',height:'59%'},
			hAxis: {
				textStyle: {
					color: '#9C3A47',
					fontName: 'Verdana',
					fontSize: 13
				},
				slantedText : true,
				slantedTextAngle : 30
			},
			vAxis: {
				textStyle: {
					color: '#9C3A47',
					fontName: 'Verdana',
					fontSize: 13
				}
			},
			titleTextStyle: {
					color: '#9C3A47',
					fontName: 'Verdana',
					fontSize: 15
			},
			tooltip: {
				textStyle: {
					color: '#9C3A47',
					fontName: 'Verdana',
					fontSize: 14
				}
			}
		};
	for(var i = 0; i < data.files.length; i ++) {
		if(data.files.hasOwnProperty(i) && data.names.hasOwnProperty(i)) {
			(function (i) {
				$.ajax({url: data.url + data.files[i], dataType:"json"}).done(function (result) {
					$('#cssmenu .cssmenu-left > ul').append('<li><a href="#">' + data.names[i] + '</a><ul class="chart-list"><li><div class="chart-data"></div></li></ul></li>');
