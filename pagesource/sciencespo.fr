	<script type="text/javascript" charset="utf-8">
	
		var likes;
		if(!likes){
			likes = new Array();
		}
		$(document).ready(function() {
			//gestion des like
			$(".likes").each(function(index) {
				$(this).click(function(event) {
					var nid = $(this).attr("id").replace('like_', '');
					if(likes.indexOf(nid) == -1){
						lookup(nid);
						likes.push(nid);
						$(this).children("img").unbind();
						var s = $(this).children("img").attr('src');
						if(s.indexOf("_on") == -1){
							var src = s.split('.');
							src[src.length - 2] = src[src.length - 2] + "_on";
							$(this).children("img").attr("src", src.join('.'));
						}
						
					}
				});
			});
			
			
		});
		function lookup(nid) {
			$.get("/ajax/like/" + nid, function(data){
				// console.log(data)
				if(data.length > 0) {
					if(data > 999){
						data = data / 1000;
						data = data.toFixed(1);
						data = data + 'K';
					}
					$('#like-counter_' + nid).html(data);
				}else{
					$('#like-counter_' + nid).html("0");
				}
			});
		}
		function loadNews(n){
			var offset = n;
			var lang = 'fr';
			if(lang == 'fr'){
				$.get("/actualites-plus/" + offset, function(data){
					if(data.length > 0) {
						$('#more-' + n).html(data);
					}else{
						$('#more-' + n).html("End of news");
					}
				});
			}else{
				$.get("/news-more/" + offset, function(data){
					if(data.length > 0) {
						$('#more-' + n).html(data);
					}else{
						$('#more-' + n).html("End of news");
					}
				});
			}
		}
		
		
		
	</script>
