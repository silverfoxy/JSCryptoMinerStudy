var iselect = 1;
var iindex = 1;
var reg_exp_name = /(\w+)\[(\d+)\]\[(\w+)\]/;
var reg_exp_id = /(\w+)_(\d+)/;
function add_more_items() {
  if (
    checkFormFields(document.form_items_quote)
  ) {
    var pack = $('#item_' + iselect).clone();
    pack.attr('id', 'item_' + ++iindex);
    var parse = '';
    pack.find('input,select,textarea,span,button').each(function() {
      var element_name = $(this).attr('name');
      if (element_name) { 
        parse = element_name.match(reg_exp_name);
        $(this).attr('name', parse[1] + '[' + iindex + '][' + parse[3] + ']');
      }
      var element_id = $(this).attr('id');
      if (element_id) {
        parse = element_id.match(reg_exp_id);
        $(this).attr('id', parse[1] + '_' + (iindex));
      }
    });
    pack.find('input,textarea').each(function() {
      $(this).css('border-color', '#bbb');
    });
    pack.appendTo('#items');
  //  select = index;
    pack.show();
    $("#ibtn_"+iindex).show();
    
    change_product($('#category_box_'+iindex).val(), 'category_box_'+iindex);
  }
}
function remove_more_items(tbl)
{
  var tbl_id = tbl.id;
  tbl_id = tbl_id.replace('irbut_', '');
  $("#item_"+tbl_id).remove();
}
