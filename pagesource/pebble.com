<script>
  function Loop(callback, delay) {
    this._delay = delay || 0;
    this._callback = callback;
    this._looping = false;
  }
  
  Loop.prototype.start = function() {
    if (!this._looping) {
      this._looping = true;
      this._loop()
    }
    return this;
  };
  
  Loop.prototype.stop = function() {
    this._looping = false;
    return this;
  };
  
  Loop.prototype._loop = function() {
    var _this = this;
    var requestAnimationFrame = window.requestAnimationFrame ||
                                window.mozRequestAnimationFrame ||
                                window.webkitRequestAnimationFrame ||
                                function(fn) { window.setTimeout(fn, 15); };
  
    if (_this._looping && this._delay) {
      window.setTimeout(function() {
        requestAnimationFrame(function() {
          _this._callback();
          _this._loop()
        });
      }, _this._delay);
    } else if (_this._looping) {
      requestAnimationFrame(function() {
        _this._callback();
        _this._loop()
      });
    }
  }
  
  
  function SmoothScroll(options) {
    var _this = this;
  
    _this._duration = 0;
    _this._start = 0;
  
    _this._target = options.target || 0;
    _this._speed = options.speed || 500; // Pixels per second
    _this._onBeforeStep = options.onBeforeStep || function() {};
    _this._onAfterStep = options.onAfterStep || function() {};
    _this._onDone = options.onDone || function() {};
    _this._clock = Date.now();
  
    _this._loop = new Loop(function() {
      var elapsed = Date.now() - _this._clock;
      var nextPosition = Math.floor(SmoothScroll._position(_this._start, _this._target, elapsed, _this._duration));
  
      _this._onBeforeStep.call(_this, nextPosition);
  
      window.scrollTo(0, nextPosition);
  
      _this._onAfterStep.call(_this);
  
      if (elapsed > _this._duration || _this._start === _this._target) {
        _this._onDone.call(_this);
        _this._loop.stop();
      }
    });
  }
  
  SmoothScroll.prototype.start = function() {
    this._start = window.pageYOffset;
  
    var distance = Math.abs(this._start - this._target)
  
    this._duration = distance / this._speed * 1000;
    this._loop.start();
  
    return this;
  };
  
  SmoothScroll.prototype.stop = function() {
    this._loop.stop();
    return this;
  };
  
  SmoothScroll._position = function(start, end, elapsed, duration) {
    if (elapsed > duration) return end;
    return start + (end - start) * SmoothScroll._easeInOutCubic(elapsed / duration);
