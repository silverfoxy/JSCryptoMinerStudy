    function parseXml(str) {
        var iedetect = 0;
        if (window.ActiveXObject || "ActiveXObject" in window) {
            var doc = new ActiveXObject('Microsoft.XMLDOM');
            doc.loadXML(str);
            return doc;
        } else if (window.DOMParser) {
            return (new DOMParser).parseFromString(str, 'text/xml');
        }
    }

    function doNothing() { }

    function downloadUrl2(url, callback) {
        var request = new XMLHttpRequest();
        request.onreadystatechange = function () {
            if (request.readyState == 4) {
                callback(request.responseText, request.status);
                request.onreadystatechange = doNothing;
            }
        };

        request.open('GET', url, true);
        request.send(null);
    }
    function SelectEF() {
        if (document.getElementById("Select1").value != "none" && parseInt(document.getElementById("Select1").value) > 0) {
            var valor = document.getElementById("Select1").value;
            var el = document.createElement('a');
            el.target = "blank";
            el.href = 'http://www.beta.inegi.org.mx/app/centrosinformacion/?ag=' + valor;
            document.body.appendChild(el);
            el.click();

        }
    }
    function seleccion(a) {
        idioma = "";
        valor = "";
        aux = "";
        if (document.getElementById('b_idi').value == 1) {
            //idioma += "lang=en";
        }
        if (a == "c") {
            if (document.getElementById('Select3').value != "none" && parseInt(document.getElementById('Select3').value) > 0) valor = document.getElementById("Select3").value;
        } else {
            valor = a;
        }
        if (valor != "") {
            if (idioma != "")
                idioma = "&" + idioma
            aux = "?ag=" + valor + idioma;
        } else {
            if (idioma != "")
                aux = "?" + idioma
        }
        top.location = 'http://www.beta.inegi.org.mx/app/areasgeograficas/' + aux;

    }

    function cambiaimagen(imag) { document.getElementById('republica').src = imag; }
    function abrir_ventana(theURL, winName, features) { window.open(theURL, winName, features); }

    function ban_lonuevo(ruta, elem1, elem2, elem3) {
        downloadUrl2(ruta, function (data) {
            var xml = parseXml(data);
            var markers = xml.documentElement.getElementsByTagName(elem1);
            var convec = 0;
            var imgcon = 0;
            var alt_img = "";
            for (var recel = 0; recel < markers.length; recel++) {
                for (var nodhijo = 0; nodhijo < markers[recel].childNodes.length; nodhijo++) {
                    var nodo = markers[recel].childNodes[nodhijo].nodeName;
                    switch (nodo) {
                        case "url":
                            var enlace = null;
                            enlace = markers[recel].childNodes[nodhijo].textContent;
                            if (enlace == undefined) {
                                enlace = markers[recel].childNodes[nodhijo].nodeTypedValue;
                            }
                            var param = enlace.split('?')[1];
                            if (param != undefined) {
                                param = param.replace(/[#]/gi, '&');
                                concatenacad = enlace.split('?')[0];
                                enlace = concatenacad + "?" + param;
                            }
                            var imghref = document.getElementById(elem2 + convec);
                            if (imghref != null)
                                imghref.href = enlace;
                            convec++;
                            break;
                        case "alt":
                            var alt_img = markers[recel].childNodes[nodhijo].textContent;
                            if (alt_img == undefined) {
                                alt_img = markers[recel].childNodes[nodhijo].nodeTypedValue;
                            }

                            imgalt = document.getElementById(elem3 + imgcon);
                            if (imgalt != null)
                                imgalt.alt = alt_img;
                            imgcon++;
                            break;
                    }

                }
            }
        });
    }

</script>
