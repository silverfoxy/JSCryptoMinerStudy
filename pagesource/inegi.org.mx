<script type="text/javascript">
    act = true;
    window.addEventListener("orientationchange", function () {


        if (jq13(window).width() > 767 && act) {
            cont('intera', '/img/home/banners/interactivo', 6, '.jpg');
            ejec_carga();
            act = false;
        }
    }, false);
    function c_wsv(r, a) {
        jq13.getJSON(r, function (data) {
            jq13.each(data, function (key, val) {
                switch (a) {
                    case "n":
                        jq13(".not_mov").append("<li><div class='con_mov_not'><a href='http://www.beta.inegi.org.mx/app/saladeprensa/noticia.html?id=" + val.IdNoticia + "' target='_blank'><img src='" + val.UrlImg + "' style='text-align:center; width:100%; height:100px;' alt='' /><p>" + val.Titulo + "</p><p style='font-size:7pt;'>" + val.FechaPublicacion + "</p></a></div></li>");
                        if (key < 3) {
                            jq13(".comunicados_home").append("<div class='row'><div style='font-family:arial;color:#666666;'>" + val.FechaPublicacion + "</div><div><b>" + val.Titulo + "</b></div><div style='font-family:arial;color:#666666;'>" + val.FechaInformacion + "</div><div>" + val.Descripcion + "</div><div style='text-align:right;'><a href='http://www.beta.inegi.org.mx/app/saladeprensa/noticia.html?id=" + val.IdNoticia + "' target='_blank' style='color:black;'>[+] Ver nota completa</a></div></div>");
                        }
                        break;
                    case "p":
                        jq13(".prod_mov").append("<li><div class='con_mov_not'><a href='" + val.url + "'><img src='" + val.imagen + "' style='text-align:center;' alt='' /><p>" + val.titulo + "</p></a></div></li>");
                        break;
                    case "s":
                        var l_sintesis = jq13("#sin_coyuntura");
                        var e_sintesis = jq13("#sin_carousel-inner");
                        var en = "";
                        if (val.enlace == "") {
                            en = val.descripcion_ind;
                        } else {
                            en = "<a href='" + val.enlace + "'>" + val.descripcion_ind + "</a>";
                        }
                        if (key == 0) {
                            l_sintesis.append("<li data-target='#sintesis_coyuntura' data-slide-to='0' class='active'></li>");
                            e_sintesis.append("<div class='item active'><div>" + en + "</div><div>Periodo:" + val.periodo + "</div><div>" + val.cifras + "</div></div>");
                        } else {
                            l_sintesis.append("<li data-target='#sintesis_coyuntura' data-slide-to='" + key + "'></li>");
                            e_sintesis.append("<div class='item'><div>" + en + "</div><div>Periodo: " + val.periodo + "</div><div>" + val.cifras + "</div></div>");
                        }
                }
            });
        });
    }
    c_wsv('http://www.beta.inegi.org.mx/app/saladeprensa/calendario/salaprensaws.asmx/ObtenerNoticias?idFechaPublicacion=0&tiposArchivos=0&aPartirDe=1&nMasElementos=3&mostrarHome=1', 'n');
    c_wsv('	http://www3.inegi.org.mx/sistemas/api/productos/interna_v1/listaBanner', 'p');
    c_wsv('http://www3.inegi.org.mx/sistemas/servicios/sintesis_coyuntura/s_coyuntura.asmx/sintesis_coyuntura', 's');
    var ani_carrusel;
    var oCurPhoto;
    function settimer() {
        i = setInterval(function () { r_ima('first'); }, 15000);
        return i;
    }
    function stopslider() {
        clearInterval(ani_carrusel);
    }
    ban_lonuevo('/lib/banner_home/xml/interactivo.xml', 'interactivo', 'ima_ban_interactivo', 'intera');
    function r_ima(a) {
        document.getElementById("activ_sig").value = "1";
        var photo = jq13("#photoShow div.current");
        var e_photo = "";
        switch (a) {
            case "first":
                e_photo = photo.next();
                break;
            case "last":
                e_photo = photo.prev();
                break;
        }
        if (e_photo.length == 0)
            e_photo = jq13("#photoShow div:" + a);
        photo.removeClass('current').addClass('previous').animate({ opacity: 0.5 }, 4500);
        e_photo.css({ opacity: 0 }).addClass('current').animate({ opacity: 1.0 }, 4000,
            function () {
                photo.removeClass('previous');
                document.getElementById("activ_sig").value = "";
            });

    }
    function act_b(a, b) {
        a.preventDefault();
        if (document.getElementById("activ_sig").value == "1") {
            a.stopPropagation();
            return false;
        }
        r_ima(b);
        stopslider();
        ani_carrusel = settimer();
    }


    jq13(document).ready(function () {
        ani_carrusel = settimer();
        jq13(".carrusel_siguiente").click(function (e) {
            act_b(e, 'first');
        });
        oCurPhoto = jq13('#photoShow div:first');
        jq13(".carrusel_anterior").click(function (e) {
            act_b(e, 'last');
        });
        if (jq13(window).width() > 767) {
            ejec_carga();
        }

        jq13("#photoShow div a").on("click", function () {
            _gaq.push(['_trackEvent', 'interactivo', 'interactivo', jq13(this).attr('href').replace('http://', '')]);
        });
    });
    function ejec_carga() {
        document.getElementById("inp").src = "http://www3.inegi.org.mx/sistemas/widget/inp_n/IndiceDePrecios.html?id=1";
        document.getElementById("teneco").src = "http://www3.inegi.org.mx/sistemas/widget/generagraficas.html?id=1";
        cont("img_ban_nuevo", "/img/home/banners/nuevo", 3, '.png');
        cont("img_ban_act_prod", "/img/home/banners/prodserv", 4, '.png');
        ban_lonuevo('/lib/banner_home/xml/lonuevo.xml', 'lonuevo', 'ima_ban_nuevo', 'ima_ban_nuevo');
        ban_lonuevo('/lib/banner_home/xml/actividades.xml', 'actividades', 'ima_ban_actividades', 'img_ban_act');
        ban_lonuevo('/lib/banner_home/xml/prodserv.xml', 'prodserv', 'ima_ban_prod', 'img_ban_act_prod');
    }
    function cambiaidioma() {
        i = document.getElementById('b_idi').value;
        if (i == 0) {
            document.getElementById('btnIdioma').src = "/img/home/btn_espanol.gif";
            document.getElementById('ImgTitulo').src = "/img/home/titmexicoinfigures.gif";
            document.getElementById('l_mcif').textContent = "Select the state on the map or in the following list:";
            document.getElementById('btn_Ir').value = "Go";
            document.getElementById('b_idi').value = 1;
        } else {
            document.getElementById('btnIdioma').src = "/img/home/btn_english.gif";
            document.getElementById('ImgTitulo').src = "/img/home/titmexicoencifras.gif";
            document.getElementById('l_mcif').textContent = "Seleccione la entidad en el mapa o en la siguiente lista:";
            document.getElementById('btn_Ir').value = "Ir";
            document.getElementById('b_idi').value = 0;
        }
    }
</script>
