<script>
function onLoad(items_list, items_count, item_width){
	$.each(items_list, function() {
		$(this).css({'right':(items_count-1)*item_width+'px'});
	});
	$('.next').css({'display':'none'});
}
	$(document).ready(function() {
	
	//Initialize 
	 onMoving			= false;
	 item				= $('.list li');
	 itemCount       = item.length;
	 itemReverse		= item.get().reverse();
	 itemDisplayed	= 1;
	 widthToSlide		= itemDisplayed * 800;
	 currentSlide	= itemCount;
	// on load
	
	onLoad(item, itemCount, widthToSlide);
	//Next button clicked
	$('.next').click(function() {

		if(onMoving || currentSlide >= itemCount) return false;
		onMoving = true, currentSlide++;		
		
		$.each(item, function() {
			
			var i 	 = $(this).index();
			var delay = 0;

			item.eq(i).stop().animate({ 'right' : '+='+widthToSlide+'px' }, function() {
              	if(i == itemCount - 1) onMoving = false;
					
			});
			
		});
		$('.prev').css({'display':'block'});
		if (currentSlide == itemCount) {
			$('.next').css({'display':'none'});
		}
	});
	
	//Previous button clicked
	$('.prev').click(function() {

		if(onMoving || currentSlide == 1) return false;
		onMoving = true, currentSlide--;
		
		$.each(itemReverse, function() {
			
			var i 	 = $(this).index();
			var delay = 0; 
			
			$(itemReverse).eq(i).stop().animate({ 'right' : '-='+widthToSlide+'px' }, function() {
				if(i == 0) onMoving = false;
			});
						
		});
		$('.next').css({'display':'block'});
		if (currentSlide == 1) {
			$('.prev').css({'display':'none'});		
		}
	});
});
</script>
        
