window.addEventListener('load', function()
{
    function uuid4() {
        //// return uuid of form xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx
        var uuid = '', ii;
        for (ii = 0; ii < 32; ii += 1) {
            switch (ii) {
                case 8:
                case 20:
                    uuid += '-';
                    uuid += (Math.random() * 16 | 0).toString(16);
                    break;
                case 12:
                    uuid += '-';
                    uuid += '4';
                    break;
                case 16:
                    uuid += '-';
                    uuid += (Math.random() * 4 | 8).toString(16);
                    break;
                default:
                    uuid += (Math.random() * 16 | 0).toString(16);
            }
        }
        return uuid;
    };

    function getUuid() {
        var clientId = Cookies.get('_ping_id');
        if (!clientId) {
            clientId = uuid4();
            Cookies.set('_ping_id', clientId, { expires: 30 });
        }
        return clientId;
    }

    function getPixel() {
        var img = document.createElement('img');
        img.setAttribute('style','display:none;');
        var ref = window.location.href;
        var uuid = getUuid();
        img.src = 'https://forum.xda-developers.com/ping.php?uuid='+uuid+'&ref='+encodeURIComponent(ref);
        document.body.appendChild(img);
    }

