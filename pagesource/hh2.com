          <div class="ContainerGroup clearfix" data-col-pos="0" id="u11944" data-sizePolicy="fixed" data-pintopage="page_fixedLeft"><!-- stack box -->
           <div class="Container clearfix grpelem" data-col-pos="0" id="u11946" data-sizePolicy="fixed" data-pintopage="page_fixedLeft"><!-- group -->
            <div class="clearfix grpelem shared_content" id="u28927" data-content-guid="u28927_content"><!-- group -->
             <div class="clip_frame grpelem" id="u11989"><!-- svg -->
              <img class="svg temp_no_img_src" id="u11990" data-orig-src="images/pasted-svg-41124x34.svg?crc=213808463" alt="" data-mu-svgfallback="images/pasted%20svg%2041124x34_poster_.png?crc=3975211736" data-heightwidthratio="0.8292682926829268" data-image-width="41" data-image-height="34" src="images/blank.gif?crc=4208392903"/>
