function trovacasaSend() {
	var overlay = $('#trovacasaOverlay');
	var params = {};
	params['name'] = $('#name2').val();
	params['surname'] = $('#surname2').val();
	params['email'] = $('#email2').val();
	params['phone'] = $('#phone2').val();
	params['message'] = $('#message2').val();
	params['ct'] = $('#ct_text3').val();
	params['cg'] = $('#cg_text3').val();
	var tp = [];
	$('#tp_list3 dd input:checked').each(function(){
		tp.push( $(this).attr('name').substr(2) )
	})
	params['tp'] = tp.join(",");
	if ( $('#tp').val() == "0" )
		params['tp'] = "";
	params['cmn'] = $('#cmn_text3').val();
	params['prv'] = $('#prv_text3').val();
	params['p1'] = $('#p1_text3').val();
	params['p2'] = $('#p2_text3').val();
	params['s1'] = $('#s1_text3').val();
	params['s2'] = $('#s2_text3').val();

	params['a'] = "trovacasa";

	overlay.addClass("highlight");
	overlay.find(".ptop, .body").hide();
	overlay.find(".request.sending").show();
	$.getJSON("ajax_query.php",params,function(data){
		overlay.find(".request.sending").hide();
		if ( data.result ) {
			overlay.find(".request.sent").show();
		} else {
			overlay.find(".request.failed").show();
		}
		setTimeout(function(){ $('.trovacasa:first').data("overlay").close(); }, 5000 );
	});

}

function trovacasaReset( ) {
	$('#trovacasaOverlay').find(".jdropdown input").each(function(){
		if ( $(this).data("jdropdown") )
			$(this).data("jdropdown").loadDefault();
	});
}

$(document).ready(function(){
    $('#privacy3').change(privacyRequired);

	$('.trovacasa').attr('rel','#trovacasaOverlay');
	$('.trovacasa').overlay({
		fixed: false,
		closeOnEsc: false,
		closeOnClick: false,
		onBeforeLoad: function() {
			var overlay = this.getOverlay();
			overlay.removeClass("highlight");
			overlay.find(".request, span.error").hide();
			overlay.find(".ptop, .body").show();
		},
        onClose: function() {
			trovacasaReset();
        }
	});
	$('#trovacasaOverlay .input.small .dropdown:not(.categoria,.provincia)').dropdown();
	$('#trovacasaOverlay .input.small .dropdown.categoria').dropdown({
		onInit: initCategoria,
		onChange: updateTipologia
	});
    $('#trovacasaOverlay .input.small .dropdown.provincia').dropdown({
        onChange: updateComune
    });

	setMandatory( $('#trovacasaOverlay') );
	$('#trovacasaOverlay .registerbtn').click(function(){
		if ( checkMandatory( $(this).parents('.popup_overlay'),'.input' ) ) {
			trovacasaSend();
		}
	});

    $('#p1_text3, #p2_text3').change(function(){
        var v = $(this).val();
        v = v.replace(/[^\d.,]/g, '');
        var idx = v.lastIndexOf('.');
        if(idx === v.length-3)
            v = v.replace(/\./g, 'A')
                 .replace(/,/g,  '.')
                 .replace(/A/g,  ',')
        $(this).val(v);
    });

	// DEBUG
	//$('.trovacasa:first').trigger("click");
});
