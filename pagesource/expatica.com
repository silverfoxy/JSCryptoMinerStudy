(function($) {

"use strict";

// current page host
var baseURI = window.location.host;
var linkHref = '';
var linkObj = [];
var isClickDefaultPrevented = false;

function openInNewTab(url) {
	var a = document.createElement("a");
	a.target = "_blank";
	a.href = url;
	a.click();
//	$("#OpenInNewTab").attr('href', url);
//	$("#OpenInNewTab").click();
	isClickDefaultPrevented = false;
}

// click event on body
$("body").on("click", function(e) {

	// abandon if link already aborted or analytics is not available
	if (isClickDefaultPrevented || typeof ga !== "function") return;

	// abandon if no active link or link within domain
	linkObj = $(e.target).closest("a");
//console.log(link);
	if (linkObj.length != 1 || baseURI == linkObj[0].host) {
//console.log("Return: "+linkObj[0].host);
		return;
	}

	// cancel event and record outbound link
	isClickDefaultPrevented = true;
	linkHref = linkObj[0].href;
	ga('send', {
		'hitType': 'event',
		'eventCategory': 'outbound',
		'eventAction': 'link',
		'eventLabel': linkHref,
//		'hitCallback': loadOutboundPage
	});

	// redirect after one second if recording takes too long
//	setTimeout(loadOutboundPage, 1000);

	// redirect to outbound page
//	function loadOutboundPage() {
		if (linkObj[0].target == "_blank") {
//console.log("Open In New Tab: "+linkHref);
			openInNewTab(linkHref);
		}
		else {
//console.log("open in same tab: "+linkHref);
			document.location = linkHref;
		}
//	}

});

})(jQuery);
</script>

	<!--a id="OpenInNewTab" href="#" target="_blank"></a-->
	</body>
</html>