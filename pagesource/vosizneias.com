function show_hidden_comments() {
	$('#comments.comments-hidden').show();
}

function hide_shown_comments() {
	$('#comments').addClass('comments-hidden').hide();
}

$(function()
{
	$('#show_hidden_comments').click(function() {
		show_hidden_comments();
		$('.hide-comments-notify .messages').hide();

		var responses = $('.hide-comments-notify .responses');
		responses.children().hide();
		responses.find('.response-show-once').show();
		responses.show();
