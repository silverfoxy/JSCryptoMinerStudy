    <script type="text/html" id="draw-block">
        <!-- ko if: $data instanceof LotteryCompany-->
        <div class="company-block">
            <div class="company-title">
                <a href="#" data-bind="attr: {href: permalink}, text: title"></a>
            </div>
        </div>
        <!-- /ko -->
        <!-- ko if: $data instanceof LotteryGame-->
        <div class="game-block " data-bind="css: (today ? '' : 'past')">
            <div class="game-info">
                <div class="game-logo">
                    <img src="" alt="" data-bind="attr: {src: logoUrl}">
                </div>
                <div class="game-details">
                    <a class="game-title" data-bind="text: title, attr: {href: permalink}"></a>
                    <div class="session-details">
                        <!-- ko if: skip == $root.getTodayDate() && skip != date  -->
                        <span class="session-badge">No Sorteo Hoy</span>
                        <!-- /ko -->
                        <!-- ko if: delay == $root.getTodayDate() && delay != date -->
                        <span class="session-badge">Retrasado</span>
                        <!-- /ko -->
                        <span class="session-date session-badge" data-bind="text: date"></span>
                    </div>
                </div>
            </div>
            <div class="game-scores" data-bind="css: result_view_mode ? 'text-mode' : 'ball-mode'">
                <!-- ko if: !result_view_mode  -->
                    <!-- ko if: score[0].constructor === Array  -->
                        <table>
                            <tbody>
                            <!-- ko foreach: score -->
                            <tr>
                                <!-- ko foreach: $data -->
                                <td>
                                    <span class="score" data-bind="text: $data.replace(/\?|\+|\=|\!/, ''), css: $root.getBallCss($data)"></span>
                                </td>
                                <!-- /ko -->
                            </tr>
                            <!-- /ko -->
                            </tbody>
                        </table>
                    <!-- /ko -->
                    <!-- ko if: score[0].constructor !== Array  -->
                        <!-- ko foreach: score -->
                <span class="score" data-bind="text: $data.replace(/\?|\+|\=|\!/, ''), css: $root.getBallCss($data)"></span>
                        <!-- /ko -->
                    <!-- /ko -->
                <!-- /ko -->
                <!-- ko if: result_view_mode -->
                <table>
                    <tbody>
                    <!-- ko foreach: score -->
                    <tr>
                        <!-- ko foreach: $data.split(" ") -->
                        <td>
                            <span class="score" data-bind="text: $data"></span>
                        </td>
                        <!-- /ko -->
                    </tr>
                    <!-- /ko -->
                    </tbody>
                </table>
                <!-- /ko -->
            </div>

            <!-- ko if: reference  -->
            <span data-bind="text: '#' + reference" class="session-badge"></span>
            <!-- /ko -->
            <!-- ko if: money  -->
            <span data-bind="text: money" class="session-badge"></span>
            <!-- /ko -->
        </div>
        <!-- /ko -->
    </script>



