<div class="earthquake-map" id="map-canvas" title="click for the Earthquake Data Dashboard">&nbsp;</div>
</div>
<script>
(function() {

    var $ = jQuery;

    var GrayLogMgr = window.ca.oceannetworks.dmas.GrayLogMgr();

    var gMap;
    var eqLoc = [];
    var bounds;


    var EARTHQUAKE_EVENT_WINDOW = 14;   // number of days previous to now

    function draftMap() {

       var mapCanvas = document.getElementById("map-canvas");

       var mapOptions = {
           // center: new google.maps.LatLng(17, -150),
              zoom: 1,
              draggable: false,
              scrollwheel : false,
              scaleControl : false,
              mapTypeControl : false,
              streetViewControl : false,
              navigationControl : false,
              navigationControlOptions : {
                 style : google.maps.NavigationControlStyle.DEFAULT
              },
              disableDefaultUI: true,
              mapTypeId : google.maps.MapTypeId.SATELLITE
           };

       gMap = new google.maps.Map(mapCanvas, mapOptions);

        eqLoc.forEach(function(eq, i, a) {
          var pos = new google.maps.LatLng(eq.lat, eq.lon);
          var eqMarker = new google.maps.Marker({
              position: pos,
              title: eq.place,
              map: gMap
          });
        });
        
      

    }
    function fitMarkers(){
        eqLoc.forEach(function(eq, i, a) {
          var markerLatLng = new google.maps.LatLng(eq.lat, eq.lon);
          bounds.extend(markerLatLng);
        });
        if(bounds){
            gMap.fitBounds(bounds);
        }
    }
    function formatIrisDate(date) {

       var str = date.toISOString().substring(0, 19);
       //console.log("formatIrisDate: " + str);

       return str;
    }
    function parseAjaxResponse(res) {

       var dat = res.split("\n");

       return dat[1].split("|");
    }
    function renderData(rec, zone) {

       GrayLogMgr.setFnName("renderData");
       GrayLogMgr.que({pos: "enter"});

       var dts = rec[1];

       var lat = rec[2];
       var lon = rec[3];

       var kmd = rec[4];

       var mag = rec[10];
       var loc = rec[12];

       var $eqnDON = $(".earthquakes-none");
       var mapCanvas = $("#map-canvas")[0];
       
       if (mag && loc) {

          GrayLogMgr.que({msg: zone + " values: mag = " + mag + ", loc = " + loc});

          var $eqzDON = $(".earthquakes-" + zone);

          $eqzDON.attr("class", "earthquakes-" + zone);
          $eqnDON.attr("class", "earthquakes-none earthquakes-data-none");

          var $magDON = $(".earthquakes-" + zone + " .earthquake-magnitude");
          var $locDON = $(".earthquakes-" + zone + " .earthquake-location");

          $magDON.html(mag);
          $locDON.html(loc);
          
          var dateAndTime = dts.replace("T", " &nbsp; ") + " UTC";
          $locDON.append("<br/>" + dateAndTime);

          $magDON.attr("title", "Depth: " + kmd + " km");
          //$locDON.attr("title", "Time: " + dts.replace("T", " @ ") + " UTC");
          $locDON.attr("title", "Latitude: " + lat + " & Longitude: " + lon);

          if (gMap) {
             if (zone === "global") {
                gMap.setCenter(new google.maps.LatLng(lat, lon));
             } else {
                //gMap.setCenter(new google.maps.LatLng((lat+49)/2, (lon-123)/2));   // Victoria. BC ~ (49, -123)
                var newLat = (parseFloat(lat) + 49)/2;
                var newLon = (parseFloat(lon) - 123)/2;
                gMap.setCenter(new google.maps.LatLng(newLat, newLon));   // Victoria. BC ~ (49, -123)
             }
             mapCanvas.style.backgroundImage = 'none';
             var eqMarker = new google.maps.Marker({
                position: new google.maps.LatLng(lat, lon),
                title: loc,
                map: gMap
             });
          } else {
             eqLoc.push({lat: lat, lon: lon, place: loc});
          }
          fitMarkers();
       } else {

         GrayLogMgr.que({msg: "NO " + zone + " values for mag && loc"});
       }
       GrayLogMgr.que({pos: "exit"});
    }
    function getEarthquakes(callback) {

       GrayLogMgr.setFnName("getEarthquakes");
       GrayLogMgr.que({pos: "enter"});

       var glpMsg = null;

       var minmagGlobal = 6;                       // earthquakes in the world
       var minmagLocal  = 4;                       // earthquakes in the west coast
       var maxmagLocal  = (minmagGlobal - 0.001)   // will allow us to avoid duplicity of local earthquakes with higher magnitud

       var latLocal = 49.0;
       var lonLocal = -127.0;

       var minRadius = 0.0;
       var maxRadius = 5.0;

       var pageNotFound = 404;

       var endTime   = new Date();                 // current date
       var startTime = new Date();                 // to hold the date from a day ago

       startTime.setDate(endTime.getDate() - EARTHQUAKE_EVENT_WINDOW);

       endTime   = formatIrisDate(endTime);
       startTime = formatIrisDate(startTime);

       var queryLocal = "http://service.iris.edu/fdsnws/event/1/query"
                             + "?starttime=" + startTime
                             + "&endtime=" + endTime
                             + "&minmag=" + minmagLocal
                             + "&maxmag=" + maxmagLocal
                             + "&orderby=time"
                             + "&format=text"
                             + "&lat=" + latLocal
                             + "&lon=" + lonLocal
                             + "&minradius=0.0"
                             + "&maxradius=5.0"
                             + "&nodata=" + pageNotFound;

       var queryGlobal = "http://service.iris.edu/fdsnws/event/1/query"
                             + "?starttime=" + startTime
                             + "&endtime=" + endTime
                             + "&minmag=" + minmagGlobal
                             + "&orderby=time"
                             + "&format=text"
                             + "&nodata=" + pageNotFound;

       GrayLogMgr.que({pos: "$.ajax queryLocal"});

       $.ajax({
           url: queryLocal,
           type: "GET",
           crossDomain: true,
           dataType: "text",
           success: function (data){renderData(parseAjaxResponse(data), "local");}
       });

       GrayLogMgr.que({pos: "$.ajax queryGlobal"});

       $.ajax({
           url: queryGlobal,
           type: "GET",
           crossDomain: true,
           dataType: "text",
           success: function (data){renderData(parseAjaxResponse(data), "global");}
       });

       GrayLogMgr.setFnName("getEarthquakes");
       GrayLogMgr.que({pos: "exit"});
       GrayLogMgr.post();
    }
    var eqMap = document.getElementsByClassName("earthquake-map")[0];
        eqMap.addEventListener("click", function(e) {

           window.location ="/data-tools/earthquake-data-dashboard";
        });

$(document).ready(function(){

       GrayLogMgr.setConsolePrint(false);
       GrayLogMgr.setAppName("earthquakeAlert");

       GrayLogMgr.setFnName("eqAlert $(document).ready");
       GrayLogMgr.que({pos: "enter"});

       draftMap();
       getEarthquakes(renderData);

       GrayLogMgr.setFnName("eqAlert $(document).ready");
       GrayLogMgr.que({pos: "exit"});
});

    console.log("Earthquake Alert self-called"); })(); </script>

