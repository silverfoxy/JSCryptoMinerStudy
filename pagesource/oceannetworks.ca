<script type="text/javascript">
(function() {

var GrayLogMgr = window.ca.oceannetworks.dmas.GrayLogMgr();

var JSONP = 6;

var DMAS_HOST = null;
var DRUPAL_HOST = null;
var SYS_STATUS_URI =  null;

var CURR_PROTOCOL = window.location.protocol;
var SERVICE_NAME = "GeneralSystemStatusService";
var TEST_IMAGE = "/design/images/logos/onclogo_74x74.png";

var WAIT = {"database" : 12345,
             "dataAcquisition" : 2345,
             "oceans2d0" : 1000
            };

var TITLE = {"up" : "Services responding within operational parameters.",
              "down" : "Services responding below operational parameters.",
              "warning" : "Services responding at minimum operational parameters."
             };

var oceans2Status = true;

function setStatusLight(data) {

    GrayLogMgr.setFnName("setStatusLight").que({msg: "enter"});

    if (data.payload) {

       var req = data.payload;
       var name = req.name;
       var status = req.status;

       var sslDON = jQuery(".dmas-system-status-" + name + " .dmas-system-status-light");

       if (oceans2Status) {

           sslDON.attr("class", "dmas-system-status-light " + status);
           sslDON.attr("title", TITLE[status]);
       }

       GrayLogMgr.que({msg: "data: name = " + name + " : status = " + status});
    } else {
       GrayLogMgr.que({msg: "error: " + data.message});
    }
    GrayLogMgr.que({msg: "exit"}).post();
}
function checkStatusLight(cssClassName, statusName) {

    GrayLogMgr.setFnName("checkStatusLight").que({msg: "enter for " + statusName});

    var sslDON = jQuery("." + cssClassName + " .dmas-system-status-light.inactive");

    if (sslDON.length > 0) {

        GrayLogMgr.que({msg: "DOWN FORCING statusName: " + statusName});

        sslDON.attr("class", "dmas-system-status-light down");
    }
    GrayLogMgr.que({msg: "exit for " + statusName}).post();
}
function getOceansStatus(statusName) {

    GrayLogMgr.setFnName("getOceansStatus").que({msg: "enter for " + statusName});

    var dts;
    var dif;
    var img = new Image();
        img.addEventListener("load", function() {

           dif = new Date().getTime() - dts;

           GrayLogMgr.que({systemsTimeDiff: dif});

           if (dif < WAIT[statusName]) {

              GrayLogMgr.que({msg: "get src load, good dif: " + dif});

              setStatusLight({"payload":{"name":statusName, "status":"up"},"statusCode":0});
           } else {
              GrayLogMgr.que({msg: "get src load,  bad dif: " + dif + " <<< >>> " + WAIT[statusName]});

              setStatusLight({"payload":{"name":statusName, "status":"warning"},"statusCode":0});
           }
        }, false);

        img.addEventListener("error", function() {

           GrayLogMgr.que({msg: "get src error"});

           setStatusLight({"payload":{"name":"database", "status":""},"statusCode":0});
           setStatusLight({"payload":{"name":"oceans2d0", "status":"down"},"statusCode":0});
           setStatusLight({"payload":{"name":"dataAcquisition", "status":""},"statusCode":0});

           // the oceans2Status MUST be set after the setStatusLight calls
           // otherwise no changed will happen

           oceans2Status = false;

        }, false);

    dts = new Date().getTime();

    var is = CURR_PROTOCOL + "//" + DMAS_HOST + TEST_IMAGE + "?dts=" + new Date().getTime();

    GrayLogMgr.que({msg: "img.src = " + is});

    img.src = is;

    GrayLogMgr.que({msg: "exit"}).post();
}
function getSystemStatus(cssClassName, statusName) {

    jQuery.getScript(SYS_STATUS_URI + statusName);

    setTimeout(function(){ checkStatusLight(cssClassName, statusName);}, WAIT[statusName]);
}

function tempStatusForDatabase(cssClassName, statusName) {
    setStatusLight({"payload":{"name":statusName, "status":"warning"},"statusCode":0});
}


jQuery(document).ready(function() {

    GrayLogMgr.setConsolePrint(false);
    GrayLogMgr.setAppName("systemStatus").setFnName("doc ready").que({msg: "enter"});

    DRUPAL_HOST = window.ca.oceannetworks.dmas.utilities.getCurrentHost();
    DMAS_HOST = window.ca.oceannetworks.dmas.utilities.getDmasHost();

    window.ca.oceannetworks.dmas.sysStatus = {};
    window.ca.oceannetworks.dmas.sysStatus.setStatusLight = setStatusLight;

    SYS_STATUS_URI = CURR_PROTOCOL + "//" + DMAS_HOST + "/" + SERVICE_NAME + "?responseType=" + JSONP + "&callback=window.ca.oceannetworks.dmas.sysStatus.setStatusLight" + "&name=";

    jQuery(".dmas-system-status-light").attr("class", "dmas-system-status-light inactive");

    setTimeout(function(){

       getOceansStatus("oceans2d0");
       getSystemStatus("dmas-system-status-database", "database");
       getSystemStatus("dmas-system-status-dataAcquisition", "dataAcquisition");

    }, 0);

    GrayLogMgr.setFnName("doc ready").que({msg: "exit"});
});

console.log("Sys Status self-called");

})();
