<script>
  GCurrentURL = (window.location.href).replace(/.loggedin.1/g,'');
  GCurrentURL = GCurrentURL.replace(/.RegLogout.1/g,'');
  GNnrStories = '-1';
  // Logout 
  function Logout() 
  {
    document.cookie="usernamepassword=;Path=/;expires=;"
    document.cookie="UserRegID=;Path=/;expires=;"
    document.cookie="LoggedIn=;Path=/;expires=;"
    document.location=GCurrentURL + "&RegLogout=1"
  }
  function getCookie(name) 
  {
    var dc = document.cookie;
    var prefix = name + "=";
    var begin = dc.indexOf("; " + prefix);
    if (begin == -1) 
    {
      begin = dc.indexOf(prefix);
      if (begin != 0) return null;
    }
    else
    {
      begin += 2;
      var end = document.cookie.indexOf(";", begin);
      if (end == -1) {
      end = dc.length;
      }
    }
    return unescape(dc.substring(begin + prefix.length, end));
  } 
  function setCookie(c_name,value,expiredays)
  {                          
     var exdate=new Date();
     exdate.setTime(exdate.getTime() + (expiredays*1000));
     domain_name_part =document.location.host;
     domain_name = domain_name_part.slice(2);
     document.cookie=c_name+ "=" +escape(value)+ ((expiredays==null) ? "" : ";expires="+exdate.toGMTString()+";domain=."+domain_name+";path=/");
  }
  // Check Url For Specific String
  function CheckUrlForSpecificString(AString)
  {
    var LUrl = String(window.location);
    if (LUrl.search(AString) != -1) 
      return true;
    else
      return false;
  }
  function SSOStatus(AContent)
  {
		$.ajax(
		{
			type: 'GET',
			url: '/apps/pbcs.dll/misc?forceuserreg=1&status=' + encodeURIComponent(AContent) ,
			success: function(){},
			error: function(){}
		});  	         
  }
  function SSOLogin(AForm,ALoadDiv)
  {
    if(AForm.user.value != "" && AForm.password.value != "")
    {
      $(ALoadDiv).html('<img src="/images/ajax-loader.gif"> einloggen...');
      $.ajax(
      {
        type: 'POST',
        data: { user: encodeURIComponent(AForm.user.value), password: encodeURIComponent(AForm.password.value) },
        url: '/misc/misc/SSOLogin.pbs' ,
        success: function(data)
        {
        	SSOStatus(data);
          var content = data || "no response text";
          if(content.substring(0,6)=="failed")
            window.location = "/fehler";
          if(content.substring(0,8)=="allready"||content.substring(0,8)=="loggedin")       
            window.location = GCurrentURL + "&loggedin=1";
          else
            $(ALoadDiv).html(content);
        },
        error: function(){ 
        	SSOStatus("errorinajaxcall");
        	window.location = "/fehler"; 
        }
      });
    }
  }
  if (CheckUrlForSpecificString('loggedin=1') || (getCookie("LoggedIn") != '' && getCookie("LoggedIn") != null))
  {  
    $.ajax(
    {
      url: '/misc/misc/sessioninfo.pbs&Nocache=1&mime=xml&d=' + (new Date()).getTime() ,
      success: function(data){
        try
        {
          var LxmlDoc = data || "no response text";
          var LDate = new Date();
          LDate.setTime(LDate.getTime() + (600 * 60 * 1000)); //timeout 10 hours
          var LReguser        = LxmlDoc.getElementsByTagName("reguser")[0].childNodes[0].nodeValue;
          var LUserid         = LxmlDoc.getElementsByTagName("userid")[0].childNodes[0].nodeValue;
          var LUseremail      = LxmlDoc.getElementsByTagName("loginname")[0].childNodes[0].nodeValue;
          var LUserfname      = LxmlDoc.getElementsByTagName("userfname")[0].childNodes[0].nodeValue;
          var LUserlname      = LxmlDoc.getElementsByTagName("userlname")[0].childNodes[0].nodeValue;
          if(LReguser != 'null')
          {
            if(LUseremail != 'null') $("#NCS_LoggedIn").html(LUseremail + '<div class="ph-mtn-i-logout-c" style="width:180px"><div class="ph-mtn-i-log-c-i"><p class="man"><a href="https://service.volksstimme.de/CentralAuthWeb/overview">Mein Profil</a></p><p class="man"><a href="javascript:Logout();">Logout</a></p></div></div></div></div>');
            $("#NCS_LoggedIn").show();
            $(document).ready(function() {
              $.cookie("LoggedIn", "1", { expires: 1, path: '/' });
            });
          }
          else
          {
            $("#NCS_LoggedOut").show();
            document.cookie="LoggedIn=;Path=/;expires=;"
            window.location = GCurrentURL;
          }
        }
        catch(err)
        {
          console.log("Error description: " + err.message);
        }
      },
      error: function(){ console.log('Something went wrong...') }
    });      
  }
  else
    $("#NCS_LoggedOut").show();
  if(CheckUrlForSpecificString('loggedin=0'))
  	$("#NCS_loginresult").html('<p class="ph-f-terr">Benutzername oder Passwort ist nicht korrekt.</p>');
</script>

