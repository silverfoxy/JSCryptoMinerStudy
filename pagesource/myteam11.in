    <script>
        function ValidateEmail(email) {
            var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
            return emailReg.test(email);
        };

        function getNewsletter() {

            var newsEmail = $('#newletterEmail').val();
            if (!ValidateEmail($('#newletterEmail').val())) {
                $("#myModalAlertnews").modal('toggle');
                $("#ContentId").html("Enter valid email address");

            }
            else {
                if (newsEmail != "") {
                    $.ajax({

                        type: 'POST',
                        url: '/Home/newletter',
                        data: { newsEmail: newsEmail },
                        success: function (message) {

                            // alert(message);
                            $('#newletterEmail').val('');
                            $("#myModalAlertnews").modal('toggle');
                            $("#ContentId").html(message);
                        },

                    });

                }
                else {
                    $("#myModalAlertnews").modal('toggle');
                    $("#ContentId").html("Enter email address");


                }
            }

        }


    </script>
