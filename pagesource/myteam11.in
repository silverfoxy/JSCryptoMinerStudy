        function GetAppLink() {
            var mobilenumber = $('#AppMobilenumber').val();
            var error = $('#AppMobilenumber').val().length;
            if (error == 10) {
                $.ajax({
                    url: "/Home/GetAppLink",
                    type: "POST",
                    data: { Mobile: mobilenumber },
                    success: function (data) {
                        if (data.lst == 1) {                          
                            SuccessAlert(data.lst1);
                        }
                        else {
                            ErrorAlert(data.lst1);

                        }
                    }
                });
            }
            else {
                ErrorAlert("Please enter 10 digit mobile number.");
                //$('#ErrorMobile').text('Please enter 10 digit mobile number.');

            }

        }
        function GetIOSAppLink() {
