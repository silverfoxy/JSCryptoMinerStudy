//Function to switch to the current selector circle
function updateSelector(){
	jQuery(".fa-circle-o").removeClass("fa-circle-o").addClass("fa-circle");
	current = this.currentSlide;
	var currentSelector = jQuery("#selector-" + current);
	currentSelector.toggleClass("fa-circle fa-circle-o");
}

//Function to switch both scrollable div and player to specified video
function updateVideoSelect(thisObj){
	
	if (single_camera_mode) {
		return true;
	}
	
	data = thisObj.parent().data('rel_data');
	if(thisObj.attr("class").indexOf('selected') == -1){
		jQuery(".homepage_player_thumb").removeClass('selected');
		thisObj.addClass('selected');
		homePagePlayer.switchAd(data.tile_id);
	}
	else{
		location.href = data.cam_url;
	}
}

