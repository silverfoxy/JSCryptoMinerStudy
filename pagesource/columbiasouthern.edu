<!-- Functionality to separate elements into columns in Main-Nav -->
<script>
  $(function($) {
    // accessibility tweaks ------------------------------
    $('img:not([alt])').attr('alt', 'image');
    $('iframe:not([title])').attr('title', 'resource');
    $('a:not([href])').attr('href', '#').addClass('faux-link');
    $('a:empty').remove();
    $('table:not(:has(caption))').prepend('<caption class="sr-only">Information / Data</caption>');
    $('table').removeAttr('role');
    
    var almostEmptyLinks = $("a").filter(function() {
      return $.trim($(this).text()) === "&nbsp;";
    }).text('<span class="sr-only">icon</span>');
  
    setTimeout(function(){
      $('a:empty').remove();
      $('iframe:not([title])').attr('title', 'resource');
      $( '#info-form' ).contents().find( 'body' ).prepend( '<h1 class="sr-only">Information Form</h1>' );
    }, 1500);
    
    $('.faux-link').click(function(e) {
      e.preventDefault();
    });
    
   // skip main navigation
    var skipLink = '<a id="skip-nav-link" class="skip-nav-link" href="#content-start">Skip Navigation</a>';
    $( 'body' ).prepend( skipLink );
    $('#skip-nav-link').attr('tabIndex', 1);
    $("#skip-nav-link").click(function () {
        $('#skip-nav-link').attr('tabIndex', -1).focus();
    });
    // skip mobile navigation -- looking at you Section 508 //
    var skipMobileLink = '<a id="skip-nav-mobile-link" class="skip-nav-link" href="#content-start">Skip Navigation</a>';
    $( '#csu-menu' ).prepend( skipMobileLink );
    
    
    // end accessibility tweaks ------------------------------------
    
    var num_cols = 2,
        container = $('.dropdown-menu'),
        listItem = 'li',
        listClass = 'col-md-6';
    container.each(function() {
      var items_per_col = new Array(),
          items = $(this).find(listItem),
          min_items_per_col = Math.floor(items.length / num_cols),
          difference = items.length - (min_items_per_col * num_cols);
      for (var i = 0; i < num_cols; i++) {
        if (i < difference) {
          items_per_col[i] = min_items_per_col + 1;
        } else {
          items_per_col[i] = min_items_per_col;
        }
      }
    for (var i = 0; i < num_cols; i++) {
      $(this).append($('<ul ></ul>').addClass(listClass));
      for (var j = 0; j < items_per_col[i]; j++) {
        var pointer = 0;
        for (var k = 0; k < i; k++) {
          pointer += items_per_col[k];
        }
        $(this).find('.' + listClass).last().append(items[j + pointer]);
      }
    }
  });
  });
</script>

