<script>
  var togglePaineis = function(){
    $('#enquete-resultado').toggleClass('hidden');
    $('#enquete').toggleClass('hidden');
  }
  var verParciais = function() {
    var painel = $('#enquete-resultado'); 
    if(!painel.is(':visible')) {
      togglePaineis();
    }
  }
  var computarParciais = function() {
    $.getJSON('./enquete-parcial.php', function(data){
      var percent = function(p, t, d=0) {
        return p?(p/t*100).toFixed(d):0;
      }
      var parciais = '';
      var total = data.pop();

      for(var i=0; i < data.length; i++) {
        var item = data[i];

        parciais += 
        '<strong>' + item.texto + ' ('+percent(item.hits, total.hits, 2)+'%)</strong>' +
          '<div class="progress">' +
            '<div class="progress-bar" role="progressbar" aria-valuenow="'+percent(item.hits, total.hits)+'" aria-valuemin="0" aria-valuemax="100" style="width: '+percent(item.hits, total.hits, 2)+'%">' +
            '<span class="sr-only">50% Complete</span>' +
           '</div>' +
        '</div>';
      }

      $('#enquete-parcial').html(parciais);
    });
  }

  $(document).on('click', '#enquete-parciais', function(){
    togglePaineis();
  });

  $(document).on('submit', '#enquete-form', function(event){
    event.preventDefault();

    var voto = $('[name=alternativa]:checked').val();
    if(!voto || !parseInt(voto)) {
      alert('Selecione uma resposta para a enquete!');
    }

    $.ajax({
      type: "POST",
      url: './enquete-votar.php',
      data: $(this).serialize(),
    }).done(function(data){
      alert(data);
      computarParciais();
      verParciais();
      $('#enquete-voto').removeClass('hidden')
    });
  });

  $(document).ready(function(){
    computarParciais();
  });
</script>
