<script type="text/javascript">
var UGCControllers = angular.module('UGCControllers', []);

UGCControllers.controller('UGCInitController', ['$scope', '$http', '$sce', function ($scope, $http, $sce) {
    $scope.streamData = []; //Will hold data returned from TB
    $scope.productArr = []; //Will hold list of UNIQUE sku's with info specified in fetchProducts.ds
    $scope.id         = '';
    
    $scope.initStream = function(id, count) {
        $scope.id = id;
        
        var streamVals = { 
            is_graphic: true, 
            limit: count 
        };
        
        if(count < 1) {//Not passing a limit will return 20 posts by default
            streamVals = { 
                is_graphic: true
            };
        }
        
        STR.init($scope.id);
        STR.records(streamVals, $scope.attachData);
    };
    
    $scope.attachData = function(records) {
        if(records.data.length > 0) {
            $scope.streamData = records.data;
            $scope.fetchProducts(records.data);
            $scope.$apply(); //Required to update ng-repeat
        }
    };
    
    //Marks passsed URL as secure source, so url can be used in angular binding
    $scope.trustSrc = function(src) {
        return $sce.trustAsResourceUrl(src);
    }
    
    //Run click event for all buy buttons in popup
    $scope.submitAll = function(id, skuArr) {
        for(var x = 0; x < skuArr.length; x++) {
            addProductToCart(id, skuArr[x], skuArr.length);
        }
        
        function addProductToCart(id, sku, num) {
            //Need to space out calls for addToCart or else only first call runs if all called at same time
            setTimeout(function() {
                $('#m-ugc-' + id + ' .add-to-cart.product-' + sku).click();
            }, num * 100);
        }
    };
    
    $scope.fetchProducts = function(data) {
        var productSkus = Array();
        
        for(var x = 0; x < data.length; x++) {
            $scope.pullSkus(data[x].labels, productSkus);
        }
        
        var fetchUrl = "/on/demandware.store/Sites-LushCA-Site/en_CA/TradableBits-UGCProducts";
        //console.log('do ajax call for: ', fetchUrl, JSON.stringify(productSkus));
        
        //Use jQuery ajax call because angular http was not passing data attribute to pipeline dictionary
        $.ajax({
            type: 'post',
            cache: false,
            url: fetchUrl,
            data: { result : JSON.stringify(productSkus) },
            success: function(resp) {
                $scope.productArr = resp;
                
                if($scope.productArr.length > 0) {
                    for(var x = 0;x < $scope.streamData.length;x++) {
                        //Add 2 new arrays to streamData for prodInfo and skus available to sell
                        $scope.streamData[x].productInfo = Array();
                        $scope.streamData[x].availSkus   = Array();
                        
                        //Loop through skus passed from TB
                        for(var y = 0; y < $scope.streamData[x].labels.length;y++) {
                            if($scope.streamData[x].labels[y] && $scope.streamData[x].labels[y].sku) {
                                var info      = $scope.getProductInfo($scope.streamData[x].labels[y].sku);
                                var available = $scope.checkAvailability($scope.streamData[x].labels[y].sku);
                                
                                $scope.streamData[x].productInfo.push(info);
                                if(available) {
                                    $scope.streamData[x].availSkus.push($scope.streamData[x].labels[y].sku);
                                }
                            }
                        }
                    }
                    
                    $scope.$apply(); //Required to update ng-repeat
                }
                
                $('.slick-slider__ugc-card-' + $scope.id).slick({
                    dots: false,
                    mobileFirst: true,
                    infinite: false,
                    arrows: false,
                    variableWidth: true,
                    speed: 150,
                    responsive: [{
                        breakpoint: 769,
                        settings: "unslick"
                    }]
                });
                
                if($(window).width() <= 768 ) {
                    //Modals break when inside slider container
                    //Clone and attach to body to prevent broken modals
                    var modals = $('.ugc-modal-container').clone();
                    $('.ugc-modal-container').remove();
                    modals.appendTo('body');
                }
            }
        });
    };
    
    //Loop through product array and match with passed ID to return product info
    $scope.getProductInfo = function(id) {
        for(var x = 0;x < $scope.productArr.length;x++) {
            if(id === $scope.productArr[x].sku) {
                return $scope.productArr[x];
            }
        }
    };
    
  //Loop through product array and match with passed ID to return if product is avaiable to sell
    $scope.checkAvailability = function(id) {
        for(var x = 0;x < $scope.productArr.length;x++) {
            if(id === $scope.productArr[x].sku) {
                return $scope.productArr[x].isAvailable;
            }
        }
    };
    
   //Loop through array and push UNIQUE skus to passed container
   //Prevents from storing the same information twice in array
    $scope.pullSkus = function(arr, container) {
        for(var x = 0; x < arr.length; x++) {
            if(arr[x].sku && container.indexOf(arr[x].sku) === -1) {
                container.push(arr[x].sku);
            }
        }
        return container;
    };
}]);
</script>
