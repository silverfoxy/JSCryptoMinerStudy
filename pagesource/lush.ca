<script>
/* taken from footer signup */
/**Mail sign up checkbox**/
   $('form.c-email-hero-3dd54f84fcf6a53215a59e14e5 input.eh_newsletter_confirm-3dd54f84fcf6a53215a59e14e5').change(function() {
       if(this.checked) {
           if($('.signuperror').length){
                 $('.signuperror').remove();   
            }
       }
   });
    
   function isValidEmailAddress(emailAddress) {
       var pattern = new RegExp(/^((([a-z]|\d|[!#$%&'\*\+\-\/=\?\^_`{\|}~]|[---])+(\.([a-z]|\d|[!#$%&'\*\+\-\/=\?\^_`{\|}~]|[---])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[---])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[---]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[---])|(([a-z]|\d|[---])([a-z]|\d|-|\.|_|~|[---])*([a-z]|\d|[---])))\.)+(([a-z]|[---])|(([a-z]|[---])([a-z]|\d|-|\.|_|~|[---])*([a-z]|[---])))\.?$/i);
       return pattern.test(emailAddress);
   };
   
   /**Mail sign up **/
 $('form.c-email-hero-3dd54f84fcf6a53215a59e14e5').submit(function(e){
    e.preventDefault();
    var confirmCheckBox = $('input.eh_newsletter_confirm-3dd54f84fcf6a53215a59e14e5');
    var emailInput = $('[name=hero_emailinput]');
    var emailAddress = emailInput.val();
    var llSubmit = $('button[name=c-email-hero]');
   
    if(emailInput.val() == '' || emailInput.val() == null ) {
          var inputArea = $(this);  
           if($('.signuperror').length){
             $('.signuperror').remove();   
           }
           var signuptext = $(document.createElement('div'));
           var copySignup = "Please enter your email address";
           signuptext.addClass('signuperror').text(copySignup); 
           signuptext.insertAfter(emailInput).fadeIn();  
           
           emailInput.focus();
           return false;
       }else{
           
           if(!isValidEmailAddress(emailAddress)){
              if($('.signuperror').length){
                     $('.signuperror').remove();   
                   }
                   var signuptext = $(document.createElement('div'));
                   var copySignup = "The email address is not valid. Please try again";
                   signuptext.addClass('signuperror').text(copySignup); 
                   signuptext.insertAfter(emailInput).fadeIn();  
                   emailInput.focus(); 
                   return false;
              }
           
       }
    
    
   
   if(confirmCheckBox.is(':checked')) {
          if($('.signuperror').length){
             $('.signuperror').remove();   
           }
          if($('.e-message').length){
                 $('.e-message').remove();    
               }
            
           var showparams = {
                   'ltkSubscriptionCode' : $('[name=ltkSubscriptionCode]',this).val(),
                   'maillistinput': $('[name=hero_emailinput]',this).val(),
                   'getsuccess': 'yes'     
               };
       
           var shortUrl = app.urls.newsletterAjax;
           var lushlifeURL = app.util.appendParamsToUrl(shortUrl,showparams);  
            $.getJSON(lushlifeURL, function(data) {
                var message = data.message;
                if(message == 'success'){
                   var copySignupHead = "Thanks!";
                   var copySignup = "This is the beginning of a beautiful friendship!";
                   var signuptextsuccess = $("<div/>").addClass('e-message').html('<h3 class="e-message__title">'+ copySignupHead +' </h3><p class="e-message__text">'+ copySignup +'</p>');       
                   signuptextsuccess.insertAfter(emailInput).fadeIn();
                   
                   setTimeout(
                             function() 
                             {
                                 $('input[name=hero_emailinput]').val('');
                                 $('input.eh_newsletter_confirm-3dd54f84fcf6a53215a59e14e5').attr('checked', false);
                                 signuptextsuccess.fadeOut();  
                               
                             }, 3500);
                    
                }else{
                    
                    var copySignup = "Sorry there was an error while signing up. Please try again";
                    var signuptextsuccess = $("<div/>").addClass('e-message signuperror').html('<p class="e-message__text">'+ copySignup +'</p>');        
                    signuptextsuccess.insertAfter(emailInput).fadeIn(); 
                    
                       setTimeout(
                                 function() 
                                 {
                                 signuptextsuccess.fadeOut();  
                                 }, 3000);
                       
                }
                
            });

        
    }else{
        var inputArea = $(this);    
           if($('.signuperror').length){
             $('.signuperror').remove();   
           }
           var signuptext = $(document.createElement('div'));
           var copySignup = "Please be sure to check the box to sign up for Lush emails.";
           console.log(copySignup); 
           signuptext.addClass('signuperror').text(copySignup); 
           signuptext.insertAfter(emailInput).fadeIn();  
    }
   return false;
   });
</script>
            
        
        
    
        
            
