<script type="text/javascript"> 

var timerContainer = $("#reserveCart");

function time_remaining(endtime){
	var t = Date.parse(endtime) - Date.parse(new Date());
	var seconds = Math.floor( (t/1000) % 60 );
	var minutes = Math.floor( (t/1000/60) % 60 );
	var hours = Math.floor( (t/(1000*60*60)) % 24 );
	var days = Math.floor( t/(1000*60*60*24) );
	return {'total':t, 'days':days, 'hours':hours, 'minutes':minutes, 'seconds':seconds};
}


 
function run_clock(id,idcount,endtime){
	
	var clockContainer = $('#'+id); 
	timerContainer.show();
	var clock = $('.'+idcount); 

	function update_clock(){
		var t = time_remaining(endtime);
		if(t.seconds < 10){ var preP = '0'; }else{var preP = '';}
		if(t.minutes < 1){var preM = '0';}else{var preM = '';}
		clock.text(preM+t.minutes+':'+preP+t.seconds);

		if( t.minutes > 4){
			timerContainer.removeClass('warn5');
			timerContainer.removeClass('warn6');
		}
		if( (t.minutes < 5 && t.minutes >= 1) && t.seconds <= 59 ){ 
			timerContainer.addClass('warn5');
			timerContainer.removeClass('warn6');
		}
		if( t.minutes < 1 && t.seconds <= 59  && !timerContainer.hasClass('warn6') ){ 
			timerContainer.removeClass('warn5');
			timerContainer.addClass('warn6');
		}
		if( t.minutes == 1 && t.seconds == 0 ){
			cartSession.extendDialog(); 
		}
		
		if(t.total<=0){ 
			window.clearInterval(window.timeInterval);
			cartSession.releaseDialog(); 
			timerContainer.hide();
		}
	}
	update_clock(); 
	window.timeInterval = setInterval(update_clock,1000);
}

/** Cart hold custom functions **/
var cartSession = {
		 sessionKeep: "https://www.lush.ca/on/demandware.store/Sites-LushCA-Site/en_CA/Cart-ReserveInventory",
		 extendSession: function()
			{
			 
			 $.ajax({
				  url:  "https://www.lush.ca/on/demandware.store/Sites-LushCA-Site/en_CA/Cart-ReserveReq", 
				  dataType: 'json', 
				  success: function(data){
					    app.dialog.close();
					    app.page.refresh();
					  
				        },
				        error: function(xhr) {
				           console.log("Error: " + xhr.statusText);
				       }
				});
			 
			 
			},
			confirmExtension: function (){		
					Dialog.confirm('Please Confirm', 'Knock, knock. Are you still there?<br/>', function(){ cartSession.extendSession(); });
					//Order.removePriceAdjustment(id);
			},
			extendDialog: function (){
				var pageIncludeUrl = app.urls.pageInclude
				,pageparams = {
				    'cid' : 'extend-hold',
				    'format': 'ajax'
				};

			   var limiturl =  app.util.appendParamsToUrl(pageIncludeUrl,pageparams);

				app.dialog.open({url:limiturl, options:{
				    width:500,
				    height:'auto',
				    modal: false,
				    dialogClass : 'reserve-pop',
				    title:"Attention"
				}});
				
			   $("body").on("click", "#extend-hold", function (e) {	
			  		e.preventDefault();
			  		cartSession.extendSession();
			  		return false;
			  	});
			   
			  },
			  releaseDialog: function (){
					var pageIncludeUrl = app.urls.pageInclude
					,pageparams = {
					    'cid' : 'extend-release',
					    'format': 'ajax'
					};

				   var limiturl =  app.util.appendParamsToUrl(pageIncludeUrl,pageparams);

					app.dialog.open({url:limiturl, options:{
					    width:550,
					    height:'auto',
					    modal: false,
					    dialogClass : 'reserve-pop',
					    title:"Attention"
					}});
				   
				  }
}

$(function () {	

	
	
	if("true" != "true" && "null" != "null"){
	    var expireTime = parseInt("10.0") * 60;
	    
	    var expireTimeSec = parseInt("10.0");
	   
	  //10 minutes from now
	    var time_in_minutes = expireTime;
	    var current_time = Date.parse(new Date());
	    var deadline = new Date(current_time + time_in_minutes + expireTimeSec * 1000);
	    for (var i = 1; i < 3; i++)
	        window.clearInterval(window.timeInterval);
	    run_clock('reserveCart','timer',deadline);	    
	}
	
	if("true" == "true"){$("#reserveCart").hide() }
	
	
	if(window.timeInterval){$("#reserveCart").show()}
	
});



</script>




