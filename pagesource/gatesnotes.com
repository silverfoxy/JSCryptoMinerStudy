    function TGNSR_LoadTheSRs() {
        TGNSR_Json_Pool = [];
        $("#Auth_SR_View_HolderR1").html("");
        $("#Auth_SR_View_HolderR2").html("");
        if (localStorage.getItem('UserInterest_Topic_List_CSV') === null || localStorage.getItem('UserInterest_Topic_List_CSV') == "," || localStorage.getItem('UserInterest_Topic_List_CSV') == "") {
            // fill from all
            TGNSR_UseAllInterests();
        } else {
            // fill from specific interest lists
            TGNSR_AggregateTheInterests();
        }
    }
    function TGNSR_RemoveDuplicatesAndCurrentPage() {
        // Dupes
        var TGNSR_IDarray = [];
        for (var i = 0; i < TGNSR_Json_Pool.length; i++) {
            if (jQuery.inArray(TGNSR_Json_Pool[i].ArticleID,TGNSR_IDarray) == -1) {
                // add to array
                TGNSR_IDarray.push(TGNSR_Json_Pool[i].ArticleID);
            } else {
                // remove from pool
                TGNSR_Json_Pool.splice(i, 1);
            }
        }
    }
    function TGNSR_LoadIntListItems() {
        TGNSR_RemoveDuplicatesAndCurrentPage();
        var intList = TGNSR_Json_Pool;
        for (var i = 0; i < 4; i++) {
            var $TGN_SR_SlideTemplate = $('#TGNSR_SR_ItemTemplate').clone();
            $TGN_SR_SlideTemplate.attr("id","TGNSR_"+intList[i].ArticleID+"_"+i);
            $TGN_SR_SlideTemplate.attr("data-articleID",intList[i].ArticleID);
            $("#Auth_SR_View_HolderR1").append($TGN_SR_SlideTemplate);
            $TGN_SR_SlideTemplate.attr("href",intList[i].ArticleUrl);
            $TGN_SR_SlideTemplate.find(".Auth_SR_Title").html(intList[i].SearchHeadline);
            $TGN_SR_SlideTemplate.find(".Auth_SR_Teaser").html(intList[i].SearchBlurb);
            $TGN_SR_SlideTemplate.find(".Auth_SR_Tags").html(intList[i].Tags);
            $TGN_SR_SlideTemplate.find("canvas").css("background-image", "url('"+intList[i].ImageUrl+"')");
            $TGN_SR_SlideTemplate.find("canvas").attr("data-url", intList[i].ImageUrl);
            $TGN_SR_SlideTemplate.find("canvas").attr("data-dateattr", intList[i].RollOverDate);
            $TGN_SR_SlideTemplate.find("canvas").attr("data-dateval", intList[i].Date);
            $TGN_SR_SlideTemplate.find("canvas").attr("data-ctaattr", intList[i].BlogrollCTATag);
            $TGN_SR_SlideTemplate.find("canvas").attr("id","TGN_SR_SlideimageL"+i);
        }
        for (var i = 4; i < 8; i++) {
            var $TGN_SR_SlideTemplate = $('#TGNSR_SR_ItemTemplate').clone();
            $TGN_SR_SlideTemplate.attr("id","TGNSR_"+intList[i].ArticleID+"_"+i);
            $TGN_SR_SlideTemplate.attr("data-articleID",intList[i].ArticleID);
            $("#Auth_SR_View_HolderR2").append($TGN_SR_SlideTemplate);
            $TGN_SR_SlideTemplate.attr("href",intList[i].ArticleUrl);
            $TGN_SR_SlideTemplate.find(".Auth_SR_Title").html(intList[i].SearchHeadline);
            $TGN_SR_SlideTemplate.find(".Auth_SR_Teaser").html(intList[i].SearchBlurb);
            $TGN_SR_SlideTemplate.find(".Auth_SR_Tags").html(intList[i].Tags);
            $TGN_SR_SlideTemplate.find("canvas").css("background-image", "url('"+intList[i].ImageUrl+"')");
            $TGN_SR_SlideTemplate.find("canvas").attr("data-url", intList[i].ImageUrl);
            $TGN_SR_SlideTemplate.find("canvas").attr("data-dateattr", intList[i].RollOverDate);
            $TGN_SR_SlideTemplate.find("canvas").attr("data-dateval", intList[i].Date);
            $TGN_SR_SlideTemplate.find("canvas").attr("data-ctaattr", intList[i].BlogrollCTATag);
            $TGN_SR_SlideTemplate.find("canvas").attr("id","TGN_SR_SlideimageL"+i);
        }
    }

    function TGNSR_UseAllInterests() {
        TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_SavingLives.ArticleList)
        TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_EnergyInnovation.ArticleList)
        TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_ImprovingEducation.ArticleList)
        TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_Philanthropy.ArticleList)
        TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_BookReviews.ArticleList)
        TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_AboutBillGates.ArticleList);
        TGNSR_Json_Pool = TGNSR_Json_Pool.sort(TGNSR_custom_sort);
        TGNSR_LoadIntListItems();
    }

    // sort AL by date
    function TGNSR_custom_sort(a, b) {
        return new Date(b.Date).getTime() - new Date(a.Date).getTime();
    }

    function TGNSR_AggregateTheInterests() {
        var TGNSR_myIntList = localStorage.getItem('UserInterest_Topic_List_CSV');
        if (TGNSR_myIntList.indexOf('Saving Lives') > -1) {
            TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_SavingLives.ArticleList);
        }
        if (TGNSR_myIntList.indexOf('Energy Innovation') > -1) {
            TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_EnergyInnovation.ArticleList);
        }
        if (TGNSR_myIntList.indexOf('Improving Education') > -1) {
            TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_ImprovingEducation.ArticleList);
        }
        if (TGNSR_myIntList.indexOf('Philanthropy') > -1) {
            TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_Philanthropy.ArticleList);
        }
        if (TGNSR_myIntList.indexOf('Book Reviews') > -1) {
            TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_BookReviews.ArticleList);
        }
        if (TGNSR_myIntList.indexOf('About Bill Gates') > -1) {
            TGNSR_Json_Pool = TGNSR_Json_Pool.concat(TGNSR_Json_AboutBillGates.ArticleList);
        }
        TGNSR_Json_Pool = TGNSR_Json_Pool.sort(TGNSR_custom_sort);
        TGNSR_LoadIntListItems();
    }
