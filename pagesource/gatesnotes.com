    function TGN_CheckJSONForQuoteMatch(JSON, lookfor) {
        //console.log(JSON + JSON.viewedItems.length + lookfor);
        for (var index = 0; index < JSON.viewedQuoteIDs.length; ++index) {
            var item = JSON.viewedQuoteIDs[index];
            //console.log(item.viewedQuoteID + "   " + lookfor);
            if (item.viewedQuoteID == lookfor) {
                return true;
            }
        }
        return false;
    }
    function TGNF_MarkQuoteAsViewed() {
        $("#TGN_Footer_QuoteTemplate").each(function (i, obj) {
            if ($(this).is(':visible') && TGNFQisOnScreen($(this))) {
                if ($(this).attr("data-sent") != "sent") {
                    // Handle Quote View Local Storage
                    if (localStorage.getItem("TGNViewedQuoteIDs") === null) {
                        storedQuoteIDs = {};
                        storedQuoteIDs.viewedQuoteIDs = viewedQuoteIDs;
                        var viewedQuoteID = {
                            "viewedQuoteID": $("#TGN_Footer_QuoteTemplate").attr("data-guid")
                        }
                        storedQuoteIDs.viewedQuoteIDs.push(viewedQuoteID);
                        localStorage.setItem("TGNViewedQuoteIDs", JSON.stringify(storedQuoteIDs));
                    } else {
                        var quotealreadyviewed = TGN_CheckJSONForQuoteMatch(storedQuoteIDs, $("#TGN_Footer_QuoteTemplate").attr("data-guid"));
                        if (!quotealreadyviewed) {
                            var viewedQuoteID = {
                                "viewedQuoteID": $("#TGN_Footer_QuoteTemplate").attr("data-guid")
                            }
                            storedQuoteIDs.viewedQuoteIDs.push(viewedQuoteID);
                            localStorage.setItem("TGNViewedQuoteIDs", JSON.stringify(storedQuoteIDs));
                        }
                    }
                    // send view to AI
                    if (TGNUserID != null && TGNUserID != "") {
                        appInsights.trackEvent
                            ("Quote Viewed",
                                // String properties:
                                    { ID: TGNPageID, QUOTE: $(this).attr("data-guid"), USERID: TGNUserID, EMAIL: TGNUserEMAIL },
                                // Numeric metrics:
                                    { UNIT: 1 }
                            );
                    }
                    $(this).attr("data-sent", "sent");
                }
            }
        });
    }
    function TGNFQisOnScreen(element) {
        var curQPos = element.offset();
        var curQTop = curQPos.top;
        var screenHeight = $(window).height();
        //console.log("curQTop:" + curQTop + " screenHeight:" + screenHeight);
        if (curQTop < screenHeight) {
            return true;
        } else {
            return false;
        }
    }
