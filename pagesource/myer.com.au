            var htmlLevelContent = ((level1[i].innerText).trim()).toLowerCase();
            if (htmlLevelContent === category) {
                var elementParent = $('#' + level1[i].id).parent().parent();
                var listHolderCol = elementParent.find('.listHolder .listCol');
                for (j = 0; j < listHolderCol.length; j++) {
                    $(listHolderCol[j]).find('.navHeading')
                        .bind('click', false)
                        .css('pointer-events', 'none');
                }
            }
        }
    }
