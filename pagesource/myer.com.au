					wc.render.declareRefreshController({
						id: "ExperienceControllerRefresh_Controller",
						renderContext: wc.render.getContextById("ExperienceControllerRefresh_Context"),
						url: "",
						formId: "",
					    
						renderContextChangedHandler: function(message, widget) {
							var controller = this;
							var renderContext = this.renderContext;
							var status = message.status;
							if (controller.testForChangedRC(["status"])) {
								renderContext.properties["status"] = status;
								widget.refresh(renderContext.properties);
							}
						},
								
						postRefreshHandler: function(widget) {
							var controller = this;
							var renderContext = this.renderContext;
							if (dojo.byId("displayNewExperience") && dojo.byId("displayNewExperience").value === "true") {
								dojo.publish("createExperience", [true]);
							} else {
								dojo.publish("createExperience", [false]);
							}
							cursor_clear();
						}
					});
					
					dojo.addOnLoad(function() {
						dojo.destroy("displayNewExperience"); 
						
						var waitForWidget = function() {
							clearTimeout(waitForWidget);
							
							if(dijit.byId("MyerExperienceControllerESpot") ) {
								var refreshInput = dojo.byId("refreshExperience"); 
								
								if (refreshInput) {
									switch (refreshInput.value) {
										case "true":
											dojo.publish("changeExperience", [true]);
											break;
										case "false":
											dojo.publish("changeExperience", [false]);
											break;
										case "remove":
										case "delete":
											dojo.publish("changeExperience", [null]);
											break;
									}
								} else if (wc.render.getRefreshControllerById("ExperienceControllerRefresh_Controller") != null && (common.ExperienceController.getExperience() == common.ExperienceController.UNKNOWN_STATE)) {
									var params = {
										emsName: "ExperienceControllerESpot",
										pageSize: "",
										pageView: "",
										langId: "-1",
										storeId: "10251",
										catalogId: "10051"
									};
									wc.render.getRefreshControllerById("ExperienceControllerRefresh_Controller").url = getAbsoluteURL() + "AjaxExperienceControllerView?" + dojo.objectToQuery(params);
									wc.render.updateContext("ExperienceControllerRefresh_Context", {status:"load"});
								}
							}
							else {
								setTimeout(waitForWidget, 20);
							}
						};
						waitForWidget(20);
					});
				</script>
				
				
