}

function infoWindow(marker, map, title, address, url, slug) {
    google.maps.event.addListener(marker, 'click', function () {
        var html = "<div class='infowindow'><h4>" + title + "</h4>" +
                "<p>" + address + "<br>" +
                "<a class='loc-mp-btn' href='" + url + "'>View location</a>" +
                "<a href='.?store="+ slug +"' class='shp-mp-btn'>Shop now</a></p></div>";
        iw.setContent(html);
        iw.open(map, marker);
    });
}
