            maxWidth: 350
        });

function setMarker(locations, i) {
    var title = locations[i][0];
    var address = locations[i][1];
    var url = locations[i][2];
    var myLatLng = new google.maps.LatLng( locations[i][3], locations[i][4] );
    var slug = locations[i][5];

    var marker = new google.maps.Marker({
        icon: 'https://maps.google.com/mapfiles/ms/icons/blue.png',
        map: map,
        position: myLatLng,
        title: title,
        animation: google.maps.Animation.DROP,
        address: address,
        url: url
    })
    infoWindow(marker, map, title, address, url, slug);
    bounds.extend(marker.getPosition());
