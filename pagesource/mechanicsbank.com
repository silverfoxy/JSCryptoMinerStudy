<script>
function getHeader(name)
{
  var req = new XMLHttpRequest();
  req.open('GET', document.location, false);
  req.send(null);
  var response = req.getAllResponseHeaders().toLowerCase().trim();
  var items = response.split("\n");
  var header = [];
  for(var i = 0; i < items.length; i++) {
    var parts = items[i].split(': ');
    header[parts[0].trim()] = parts[1].trim();
  }

  var ret = header[name] || false;
  return ret;
}
function addFooterImage(id)
{
	var elem = document.createElement("img");
	var imgNumber = getHeader('x-mbk');
	if (imgNumber) {
		elem.setAttribute("src", "/images/num-" + imgNumber + ".png");
		elem.setAttribute("height", "2");
		elem.setAttribute("width", "10");
		elem.setAttribute("alt", imgNumber);
		document.getElementById(id).appendChild(elem);
	}
}
addFooterImage('footerImage');
</script>

