<script type="text/javascript">
var productDataFromSubIndustry = "";
function searchProductFromSubIndustry(subindustryid){
	productDataFromSubIndustry = "";
	$("#subCategoryFirstUL").empty();
	$("#subCategorySecondUL").empty();
	$("#txtProduct").val("");
	$("#txtSubIndustryId").val(subindustryid);
	$.ajax({
		type : 'GET',
		url : baseUrl +"/ajax/getproductfromsubindustry",
		data : {
			txtSubIndustry : subindustryid
		},
		success : function(data) {
			productDataFromSubIndustry = data.response;
			var firstLiItems = "";
			var secondLiItems = "";
			$("#subCategoryFirstUL").empty();
			$("#subCategorySecondUL").empty();
			for (var i = 0; i < data.response.length; i++) {
				var productId = data.response[i].ProductId;
				var productName = data.response[i].ProductName;
				var str = replaceAll(' ', '+', productName);
				if(i % 2 == 0){
					firstLiItems+= '<li><span><a href="/category/'+str+'+Tenders">'+productName+'</a></span></li>';
				}else{
					secondLiItems+= '<li><span><a href="/category/'+str+'+Tenders">'+productName+'</a></span></li>';					
				}
			}
			$("#subCategoryFirstUL").append(firstLiItems);
			$("#subCategorySecondUL").append(secondLiItems);
		},
		error : function(XMLHttpRequest, textStatus, errorThrown) {
			alert(textStatus);
		},
		dataType : "json"
	});
}
function replaceAll(find, replace, str) {
	return str.replace(new RegExp(find, 'g'), replace);
}
function searchCriteria(n,t){
	$("#subCategoryFirstUL").empty();
	$("#subCategorySecondUL").empty();
	var count = 0;
	$.each(productDataFromSubIndustry, function(idx, obj) {
		if(obj.ProductName.toLowerCase().indexOf(t.toLowerCase()) >= 0){
			count++;
			var str = replaceAll(' ', '+', obj.ProductName);
			var firstLiItems = "";
			var secondLiItems = "";
			if(count % 2 == 0)
				secondLiItems+= '<li class="sub-location"><span class="grey_button"><a href="/location/'+str+'+Tenders">'+obj.ProductName+'</a></span></li>';
			else
				firstLiItems+= '<li class="sub-location"><span class="grey_button"><a href="/location/'+str+'+Tenders">'+obj.ProductName+'</a></span></li>';
				
				
			$("#subCategoryFirstUL").append(firstLiItems);
			$("#subCategorySecondUL").append(secondLiItems);
		}
	});
}
$( document ).ready(function() {
	$("#txtProduct").keyup(function() {
        var n = $("#txtProduct").val();
        searchCriteria("sub-category", n)
    })
});
</script>
