<!-- jQuery Form Validation code -->
<script>
	function setCookie(cname, cvalue, exdays) {
		var d = new Date();
		d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
		var expires = "expires=" + d.toUTCString();
		document.cookie = cname + "=" + cvalue + "; " + expires;
	}

	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for (var i = 0; i < ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == ' ')
				c = c.substring(1);
			if (c.indexOf(name) == 0)
				return c.substring(name.length, c.length);
		}
		return "";
	}

	
	
	

	// When the browser is ready...
	$(function() {
		$('#myModal').on('shown.bs.modal', function() {
			$('#txtEEmailID').focus();
		});

		// Setup form validation on the #register-form element
		$("#login-form").validate({
			// Specify the validation rules
			rules : {
				txtEEmailID : {
					required : true,
					email : true,
					maxlength : 500
				},
				txtPPassword : {
					required : true,
					password : true,
					minlength : 6,
					maxlength : 30
				}
			},

			// Specify the validation error messages
			messages : {
				txtPPassword : {
					required : 'Please enter Password'
				},
				txtEEmailID : {
					required : 'Please enter E-mail ID',
					email : 'Enter valid E-mail ID '
				}
			},
			// Do not change code below
			errorPlacement : function(error, element) {
				error.insertAfter(element.parent());
			},
			submitHandler : function(form) {
				submitLogin();
			}
		});

	});

	function submitLogin() {
		$('#LoginLoadingImage').show();
		$('#spanLoginErrorMsg').hide();
		if ($.trim($("#txtEEmailID").val()) != '') {
			jQuery.ajax({
				type : "POST",
				url : "/ajax/submitlogin",
				data : {
					txtEmailID : $("#txtEEmailID").val(),
					txtPassword : $("#txtPPassword").val()
				},
				dataType : "text",
				success : function(data) {
					if (data.indexOf("~") != -1) // Success
					{
						if ((data.split("~")).length >= 4) {
							setCookie("UserQueryWord_" + data.split("~")[2], data.split("~")[3], 1);
						}
						$('#spanLoginErrorMsg').hide();
						var companyServiceIds = data.split("~")[1];
						//alert(companyServiceIds);
						companyServiceIds = companyServiceIds.split(',');
						var serviceId = "0";
						for (var i = 0; i < companyServiceIds.length; i++) {
							if (companyServiceIds[i] == "1") // Indian Tenders
							{
								serviceId = "1";
								break;
							} else if (companyServiceIds[i] == "2") // Global Tenders
							{
								serviceId = "2";
								break;
							} else if (companyServiceIds[i] == "3") // Tenders Result
							{
								serviceId = "3";
								break;
							}
						}

						//	alert(serviceId);
						if (serviceId == "1") {
							$('#cmbSearchIn').val("1");
							$('#tenderListing_form').attr('action','/tenderListing');
						} else if (serviceId == "2") {
							$('#cmbSearchIn').val("2");
							$('#tenderListing_form').attr('action','/tenderListing');
						} else if (serviceId == "3") {
							$('#tenderListing_form').attr('action','/tenderResultListing');
						}
						$('#tenderListing_form').submit();
					} else if (data == 'invalid_credential') {
						$('#LoginLoadingImage').hide();
						$('#spanLoginErrorMsg').show();
						$('#spanLoginErrorMsg').html("Invalid EmailID or Password.");
					} else if (data == 'service_not_activated') {
						$('#LoginLoadingImage').hide();
						$('#spanLoginErrorMsg').show();
						$('#spanLoginErrorMsg').html("Only Subscriber is allowed to Login. Subscribe our service to access Lacs of Tenders.");
					} else if (data == 'service_expired') {
						$('#LoginLoadingImage').hide();
						$('#spanLoginErrorMsg').show();
						$('#spanLoginErrorMsg').html("Your Subscription period is Over. Renew your service to login.");
					} else if (data == 'service_inacive') {
						$('#LoginLoadingImage').hide();
						$('#spanLoginErrorMsg').show();
						$('#spanLoginErrorMsg').html("Your service is inactive now. Call us 9512 247 247 for more detail.");
					} else {
						$('#LoginLoadingImage').hide();
						$('#spanLoginErrorMsg').show();
						$('#spanLoginErrorMsg').html("Error occurred while processing the request. Please try again later.");
					}
				},
				async : false
			});
		}
	}
</script>
