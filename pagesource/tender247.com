function checkUserStatus(){
	  var result=null;
	  registrationDone=false;
	  var emailIdExistStatus="0";
	  var emailIdExist=false;
	  if($.trim($("#txtPEmailID").val())!= '' && $("#payonline-form").valid()){
		$('#PayOnlineLoadingImage').show();
 		jQuery.ajax({
			  type: "POST",
			  url: baseUrl+"/ajax/checkEmailIdExists",
			  data:{ txtEmailID : $('#txtPEmailID').val(), cmbCompanyService : $("#cmbCompanyServiceID").val()},
			  dataType :"json",
			  success: function(data) 
			  {
				  result=data;
				  if(data!= null && data.status_code != '103')
				  {
					  emailIdExist = true;
					  emailIdExistStatus=$.trim(data.status_code);
            	  }
				  
 	         },
		 	 async: false
		 	 
	    });
 		 
	     if(emailIdExistStatus == '109')
		 {
		      window.location= '/ExistingSubscriber';
		 }
		 else if(emailIdExistStatus == '110')
		 {
			 window.location= '/SubscriberActivationPening';
		 }
		 else if(emailIdExistStatus == '111')
		 {
			 //orderDetails=fetchSubscriptionOrderDetailsForPayOnline(result.UserId);
			 if(result.PaymentUrl!=null)
			 {
				 window.location= baseUrl+"/"+result.PaymentUrl;
			 }
			 else
			 {
				 window.location= '/PayNow';
			 }
		 }
		 else if(emailIdExistStatus == '100')
		 {
			window.location= '/subscribeuser';					
		 }
		 else
		 {
			 window.location= '/subscribeuser';	
		 }
 	  }
	  return true;
  }
  
function fetchPayOnlineClientLocation(){
	var clientCountryCode = getCookie("tender247ClientCountry").toString();
	if(clientCountryCode == null || clientCountryCode == '' || typeof clientCountryCode == 'undefined'){
		jQuery.ajax( {
			  url: 'http://ip-api.com/json/',
			  type: 'POST',
			  async: "false",
			  dataType: 'jsonp',
			  success: function(location) {
			    // If the visitor is browsing from Canada.
				  clientCountryCode = location.countryCode;
				  setCookie("tender247ClientCountry",clientCountryCode,7);
			  }
			} );
	}
	if(clientCountryCode == 'IN' || clientCountryCode == null || clientCountryCode == '' || typeof clientCountryCode == 'undefined'){
		$("#companyServiceSection").css("display", "block");
	}else{
		$("#cmbCompanyServiceID").val("2").change();
		$("#companyServiceSection").css("display", "none");
	}
}
</script>

