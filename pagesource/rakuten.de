<script>

    function setMiniCart() {
       $.ajax({
          url: "https://www.rakuten.de/rakuten-ajax/headerinfo/showcart",
          async: true,
          cache: false,
          success: function(data) {
             if (jQuery.trim(data) > "") {
                setCartData(data);
             }
          },
          error: function(data) {
             // do nothing...
          }
       });
    }

    function deleteFromMiniCart(cartId) {
       $.ajax({
          url: "https://www.rakuten.de/rakuten-ajax/headerinfo/deletecart" + "/" + cartId,
          async: true,
          cache: false,
          success: function(data) {
             if (jQuery.trim(data) > "") {

                if ($(".merchant_cart").length > 0) {
                   var url = document.URL.split("?");
                   window.location.href = url[0];
                } else {
                   setCartData(data);
                }
             }
          },
          error: function(data) {
             // do nothing...
          }
       });
    }

    function getLastNotice() {
       $.ajax({
          url: "https://www.rakuten.de/rakuten-ajax/headerinfo/notice",
          async: true,
          cache: false,
          success: function(data) {
             if (jQuery.trim(data) > "") {
                   $("#flyout_watchlist").html(data);
             }
          },
          error: function(data) {
             // do nothing...
          }
       });
    }

    function setCartData(data) {

       $('#cart_scroller').html(data);

       if($('#count_cart').length > 0) {

          $('#cart_counter').html($('#count_cart').val());
       }
       $(".cancel_cart").click(function () {
          deleteFromMiniCart($(this).attr("id"));
       });
    }

    function setNaviShop() {
      if($("#navi_shop").length > 0 && $("#navi_shop").html() == ""){
        $.ajax({
             url: "https://www.rakuten.de/rakuten-ajax/headerinfo/shop",
             async: true,
             cache: false,
             success: function(data) {
               $("#navi_shop").html(data)
             },
             error: function(data) {
                // do nothing...
             }
          });
      }
    }

</script>
