<script type="text/javascript">
	jQuery(document).ready(function($){
		
		if(Modernizr.touch){
			TweenMax.to($("footer"), .5, {bottom: '0px', ease:  Bounce.easeOut, delay: 1});
			TweenMax.to($("#visible-footer-bar #kean-found-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.1});
			TweenMax.to($("#visible-footer-bar #kean-wise-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.2});
			TweenMax.to($("#visible-footer-bar #kean-ex-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.3});
			TweenMax.to($("#visible-footer-bar #kean-mail-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.4});
			TweenMax.to($("#visible-footer-bar #kean-bb-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.5});
			TweenMax.to($("#visible-footer-bar #kean-dir-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.6});
			TweenMax.to($("#visible-footer-bar #kean-campus-alert"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.7});
		}else{
			//initial animation for footer
			TweenMax.to($("footer"), .5, {bottom: '0px', ease:  Bounce.easeOut, delay: 1});
			TweenMax.to($("#visible-footer-bar .social-icons#icon1"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.1});
			TweenMax.to($("#visible-footer-bar .social-icons#icon2"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.2});
			TweenMax.to($("#visible-footer-bar .social-icons#icon3"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.3});
			TweenMax.to($("#visible-footer-bar #kean-found-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.4});
			TweenMax.to($("#visible-footer-bar #kean-wise-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.5});
			TweenMax.to($("#visible-footer-bar #kean-ex-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.6});
			TweenMax.to($("#visible-footer-bar #kean-mail-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.7});
			TweenMax.to($("#visible-footer-bar #kean-bb-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.8});
			TweenMax.to($("#visible-footer-bar #kean-dir-icon"), .5, {top: '0px', ease: Quad.easeIn, delay: 1.9});
			TweenMax.to($("#visible-footer-bar #kean-campus-alert"), .5, {top: '0px', ease: Quad.easeIn, delay: 2.0});
		}

		/*$("footer").hover(function(){
			//TweenMax.to($("footer"), .3, {bottom: '-5px'});
		}, function(){
			//TweenMax.to($("footer"), .3, {bottom: '0px'});
		});*/
		

		$("#visible-footer-bar").click(function(e){
			
			var targId = e.target.id;
			
			switch(targId){
				case "visible-footer-bar":
					//if visibilte footer bar
					if($("#bottom-footer-container").hasClass("out")){
						$("#bottom-footer-container").slideUp();
						$("#bottom-footer-container").removeClass("out");
						TweenMax.to($("#footer-arrow"), 1, {rotation: 0});
						$("#exout-overlay").hide();
					}else{
						//check if menu is out
						if($("#menu-cont").hasClass("out")){
							$("#menu-cont").slideUp(function() {
							    $("#menu-cont").removeClass("sliding");
							    $("#menu-cont").removeClass("active");
							    $("#main-menu-container .menu-link.with-subnav").removeClass("active");
							    $("#menu-cont").removeClass("out");
							});
						}
						
						$("#bottom-footer-container").slideDown();
						$("#bottom-footer-container").addClass("out");
						TweenMax.to($("#footer-arrow"), 1, {rotation: 180});
						$("#exout-overlay").show();
					}
					break;
				case "info-icon":
					if($("#bottom-footer-container").hasClass("out")){
						$("#bottom-footer-container").slideUp();
						$("#bottom-footer-container").removeClass("out");
						
						TweenMax.to($("#footer-arrow"), 1, {rotation: 0});
						$("#exout-overlay").hide();
					}else{
						//check if menu is out
						if($("#menu-cont").hasClass("out")){
							$("#menu-cont").slideUp(function() {
							    $("#menu-cont").removeClass("sliding");
							    $("#menu-cont").removeClass("active");
							    $("#main-menu-container .menu-link.with-subnav").removeClass("active");
							    $("#menu-cont").removeClass("out");
							});
						}
						
						$("#bottom-footer-container").slideDown();
						$("#bottom-footer-container").addClass("out");
		
						TweenMax.to($("#footer-arrow"), 1, {rotation: 180});
						$("#exout-overlay").show();
					}
					break;
				case "more-tweets-inner":
					if($("#hp-tweets-block").hasClass("out")){
						
						$("#hp-tweets-block .item-list li").not(":first").css('display', 'none');
						$("#hp-tweets-block .view-id-tweets").css('height', 'auto');
						
						$("#hp-tweets-block").removeClass("out");
						$("#more-tweets-inner").html("MORE TWEETS");
						
					}else{
						
						TweenMax.to($("#hp-tweets-block .view-id-tweets"), .5, {height: '300px', ease: Quad.easeOut});
						TweenMax.to($("#hp-tweets-block .item-list li"), .5, {display: 'block', ease: Quad.easeOut});
						$("#hp-tweets-block").addClass("out");
						$("#more-tweets-inner").html("LESS TWEETS");
					}
					
					break;
			}
			
		});



	});
</script>
