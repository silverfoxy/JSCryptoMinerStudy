<script type="text/javascript">
	jq183(document).ready(function($){
		//show events
		/*$(".cal-item").click(function(){
			window.location.href = "http://events.kean.edu/mastercalendar/";
		});*/
		
		$("#calendar-overlay").click(function(){
			$("#calendar-overlay").hide();
			$("#calendar-container").hide();
		});
		
		$("#bigex").click(function(){
			$("#calendar-overlay").hide();
			$("#calendar-container").hide();
		});
		
		$("#hp-events-block").click(function(){
			$("#calendar-overlay").show();
			$("#cal-hp-loading").show();
			makeRequest();
		});
		
		function makeRequest(){
			$.ajax({
			  type: "GET",
			  url: "./calendarendpoint.php?request_type=hpfeatured"
			})
			.done(function( data ) {
				 if(data.length){
					 //hide loader
					 //$("#calendar-loading").hide();
					 var json = JSON.parse(data);
					 buildHTML(json);
				 }   
			})
			.fail(function(jqXHR, textStatus){
				console.log("AJAX Request Failed: "+textStatus);
			});
		}
		
		
		function buildHTML(items){
			var itemhtml = "",
				finalhteml = "",
				l_data = items['Data'];
				
			for(var i = 0; i < l_data.length; i++){
				var $item = $("#item"+i),
					$title = $item.find(".cal-title"),
					$location = $item.find(".location"),
					$time = $item.find(".time"),
					$dateday = $item.find(".date-day"),
					$datemonth = $item.find(".date-month"),
					caldate = formatCalDate(l_data[i].EventDate);
				
				$title.html(l_data[i].Title);
				$location.html(l_data[i].Location);
				$time.html(formatStartEnd(l_data[i].TimeEventStart,l_data[i].TimeEventEnd));
				$dateday.html(caldate[1]);
				$datemonth.html(caldate[0]);
					eventDetailId(l_data[i].EventDetailID, i);
			}
			
			$("#cal-hp-loading").hide();
			$("#calendar-container").show();
			
		
		}
		
		/*
		
			getting image for event
		
		*/
		
		function eventDetailId(eventId, location_id){
					var location_id = location_id;
					$.ajax({
					  type: "GET",
					  url: "./calendarendpoint.php?eventID="+eventId+"&request_type=single"
					})
					.done(function( data ) {
						 if(data.length){
							 //hide loadeer
							 //$("#calendar-loading").hide();
							 var json = JSON.parse(data);
							 //buildHTML(json);
							  //make sure Data is defined
								if(json['Data'] != undefined){
									var m_data = json['Data'];
									if(m_data.ImageId && m_data.ImageId !=0){
												if($("#item"+location_id).hasClass('image')){
												 $("#item"+location_id).find('.box-overlay').after('<img class="cal-image" alt="Embedded Image" src="data:image/png;base64,'+m_data.Image+'" />');
												}
									}
									
									//$('#calendar-main-container .calendar-item').attr("eventID")
								}
						 }   
					})
					.fail(function(jqXHR, textStatus){
						console.log("AJAX Request Failed: "+textStatus);
					});						
			
		}
		
		function formatCalDate(eventdate){
			var arrdate = eventdate.split("-"),
				datenum = parseInt(arrdate[1]),
				arrday = arrdate[2].split("T"),
				day = arrday[0];
		
			return [getMonthName(datenum-1).substr(0, 3),day];
		}
		
		function formatStartEnd(start,end){
			var tempstart = start.split("T"),
				arrstart = tempstart[1].split(":"),
				startH = arrstart[0],
				startM = arrstart[1],
				finalstart = "";
			if(startH > 12){
				startH = startH - 12;
				finalstart = startH+":"+startM+"PM";
			}else{
				finalstart = startH+":"+startM+"AM";
			}
			
			var tempend = end.split("T"),
				arrend = tempend[1].split(":"),
				endH = arrend[0],
				endM = arrend[1],
				finalend = "";
			if(endH > 12){
				endH = endH - 12;
				finalend = endH+":"+endM+"PM";
			}else{
				finalend = endH+":"+endM+"AM";
			}
			
			
			return finalstart+" - "+finalend;
		}
		
		var monthNames = [
		    "January", "February", "March",
		    "April", "May", "June",
		    "July", "August", "September",
		    "October", "November", "December"
		];
		function getMonthName(num) {
		    return monthNames[num];
		}
		
	});
</script>      </div>
