<script>

    var bhteamPromoBanners = {};

    bhteamPromoBanners.abTest = {

        container: $('.banners-below-nav'),

        items: {},

        containerHeight: 0,

        //numberVisible: 1,

        intervalSec: 3000,

        init: function(){
            var  mobileSetupRan = false;
            this.items = this.container.find('.banner-below-nav');

            this.promoBannerTestFooter();
            this.bindUIActions(mobileSetupRan);
            if (this.isMobile()) {

                this.mobileSetup();
                this.bindUIActions();
                this.mobileDropDownUIAction();
            } else {
                clearInterval(this.switcher);
            }
        },

        bindUIActions: function(){

            window.addEventListener('resize', this.debounce(this.onResize, 500).bind(this));
        },

        onResize: function() {
            if (this.isMobile() && !this.mobileSetupRan ) {
                this.mobileSetup();
                this.mobileDropDownUIAction();
            } else if (!this.isMobile()) {
                clearInterval(this.switcher);
            }
        },

        //https://davidwalsh.name/javascript-debounce-function
        debounce: function(func, wait, immediate) {
            var timeout;
            return function() {
                var context = this, args = arguments;
                var later = function() {
                    timeout = null;
                    if (!immediate) func.apply(context, args);
                };
                var callNow = immediate && !timeout;
                clearTimeout(timeout);
                timeout = setTimeout(later, wait);
                if (callNow) func.apply(context, args);
            };
        },

        isMobile: function() {
            //return $(window).width() + 17 < 968;
            return window.innerWidth < 968;
        },

        mobileDropDownUIAction: function(){
            this.container.click($.proxy(function () {
                this.dropDownOnMobile();
            }, this));
        },

        mobileSetup: function(){

            var maxHeight = -1;

            $('.banners-below-nav .banner-below-nav a').each(function() {
                maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
            });

            var maxPromoHeight = maxHeight + 10; //height of tallest promotion

            //$('.banners-below-nav .banner-below-nav').each(function() {
            // maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
            //$(this).addClass('max-height');
            //});

            //$('.banners-below-nav .banner-below-nav').css('height', '100%');

            $('.banners-below-nav .banner-below-nav').each(function() {
                $(this).css('height', maxPromoHeight );
            });

            $('.top-banner-all-offers').css('line-height', maxPromoHeight + "px" );

            $('.banners-below-nav-wrapper').height(maxPromoHeight );

            //Multiple first classes? Is that a problem?
            if(!this.container.find('.banner-below-nav:first').hasClass("first")){
                this.container.find('.banner-below-nav:first').addClass("first");
            }

            this.container.css({ height: maxPromoHeight, overflow: "hidden" });

            this.vertCycle(maxPromoHeight);

            this.mobileSetupRan = true;

        },

        desktopSetup: function(){

        },

        dropDownOnMobile: function(){
            var maxPromoHeight = 0; //height of tallest promotion

            $('.banners-below-nav .banner-below-nav').each(function() {
                maxPromoHeight = maxPromoHeight > $(this).height() ? maxPromoHeight : $(this).height();
                $(this).addClass('max-height');
            });

            if (this.container.parent().hasClass('expanded')) {
                this.container.parent().removeClass('expanded');
                //$('.top-banner-all-offers').html('See All Offers');
                this.vertCycle(maxPromoHeight);

                this.container.animate({
                    height: maxPromoHeight,
                }, 500, function () {
                });
            } else {
                this.container.parent().addClass('expanded');
                //$('.top-banner-all-offers').html('See Fewer Offers');

                clearInterval(this.switcher);

                var totalHeight = 0;
                $(".banner-below-nav", this.container).each(function () {
                    totalHeight += $(this).outerHeight();
                });

                // Animate height to 0% in 1 millisecond
                this.container.animate({height: maxPromoHeight}, 1);

                // Do the animation using the new fixed height.
                this.container.animate({
                    height: totalHeight,
                }, 500, function () {
                });

            }
        },

        domChanges: function(){
            $('.footer .footer-promo').removeClass('hidden-on-mobile');
            //this.container.wrap('<div class="banners-below-nav-wrapper"></div>');
            this.container.prepend('<p class="top-banner-all-offers vertical-align-text icon-cs-arrow"></p>');
            //$('.js-clock-timer').appendTo(this.container).wrap( "<div class='banner-below-nav'></div>" ).wrap( "<div class='banner-link-wrapper'></div>" );
            $('.top-banner .header-banner-timer-wrapper .js-clock-timer').clone().appendTo('.pull-countdown-timer');
            this.container.removeClass('js-mobile-opacity-slider');
            this.container.removeClass('mobile-opacity-slider');
        },

        vertCycle: function(maxPromoHeight) {

            var firstItem;
            this.switcher = setInterval($.proxy(function() {
                firstItem = this.container.find('.banner-below-nav.first').html();
                this.container.removeClass('expanded');

                this.container.find('.banner-below-nav.first').each($.proxy(function(){

                    this.container.find('.banner-below-nav:first').addClass('fader-banner').animate({ opacity: "0" }, 600);
                    setTimeout($.proxy(function() {
                        this.container.append('<div class="banner-below-nav" style="height:' + maxPromoHeight + 'px;">'+firstItem+'</div>');
                        if (firstItem.indexOf("js-clock-timer") >= 0){
                            $('.banners-below-nav .js-timer').html('{{#days}}{{days}}d {{/days}}{{hours}}h {{minutes}}m {{seconds}}s');
                            //app.components.initComponent('bannertimer').initializeClock($('.js-clock-timer'));




                            function initializeClock($elem) {
                                var $timer = $elem.find('.js-timer'),
                                    template = $timer.html(),
                                    startTime = new Date(Date.parse($elem.data('spoofDateStart'))),
                                    endTime = new Date(Date.parse($elem.data('dateTimeEnd'))),
                                    timeEndData = $elem.data('timeEnd');

                                //Set initial start date current time if DW site preview is not being used to spoof date and time
                                if (window.location.href.indexOf('__siteDate=') === -1) {
                                    startTime = Date.parse(new Date());
                                }

                                if ($elem.data('hourlyTimer') === true) {
									endTime = getNextHour(new Date(startTime));
								} else if (timeEndData) {
									endTime = setCountdownTime(startTime, timeEndData);
								}

                                if (!template) {
                                    return;
                                }

                                $elem.removeClass('hide');
								
								function getNextHour(startTime) {
									var timeNextHour = new Date(startTime.toUTCString());

									timeNextHour.setHours(timeNextHour.getHours() + 1);
									timeNextHour.setMinutes(0);
									timeNextHour.setSeconds(0);

									return timeNextHour;
								}

                                function updateClock($timer) {
                                    //Set start date current time if DW site preview is not being used to spoof date and time
                                    if (window.location.href.indexOf('__siteDate=') === -1) {
                                        startTime = Date.parse(new Date());
                                    }

                                    var t = getTimeRemaining(startTime, endTime);
                                    $timer.html(Mustache.render(template, t));

                                    if (t.total <= 0) {
                                        clearInterval(timeinterval);
                                        $elem.addClass('hide');
                                    }
                                }

                                updateClock($timer);
                                var timeinterval = setInterval(updateClock, 1000, $timer);
                            }

                            function getTimeRemaining(startTime, endTime) {
                                var t = Date.parse(endTime) - startTime,
                                    seconds = Math.floor((t / 1000) % 60),
                                    minutes = Math.floor((t / 1000 / 60) % 60),
                                    hours = Math.floor((t / (1000 * 60 * 60)) % 24),
                                    days = Math.floor(t / (1000 * 60 * 60 * 24));
                                return {
                                    'total': t,
                                    'days': days,
                                    'hours': ('0' + hours).slice(-2),
                                    'minutes': ('0' + minutes).slice(-2),
                                    'seconds': ('0' + seconds).slice(-2)
                                };
                            }

                            /**
                             *
                             * @param timeEndData
                             * @returns {Date}
                             */
                            function setCountdownTime(startTime, timeEndData) {
                                var now = startTime,
                                    targetTime = new Date(now),
                                    userDefinedTime = new Date(timeEndData);

                                targetTime.setHours(userDefinedTime.getHours());
                                targetTime.setMinutes(userDefinedTime.getMinutes());
                                targetTime.setSeconds(userDefinedTime.getSeconds());

                                //If target time is less then current date, set targetTime to the next day
                                if (targetTime < now) {
                                    targetTime.setDate(targetTime.getDate() + 1);
                                }

                                return targetTime;
                            }

                            initializeClock($('.banners-below-nav .js-clock-timer'));





                        }
                        firstItem = '';
                        this.container.find('.banner-below-nav:first').remove();
                        this.container.find('.banner-below-nav:first').addClass("first");
                    }, this), 600);


                }, this));
            }, this ), this.intervalSec);
        },

        promoBannerTestFooter: function() {
            var footerMoreOffers = $('.footer-more-offers'),
                footerOffersContainer = $('.footer .footer-promo'),
                footerMoreOffersHeight = footerMoreOffers.outerHeight(),
                maxPromoHeight = 0;//height of tallest promotion

            //$('.js-clock-timer').clone().appendTo( footerOffersContainer ).wrap( "<div class='banner-below-nav'></div>" ).wrap( "<div class='banner-link-wrapper'></div>" );

            footerMoreOffers.click(function(){
                if(footerOffersContainer.hasClass('expanded')){
                    footerOffersContainer.removeClass('expanded');
                    footerMoreOffers.html('See All Offers');

                    footerOffersContainer.animate({
                        height: 40,
                    }, 500, function() {
                    });
                } else {
                    footerOffersContainer.addClass('expanded');
                    footerMoreOffers.html('See Fewer Offers');

                    var totalHeight = 0;
                    $(".footer .footer-promo > div").each(function(){
                        totalHeight += $(this).outerHeight();
                    });

                    footerOffersContainer.animate({
                        height: totalHeight,
                    }, 500, function() {
                    });

                }
            });
        }

    };

        if($('html').hasClass('desktop')) {
        bhteamPromoBanners.abTest.domChanges();
    }

    $( document ).ready(function() {

if($('html').hasClass('desktop')) {

            $('body').addClass('fd40-abtest');

            $('#wrapper .top-banner div.below-navigation-banners').addClass('ready');
            bhteamPromoBanners.abTest.init();
}
    });
</script>
