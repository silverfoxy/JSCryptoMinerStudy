function convertToSlug(Text)
{
    return Text.toLowerCase().replace(/ /g,'-').replace(/[^\w-]+/g,'');
}
function get_action() 
{
	var search_text = document.getElementById('search_text').value;
	var newurl = convertToSlug(search_text);
	if(newurl != ''){
		form_action = 'https://www.ncrypted.net/q/'+newurl;
		$('#search_form').attr('action',form_action);
	}
	else{
		$('#search_text').css("border", "1px solid red");
		$('#search_msg').html("Please enter text.");
		return false;
	}
}
