<script>  
//Cookies
var xmlhttpcookies;
function closecookiepopup(clicktype) {
    xmlhttpcookies = false;
    // branch for native XMLHttpRequest object
    if (window.XMLHttpRequest && !(window.ActiveXObject)) {
        try {
            xmlhttpcookies = new XMLHttpRequest();
        } catch (e) {
            xmlhttpcookies = false;
        }
        // branch for IE/Windows ActiveX version
    } else if (window.ActiveXObject) {
        try {
            xmlhttpcookies = new ActiveXObject("Msxml2.XMLHTTP");
        } catch (e) {
            try {
                xmlhttpcookies = new ActiveXObject("Microsoft.XMLHTTP");
            } catch (e) {
                xmlhttpcookies = false;
            }
        }
    }

    if (xmlhttpcookies) {

        var url = ''

        if (clicktype == 1) {
            url = '/Content/Shared/cookieconsent.aspx?clicktype=ClickedX';
        }

        if (clicktype == 2) {
            url = '/Content/Shared/cookieconsent.aspx?clicktype=TimedOut';
        }

        xmlhttpcookies.onreadystatechange = changeClass;
        xmlhttpcookies.open("GET", url, true);
        xmlhttpcookies.send(null);
    }
}

function changeClass() {
    if (xmlhttpcookies.readyState == 4) {
        if (xmlhttpcookies.status == 200) {
            document.getElementById("cookie-message").style.display = "none";
        }
    }
}
    </script>  



    
