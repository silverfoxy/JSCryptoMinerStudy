function adv_getExpireTimeLong() {
    var d = new Date();
    var e;
    var h = parseInt(d.getHours());
    d.setMinutes(59);
    d.setSeconds(59);  
    if (h < 12) {
    	d.setHours(11);     
	} else if (h < 16) { 
    	d.setHours(15); 
    } else {
    	d.setHours(23); 
    }
    return d.toUTCString();
}

function adv_getExpireTimeShort(seconds) {
	var d = new Date();
	d.setTime(d.getTime() + (seconds*1000));
    return d.toUTCString();
}

