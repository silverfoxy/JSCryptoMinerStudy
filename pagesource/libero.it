	biTrack:function (objBiToTrack){
		// HACK! - target self se mobile e tablet
		if(document.body.className.indexOf("hp_mobile")>-1){
			var jump = true;
			var realObj = objBiToTrack;
			if(!objBiToTrack.getAttribute('href')){	
				var a = objBiToTrack.getElementsByTagName('a');
				if(a[0]) objBiToTrack = a[0];
			}	
			// eccezioni link a...
			if(objBiToTrack.getAttribute('href')){				
				if(objBiToTrack.getAttribute('href').indexOf('/1254.virgilio.it')>=0) jump = false;
				if(objBiToTrack.getAttribute('href').indexOf('/www.3bmeteo.com')>=0) jump = false;
				if(objBiToTrack.getAttribute('href').indexOf('/elevensports.it')>=0) jump = false;
				if(objBiToTrack.getAttribute('href').indexOf('/www.elevensports.it')>=0) jump = false;
				if(objBiToTrack.getAttribute('href').indexOf('/www.pgcasa.it')>=0) jump = false;
				if(objBiToTrack.getAttribute('href').indexOf('/www.laleggepertutti.it')>=0) jump = false;
			}
			// eccezioni lanci manuali...
			if(objBiToTrack.getAttribute('data-bi-canale')) {
					if(objBiToTrack.getAttribute('data-bi-canale').indexOf("manuale")>=0) jump = false;
			}
			// eccezioni LABEL lanci manuali...
			if(objBiToTrack.id) {
				if((objBiToTrack.id.indexOf('topslider_')==0)&&(objBiToTrack.id.indexOf('_label')>0)){
					var o = document.getElementById(objBiToTrack.id.replace('_label','_link'));
					if(o){
						if(o.getAttribute('data-bi-canale')) {
							if(o.getAttribute('data-bi-canale').indexOf("manuale")>=0) jump = false;
						}
					}
				}
			}
			if(jump) if(objBiToTrack.getAttribute('target')) objBiToTrack.setAttribute("target", "_self");
			objBiToTrack = realObj;
		}
		// /HACK! - target self se mobile e tablet

		var vBiParamMacroArea="", vBiParamArea="", vBiParamSubArea="", vDataBiDomain="";
		var _vFunction = objBiToTrack.getAttribute('data-bi-preFnc');

		if (_vFunction){
			eval(_vFunction)(objBiToTrack);
		}

		var vDataBiCanale=objBiToTrack.getAttribute('data-bi-canale');
		if (vDataBiCanale){
			if (vDataBiCanale=="piuvisti"){vDataBiCanale="";}
		}
		var vDataBiUrl=objBiToTrack.getAttribute('data-bi-url');
		if (objBiToTrack.getAttribute('data-bi-url') == null){vDataBiUrl=objBiToTrack.getAttribute('data-bi-urlgo');}
		if (vDataBiUrl){
			try{
				vDataBiUrl = this.removeParam("rndPrx",vDataBiUrl);
				vDataBiUrl = this.removeParam("pnc",vDataBiUrl);
			}catch(ex){}

			try{
				var _vArrDataBiDomain = vDataBiUrl.split("/");
				if (objBiToTrack.getAttribute('href')){
					_vArrDataBiDomain = objBiToTrack.getAttribute('href').split("/");
				}

				var _vDataBiDomain = _vArrDataBiDomain[2];
				var _vArrDataBiDomainTmp = _vDataBiDomain.split("?");
				vDataBiDomain = _vArrDataBiDomainTmp[0];
			}catch(ex){}

			try{
				if (!vDataBiCanale){
					vDataBiCanale = this.remappingChannelFromUrl(vDataBiUrl);
					if (!vDataBiCanale){vDataBiCanale=vDataBiDomain;}
				}
			}catch(ex){}
		}

		var vDataBiId=objBiToTrack.getAttribute('data-bi-id');
		var vDataBiParam=objBiToTrack.getAttribute('data-bi-param');

		var vArrDataBiParam = vDataBiParam.split("|");

		var vDataBiResolution= this.biGetResolutionIolEvent();
		try{vBiParamMacroArea= vArrDataBiParam[0];}catch(ex){}
		try{vBiParamArea= vArrDataBiParam[1];}catch(ex){}
		try{vBiParamSubArea= vArrDataBiParam[2];}catch(ex){}



		if (this.debug){
			alert("BI param: " + vArrDataBiParam + "\n\nCanale: " + vDataBiCanale +  "\nUrl: " + vDataBiUrl);
		}

		try{
			var vTypeBiEvnt=objBiToTrack.getAttribute('data-bi-typeEvnt');
			if (vTypeBiEvnt){
				if ((vTypeBiEvnt=="iolmpck")||(vTypeBiEvnt=="iolck")){
					var _dataBiResolution = vDataBiResolution;
					if (_dataBiResolution == "0-424"){_dataBiResolution="424";}
					this.biClickIolEvent(objBiToTrack, vBiParamMacroArea, vBiParamArea, vBiParamSubArea, vDataBiCanale, _dataBiResolution, vDataBiUrl, vDataBiDomain, _ABTestingTrack);
				}
			}
		}catch(ex){}
		if (this.typeComscore==true){
		Libero.click(objBiToTrack, {labels: {wa_cksrv_par_a: 'libero', wa_cksrv_par_b: lBI.biContainer, wa_cksrv_par_c: vBiParamMacroArea, wa_cksrv_par_d: vBiParamArea, wa_cksrv_par_e: vDataBiCanale, wa_cksrv_par_f: vBiParamSubArea,  wa_cksrv_resolution: vDataBiResolution, wa_cksrv_to: vDataBiUrl, wa_cksrv_domain_to: vDataBiDomain, wa_test_ab: _ABTestingTrack}});
		}
