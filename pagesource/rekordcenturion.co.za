				if(isSales) prefix = 'sales';
				else prefix = 'news';				
				var element = jQuery('.' + prefix + '-contact-form');
				element.slideUp(time, function() {	
					element.find('.main-message').html('<p>' + message + '</p>');
					if(hide) element.find('.main-form').hide();
					jQuery('#' + prefix + '-contact-loading').hide();
					element.delay(time).slideDown(time);
				});
			}
			jQuery('#newsroom-contact').click(function(e) {
				e.preventDefault();			
				if(isSales == 1) {
					jQuery('.sales-contact-form').slideUp(time);
					isSales = 0;
				}
				if(isNews == 0) {
					isNews = 1;
					jQuery('.news-contact-form').slideDown();		
				} else {
					isNews = 0;
					jQuery('.news-contact-form').slideUp();		
				}
			});
			jQuery('#sales-contact').click(function(e) {
				e.preventDefault();
				if(isNews == 1) {
					jQuery('.news-contact-form').slideUp(time);
					isNews = 0;
				}
				if(isSales == 0) {
					isSales = 1;
					jQuery('.sales-contact-form').slideDown();		
				} else {
					isSales = 0;
					jQuery('.sales-contact-form').slideUp();		
				}
			});
			jQuery('#news-talk-to-us-form').submit(function(e) {
				e.preventDefault();
				var validate = doCheckForm('news');
				if(validate) {
					jQuery('#news-contact-loading').fadeIn(100);
					document.getElementById("news-contact-submit").disabled=true;
					runTheAjax(false);
				}
			});
			jQuery('#sales-talk-to-us-form').submit(function(e) {
				e.preventDefault();
				var validate = doCheckForm('sales');
				if(validate) {
					jQuery('#sales-contact-loading').fadeIn(100);
					document.getElementById("sales-contact-submit").disabled=true;					
					runTheAjax(true);
				}
			});			
			function doCheckForm(form) {
				var currentCheck = jQuery('#' + form + '-contact-name').val();
				var hasError = 0;
				jQuery('.contact-form-error').html('');
				if(currentCheck.length < 3) {
					jQuery('#' + form + '-contact-error-name').html('That name is not valid');
					hasError = 1;
				}
				currentCheck = jQuery('#' + form + '-contact-email').val();
				var emailRegex = new RegExp('.+@.+[\.][A-Za-z]+');
				if(!emailRegex.test(currentCheck)) {
					jQuery('#' + form + '-contact-error-email').html('Invalid e-mail address format');
					hasError = 1;
				}
				currentCheck = jQuery('#' + form + '-contact-message').val();
				if(currentCheck.length < 6) {
					jQuery('#' + form + '-contact-error-message').html('Your message has to be longer than 6 characters');
					hasError = 1;
				}
				if(hasError == 1) return false;
				else return true;
			}

		});	
		</script>
