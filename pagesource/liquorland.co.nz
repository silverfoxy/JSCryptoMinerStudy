        $('#MainContainer').on('click', 'a.ecommerce-store-link', function(e) {
            e.preventDefault();
            var storeId = $(this).data('store-id'); //data-store-id
            if (storeId)
            {
                console.log('changing store to ' + storeId);
                var date = new Date();
                var seconds = 28800;
                date.setTime(date.getTime() + (seconds * 1000));
                $.cookie('selectedStore', storeId, { path: '/', expires: date, domain: 'liquorland.co.nz' });
            }
            window.location.href =$(this).attr('href');
        });

    });
