<script type="text/javascript">
    $(document).ready(function () {
        SetImagesSlider();
    });
    $(window).resize(function () {
        SetImagesSlider();
    });

    function SetImagesSlider() {
        var width = $(window).width()

        var img1 = $("#homepagemainslider-Image1")
        var img2 = $("#homepagemainslider-Image2")
        var img3 = $("#homepagemainslider-Image3")
        var img4 = $("#homepagemainslider-Image4")

        if (width > 0 && width <= 767) {
            //mobile   
            img1.attr("src", '/img/cached/homepage/slider/TMI_00371 Switch Banners Homepage - M.jpg').removeAttr('class').attr('class', "banner1");
            img2.attr("src", '/img/cached/homepage/slider/TMI_00350 Banners - Mar 18 - S7 M.jpg').removeAttr('class').attr('class', "banner2");
            img3.attr("src", '/img/cached/homepage/slider/Samsung-S9-Mobile.jpg').removeAttr('class').attr('class', "banner3");
            img4.attr("src", '/img/cached/homepage/slider/TMI_00350 Banners - Mar 18 - P10 M.jpg').removeAttr('class').attr('class', "banner4");
        }
        else if (width > 767 && width <= 992) {
            //tablet 
            img1.attr("src", '/img/cached/homepage/slider/TMI_00371 Switch Banners Homepage - T.jpg').removeAttr('class').attr('class', "banner1");
            img2.attr("src", '/img/cached/homepage/slider/TMI_00350 Banners - Mar 18 - S7 T.jpg').removeAttr('class').attr('class', "banner2");
             img3.attr("src", '/img/cached/homepage/slider/Samsung-S9-Tablet.jpg').removeAttr('class').attr('class', "banner3");
            img4.attr("src", '/img/cached/homepage/slider/TMI_00350 Banners - Mar 18 - P10L T.jpg').removeAttr('class').attr('class', "banner4");
        }
        else if (width > 992) {
            //desktop
            img1.attr("src", '/img/cached/homepage/slider/TMI_00371 Switch Banners Homepage - D.jpg').removeAttr('class').attr('class', "banner1");
            img2.attr("src", '/img/cached/homepage/slider/TMI_00350 Banners - Mar 18 - S7 D.jpg').removeAttr('class').attr('class', "banner2");
             img3.attr("src", '/img/cached/homepage/slider/Samsung-S9-Desktop.jpg').removeAttr('class').attr('class', "banner3");
            img4.attr("src", '/img/cached/homepage/slider/TMI_00350 Banners - Mar 18 - P10L D.jpg').removeAttr('class').attr('class', "banner4");
        }
    }

    //Page: Homepage - Main Slider
    if ($(window).width() < 768) {

        //Page: Homepage - Main Slider Mobile Devices
        if ($("#homepagemainslider").length) {
            $("#homepagemainslider").owlCarousel({
                slideSpeed: 300,
                rewindSpeed: 500,
                navigation: true,
                navigationText: ["<span class='left-arrow'></span>", "<span class='right-arrow'></span>"],
                dragBeforeAnimFinish: true
            });
        };
    }

    else {

        //Page: Homepage - Main Slider - Captions, The function below adds the html to the pagnation after the carousel has intialized
        function afterOWLinit() {

            $('.owl-controls .owl-page:nth-child(1)').append('<div class="caption hidden-xs hidden-sm"><span class="top-caret"></span><h4>Prepay and Bill Pay offers</h4><p>Click for more</p></div>');

            $('.owl-controls .owl-page:nth-child(2)').append('<div class="caption hidden-xs hidden-sm"><span class="top-caret"></span><h4>Samsung Galaxy S7</h4><p>Our best deal yet</p></div>');


              $('.owl-controls .owl-page:nth-child(3)').append('<div class="caption hidden-xs hidden-sm"><span class="top-caret"></span><h4>Samsung Galaxy S9</h4><p>The all new smartphone</p></div>');


            $('.owl-controls .owl-page:nth-child(4)').append('<div class="caption hidden-xs hidden-sm"><span class="top-caret"></span><h4>Huawei P10 Lite</h4><p>Half price Huawei</p></div>');



        }

        //Page: Homepage - Main Slider All Other Devices
        if ($("#homepagemainslider").length) {
            $("#homepagemainslider").owlCarousel({
                autoPlay: true,
                slideSpeed: 7000,
                items: 1,
                itemsTablet: [768, 1],
                itemsDesktopSmall: [1024, 1],
                itemsDesktop: [1180, 1],
                slideSpeed: 2000,//speed in milliseconds
                pagination: true,
                navigation: true,
                navigationText: ["<span class='left-arrow'></span>", "<span class='right-arrow'></span>"],
                addClassActive: true,
                afterInit: afterOWLinit, // do some work after OWL init
                afterUpdate: afterOWLinit // do some work after Update
            });
        };

    };
</script>



