<script type="text/javascript">

	var marginOffset = 60;

	function updateMargin() {
		$('#under-video').css('margin-top', $('.video-container').height() - marginOffset);
		$("#nav").data('bs.affix').options.offset = $('.video-container').height();
	}
	
	$('#nav').affix({ offset: { top: $('.video-container').height() } });
	
    $("#nav").on('affixed.bs.affix', function(){
        marginOffset = 0;
		updateMargin();
    });
    $("#nav").on('affixed-top.bs.affix', function(){
        marginOffset = 60;
		updateMargin();
    });
	
	$(window).resize(function(e) {
		updateMargin();
	});
	
	$(function(){
		updateMargin();
	});

</script>




