        // autocomplete ask
        $('#txt_ask').autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: site_url + 'ask/get_ask_list',
                    type: 'post',
                    dataType: 'json',
                    data: {
                        request: request.term
                    },
                    success: function (ask) {
                        response($.map(ask, function (row) {
                            return {
                                label: row.ask_content,
                                value: row.ask_content,
                                ask_id: row.ask_id
                            };
                        }));
                    }
                });
            },
            search: function () {
                var term = this.value;
                if (term.length < 3) {
                    return false;
                }
            },
            minLength: 2,
            focus: function () {
                return false;
            },
            select: function (event, ui) {
                if(ui.item.ask_id == 0)
					$('#ask-textarea').toggleClass('active');
                    //location.href = site_url + "ugc/ask/add/";
                else
                    location.href = site_url + "ask/read/" + ui.item.ask_id;
            }
        });
    </script>
