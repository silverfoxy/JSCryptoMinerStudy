	function sliderJumpToArea(a) {
		if (a!=area) {
			area = a;
			var e = document.getElementById('area-pointer');
			e.className = 'which-area-pointer-'+a;
			for (i=0; i<4; i++) {
				e = document.getElementById('area-'+i);
				if(i==a) {
					e.className = 'which-area-header-selected';
				} else {
					e.className = 'which-area-header';
				}
			}
		}
		updateDisplayed();
	}

	function updateDisplayed() {
		var pStartIndex = prod[area];
		var pLastIndex = p[area].p.length;
		var pIndex;
		var prodAbbrev;
		var allIndex;
		var imgName;
		var title;
		var areaName;
		var link;
		var lBase;

		var slot = 0;
		for(i=pStartIndex;i<(pStartIndex + 4);i++) {
			// get actual index for product list
			if(i>=pLastIndex) {
				pIndex = i-pLastIndex;
			} else {
				pIndex = i;
			}
			prodAbbrev = p[area].p[pIndex];
			allIndex = all.indexOf(prodAbbrev);
			imgName = alli[allIndex];
			title = allt[allIndex];
			areaName = p[area].g[p[area].gp[pIndex]];
			if (prodAbbrev=='cagn') {
				link = cagnl;
			} else {
				lBase = lb[area];
				if (area==3) {
					link = lBase + allhl[allIndex];
				} else {
					link = lBase + allwl[allIndex];
				}
			}
			document.getElementById('pslot'+slot).href = link;
			document.getElementById('pslotc'+slot).src='/images/interface/homepageProductSlider/' + imgName + '.png';
			// document.getElementById('phslotc'+slot).src='/images/interface/homepageProductSlider/' + imgName + '-blur.png';
			document.getElementById('pslott'+slot).innerHTML = title;
			document.getElementById('pslota'+slot).innerHTML = areaName;
			document.getElementById('pslots'+slot).innerHTML = '(' + (pIndex+1) + '/' + pLastIndex + ')';
			slot++;
		}
	}

	function updateBlurred() {
		var pStartIndex = prod[area];
		var pLastIndex = p[area].p.length;
		var pIndex;
		var prodAbbrev;
		var allIndex;
		var imgName;

		var slot = 0;
		for(i=pStartIndex;i<(pStartIndex + 4);i++) {
