			allIndex = all.indexOf(prodAbbrev);
			imgName = alli[allIndex];
			document.getElementById('phslotc'+slot).src='/images/interface/homepageProductSlider/' + imgName + '-blur.png';
			slot++;
		}
		hideBlurs();
	}


	function leftArrowClicked() {
		var pStartIndex = prod[area];
		var pLastIndex = p[area].p.length;

		// populate outer left 4 "blur" images & animate
		var i;
		var slot = -1;
		for(i=(pStartIndex-1);i>(pStartIndex - 5);i--) {
			// get actual index for product list
			if(i<0) {
				pIndex = pLastIndex+i;
			} else {
				pIndex = i;
			}
			var prodAbbrev = p[area].p[pIndex];
			var allIndex = all.indexOf(prodAbbrev);
			var imgName = alli[allIndex];
			document.getElementById('phslotc'+slot).src='/images/interface/homepageProductSlider/' + imgName + '-blur.png';
			slot--;
		}
		showBlurs();
		setPrevDisplayed();
		animateBlurs('left');
	}

	function rightArrowClicked() {
		var pStartIndex = prod[area];
		var pLastIndex = p[area].p.length;
		// populate outer right 4 "blur" images & animate
		var i;
		var slot = 4;
		for(i=(pStartIndex+4);i<(pStartIndex + 8);i++) {
			// get actual index for product list
			if(i>=pLastIndex) {
				pIndex = i-pLastIndex;
			} else {
				pIndex = i;
			}
			var prodAbbrev = p[area].p[pIndex];
			var allIndex = all.indexOf(prodAbbrev);
			var imgName = alli[allIndex];
			document.getElementById('phslotc'+slot).src='/images/interface/homepageProductSlider/' + imgName + '-blur.png';
			slot++;
		}
		showBlurs();
		setNextDisplayed();
		animateBlurs('right');
	}

	function showBlurs() {
		document.getElementById('slider-products-hidden').style.visibility = 'visible';
		document.getElementById('slider-products').style.visibility = 'hidden';
	}

	function animateBlurs(way) {
		var i;
		var aSteps = 80;
		var bump;
		var delay = 4;

		if (way=='right') {
			// animate right-to-left sliding
			for(i=1;i<=aSteps;i++){
				bump = setTimeout (slideLeft, delay*(i-1));
			}
			bump = setTimeout (updateBlurred, delay*aSteps)
		} else {
			// animate left-to-right sliding
			for(i=1;i<=aSteps;i++){
				bump = setTimeout (slideRight, delay*(i-1));
			}
			bump = setTimeout (updateBlurred, delay*aSteps)
		}
	}

	function slideLeft() {
		var i;
		var mLeft;
		var newMLeft;
		var mStep = 8;

		for(i=0;i<8;i++) {
			mLeft = document.getElementById('phslot'+i).style.marginLeft;
			newMLeft = parseInt(mLeft.substring(0,mLeft.length-2)) - mStep + 'px';
			document.getElementById('phslot'+i).style.marginLeft = newMLeft;
		}
	}

	function slideRight() {
		var i;
		var mLeft;
		var newMLeft;
		var mStep = 8;

		for(i=-4;i<4;i++) {
			mLeft = document.getElementById('phslot'+i).style.marginLeft;
			newMLeft = parseInt(mLeft.substring(0,mLeft.length-2)) + mStep + 'px';
			document.getElementById('phslot'+i).style.marginLeft = newMLeft;
		}
	}

	function setNextDisplayed() {
		var pLastIndex = p[area].p.length;

		prod[area] = prod[area] + 4;
		if (prod[area]>=pLastIndex) {
			prod[area] = prod[area] - pLastIndex;
		}
		updateDisplayed();
	}


	function setPrevDisplayed() {
		var pLastIndex = p[area].p.length;

		prod[area] = prod[area] - 4;
		if (prod[area]<0) {
			prod[area] = prod[area] + pLastIndex;
		}
		updateDisplayed();
	}

	function hideBlurs() {
		document.getElementById('slider-products').style.visibility = 'visible';
		document.getElementById('slider-products-hidden').style.visibility = 'hidden';
		resetBlurMargins();
	}

	function resetBlurMargins() {
		var i;
		var mLeft;
		for (i=-4;i<8;i++) {
			mLeft = ((i+4)*160-640) +'px'
			document.getElementById('phslot'+i).style.marginLeft = mLeft;
		}
	}


</script>

