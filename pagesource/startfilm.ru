function drawPager(page){
	var start = 1;
	if(page > 3) start = page - 2;
	var end = 5;
	if(page + 5 > max_page) end = max_page - page + 1;
	if(end < max_page) end = max_page;
	for(i=0; i<end;i++){
		$('#feed-pager-'+(i+1))
			.html(start+i)
			.attr('href', getDoUrl()+(start+i)+getDoUrlEnd())
			.removeClass('feed-pager-current')
			.removeClass('pager-item-hide');
		if((start + i) == page){
			$('#feed-pager-'+(i+1)).addClass('feed-pager-current');
		}
	}
}
function setPage(page){
	drawPager(page);
	if(window.page > 0){
		window.history.replaceState("", "", getDoUrl()+window.page+"/"+window.tag);
		yaCounter6419.hit(getDoUrl()+window.page+"/"+window.tag);
	}else{
		window.history.replaceState("", "", getDoUrl()+window.tag);
		yaCounter6419.hit(getDoUrl()+window.tag);
	}
	ga('send', 'pageview', location.pathname);
}
function getDoUrl(){
	var url = '/';
	if (window.location.href.indexOf('/tag/')>0)  {
		url='/tag/';
	} else if (window.location.href.indexOf('/hot/')>0){
		url='/hot/';
	} else if (window.location.href.indexOf('/myfeed/')>0){
		url='/myfeed/';
	}
	return url;
}
function getDoUrlEnd(){
	var url = '';
	return url;
}

window.page=parseInt('1');
window.tag='';
window.max_page = parseInt('515');
drawPager(page);
window.isLoadFeed = 0;
window.isBarAnimate = 0;
window.isBarHidden = 1;
var opts = { zIndex: 100 }
spinner = new Spinner(opts).spin();
$(window).scrollTop(0);
$(window).scroll(function() {
	if(page == max_page){
		return;
	}
	if($("#sidebar").offset().top + $("#sticky-sidebar").height() + 38 < $(window).scrollTop() + $(window).height()){
		$("#sticky-sidebar").css({'position': 'fixed','bottom': '38px'}, 1000);
	}
	if($("#sidebar").offset().top + $("#sticky-sidebar").height() + 38 > $(window).scrollTop() + $(window).height()){
		$("#sticky-sidebar").css({'position': 'static'});
	}
	
	var t = $(window).scrollTop() - $('#feed-container').offset().top + 210;
	if(t > 50 && isBarAnimate == 0 && isBarHidden){
		isBarAnimate = 1;
		isBarHidden = 0;
		$('#feed-bar').animate({top:'50px'}, 1000, function(){isBarAnimate = 0;});
	}
	if(t < 0 && isBarAnimate == 0 && isBarHidden == 0){
		isBarAnimate = 1;
		isBarHidden = 1;
		$('#feed-bar').animate({top:'0px'}, 1000, function(){isBarAnimate = 0;});
	}
	prev_page = $('#feed-page-'+(page));
	if(prev_page.size() > 0){
		if($(window).scrollTop() + $(window).height() < prev_page.offset().top - 210 && page > 0){
			page--;
			setPage(page);
		}
	}
	next_page = $('#feed-page-'+(page + 1));
	if(next_page.size() > 0){
		if($(window).scrollTop() + $(window).height() > next_page.offset().top - 210){
			page++;
			setPage(page);
		}
	}

	if($(window).scrollTop() + $(window).height() >= $(document).height() -  $('footer').height() - 50 && window.isLoadFeed == 0) {
		window.isLoadFeed = 1;
		$('#page_container').append('<div id="spinner-container" style="width:100%; height:76px; position:relative;"></div>')
		var spinner_container = document.getElementById('spinner-container');
		spinner_container.appendChild(spinner.el);
	window.page++;
		var jsonUrl = '/feedjson/';
		if (window.location.href.indexOf('/tag/')>0)  {
			jsonUrl='/tagjson/';
		} else if (window.location.href.indexOf('/hot/')>0){
			jsonUrl='/hotjson/';
		}else if (window.location.href.indexOf('/myfeed/')>0){
			jsonUrl='/myfeedjson/';
		}
	$.get(jsonUrl+window.page+"/"+window.tag, function(data){
			if (spinner_container) {
				spinner_container.parentNode.removeChild(spinner_container);
			}
			window.isLoadFeed = 0;
			if(data){
				$('#feed-container').append(data);
				setPage(page);
			}
		}
	);
   }
});
