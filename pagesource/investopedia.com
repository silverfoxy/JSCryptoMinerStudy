<script language="JavaScript">
  function trimRailsUK()
  {
    // don't run for mobile. refer to CSS media queries
    // for meaning of the value of 30.
    if ($('#state-indicator').css('z-index') < 30) {
      return;
    }
    var pageWidth  = $(window).width();
    var sidebar = $('section.homepage .sidebar.secondary');
    var bound = sidebar.offset().top + sidebar.outerHeight();
    var articles = $('.sidebar.primary .ai-articles');

    $('.content, .sidebar.primary').css({ maxHeight: sidebar.outerHeight() + 'px' });
    $('.sidebar.primary .cl > li').each(function ()
    {
      $(this).removeClass('hidden');
      $(this).prev().removeAttr('style'); // removes 'hide border' code below.
        if ($(this).offset().top + $(this).outerHeight() > bound) {
          $(this).addClass('hidden');
          $(this).prev().not('.videos').css('border', 'none');
      }
    });
      $('.content .cl-5 > li').each(function (index, el) {
        if (pageWidth > 930) {
          if(index > 1) {
            $(this).addClass('hidden');
          }
        } else {
          $(this).removeClass('hidden');
          $(this).prev().removeAttr('style'); // removes 'hide border' code below.
          if ($(this).offset().top + $(this).outerHeight() > bound) {
            $(this).addClass('hidden');
            $(this).prev().not('.videos').css('border', 'none');
          }
        }
      });

    if ($('.videos').is('.hidden')) { // show hide border depending on video visibility.
      $('.vid-sibling').removeAttr('style');
    } else {
      $('.vid-sibling').css('border', 'none');
    }
  }



  function trimRails()
  {
    // don't run for mobile. refer to CSS media queries
    // for meaning of the value of 30.
    if ($('#state-indicator').css('z-index') < 30) {
      return;
    }

    var content = $('section.homepage .content');
    var bound = content.offset().top + content.outerHeight();
    var articles = $('.sidebar.secondary .ai-articles');

    $('.sidebar').css({ maxHeight: $('section.homepage .content').outerHeight() + 'px' });
    $('.sidebar.primary .cl > li, .sidebar.secondary .ai-row').each(function ()
    {
      $(this).removeClass('hidden');
      $(this).prev().removeAttr('style'); // removes 'hide border' code below.

      if ($(this).is('.ai-row')) {
        b = bound - articles.outerHeight();
      } else {
        b = bound;
      }

      if ($(this).offset().top + $(this).outerHeight() > b ) {
        $(this).addClass('hidden');
        $(this).prev().not('.videos').css('border', 'none');
      }
    });

    if ($('.videos').is('.hidden')) { // show hide border depending on video visibility.
      $('.vid-sibling').removeAttr('style');
    } else {
      $('.vid-sibling').css('border', 'none');
    }
  }

  function aspectFix()
  {
    var cl = $('.cl-2 .thumbnail, .cl-3 .thumbnail');
    cl.css('display','none');
    cl.offsetHeight;
    cl.css('display','');
  }
  
  var path = window.location.pathname;
  $(window).on('resize', (path === "/uk/" ? trimRailsUK : trimRails));
  $(window).on('resize', aspectFix);

  // Newsletter Placeholder
  $(document).ready(function(){
    var nlForm = $('form.newsletter-widget-form-homepage');
    nlForm.find('input[type=text]').attr('placeholder','Enter Your Email');
    nlForm.submit(function (e) {
          var result = false;
          $(this).find("input[name=inv_id]").validate({
            rules: [
              { type: "notempty", msg: "Please provide your email address." },
              { type: "email" }
            ],
            onComplete: function (res, elm) {
              result = res;
            } });
          return result;
        }
    );

    // Moved our trimRails function into a doc ready with a small timeout
    setTimeout( function(){
      if(path === "/uk/") {
        trimRailsUK();
      } else {
        trimRails();
      }
    }, 1500);
  });

</script>

