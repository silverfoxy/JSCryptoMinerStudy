<script>

    window.addEventListener('DOMContentLoaded', function() {
        $(function () {
            var preloadIndices = {"error":{}};
            var marketsApiPath = 'markets/api/marketsbar/';

            // Fail gracefully
            if (preloadIndices != null && preloadIndices['error']){
                $('.markets-bar').addClass('hidden');
                return;
            }

            // Controls for the markets selector drop menu
            $('.market-list-item').click(function(event){
                $('.dropmenu .title').text( $(event.target).text() );
                $(this).closest('ul').find('.market-list-item > a').removeClass('selected');
                $(this).find('> a').addClass('selected');

                var category = $(this).find('a').data('type');
                $('.markets-row').not('.'+category).addClass('hidden');
                $('.markets-row.'+category).removeClass('hidden');
            });

            // Fetch and render market data
            $('.markets-row').not('.preload').each(loadMarket);

            if ($('.markets-row.preload').length > 0){
                renderMarketData($('.markets-row.preload'), preloadIndices)
            }

            // Function reads data attributes from the HTML and uses it to gather and render the appropriate market data
            // through the corresponding data api.
            function loadMarket(){
                var elm = $(this);
                var api = elm.data('type');


                var queryData = {};
                elm.find('.markets-item').each(function(i, row) {
                    var key = "symbols[" + $(row).attr('id') + ']';
                    queryData[key] = $(row).data('symbol');
                });

                // Get data for all row elements in one request - this helps combat request overhead
                var requestURL = marketsApiPath + api + '/';

                // response format : { id: [value, percent-change, [points]] }
                $.getJSON(requestURL, queryData, function(response){
                    renderMarketData(elm, response);
                });
            }

            // Takes an associative json array of the format
            //  data = { id: [value, percent-change, [points]] }
            // and uses the data to fill in fields, create a chart and display up or down indicators
            function renderMarketData(elm, response){
                var blankChart = {
                    credits: {
                        enabled: false
                    },
                    chart: {
                        type: 'area',
                        spacing: [0, 0, 0, 0],
                        width: 120,
                        height: 60
                    },
                    exporting: {
                        enabled: false
                    },
                    title: {
                        text: '',
                        enabled:false
                    },
                    xAxis: {
                        title: {text: ''},
                        labels: {enabled: false},
                        lineWidth: 0,
                        minorGridLineWidth: 0,
                        lineColor: 'transparent',
                        minorTickLength: 0,
                        tickLength: 0
                    },
                    yAxis: {
                        title: {text: ''},
                        labels: {enabled: false},
                        gridLineColor: 'transparent'
                    },
                    legend: {
                        enabled: false
                    },
                    tooltip: {
                        enabled: false
                    },
                    plotOptions: {
                        series: {
                            enableMouseTracking: false,
                            animation: false
                        },
                        area: {
                            fillOpacity: .05,
                            marker: {enabled: false}
                        }
                    },
                    series: [{
                        name: '',
                        data: 1
                    }]
                };

                $.each(response, function(id, data) {
                    var marketItem = elm.find('#' + id);
                    var value = data[0];
                    var percentChange = data[1];
                    var close = data[2];
                    var points = data[3];


                    var rounding = 2;
                    if (elm.hasClass('bonds')){
                        rounding = 3;
                    }

                    // Update stock trend arrow
                    if (percentChange > 0) {
                        marketItem.find('.name').addClass('value-up');
                    } else if (percentChange < 0) {
                        marketItem.find('.name').addClass('value-down');
                    }

                    // Update value
                    marketItem
                        .find('.value')
                        .text(
                            (Math.round(value * Math.pow(10, rounding+1)) / Math.pow(10, rounding+1)).toFixed(rounding)
                        );

                    var percentString = (
                        Math.round(percentChange * Math.pow(10, rounding+1)) / Math.pow(10, rounding+1))
                            .toFixed(rounding) + "%";
                    if (percentChange > 0){ percentString = "+"  + percentString; }
                    marketItem.find('.percent').text( percentString );

                    // Update chart
                    var chartConfig = blankChart;
                    chartConfig.series = [{
                        name: '',
                        data: points
                    }];

                    var range = points.slice(0);

                    if (close) {
                        range.push(close);
                    }

                    var minPoint = Math.min.apply(Math, range);
                    chartConfig.yAxis.min = minPoint - (minPoint * 0.001);

                    var maxPoint = Math.max.apply(Math, range);
                    chartConfig.yAxis.max = maxPoint + (maxPoint * 0.001);

                    if (close){
                        chartConfig.yAxis.plotLines = [{
                            value: close,
                            color: 'lightgrey',
                            dashStyle: 'shortdash',
                            width: 1
                        }]
                    };

                    marketItem.find('.chart').highcharts(chartConfig);
                });
            }

        });
    });

</script>

