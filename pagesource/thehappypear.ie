    height: 100%;
    top: 0;
    right: 0;
    position: fixed;
    z-index: 9998;
    overflow: scroll;
    border-top: none;
    padding-top: 60px !important;
}

.et_mobile_menu li a {
    color: #E35205 !important;
    width: 100%;
    float: left;
    text-align: left;
    border-bottom: 1px solid #ddd;
    margin: 5px;
}
