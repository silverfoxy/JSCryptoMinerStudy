<script type="text/javascript">
    // Call this function when a user clicks on a product link,This function uses the event
    //callback datalayer variable to handle navigation after the ecommerce data has been sent to Google Analytics.
    function userClicks(productName, sku, price, position, productUrl , category, actionList){

        dataLayer.push({
            'event': 'productClick',
            'ecommerce': {
                'click': {
                    'actionField': {'list': actionList},
                    'products':[{
                        'name': productName,
                        'id': sku,
                        'price':price,
                        'category':category,
                        'position':position
                    }]
                }
            },
            'eventCallback': function() {
                document.location = productUrl
            }
        });

    }

    //CON-648 Enable predefined product click
    function searchClick(productName){

        dataLayer.push({
            'event': 'predefinedSearch',
            'preClickedProduct': productName
        });

    }

    function relatedClick(productName,productObj){

        var $items = jQuery(".you-might-also-like .item-info > a");
        var currentIndex = $items.index( jQuery(productObj))+1;

        dataLayer.push({
            'event': 'relatedProducts',
            'relatedproductName': productName,
            'productSlot' : currentIndex
        });

    }

    function recentClick(productName,slot){
        dataLayer.push({
            'event': 'recentProducts',
            'recentproductName': productName,
            'productSlot':slot
        });

    }
</script>
