    function dailySnsShare(shareType){
        var url = ""
            , title = ""
            , date	= new Date()
            , year	= date.getFullYear()
            , month = addZeros(date.getMonth() + 1, 2)
            , day	= addZeros(date.getDate(), 2)
            , today = year + month + day;

        title = year + '.' + month + '.' + day + ' SBS Daily Conpick';

        title = title.replace(/replace-quotes-072/g, "\"");
        title = title.replace(/replace-one-quotes-282/g, "'");

        url = "http://w3.sbs.co.kr/dailySnsPage.do?searchDate=" + today;

        var kakaoTitle = title;
        title = encodeURIComponent(title);

        var kakaoUrl = url;
        url = encodeURIComponent(url);

        switch (shareType){
            // Facebook
            case 'F':
                url = "http://www.facebook.com/sharer.php?u=" + url;
                break;
            // Twitter
            case 'T':
                url = "http://www.twitter.com/home?status="
                    + title
                    + "%0A" + url;
                break;
            // kakao
            case 'K':
                Kakao.Story.share({
                    url: kakaoUrl,
                    text: kakaoTitle
                });
                break;
            // BAND
            case 'B':
                url = "http://share.naver.com/web/shareView.nhn?url=" + url + "&title=" + title;
                break;
            default:
                break;
        }

        if(shareType != 'K'){
            window.open(url, 'ShareonSNS', 'width=510, height=500, scrollbars=no, resizeadble=no');
        }
    }

    function addZeros(n, digits){
        var zero = '';

        n = n.toString();

        if(n.length < digits){
            for(var ii=0; ii<digits - n.length; ii++){
                zero += '0';
            }
        }

        return zero+n;
    }

	/*
	 * 설정된 시간에 맞추어 메인에 GNB를 드로잉한다.
	 */
    function gnbTimeSet(){

				var mainTopBannerList      = null
						, cnrId = null
						, pcImgUrl = null
						, pcImgAlt = null
						, pcCloseImgUrl = null
						, closeImgAlt = null
						, bgColor = null
						, pcLnkUrl = null
						, itmBegDt = null
						, itmEndDt = null
						, svcYn = null;

				$.ajax({
						url: 'http://ccapi.sbs.co.kr/openapi/rest/main/TopBannerList',
						type: 'get',
						dataType: 'jsonp',
						success: function (data) {
								mainTopBannerList = data[0];
								cnrId = mainTopBannerList.cnrId;
								pcImgUrl = mainTopBannerList.pcImgUrl;
								pcImgAlt = mainTopBannerList.pcImgAlt;
								pcCloseImgUrl = mainTopBannerList.pcCloseImgUrl;
								closeImgAlt = mainTopBannerList.closeImgAlt;
								bgColor = mainTopBannerList.bgColor;
								pcLnkUrl = mainTopBannerList.pcLnkUrl;
								itmBegDt = mainTopBannerList.itmBegDt;
								itmEndDt = mainTopBannerList.itmEndDt;
								svcYn = mainTopBannerList.svcYn;

								var bYear = itmBegDt.substring(0,4);
                var bMonth = itmBegDt.substring(4,6)-1;
                var bDay = itmBegDt.substring(6,8);
                var bHours = itmBegDt.substring(8,10);
                var bMinutes = itmBegDt.substring(10,12);
                var bSeconds = "00";

                var eYear = itmEndDt.substring(0,4);
                var eMonth = itmEndDt.substring(4,6)-1;
                var eDay = itmEndDt.substring(6,8);
                var eHours = itmEndDt.substring(8,10);
                var eMinutes = itmEndDt.substring(10,12);
                var eSeconds = "00";

                var nowDate = new Date()
									, bDate = new Date(bYear, bMonth, bDay, bHours, bMinutes, bSeconds)       		// 시작 날짜(year, month, day, hours, minutes, seconds, milliseconds)
									, eDate = new Date(eYear, eMonth, eDay, eHours, eMinutes, eSeconds);        	// 종료 날짜(year, month, day, hours, minutes, seconds, milliseconds)

                var htmlStr = '';

                if(nowDate > bDate && nowDate < eDate){
                    htmlStr += '<div class="bigevent_bnr_w" style="background:none; background-color:#'+bgColor+';">';
                    htmlStr += '    <div class="bigevt_bnr"><a href=\"'+pcLnkUrl+'\"><img src=\''+pcImgUrl+'\' alt=\''+pcImgAlt+'\' /></a></div>';
                    htmlStr += '        <button type="button" class="bigevt_closed" style="background-image:url(\''+pcCloseImgUrl+'\');"><i class="ir">닫기</i></button>';
                    htmlStr += '</div>';

                    if(svcYn == 'Y'){
                        $('body').prepend(htmlStr);

                        $('.bigevt_closed').click(function(){
                            $(this).parent('.bigevent_bnr_w').css('display','none');
                        });
										}
                }
						},
						error: function (xhr, status, error) {
						}
				});
	  }
	</script>
