                                promContentsStr += '    		<span class="mprm_title">' + contents + '</span>\n';
                                promContentsStr += '		</strong>\n';
                                promContentsStr += '		<img src="' + imgUrl + '" alt="' + imgDsc + '" class="mprm_thumb" wid;th="355" height="520">\n';
                                promContentsStr += '    	<span class="text_gra_dim"></span>\n';
                                promContentsStr += '    </a>\n';
                                promContentsStr += '</li>\n';
                            }

                            if(tabNum == '3'){			//	- 배너 영역
                                promBannerStr += '<li class="mprm_blcont_top">\n';
                                promBannerStr += '    <a href="' + lnkUrl + '" class="mprm_bl_link" target="' + newWindowYn + '">\n';
                                promBannerStr += '    	<img src="' + imgUrl + '" alt="' + imgDsc + '" class="mprm_thumb" width="300" height="260">\n';
                                promBannerStr += '    </a>\n';
                                promBannerStr += '</li>\n';
                            }
                        }
                    }
                }
            }

            if(pickTabExistFlag){		// pick 탭을 사용 할 경우
                $('.matl_cont_pick').show();
                pickTimeObj.html(pickTimeStr);
                pickContentsObj.prepend(pickContentsStr);
                pickSubContentsObj.prepend(pickSubContentsStr);

                if(!pickRandomFlag){
                    randomFlagArr.push('pick');
                }

                existFlagArr.push(pickTabExistFlag);
            }

            if(spotTabExistFlag){		// spot 탭을 사용 할 경우
                $('.matl_cont_spor').show();
                spotTimeObj.html(spotTimeStr);
                spotContentsObj.prepend(spotContentsStr);

                if(!spotRandomFlag){
                    randomFlagArr.push('spor');
                }

                existFlagArr.push(spotTabExistFlag);
            }

            if(promTabExistFlag){		// prom 탭을 사용 할 경우
                $('.matl_cont_prom').show();
                promTimeObj.html(promTimeStr);
                promContentsObj.prepend(promContentsStr);
                promBannerObj.html(promBannerStr);

                if(!promRandomFlag){
                    randomFlagArr.push('prom');
                }

                existFlagArr.push(promTabExistFlag);
            }

            var xCount = 0;

            for(var ii=0; ii<3; ii++){
                if(ii == 0){
                    if(templateOrderArr['pick'] != null){
                        templateOrderFixArr.push(templateOrderArr['pick']);
                    }else{
                        templateOrderFixArr.push('x');
                        xCount++;;
                    }
                }else if(ii == 1){
                    if(templateOrderArr['spot'] != null){
                        templateOrderFixArr.push(templateOrderArr['spot']);
                    }else{
                        templateOrderFixArr.push('x');
                        xCount++;;
                    }
                }else{
                    if(templateOrderArr['prom'] != null){
                        templateOrderFixArr.push(templateOrderArr['prom']);
                    }else{
                        templateOrderFixArr.push('x');
                        xCount++;;
                    }
                }
            }

            for(var jj=0; jj<templateOrderFixArr.length; jj++){
                for(var kk=0; kk<3; kk++){
                    if(xCount < 2){
                        if(templateOrderFixArr[jj] == kk){
                            reSetTabArr[kk] = randomTabArr[jj];

                            if(templateOrderFixArr[jj] == 'x'){
                                reSetTabArr[kk] = '';
                            }
                        }
                    }else{
                        if(templateOrderFixArr[jj] == 'x'){
                            reSetTabArr[jj] = '';
                        }else{
                            reSetTabArr[jj] = randomTabArr[kk];
                        }
                    }
                }
            }

            if((3-xCount) > 1 && randomFlagArr.length > 1){
                randomFlagArr = drawRandomTabContents(randomFlagArr);
            }

            randomTabObj.html(reSetTabArr);

            for(var kk=0; kk<reSetTabArr.length; kk++){
                if(!isNull(reSetTabArr[kk])){
                    randomTabName = $(reSetTabArr[kk]).attr('class');

                    if(randomTabName.match(randomFlagArr[0])){
                        $(reSetTabArr[kk]).addClass('current');

                        $('.main_right_pick').removeClass('current');
                        $('.main_right_spor').removeClass('current');
                        $('.main_right_prom').removeClass('current');

                        if(randomTabName.match('pick')) $('.main_right_pick').addClass('current');		// 탭 영역별 활성화 (Pick)
                        if(randomTabName.match('spor')) $('.main_right_spor').addClass('current');      // 탭 영역별 활성화 (Spor)
                        if(randomTabName.match('prom')) $('.main_right_prom').addClass('current');      // 탭 영역별 활성화 (Prom)
                        break;
                    }
                }
            }

            mainTabCount = existFlagArr.length;		// 활성화 메뉴 수

        }
    }

    // 메인 탭 랜덤 기능
	/*
	 *	배열의 수 만큼 선택 할 탭 영역 정보를 넣음
	 */
    function drawRandomTabContents(obj){
        var randomArr        = obj
            , dummyObject    = []
            , probabilityArr = []
            , returnObject   = []
            , addProbability = 10;

        if(randomArr.length > 2){
            probabilityArr = [60, 40, 0];

        }else{
            probabilityArr = [100, 0];
        }

        for(var ii=0; ii<probabilityArr.length; ii++){
            if(randomArr[ii].indexOf('spot') > -1){
                probabilityArr[ii] += addProbability;
            }else{
                if(randomArr.length < 2){
                    probabilityArr[ii] -= addProbability/2;
                }
            }

            for(var jj=0; jj<probabilityArr[ii]; jj++){
                dummyObject.push(randomArr[ii]);
            }
        }

        returnObject.push(dummyObject[Math.floor(Math.random()*100)]);

        return returnObject;
    }

