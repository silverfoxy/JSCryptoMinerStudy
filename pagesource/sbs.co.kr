            , leftArrowObj		= indicObj.find('.mncb_arrow_l')	// 좌측 화살표 오브젝트
            , rightArrowObj		= indicObj.find('.mncb_arrow_r');	// 우측 화살표 오브젝트

        if(indicClassName == '#promotionBannerLeft')  targetObj = $('.mnpb_w_l .mnpb_list .mnpb_cont');				// 1. 메인 프로모션 좌측일 경우
        if(indicClassName == '#promotionBannerRight') targetObj = $('.mnpb_w_r .mnpb_list .mnpb_cont');				// 2. 메인 프로모션 우측일 경우
        if(indicClassName == '#groupPromotionBanner') targetObj = $('.mn_btm_bnr .mnb_bnr_list .mnpb_cont');		// 3. 구룹사 홍보 배너일 경우
        if(indicClassName == '#groupLogoBanner')	  targetObj = $('.mnb_etc_inner .mnb_etc_list .mnb_el_cont');	// 4. 구룹사 로고일 경우

        // 좌측 버튼 클릭 시
        leftArrowObj.on('click', function(){
            indicNumObj = indicObj.find('.mncb_num');
            indicNum	= Number(indicNumObj.text());

            if(indicNum > 1){
                targetObj.eq(indicNum-1).hide();
                targetObj.eq(indicNum-2).show();
                indicNumObj.text(indicNum-1);
            }
        });

        // 우측 버튼 클릭 시
        rightArrowObj.on('click', function(){
            indicNumObj = indicObj.find('.mncb_num');
            indicNum	= Number(indicNumObj.text());

            if(indicNum < indicAllNum){
                targetObj.eq(indicNum-1).hide();
                targetObj.eq(indicNum).show();
                indicNumObj.text(indicNum+1);
            }
        });
    }

