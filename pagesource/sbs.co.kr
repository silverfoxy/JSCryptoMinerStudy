    <script type="text/javascript">
        $(document).ready(function(){
            mainDrawLayoutBookMark();		// 메인 draw 관련 이벤트 북마크(1순위)
            gnbTimeSet();
        });

    document.domain = 'sbs.co.kr';

    var noticeStatus  = false;			// 공지사항 로딩여부

    var mainTabCount	   = 0
        , existFlagArr	   = []
        , pickTabExistFlag = false		// pick탭 데이타 존재여부
        , spotTabExistFlag = false      // spot탭 데이타 존재여부
        , promTabExistFlag = false;     // prom탭 데이타 존재여부

    var sbsGnbContainer = {};			// GNB 객체 생성 영역

    // 메인 draw 관련 이벤트 북마크
    function mainDrawLayoutBookMark(){
        var indexInfo = {
            gnbLayout : function(){		// gnb 레이아웃 관련 이벤트
                drawBanner();	// 2017추석 특집 배너 드로잉(타이머).
                //drawMainGnbContents();	//	- 메인 GNB html 드로잉
            },
            bodyLayout : function(){	// body 레이아웃 관련 이벤트
                setMainContents();      //  - 메인 콘텐츠 데이터 바인딩 및 html 드로잉
                setNotice();			//	- 메인 공지사항 데이터 바인딩 및 html 드로잉
            },
            footerLayout : function(){	// footer 레이아웃 관련 이벤트
            }
        };

        indexInfo.gnbLayout();
        indexInfo.bodyLayout();

    }

    //start : 2017 추석특집 페이지 배너노출을 위해 생성.
    function drawBanner() {

        var originBannerOption = {	//기본옵션
            title: '',
            isOpen: true,
            url: {
                image: '',
                link: ''
            }
        };

        var chuseokBannerOption = {	//추석옵션
            title: 'SBS 2017년 추석 특집 페이지',
            isOpen: true,
            url: {
                image: 'http://img.sbs.co.kr/event/2017_chooseok/@bnr_2017chooseok.jpg',
                link: 'http://tv.sbs.co.kr/2017chuseok/index.jsp'
            }
        };

		var d = new Date();
        var today = d.getFullYear() + ('0' + (d.getMonth() + 1)).slice(-2) + ('0' + d.getDate()).slice(-2);
        if(today > 20170929 && today < 20171010) {
            drawMainGnbContents(chuseokBannerOption);	//	- 메인 GNB html 드로잉
        }else{
            drawMainGnbContents(originBannerOption);
		}
    }
    //end : 2017 추석특집 페이지 배너노출을 위해 생성.

    // 메인 GNB html 드로잉
    function drawMainGnbContents(bannerOption){

        sbsGnbContainer = new SbsGnbContainer({
            render: 'server',  // server, browser
            charset : 'utf8',
            root: '',
            header: {
                id: 'header',
                service: '',
                type: 'normal',      // normal, simple
                layout: 'normal',    // wide, normal, narrow
                /*banner: {
                    title: '',
                    isOpen: true,
                    url: {
                        image: '',
                        link: ''
                    }
                }*/
                banner : bannerOption
            },
            footer: {
                id: 'footer',
                type: 'normal',      // normal, simple
                layout: 'normal'    // wide, normal, narrow
            }

        });

        sbsGnbContainer.initialize();
        sbsGnbContainer.render();
    }

    // 메인 레이아웃 이벤트 북마크(footer까지 포함)
    function mainLayoutBookMark(){
        var indexInfo = {
            layoutEvent : function(){						                // 레이아웃 관련 이벤트
            },
            dynamicEvent : function(){										// 동적 관련 이벤트
                redirectDevice();                                           //  - 모바일 디바이스로 접근시 리다이렉트 처리
                setMainTabEvent();						                    //	- 메인 탭 이벤트 셋팅
                setMainSubTab();							                //	- 메인 pick탭 pdNote/Clip 탭 이벤트 셋팅
                setMainBannerIndic('#promotionBannerLeft');	                //	- 메인 좌측 프로모션 배너 좌, 우 이동기능
                setMainBannerIndic('#promotionBannerRight');                //	- 메인 우측 프로모션 배너 좌, 우 이동기능
                setMainBannerIndic('#groupLogoBanner');		                //	- 구룹사 로고배너 좌, 우 이동기능
                setMainBannerIndic('#groupPromotionBanner');                //	- 구룹사 홍보베너 좌, 우 이동기능
                setNewsTabEvent('.mnn_tab_list');							//	- 비디오 머그/스브스 뉴스 탭 전환 기능
                setFooterSelectBanner('.sfms_inner_family .sfms_title');	// 	- 패밀리 사이트 셀렉트 박스 ON/OFF
                setFooterSelectBanner('.sfms_inner_area .sfms_title');		//	- 지역민영방송 셀렉트 박스 ON/OFF
                setScrollEvent();											//	- GNB 스크롤 이벤트
            }
        };

        indexInfo.dynamicEvent();
    }

    // 메인 콘텐츠 데이터 바인딩 및 html 드로잉
    function setMainContents(){
        var mainTabContents      = null
            , mainPdNoteContents = null
            , mainClipContents	 = null
            , promBannerLeft     = null
            , promBannerRight    = null
            , groupPromBanner    = null
            , groupLogoBanner    = null
            , mainNewsContents	 = null;

        $.ajax({
            url: '/json/newMain.js',
            type: 'get',
            dataType: 'json',
            success: function(data){
                mainTabContents		= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].mainTabContents;
                mainPdNoteContents	= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].mainPdNoteContents;
                mainClipContents	= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].mainClipContents;
                promBannerLeft		= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].promotionLeft;
                promBannerRight		= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].promotionRight;
                groupPromBanner		= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].groupPromotion;
                groupLogoBanner		= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].groupLogo;
                mainNewsContents	= data.HashMap.newAllMainDisplays.newAllMainDisplayList[0].mainNewsContents;

                drawMainTabContents(mainTabContents);                            	// 메인 탭 html 출력
                drawMainPdNoteContents(mainPdNoteContents, "#mainPdNoteContents");	// 메인 피디노트 순위 html 출력
                drawMainClipContents(mainClipContents, "#mainClipContents");		// 메인 클립 순위 html 출력
                drawMainBannerList(promBannerLeft, '#promotionBannerLeft');        	// 메인 프로모션 좌측 html 출력
                drawMainBannerList(promBannerRight, '#promotionBannerRight');      	// 메인 프로모션 우측 html 출력
                drawMainBannerList(groupPromBanner, '#groupPromotionBanner');      	// 구룹사 홍보 배너 html 출력
                drawMainBannerList(groupLogoBanner, '#groupLogoBanner');           	// 구룹사 로고 배너 html 출력
                drawMainNewsContents(mainNewsContents);

            },
            complete : function(){
                mainLayoutBookMark();		// 메인 layout 이벤트 북마크(footer까지 포함)
                $('#container').show();
            },
            error: function (xhr, status, error) {
            }
        });
    }

    // 메인 탭 html 출력
    function drawMainTabContents(obj){
        var mainTabContents = obj
            , templateType  = ''
            , tabNum        = ''
            , title         = ''
            , contents      = ''
            , lnkUrl        = ''
            , lnkUseYn      = ''
            , newWindowYn   = ''
            , imgUrl        = ''
            , imgDsc        = ''
            , pcYn          = ''
            , etc1          = ''
            , etc2          = ''
            , etc3          = ''
            , repreTitle    = ''
            , repreImgUrl   = ''
            , repreImgDsc   = ''
            , orderNum		= ''
            , randomFlag	= true		// true:고정, false:미고정
            , tabUseYn		= 'N'
            , templateOrdNum = ''
            , templateOrderArr = new Array()
            , templateOrderFixArr = new Array();

        var pickTimeStr				= ''
            , pickContentsStr		= ''
            , pickSubContentsStr	= ''
            , pickTimeObj			= $('.matl_cont_pick .matl_program_list')			// Pick 프로그램 시간표
            , pickContentsObj		= $('.main_rinner_pick .mr_pick_l')					// Pick 메인 콘텐츠
            , pickSubContentsObj	= $('.main_rinner_pick .mr_pick_r .mr_pick_list')	// Pick 서브 콘텐츠
            , pickRandomFlag		= true;		// pick 탭 위치 랜덤여부(true:고정, false:미고정)

        var spotTimeStr			= ''
            , spotContentsStr	= ''
            , spotTimeObj		= $('.matl_cont_spor .matl_program_list')				// Spotlight 프로그램 시간표
            , spotContentsObj	= $('.mr_sportlight_inner .mrsl_list')					// Spotlight 메인 콘텐츠
            , spotRepreObj		= $('.main_right_spor')									// Spotlight 배경
            , spotRepreTitleObj	= $('.mr_sportlight_inner .mr_sportlight_title')		// Spotlight 배경 타이틀
            , spotRandomFlag	= true;		// spot탭 위치 랜덤 여부(true:고정, false:미고정)

        var promTimeStr			= ''
            , promContentsStr	= ''
            , promBannerStr		= ''
            , promTimeObj		= $('.matl_cont_prom .matl_program_list')				// Promotion 프로그램 시간표
            , promContentsObj	= $('.mr_promotion_inner .mprm_list')					// Promotion 메인 콘텐츠
            , promBannerObj		= $('.mprm_bnr_list')									// Promotion 배너
            , promRandomFlag	= true;		// prom탭 위치 랜덤 여부(true:고정, false:미고정)

        var randomTabObj		= $('.main_acodientab_list')
            , randomTabLiObj	= $('.main_acodientab_list > li')
            , randomTabArr 		= randomTabLiObj
            , randomFlagArr     = []
            , reSetTabArr		= []
            , randomTabName = '';

        if(mainTabContents.length > 0){
            for(var ii=0; ii<mainTabContents.length; ii++){
                templateType = mainTabContents[ii].templateType;
                tabNum       = mainTabContents[ii].tabNum;
                title        = isNull(mainTabContents[ii].title) ? '' : mainTabContents[ii].title;
                contents     = isNull(mainTabContents[ii].contents) ? '' : mainTabContents[ii].contents;
                lnkUrl       = mainTabContents[ii].lnkUrl;
                lnkUseYn     = mainTabContents[ii].lnkUseYn;
                newWindowYn  = mainTabContents[ii].newWindowYn;
                imgUrl       = mainTabContents[ii].imgUrl;
                imgDsc       = mainTabContents[ii].imgDsc;
                pcYn         = mainTabContents[ii].pcYn;
                etc1         = mainTabContents[ii].etc1;
                etc2         = mainTabContents[ii].etc2;
                etc3         = mainTabContents[ii].etc3;
                repreTitle   = isNull(mainTabContents[ii].repreTitle) ? '' : mainTabContents[ii].repreTitle;
                repreImgUrl  = mainTabContents[ii].repreImgUrl;
                repreImgDsc  = mainTabContents[ii].repreImgDsc;
                orderNum	 = mainTabContents[ii].ord;
                randomFlag	 = mainTabContents[ii].displayRandomYn;
                tabUseYn	 = mainTabContents[ii].tabPcUseYn;
                templateOrdNum = mainTabContents[ii].templateOrdNum;

                if(pcYn == 'N') templateType = 'ignore';
                if(lnkUseYn == 'N') lnkUrl = 'javascript:;';
                if(newWindowYn == 'Y'){ newWindowYn = '_blank'; }else{ newWindowYn = '_self'; }
                if(randomFlag == '01'){ randomFlag = true; }else{ randomFlag = false; }

                if(tabUseYn == 'Y'){
                    if(templateType == '01'){		// Pick 일 경우
                        pickTabExistFlag = true;

                        if(orderNum == '1'){
                            templateOrderArr['pick'] = (templateOrdNum-1);
                            //templateOrderArr.push(templateOrdNum-1);
                        }

                        if(pickTabExistFlag){
                            pickRandomFlag	 = randomFlag;

                            if(tabNum == '1'){			//	- 프로그램 편성표
                                pickTimeStr += '<li class="matl_pl_cont">\n';
                                pickTimeStr += '    <a href="' + lnkUrl + '" class="matl_pl_link" target="' + newWindowYn + '">\n';
                                pickTimeStr += '		<span class="matl_logo">\n';
                                pickTimeStr += '	    	<img src="' + imgUrl + '" alt="' + imgDsc + '" width="64" height="64">\n';
                                pickTimeStr += '			<span class="matl_logo_mask"></span>\n';
                                pickTimeStr += '		</span>\n';
                                pickTimeStr += '	    <span class="matl_pl_text">' + title + '</span>\n';
                                pickTimeStr += '	</a>\n';
                                pickTimeStr += '</li>\n';

                            }else if(tabNum == '2'){	//	- 메인 콘텐츠
                                pickContentsStr += '<a href="' + lnkUrl + '" class="mr_pick_link" target="' + newWindowYn + '">\n';
                                pickContentsStr += '    <strong class="matl_title">' + title + '</strong>\n';
                                pickContentsStr += '    <img src="' + imgUrl + '" alt="' + imgDsc + '" class="matl_thumb" width="410" height="520">\n';
                                pickContentsStr += '    <span class="text_gra_dim"></span>\n';
                                pickContentsStr += '</a>\n';

                            }else{						//	- 서브 콘텐츠(tabnum :3, 4, 5)
                                if(tabNum == '3'){			//	- 서브 콘텐츠
                                    pickSubContentsStr += '<li class="mrpl_cont">\n';
                                    pickSubContentsStr += '	<a href="' + lnkUrl + '" class="mrpl_link" target="' + newWindowYn + '">\n';
                                    pickSubContentsStr += '    	<strong class="mrpl_title">\n';

                                    if(etc1 == 'Y'){
                                        pickSubContentsStr += 	'    		<span class="mnicn mrpl_label">Global</span>\n';
                                    }

                                    pickSubContentsStr += '    		<span class="mrpl_tit">' + title + '</span>\n';
                                    pickSubContentsStr += '		</strong>\n';
                                    pickSubContentsStr += '		<img src="' + imgUrl + '" alt="' + imgDsc + '" width="300" height="170" class="mrpl_thumb">\n';
                                    pickSubContentsStr += '    </a>\n';
                                    pickSubContentsStr += '</li>\n';

                                    if(orderNum == '1'){
                                        pickSubContentsStr += '<li class="mrpl_cont_even">\n';
                                        pickSubContentsStr += '</li>\n';
                                    }
                                }
                            }
                        }
                    }

                    if(templateType == '02'){		// Spotlight 일 경우
                        spotTabExistFlag = true;

                        if(orderNum == '1'){
                            templateOrderArr['spot'] = (templateOrdNum-1);
                            //templateOrderArr.push(templateOrdNum-1);
                        }

                        if(spotTabExistFlag){
                            spotRandomFlag	 = randomFlag;

                            if(tabNum == '1'){			//	- 프로그램 편성표
                                spotTimeStr += '<li class="matl_pl_cont">\n';
                                spotTimeStr += '    <a href="' + lnkUrl + '" class="matl_pl_link" target="' + newWindowYn + '">\n';
                                spotTimeStr += '		<span class="matl_logo">\n';
                                spotTimeStr += '	    	<img src="' + imgUrl + '" alt="' + imgDsc + '" width="64" height="64">\n';
                                spotTimeStr += '			<span class="matl_logo_mask"></span>\n';
                                spotTimeStr += '		</span>\n';

                                if(orderNum == '1'){
                                    spotTimeStr += '	    <span class="matl_pl_text">' + title + '</span>\n';
                                    spotTimeStr += '	</a>\n';
                                    spotTimeStr += '</li>\n';
                                }
                            }

                            if(tabNum == '2'){			//	- 콘텐츠 영역
                                spotRepreObj.css('background-image', 'url("' + repreImgUrl + '")');
                                spotRepreTitleObj.html(repreTitle);

                                if(orderNum == '2'){
                                    spotContentsStr += '<li class="mrsl_cont_img">\n';
                                }else{
                                    if(etc3 == '01'){
                                        spotContentsStr += '<li class="mrsl_cont_vod">\n';
                                    }else{
                                        spotContentsStr += '<li class="mrsl_cont_txt">\n';
                                    }
                                }

                                spotContentsStr += '    <a href="' + lnkUrl + '" class="mrsl_link" target="' + newWindowYn + '">\n';
                                spotContentsStr += '    	<strong class="mrsl_text_w">\n';
                                spotContentsStr += '    		<span class="mrsl_pro_value">' + title + '</span>\n';
                                spotContentsStr += '			<span class="mrsl_info_txt">' + contents + '</span>\n';
                                spotContentsStr += '   		</strong>\n';

                                if(orderNum == '2'){
                                    spotContentsStr += '    	<img src="' + imgUrl + '" alt="' + imgDsc + '" class="mrsl_thumb">\n';
                                    spotContentsStr += '		<span class="text_gra_dim"></span>\n';
                                }else{
                                    if(etc3 == '01'){
                                        if(etc2 == 'Y'){
                                            spotContentsStr += '    	<span class="btn_mnplay"><span class="mnicn icon_mnplay"><i class="ir">play</i></span></span>"\n';
                                        }

                                        spotContentsStr += '    	<img src="' + imgUrl + '" alt="' + imgDsc + '" class="mrsl_thumb">\n';
                                    }

                                    if(etc3 == '02'){
                                        spotContentsStr += '    	<span class="full_dim"></span>\n';
                                        spotContentsStr += '    	<span class="txt_align"></span>\n';
                                    }
                                }

                                spotContentsStr += '    </a>\n';
                                spotContentsStr += '</li>\n';
                            }
                        }
                    }

                    if(templateType == '03'){		// Promotion 일 경우
                        promTabExistFlag = true;

                        if(orderNum == '1'){
                            templateOrderArr['prom'] = (templateOrdNum-1);
                        }

                        if(promTabExistFlag){
