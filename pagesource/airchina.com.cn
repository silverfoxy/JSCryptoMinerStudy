<script>	
	function loadCssJsFiles(fileArray){
		for(var i = 0;i<fileArray.length;i++){
			var file = fileArray[i];
			var fileref = null,
				fileExt;
			
			if(file) fileExt = file.substring(file.lastIndexOf('.')+1);
			
			if(fileExt == 'js'){
				fileref = document.createElement('script');
				fileref.setAttribute('type','text/javascript');
				fileref.setAttribute('src',file);
				
			}else if(fileExt == 'css'){
				fileref = document.createElement('link');
				fileref.setAttribute("rel","stylesheet");
				fileref.setAttribute("type","text/css");
				fileref.setAttribute("href",file);
			}

			if(fileref){
				document.getElementsByTagName("head")[0].appendChild(fileref);
			}
			
		}		
	}
	function showLoading(){
		createConver();
		var scrollerTop = $(document).scrollTop();
		$('.loadingIcon').css('margin-top',(300+scrollerTop) + "px").show();
		$('.coverDiv').show();
		//setTimeout('hideLoading()',2000)				
	}
	function hideLoading(){
		$(".coverDiv").remove();
	}
	function createConver(){
		var loadingDiv = $("<div class='loadingIcon'>").css({margin: '300px auto auto',display:'none'}).append("<img src='/cn/images/info_and_services/loading110.gif'/>");
		$("<div class='coverDiv'>").css('display','none').append(loadingDiv).appendTo('body');
	}
	function tiaozhuan(){
			  var appendhtml = "<form target='_parent' method='post' id='tiaozhuan' action='http://webcall1.airchina.com.cn:20040/IM/login/wzlogin.do'><input name='data' value='"+getCookie("userName")+"'></form>";
			  $("body").append(appendhtml);
			  $("#tiaozhuan").submit();
	}
function getCookie(name) {
  var cookieValue="";
  var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
  if (arr != null) {
	  cookieValue= arr[2];
  }
  else{
	  cookieValue= null;
  }
	if (cookieValue != null && cookieValue != "" && cookieValue != "undefined"){
	  return unescape(cookieValue);}
	else
	{

		if(name=="locationName0"||name=="locationName1"||name=="locationName2"||name=="locationName3"){
			if(languageType=="cn"){
				return "中文/英文/拼音";
			}else{
				return "Chinese/English/PinYin";
			}
		}
		else{
			return "";
		}
		

	}
};
