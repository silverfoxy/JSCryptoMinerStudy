//获取凭证号码类型
function getIdType(ID_NO) {
    var isIDCard = /^[1-9]\d{5}[1-9]\d{3}((0\d)|(1[0-2]))(([0|1|2]\d)|3[0-1])\d{3}([0-9]|X)$/;
    var isEticket = /^[0-9]{13}$/;
    if (ID_NO.match(isIDCard)) {
        return 'NI';
    } else if (ID_NO.match(isEticket)) {
        return 'TN';
    } else {
        return 'PP';
    }
}
