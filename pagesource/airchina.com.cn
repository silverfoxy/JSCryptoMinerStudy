<script type="text/javascript">
var Request=new Object();   
Request=GetRequest(); 
function GetRequest(){
	var urlprams = location.search; //获取url中"?"符后的字串   
	var theRequest = new Object();   
	if(urlprams.indexOf("?") != -1)   
	{   
		var str = urlprams.substr(1);   
		strs = str.split("&");   
		for(var i = 0; i < strs.length; i ++)   
		{   
			theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);   
		}   
	}   
	return theRequest;   
}
