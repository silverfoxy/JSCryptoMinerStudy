function flightDynamicsSubmit(){
		var depAirport = $('#depAirportName').val();
		var arrAirport = $('#arrAirportName').val()
		var nowDate=new Date();
		var msg="";
		nowDate=new Date(nowDate.getFullYear(),nowDate.getMonth(),nowDate.getDate());
		if($("#zhanghaiyang").val()=="city"){
			if(depAirport==""){msg+="请填写起飞城市！\n";}
			if(arrAirport==""){msg+="请填写到达城市！\n";}
			if((arrAirport!="")&&(depAirport!="")&&(arrAirport==depAirport)){
				msg+="起飞城市与到达城市不能相同！\n";
			}
		}
		else{
			var flightNoFmt=/^([A-Za-z]{2}){0,1}[0-9]{3,4}[A-Za-z]{0,1}$/;
			
			if(!flightNoFmt.test(document.formFlightDynamic.flightNO.value)){
				msg+="航班号格式错误！\n";
			}
		}
		if(msg!=""){
				//alert(msg);
				alert("请您输入航班号/起飞城市/到达城市");
				return;
		}
		var depDate=document.formFlightDynamic.depDate.value.split("-");
	 	if('' == depDate){
	 		alert("请选择起飞日期");
	 		return;
	 	}
	 
		document.formFlightDynamic.requesttype.value=$("#zhanghaiyang").val();
		 
		depDate2=new Date(depDate[0],depDate[1]-1,depDate[2]);
		if(((nowDate.getTime()-86400000*2)>depDate2.getTime())||(depDate2.getTime()>(nowDate.getTime()+86400000*2)))
		{
			alert("进出港信息现只提供前后两天内的信息");
			return ;
		}
		var fromParams = 'arrAirport='+$('#arrAirportName').val()+'&depAirport='+ $('#depAirportName').val()+
						'&flightNO='+ $('#flightNO').val().toUpperCase() +"&companyCode="+$('#companyCode').val().toUpperCase()+
						'&depDate='+$('#DynamicDate').val()+'&requesttype='+$('#requesttype').val()+'&language=cn';
		location.href='/cn/service/flight-status-search/flight-dynamics.shtml?'+fromParams;
}


