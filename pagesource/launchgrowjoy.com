<style type="text/css" data-type="vc_custom-css"> 


/* HEADER  */

#bc-optin_header {
    background: url(/wp-content/uploads/optin_header_strip.png)no-repeat bottom center , url(/wp-content/uploads/BG-header_optin.jpg);
    background-size: contain, cover;
    padding-bottom: 60px;
}

/* TRAININGS  */

#bc-section-trainings {
    background: url(/wp-content/uploads/strip-training_section.jpg)no-repeat bottom center , url(/wp-content/uploads/BG-trainings.jpg);
    background-size: contain, cover;
    padding-bottom: 60px;
}

/* TESTIMONIAL */

#bc-section-testimonials .l-section-h{
    padding-top:20px;
}

.bc-testimonial-box .w-person-content {
    background: #faf5f0;
    text-align: left;
    margin-top: -30px;
    padding:15px;
    border: 1px solid #f39229;
    position:relative;
    z-index:1;
    box-shadow: 0px 0px 0px 15px #faf5f0, 0px -10px 0px 15px #f39229;
}

.bc-testimonial-box .w-person-content:before {
    content: url(/wp-content/uploads/icon-blockquote-left-sm.jpg);
    position:absolute;
    top: -10px;
    left: 10px;
}

.bc-testimonial-box .w-person-content:after {
    content: url(/wp-content/uploads/icon-blockquote-right-sm.png);
    position:absolute;
    bottom: -18px;
    right: 10px;
}

.bc-testimonial-box .w-person-description {
    margin-top: 25px;
}

.bc-testimonial-box .w-person-image {
    position:relative;
    z-index: 2;
}

.bc-testimonial-box .w-person-image img {
    width:120px;
    border-radius:50%;
    margin: 0 auto;
}

.bc-testimonial-box .bc-testimonial-name {
    margin-top: 40px;
    text-align: right;
    color: #1c4169;
}

.bc-testimonial-box .bc-testimonial-company {
    color: #879aae;
    text-align: right;
}

/* BLOG SECTION */

#bc-blog-section {
    padding-top:60px;
}


/* BOTTOM OPTIN */

.bc-curve-top-section {
    background: url(/wp-content/uploads/BG-curve.png)no-repeat top center #1b2938;
    background-size: contain;
    padding-top:60px;
}

#bc-section-sm .l-section-h {
    padding:40px 0 60px;
}

.bc-no-padding-bottom .l-section-h {
    padding-bottom:0px ;
}

/* BLOG 
============= */

.bc-blog-grid  .w-blog-post-h {
    border-radius:0 !important;
    box-shadow: 0 0px 4px 4px rgba(0,0,0,0.1) !important;
}

.bc-blog-grid .w-blog-post-h {
    font-size: 0.8rem;
    line-height: 1.6;
}

.bc-blog-grid .w-blog-post-body {
    padding: 20px !important;
}

.w-btn.style_outlined:after {
    border-width: 0px; 
    border-style: none; 
}

.bc-blog-grid .w-blog-post-more {
    text-align: right;
    padding: 0;
    display: block;    
    color: #b4c3d4 !important;
}

.w-btn.color_light.style_outlined:before {
    background-color: #fff !important;
}


/* IMAGES 
============= */

.bc-training-image img {
    background: rgba(255,255,255,0.24);
    padding:30px;
    border-radius:5px;
}


/* HELPERS
============= */

.bc-separator {
    margin-top: 0px !important;
    margin-bottom: 0px !important;
    height: auto;
}

.bc-text-center {
    text-align:center;
}

.bc-mt-60 {
    margin-top:60px;
}


/* MEDIA QUERY
=================== */

@media screen and (max-width:768px) {
    #bc-optin_header {
        padding-bottom:0px;
    }
    #bc-blog-section {
        padding-top:20px;
    }
    #bc-section-testimonials {
        padding-bottom:30px;
    }
}

@media screen and (max-width:767px) {
    
    .bc-mt-60 {
        margin-top: 20px;
    }
