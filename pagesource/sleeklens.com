	<script type="text/javascript">
		jQuery(document).ready(function() {
			jQuery('.main-slider').slick({
				dots: true,
				infinite: true,
				speed: 800,
				fade: true,
				autoplay: true,
				autoplaySpeed: 5000,
				cssEase: 'linear'
			});
			setTimeout(function(){
				jQuery('.main-slider').css('opacity', '1');
			}, 10);
		});

		$(document).ready(function() {
			var flag_xs = false;
			var mobileClass = [
				[ 0, 'xs' ],
				[ 500, 'xs' ],
				[ 768, '' ]
			];
			var attachMobileClass = function(){
				var w = $(window).width();
				var currentClass = mobileClass[0][1];
				var allClasses = [];
				for ( var i = 0; i < mobileClass.length; i++ ){
					if ( mobileClass[i][0] < w ){
						currentClass = mobileClass[i][1];
					}
					allClasses.push( mobileClass[i][1] );
				}
				$('body').removeClass( allClasses.join(' ') ).addClass( currentClass );

				if (currentClass == 'xs' && ! flag_xs) {
					flag_xs = true;
				}
				if (currentClass != 'xs') {
					flag_xs = false;
				}
			};
			attachMobileClass();
			function resizeImg() {
				var factorBanner = 1180 / 350;
				if($('body').hasClass('xs')){
				/*	$('.banner-main img').css('height', $('.container').width());*/
					$('.banner-main img,.slider-img-mobile img').css('width', $('.banner-main img').height()*factorBanner);
				}
			}
			resizeImg();
			$(window).on('resize', function() {
				resizeImg();
				attachMobileClass();
			});
		});
	</script>

