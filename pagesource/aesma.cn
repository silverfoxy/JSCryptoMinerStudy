<script>
$("#contactModal input[name=user]").bind('input propertychange', function (e) {
    var _this = $(this);
    if (_this.val().length < 6){
        _this.nextAll('.inerror').removeClass('inhide')
    } else {
        _this.nextAll('.inerror').addClass('inhide')
    }
});
$("#contactModal input[name=pass]").bind('input propertychange', function (e) {
    var _this = $(this);
    if (_this.val().length < 6){
        _this.nextAll('.inerror').removeClass('inhide')
    } else {
        _this.nextAll('.inerror').addClass('inhide')
    }
});
$("#contactModal input[name=repass]").bind('input propertychange', function (e) {
    var _this = $(this);
    if (_this.val() != $("#contactModal input[name=pass]").val()){
        _this.nextAll('.inerror').removeClass('inhide')
    } else {
        _this.nextAll('.inerror').addClass('inhide')
    }
});
$('.forget').on("click", function (){
    var fori = $('.forget-info');
    if (fori.hasClass("inhide")){
        fori.removeClass('inhide')
    } else {
        fori.addClass('inhide')
    }
});
