<script>
	var queryCountry = (function () {
        var onSuccess = function (geoipResponse) {
			var switchFunction = function(arg) {
				switch (arg){
					case 'QLD':
						$('.qld-phone').show();
						console.log('qld');
						break;
					case 'NSW':
						$('.nsw-phone').show();
						console.log('nsw');
						break;
					case 'VIC':
						$('.vic-phone').show();
						console.log('vic');
						break;
					case 'NT':
						$('.nt-phone').show();
						console.log('nt');
						break;
					case 'ACT':
						$('.act-phone').show();
						console.log('act');
						break;
					case 'SA':
						$('.sa-phone').show();
						console.log('sa');
						break;
					case 'WA':
						$('.wa-phone').show();
						console.log('wa');
						break;
					case 'TAS':
						$('.tas-phone').show();
						console.log('tas');
						break;
					default:
						console.log('default phone number trigger');
						$('.default-phone').show();
				}
			}
			var state = geoipResponse.subdivisions[0].iso_code;
			var cookieVal = $.cookie('geoState');
			if (cookieVal) {
				switchFunction(cookieVal);
			}
			else {
				$.cookie('geoState', state);
				switchFunction(state);
			}
        };
        var onError = function (error) {
            var state = none
        };
        return function () {
            geoip2.city(onSuccess, onError);
        };
    }());
    queryCountry();
</script>
