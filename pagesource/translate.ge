<script type="text/template" id="searchbox-template">
    <form class="form-horizontal" role="form">
      <div class="form-group has-feedback">
        <div class="col-sm-12">
          <input type="text" autocomplete="off" class="form-control searchbox" placeholder="Type word ..." value="<%= data.word %>" id="query_box">
          <span name="boxdefault" class="glyphicon glyphicon-search form-control-feedback" style="cursor: pointer;"></span> 
          <span name="boxdelete" class="glyphicon glyphicon-remove form-control-feedback" style="cursor: pointer; display:none;"></span> 
        </div>
      </div>
    </form>                  
</script>



<script type="text/template" id="suggest-template">
    
      <p>           
        <% if(_.isUndefined(data.first_suggestion)) return;  %>

        <% _.each(data.first_suggestion[0].options, function(item, key) { %>
                
                <% if(key===0) { %>
                      <span><span class="glyphicon glyphicon-tag"></span>  Did you mean ? :   &nbsp; </span>               
                <% } %>
                <a href="#search/<%= item.text %>"> <%=  item.text  %> </a> &nbsp;
          <% }) %>
      </p>

</script>


<script type="text/javascript">

    var dicionaries = ["Hindi;)", "Common", "Juridical",  "Marketing", "Economics", "Medicine", " Comprehensive Georgian-English Dictionary" ]
    var dictNames = { "geo": "Georgian-English", "eng": "English-Georgian" }

</script>

<script type="text/template" id="result-template">

        <%  _.each(data.rows, function(item) {  %>

          <div class="line_cnt">

              <span name="words_toolbar" class="toolbox"> 

                          <i><span class="dictname"> Dict:  <%= dicionaries[item.value.DictType] %> </span> </i> &nbsp; &nbsp;
                          <i text="<%= item.key %>" wordid="<%= item.id %>" class="icon-star-empty" title="Add favorites"></i>                  
                          <i wordid="<%= item.id %>" class="icon-list" title="Expand View"></i>
                          <a href="#suggest/<%= item.id %>" class="icon-edit" title="Suggest Edit"></a>

              </span>

              <span class="word"> <b><%=  item.key %> </b>  </span> -  <i dataattr="sound" class="icon-volume-up sound" name="<%=  item.key %>"></i>
              
              <!--
              <%  if(!_.isUndefined(item.value.soundcode)) { %>
                  <%  if(item.value.soundcode === 200) { %>
                    <i dataattr="sound" class="icon-volume-up sound" name="<%=  item.key %>"></i>
                  <%  } %>    
              <% } %>
              -->

                <% var text = item.value.Text.replace("noun", "<span class='nva'>noun</span>") %>
                <% text = text.replace("verb", "<span class='nva'>verb</span>") %>
                <% text = text.replace("adjective", "<span class='nva'>adjective</span>") %>

                <span class="translation" id="<%= item.id  %>"><%= text %></span> 
            
          </div>

          <div suggest-template-id="<%= item.id  %>" class="form_suggest_edit">  </div>
                     
        <%  }); %>
         
</script>

<script type="text/template" id="fulltext-result-template"> 

          <div class="list-group">
              <%  if(_.isUndefined(data.hits)) {  return; }   %>  

              <% if(data.hits.hits.length !== 0) { %>
                    <div class="fulltextheader">
                        <span class="glyphicon glyphicon-search form-control-feedback"></span> &nbsp;    
                        <strong> Full text search result: </strong>
                     </div>      

                     <br>       
              <% } %>

              <%  _.each(data.hits.hits, function(item, val) {  %>
                    
                    <% console.log('item : ', item ); %>

                    <div class="list-group-item">
                      <%  var pattern = new RegExp(data.id, 'g'); %>
                         <h4 class="list-group-item-heading">
                               <%= item._source.Word %>  
                         </h4> 

                     <% if(data.id.length > 3) { %>
                        <span class="list-group-item-text" id="full<%= item._source._id %>">  <%= item._source.value.Text.replace(pattern, "<em> " +  data.id + " </em>")  %> </span>
                      <% }else{  %>
                          <span class="list-group-item-text" id="full<%= item._source._id %>">  <%= item._source.value.Text  %> </span>
                      <% } %>
                      <br>

                      <div class="row" style="margin-top: 2px;">
                        <div class="col-md-12">
                          <div class="pull-left">
                          </div>
                           <div class="pull-right toolbox-fulltext">
                              <small> <%=  dictNames[item._source.value.DictName] %>  <i> <%= dicionaries[item._source.value.DictType] %> </i> </small>
                              <!-- 
                              <span name="addfavorite" wordid="<%= item._source._id %>" text="<%= item._source.Word %>" title="Add to favorites"  class="icon-star-empty"></span>
                              <span name="expandview"  title="Expand text View"  class="icon-list"></span> 
                              <span name="suggestEdit" title="Suggest Edit" class="icon-edit"></span> 
                              -->

                           </div>
                        </div>
                     </div>                                                
                    </div>  
                    <hr style="margin: 10px;">      
              <%  }); %>

          </div>
 
    
        </div>
</script>


<script type="text/template" id="template-suggest">

          <br>
          <div name="editSuggetSaveResult"  class="alert alert-success alert-dismissable" style="display:none;">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                <strong> Thank you ! </strong> Your feedback has been successfully saved
          </div>

          <div name="editsuggestcontent">
              <label> Suggest your translation or Submit a copyright takedown notice </label>
              <form role="form">
                  <div class="form-group">
                      <textarea class="form-control" rows="2" placeholder="Description"></textarea>
                  </div>                  
                  <button type="button" name="sendfeedback" class="btn btn-default">Send </button>
              </form>
              <hr>
          </div>

</script>



<script type="text/template" id="favorites-template">

        <hr>
          <a class="btn btn-default"  href="#"> <span class="glyphicon glyphicon-arrow-left"> </span> Back  </a>

          <h2> My Favorites  <small> Local offline dictionary </small> </h2> 
        <hr>

        <% _.each(data, function(item) {   %>

             <a class="list-group-item">

                      <h4 class="list-group-item-heading"> <%= item.word  %>  </h4>
                      <p class="list-group-item-text">  <%= item.wordtext %> </p>
                      
                      <div class="row" style="margin-top: 5px;">
                        
                        <div class="col-md-12">
                             <div class="pull-right" style="color: #EDEDED;" >
                                <span wordid="<%= item.wordid %>" name="deletelocal" title="Delete" style="font-size: 22px;" class="glyphicon glyphicon-trash"></span>
                             </div>
                        </div>

                     </div>     
                    </a>  
              <hr>
          
        <% }); %>

</script>


<script type="text/template" id="template-google">
      <small> Google Translate </small>
      <div> 
            <% _.each(data.data.translations, function(item){  %>
                 <p class="lead"> <%= item.translatedText  %> </p>
                 <hr>
            <% }); %>
      </div>
</script>
