/* The animation code */
@keyframes cloud {
    0%   {left:-20%;}
    50% {left: 50%;}
    100% {left:120%;}
}

#hero {
    position:relative;
background-color: #0a5896; /* Old browsers */
background-image: linear-gradient(#0A5896, #3896B5); /* W3C */
}

.hero-inner {
    position:relative;
    margin:0px;width:100%;height:100%;padding:0px;overflow:hidden;
}

#hero-background img {
    position:absolute;
    width:100%;
    z-index:0;
    bottom:0px;
    text-align:center;
}


.hero-logo img {z-index:1000; position:relative; margin-bottom:70px;padding:0px 0px;}

.cloud { 
    
    animation: cloud 64s linear infinite;
    position:absolute;
    background-color:#599bb6; 
    border-radius: 50% / 100%;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
}

.cloud:before, .cloud:after {
    content: '';
    visibility:hidden;
    position: absolute;
    background: #81b9cb;
    border-radius: 50% / 100%;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
}

.cloudone
{width:200px; height:29px; top: 100px; }

.cloudone:before {	
  visibility:visible;width: 80px;height: 19px;top: 10px;left: -31px;
}

.cloudtwo
{width:50px; height:10px; top: 160px;  animation-duration: 100s; 
margin-left: 30%; }

.cloudtwo:after {	
  visibility:visible;width: 20px;height: 5px;top: 5px;right: -11px;
  
}


.cloudthree
{width:300px; height:50px; top: 90px;  animation-duration: 70s; 
margin-left: 80%; background-color:#4084a7; }

.cloudthree:before {	
  visibility:visible;width: 136px;height: 19px;top: 31px;left: -54px;
}

.cloudthree:after {	
  visibility:visible;width: 93px;height: 20px;top: 30px;right: -50px;
   background-color:#4084a7;
}

.cloudfour
{width:70px; height:10px; top: 380px;  animation-duration: 300s; 
margin-left: 50%; background-color:#d8f0fb; }

.cloudfour:before {	
  visibility:visible;width: 22px;height: 5px;top: 5px;left: -10px;
}

.cloudfive
{width:70px; height:10px; top: 310px;  animation-duration: 500s; 
