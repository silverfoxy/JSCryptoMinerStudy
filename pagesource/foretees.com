<script type="text/javascript">

var slideTimer = undefined;

var scrolldata_apps = {
    features: ['reservations', 'notifications', 'beacons', 'ordering', 'checkin', 'valet'],
    stories: ['res', 'not', 'bea', 'ord', 'che', 'val']
};
var scrolldata_reservations = {
    features: ['teetimes', 'golf', 'dining', 'courts', 'fitness']
};

var scrolldata_websites = {
    features: ['sanctuary', 'mesaverde', 'firerock', 'waialae', 'hallbrook', 'skaneateles']
};

var scrolldata_clubmanagement = {
    features: ['finance', 'crm', 'events', 'reports', 'pos']
};

function resetSlideTimer() {
    if (slideTimer) {
        clearInterval(slideTimer);
    }

    slideTimer = setInterval(function() {
        slideToNext();
    }, 8000);
}

function openSlideItem(evt, featureName, iconClassName) {
    // Declare all variables
    var i, tabcontent, iconlinks;

    // Get all elements with class="tabcontent" and hide them
    jQuery('.' + iconClassName).css('display', 'none');

    jQuery('.wpb_wrapper a.iconlinks').removeClass('active');

    // Show the current tab, and add an "active" class to the link that opened the tab
    jQuery('#' + featureName).css('display', 'block');
    jQuery('#' + featureName).addClass('active-slide');
    if (evt)
        jQuery(evt.currentTarget).addClass('active');

   resetSlideTimer();
}

function openFeature(evt, featureName) {
   openSlideItem(evt, featureName, 'tabcontent');

}

function openStory(evt, featureName) {
    openSlideItem(evt, featureName, 'intro');
}

function getCurrentIconIndex() {
    var icons = jQuery('.wpb_wrapper a.iconlinks');
    for (var i = 0; i < icons.length; i ++) {
        var icon = jQuery(icons[i]);
        if (icon.hasClass('active')) {
            return i;
        }
    }

    return 0;
}

function setCurrentIconIndex(scrolldata, index) {
    var features = scrolldata.features;
    var stories = scrolldata.stories;
    if (index < 0) {
        index = features.length - 1;
    } else if (index >= features.length) {
        index = 0;
    }

    if (features) {
        openFeature(null, features[index]);
    }
    if (stories) {
        openStory(null, stories[index]);
    }

    var icons = jQuery('.wpb_wrapper a.iconlinks');
    var icon = jQuery(icons[index]);
    icon.addClass('active');
}

function isAppsPage() {
    return (document.getElementById(scrolldata_apps.features[0]) != undefined);
}

function isReservationsPage() {
    return (document.getElementById(scrolldata_reservations.features[0]) != undefined);
}

function isWebsitesPage() {
    return (document.getElementById(scrolldata_websites.features[0]) != undefined);
}

function isClubmanagementPage() {
    return (document.getElementById(scrolldata_clubmanagement.features[0]) != undefined);
}

function slideToPrev_Apps() {
    setCurrentIconIndex(scrolldata_apps, getCurrentIconIndex() - 1);
}

function slideToNext_Apps() {
    setCurrentIconIndex(scrolldata_apps, getCurrentIconIndex() + 1);
}

function slideToPrev_Reservations() {
    setCurrentIconIndex(scrolldata_reservations, getCurrentIconIndex() - 1);
}

function slideToNext_Reservations() {
    setCurrentIconIndex(scrolldata_reservations, getCurrentIconIndex() + 1);
}

function slideToPrev_Websites() {
    setCurrentIconIndex(scrolldata_websites, getCurrentIconIndex() - 1);
}

function slideToNext_Websites() {
    setCurrentIconIndex(scrolldata_websites, getCurrentIconIndex() + 1);
}

function slideToPrev_Clubmanagement() {
    setCurrentIconIndex(scrolldata_clubmanagement, getCurrentIconIndex() - 1);
}

function slideToNext_Clubmanagement() {
    setCurrentIconIndex(scrolldata_clubmanagement, getCurrentIconIndex() + 1);
}


function slideToPrev() {
    if (isAppsPage()) {
        slideToPrev_Apps();
    } else if (isReservationsPage()) {
        slideToPrev_Reservations();
    } else if (isWebsitesPage()) {
        slideToPrev_Websites();
    } else if (isClubmanagementPage()) {
        slideToPrev_Clubmanagement();
    }
}

function slideToNext() {
    if (isAppsPage()) {
        slideToNext_Apps();
    } else if (isReservationsPage()) {
        slideToNext_Reservations();
    } else if (isWebsitesPage()) {
        slideToNext_Websites();
    } else if (isClubmanagementPage()) {
        slideToNext_Clubmanagement();
    }
}

resetSlideTimer();

</script>

