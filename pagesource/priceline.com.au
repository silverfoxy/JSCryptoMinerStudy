    function getParameterByName(name) {
        var url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    function isFromCriteo() {
        return getParameterByName('referer') === 'criteo';
    }

    function isNotCriteo() {
        return !!document.referrer && document.referrer.indexOf('priceline.com.au') < 0 && document.referrer.indexOf('paypal.com') < 0;
    }

    function flagFromCriteo() {
        Cookies.set('is_criteo', 1, { expires: 30 });
    }

    function unFlagFromCriteo() {
        Cookies.remove('is_criteo');
    }

    if (isFromCriteo()) {
        flagFromCriteo();
    } else if (isNotCriteo()) {
        unFlagFromCriteo();
    }
})();
