        var win = new Window({
            className:              'newsletter-container',
            width:                  250,
            height:                 280,
            recenterAuto:           false,
            resizable:              false,
            minimizable:            false,
            maximizable:            false,
            showEffectOptions:      { duration: 0.4 },
            hideEffectOptions:      { duration: 0.4 },
            destroyOnClose:         true,
            draggable:              false,
            closeOnEsc:             false
        });

        function closePopup() {
            if(win) {
                win.close();
                Mage.Cookies.set('popup-closed', true);
            }
        }

        window.onload = function() {

            if(window.location.href.indexOf('mp-registration') !== -1) {
                Mage.Cookies.set('popup-closed', true);
            }

            Mage.Cookies.set('popup-show', !(Mage.Cookies.get('popup-show') == 'true'));

            function reqListener () {
                if(this.status === 200) {
                    var json = JSON.parse(this.responseText);
                    if(json['success'] === 1) {
                        var formContent = json['content'];

                        win.getContent().update(formContent);
                        win.setZIndex(100);
                        var height = window.innerHeight - 298;
                        var width = window.innerWidth - 275;
                        win.showCenter(false, height, width);

                        var id='#'+win.getId();
                        var target = document.querySelector(id);

                        var observer = new MutationObserver(function() {
                            var element = document.getElementById(win.getId());
                            if(element) {
                                // for responsive style
                                element.style.top = null;
                                element.style.left = null;
                                element.style.right = '10px';
                                element.style.bottom = '0';
                                document.getElementById(win.getId()+'_row1').style.display = 'none';
                                document.getElementById(win.getId()+'_row3').style.display = 'none';
                                element.style.position = 'fixed';
                            }
                            observer.disconnect();
                        });
                        var config = { attributes: false, childList: true, characterData: false };
                        observer.observe(target, config);
                    }
                }
            }

            var closedPopup = (Mage.Cookies.get('popup-closed') == 'true');

            if (!closedPopup && (Mage.Cookies.get('popup-show') == 'true')) {
                var oReq = new XMLHttpRequest();
                oReq.addEventListener('load', reqListener);
                oReq.open('POST', "https://www.worldofbooks.com/wob-newsletter/subscriber/popupAjax");
                var formData = new FormData();
                formData.append('form_key', 'sYJ33BFadkPTjYii');
                oReq.send(formData);
            }
        };


        function subscribeToNewsletter(form) {
            var email = form.querySelector('#newsletter-email').value;

            function reqListener (scope, form) {
                if(scope.status === 200) {
                    var json = JSON.parse(scope.responseText);
                    if (json['success'] === 1) {
                        var elements = form.elements;
                        for (var i = 0, len = elements.length; i < len; ++i) {
                            elements[i].disabled = true;
                        }
                        form.parentNode.querySelector('#message').innerHTML = 'Thank you for subscribing to the World of Books Newsletter!';
                        window.setTimeout(closePopup, 3000);
                    } else {
                        form.parentNode.querySelector('#message').innerHTML = json['error'];
                    }
                } else {
                    form.parentNode.querySelector('#message').innerHTML = 'Sorry, we were not able to add you to the World of Books Newsletter.';
                }
            }

            var oReqSubs = new XMLHttpRequest();
            oReqSubs.addEventListener('load', function() {
                reqListener(this, form);
            });
            oReqSubs.open('POST', 'https://www.worldofbooks.com/wob-newsletter/subscriber/subscribeAjax');
            var formData = new FormData();
            formData.append('newsletter-email', email);
            formData.append('form_key', 'sYJ33BFadkPTjYii');
            oReqSubs.send(formData);

        }

    </script>

