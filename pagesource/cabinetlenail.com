<script type="text/javascript">
function voirtel(tracking,telephone) {
	telephone = telephone.replace('+33 (0)', '0');
	telephone = telephone.replace('+33(0)', '0');
	telephone = telephone.replace(' ', '-');
	telephone = telephone.replace(' ', '-');
	telephone = telephone.replace(' ', '-');
	telephone = telephone.replace(' ', '-');
	telephone = telephone.replace(' ', '-');
	var button = document.getElementById("but_"+tracking);
	var span = document.getElementById("span_"+tracking);
	button.style.display = "none";
	span.style.display = "inline";
	//_gaq.push(['_trackEvent', 'Click', 'Phone-'+telephone+'' ,''+document.location.href+'']);
	ga('send', {
		'hitType': 'event',          // Required.
		'eventCategory': 'Click',   // Required.
		'eventAction': 'Phone-'+telephone+'',      // Required.
		'eventLabel': ''+document.location.href+''
	});
}

function clicksocial(social) {
	//_gaq.push(['_trackEvent', 'Click', 'Social_'+social+'' ,''+document.location.href+'']);
	ga('send', {
		'hitType': 'event',          // Required.
		'eventCategory': 'Click',   // Required.
		'eventAction': 'Social_'+social+'',      // Required.
		'eventLabel': ''+document.location.href+''
	});
}
