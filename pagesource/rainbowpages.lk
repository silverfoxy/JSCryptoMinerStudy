<script type="text/javascript">
  jQuery.noConflict();

  jQuery.extend( jQuery.ui.autocomplete.prototype, {
    _renderItem: function( ul, item ) {
      var term = this.term;
      var regex = new RegExp(term, "gi");
      var newText = String(item.label).replace(regex, "<span class='ui-state-highlight'>$&</span>");
      var a = jQuery("<a></a>").html(newText);
      return jQuery("<li></li>").data("item.autocomplete", item).append(a).appendTo(ul);
    }


  });

jQuery.expr[':'].Contains = function(a, i, m) {
  return jQuery(a).text().toUpperCase()
      .indexOf(m[3].toUpperCase()) >= 0;
};


jQuery.widget("custom1.catAutocomplete1", jQuery.ui.autocomplete, {
    _renderMenu: function(ul, items) {
        var that = this, 
            currentCategory = "";
       jQuery.each(items, function(index, item) {
            if (item.category != currentCategory) {
                ul.append("<li class='ui-autocomplete-category'>" + item.category + " </li>");
                // currentCategory = item.category;
            }
            that._renderItemData(ul, item);
        });
    }
});

jQuery.widget("custom1.catAutocomplete2", jQuery.ui.autocomplete, {
    _renderMenu: function(ul, items) {
        var that = this, 
            currentCategory = "";
       jQuery.each(items, function(index, item) {
            if (item.category != currentCategory) {
                ul.append("<li class='ui-autocomplete-category'>" + item.category + " </li>");
                currentCategory = item.category;
            }
            that._renderItemData(ul, item);
        });
    }
});


jQuery.widget("custom.catAutocomplete", jQuery.ui.autocomplete, {
    _create: function() {
        this._super();
        this.widget().menu( "option", "items", "> :not(.ui-autocomplete-category)" );
    },
    _renderMenu: function( ul, items ) {
        var that = this,
            currentCategory = "";

                    // Sort by category
                    items.sort( function( el1, el2 ) {
                    return  el1.category.toUpperCase().localeCompare(el2.category.toUpperCase()) * -1;
                    });

                           
        jQuery.each( items, function( index, item ) {
            var li;
            if ( item.category != currentCategory ) {
                ul.append( "<li class='ui-autocomplete-category'>" + item.category + "</li>" );
                currentCategory = item.category;
            }
            li = that._renderItemData( ul, item );
            if ( item.category ) {
                li.attr( "aria-label", item.category + " : " + item.label );
            }
        });
    }

});

