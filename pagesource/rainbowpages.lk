jQuery.noConflict();
jQuery(function(){
    var selectedValue = "";

    jQuery("#searchc").catAutocomplete({
       source:function( request, response ) {
             jQuery.ajax({
               url: "http://rainbowpages.lk/modules/mod_jomdirectory_search/tmpl/term.php",
               dataType: "json",
               data: {
                 term : jQuery('#searchc').val().trim(),
                 tab:"tab1",
                 address:jQuery('#addressc').val().trim()
               },


               success: function( data ) {


                      var arrItems = [],
                      uniqItems = [];

                    // Remove dulpicates and add matched metas as new items
                    jQuery.each( data, function( i, el ) {
                        // Create a unique value for current element
                        var itemHash = el.label + el.category;
                        var labelNoSpace = el.label.replace(/\s/ig, '').toUpperCase();
                        var termNoSpace = request.term.trim().replace(/\s/ig, '').toUpperCase();

                        if (((labelNoSpace.indexOf(termNoSpace) != -1) && (jQuery.inArray( itemHash, arrItems ) == -1 )) || (el.cat_id)) {
                            arrItems.push( itemHash );
                            uniqItems.push( el );
                        }

                        // Split meta value
                        if ( el.hasOwnProperty("metadesc") ) {
                            var splitMeta = el.metadesc.split( "," );
                            jQuery.each( splitMeta, function( mk, mv ) {
                                if(mv)
                                {
                                  mv = mv.trim();
                                  itemHash = mv + "Product & Services";
                                  
                                  var mvNoSpace = mv.replace(/\s/ig, '').toUpperCase();
                                  // var termNoSpace = request.term.trim().replace(/\s/ig, '').toUpperCase();
                                  
                                  if ((mvNoSpace.indexOf(termNoSpace) != -1) && ( jQuery.inArray( itemHash, arrItems ) == -1 )  ) 
                                  {
                                      arrItems.push( itemHash );
                                      uniqItems.push( { value: mv, label: mv, cat_id: el.cat_id,  category: "Product & Services" } );
                                  }
                                }
                            });
                        }
                    });
                             

                   response(uniqItems);


                 }
               });
             },
       delay: 200,
       minLength: 3,
      select: function(event, ui) {
        jQuery('#au_sug_used').val("true");
        selectedValue = ui.item.value;
        if (ui.item.cat_id != "") {
          jQuery('#categories_idclas').val(ui.item.cat_id);
        }
