			gc.loadCachedJSON('instagram', '/wp-content/shared-gc/includes/rest-api/instagram/?count='+ count),
			gc.load('waterfall', '~/js/dist/responsive-waterfall.min.js')
		]),

		// Since photos take a while to load and position, load them a bit early as well
		offset = window.innerHeight * 2;


	// Don't load photos until near viewport
	gc.whenNearViewport(instagramContainer, initInstagram, offset);

	function initInstagram()
	{
		instagramDeps.then(function(loaded)
		{
			var instagramPosts = loaded[0],
				html,
				waterfall;

			// Generate HTML for each item
			html = instagramPosts.map(function (post)
			{
				return '<figure class="gallery__item"><a><img class="gallery__image" src="'+ post.src_standard.url +'" srcset="'+ post.src_low.url +' '+ post.src_low.width +'w, '+ post.src_standard.url +' '+ post.src_standard.width +'w" width='+ post.src_standard.width +' height='+ post.src_standard.height +'></a><figcaption><p>'+ post.text +'</p><p><a href="'+ post.link +'" class="button button--dark" target="_blank">Open in Instagram</a></p></figcaption></figure>';

			}).join('');

			// Inject HTML
			instagramContainer.innerHTML = html;

			// Setup waterfall
			waterfall = new Waterfall({
				containerSelector: '#gallery--381756025',
				boxSelector: '#gallery--381756025 .gallery__item',
				minBoxWidth: 300
			});

			// Display images when it's been setup. Try to prevent any jank
			requestAnimationFrame(function ()
			{
				instagramContainer.classList.remove('gallery--loading');
				if ( gc.forceLoadPhotoswipe ) gc.forceLoadPhotoswipe();
			});

		})
		.catch(function(err)
		{
			console.log('Unable to fetch instagram API or waterfall library', err);
		});
	}
}();
</script>

