<style>
/* Don't display video banner on mobile, just a square image-blur */
.video-banner {
	position: relative;
	height: 0;
	padding-bottom: 100%;
	overflow: hidden;
}
/* 1x1 aspect ratio image */
.video-banner .image-blur__image-wrap {
	padding-bottom: 100%;
}
/* hide video */
.video-banner__wrap {
	display: none;
}

@media (min-width: 800px) {

	.video-banner {
		/* 2.3x1 aspect ratio */
		padding-bottom: 42.5%;
	}

	.video-banner__wrap {
		display: block;
		position: relative;
		margin-top: -100%; /* compensate for 2x1 image-blur pushing it down */
		height: 0;
		padding-bottom: 42.5%; /* Tweak to fit narrow video (2.3x1) */
		overflow: hidden;

		/* initially invisible. fades in once video has buffered a bit */
		opacity: 0;
		transition: opacity 1s;
	}
	.video-banner__wrap--fadein {
		opacity: 1;
		transition: opacity 1s;
	}
	.video-banner__video {
		position: absolute;
		top: 0;
		left: 0;
		width: 100% !important;
		/* Height must be auto for Safari */
	}
	.video-banner__button {
		position: absolute;
		bottom: 15%;
		bottom: calc(10% + 2em);
    	right: 2em;
    	right: calc(1.5% + 1em);
		color: white;
		width: 3em;
		height: 3em;
		padding: .5em;
		background-color: rgba(0,0,0,.25);
		border-radius: 50%;
		transition: none;
	}
	.icon--video-banner {
		display: none;
	}
	.video-banner__button--playing .icon--pause {
		display: block;
	}
	.video-banner__button--paused .icon--play {
		margin: -2px -6px 0 0;
		display: block;
	}
	.video-banner__button--loading .icon--loading {
		display: block;
		animation: rotate 1s infinite linear;
	}
	.video-banner__button--play {
		padding: 0;
	}
}

@keyframes rotate {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}

@media (min-width: 2000px) {
	.video-banner {
		padding-bottom: 800px;
	}
}

/* transition video banner into next section with a cool curve */
.video-banner__curve {
	position: absolute;
	bottom: 0;
	height: 2em;
}
@media (min-width: 500px) {
	.video-banner__curve {
		height: 4em;
	}
}

</style>

