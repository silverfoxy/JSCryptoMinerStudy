        $(document).ready(function() {
            
            var path = location.pathname.replace(/^\/|\/$/g, '');
            if ( path ) $('#leftnav a[href$="' + path + '"]').addClass("active");

            $('#toTestimonials').click(function() {
                var positionTestimonials = $('.testimonial-videos').position().top;
                $('body,html').animate({
                    scrollTop : positionTestimonials - 100    // Scroll to testimonials
                }, 500);
            });

            $(window).scroll(function() {
                if ($(this).scrollTop() >= 50) {        // If page is scrolled more than 50px
                    $('#return-to-top').fadeIn(200);    // Fade in the arrow
                } else {
                    $('#return-to-top').fadeOut(200);   // Else fade out the arrow
                }
            });

            $('#return-to-top').click(function() {      // When arrow is clicked
                $('body,html').animate({
                    scrollTop : 0                       // Scroll to top of body
                }, 500);
            });
            
            $('.show-password-reminder-form').click(function(event) {
                event.preventDefault();
                var index = $(this).attr('data-index');
                $('.modal-login-form.'+index).slideUp();
                $('.modal-password-reminder.'+index).slideDown();
            });

            $('.show-login-form').click(function(event) {
                event.preventDefault();
                var index = $(this).attr('data-index');
                $('.modal-password-reminder.'+index).slideUp();
                $('.modal-login-form.'+index).slideDown();
            });
           
            $('.trigger-send-reminder').click(function(event) {

                event.preventDefault();
                var index      = $(this).attr('data-index');
                var isCustomer = 0;
                var email      = '';

                switch (index) {
                    case 'one':
                        email = $('#emailReminderStudent').val();
                    break;
                    case 'two':
                        email = $('#emailReminderTeacher').val();
                    break;
                    case 'three':
                        isCustomer = 1;
                        email = $('#emailReminderParent').val();
                    break;
                }
                
                if (email.length > 0) {
                    $.ajax({
                        type: "POST",
                        url: '/ajax/home/forgotPassword',
                        data: {
                            email      : email, 
                            isCustomer : isCustomer
                        },
                        dataType: 'json',

                        success: function(data, status) {
                            if (data['success'] == true) {

                                $('.modal-password-reminder.'+index).slideUp();
                                $('.modal-login-form.'+index).slideDown();

                                $('.modal-login.'+index).modal('hide');
                                $('.email-reminder-group').removeClass('has-danger');

                                $('.form-error.'+index)
                                  .hide().addClass('hidden')
                                  .html('');

                                switch (index) {
                                    case 'one':
                                        $('#emailReminderStudent').val('');
                                    break;
                                    case 'one':
                                        $('#emailReminderTeacher').val('');
                                    break;
                                    case 'three':
                                        $('#emailReminderParent').val('');
                                    break;
                                }
                               
                                alert('Your address was found and your password has been sent to: ' + email);
                                
                            } else {
                                $('.email-reminder-group').addClass('has-danger');
                                $('.form-error.'+index)
                                    .show().removeClass('hidden')
                                    .html(data['errors']);
                            }
                        }
                    }).done(function( data ) {
                        if ( console && console.log ) {
                          console.log( data );
                        }
                    });
                } else {
                    var error = 'Please enter a valid email address';
                    $('.form-error.'+index)
                                    .show().removeClass('hide')
                                    .html(error);
                }
            });

            $('.image-caption').on('click', function(event) {

                event.preventDefault();
                var targetId = $(this).attr('data-target-id');

                $('.image.read-testimonial').removeClass('active');
                $('[data-id='+targetId+']').addClass('active');

                // Show testimonial
                $('#' + targetId).removeClass('hidden').siblings().addClass('hidden');
                
            });

            $('.nav-link.teardrop').click(function(event) {
                var target_image_current = $(this).attr('data-image');
                $('.nav-link.teardrop').each(function() {
                    var target_image = $(this).attr('data-image');
                    $('#'+ target_image).attr('src', '/assets/images/' + target_image + '-o.png');
                });
                $('#'+ target_image_current).attr('src', '/assets/images/' + target_image_current + '.png');
            });

            // Free trial dropdown homepage
            $('#homeCustomerType').change(function() {
                if ($(this).val() == 'Teacher') window.location.href='/freetrial';
            });
        });

    </script>

