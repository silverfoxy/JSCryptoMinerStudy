    <script type="text/javascript">

        $(document).ready(function() {

            $.validate({
                modules : 'security, logic',
                onSuccess: function($form) {
                    $('input[type=submit].modal-login-btn').val('Signing in. Please wait...').prop('disabled', true);
                }
            });

            var signupForm       = $('#signup-form');
            var signupFormWidth  = signupForm.outerWidth();
            var signupFormHeight = signupForm.outerHeight();
            var windowWidth      = $(window).width();
            var windowHeight     = $(window).height();

            $(window).resize(function() {
                windowWidth      = $(this).width();
                windowHeight     = $(this).height();
                signupFormHeight = signupForm.outerHeight();
                signupFormWidth  = signupForm.outerWidth();
                if ((signupFormHeight + 180) < windowHeight) {
                    if (windowWidth < 990) {
                        signupForm.removeClass('fixed');
                        signupForm.css('width', '100%');
                    }
                } else {
                    signupForm.removeClass('fixed');
                    signupForm.css('width', '100%');
                }
            });

            $(window).scroll(function() {

                windowHeight = $(this).height();

                if ($(this).scrollTop() > 310) {
                    $('#cta-main-nav').removeClass('hidden-sm-down');
                } else {
                   $('#cta-main-nav').addClass('hidden-sm-down');
                }

                if ((signupFormHeight + 180) < windowHeight) {
                    if (windowWidth > 990) {
                        if ($(this).scrollTop() > 120) {
                            signupForm.addClass('fixed');

                            signupForm.css('width', signupFormWidth);
                        } else {

                            signupForm.removeClass('fixed');
                            signupForm.css('width', '100%');
                        }
                    }
                }
            });

            $('body').click(function(e) {
                $('[data-toggle="popover"]').each(function () {
                    if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('.popover').has(e.target).length === 0) {
                        $(this).popover('hide');
                    }
                });
            });

            $('[data-toggle="popover"]').popover();
            
            $('[data-toggle="popover"]').click(function(event) {
                event.preventDefault();
            });

            function validateForm() {
                var errors = 0;
                $('input[name=first_name]').validate(function(valid, elem) {
                    if (!valid) errors++;
                });
                $('input[name=last_name]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=email]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=confirm_email]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=password]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=school_code]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=is_my_school]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                return errors > 0 ? false : true;
            }

            var template = $('#code_data');

            function ajax_lookup( event ) {
                var school_code = $(this).val();
                if (school_code.length >= 4) {
                    $.ajax({
                        type     : "POST",
                        url      : "/ajax/home/validateSchoolCode",
                        data     : signupForm.serialize(),
                        success  : function(data) { 
                            var result = JSON.parse(data);
                            if (result.success) {
                                $("#code_result").html(template);
                                $("#code_result").css('display','block');
                                $("#name").html(result.data[0].Name);
                                $("#address").html(result.data[0].Address1);
                                $("#state").html(result.data[0].State);
                                $("#postcode").html(result.data[0].PostCode);
                                $("#phone_number").html(result.data[0].Phone);
                                $("#code_result").fadeIn();
                            } else {
                                $("#code_result").html(school_code + ' is not a valid school code.');
                                $('#code_result').fadeIn();
                            }
                            signupFormHeight = signupForm.outerHeight();
                        }
                    }).fail(function( data ) {
                        $("#code_result").html(school_code + ' is not a valid school code.');
                    });
                } else {
                    $("#code_result").html('');
                }
            };

            $('input[name=school_code]').keyup( $.debounce( 250, ajax_lookup ) );

            $('input[name=school_code]').blur(function() {
                if (school_code.length == 0) $("#code_result").html('');
            });

            $('#signup-button').click(function(event) {
                event.preventDefault();
                if (validateForm()) {
                    var school_code = $('input[name=school_code]').val();
                    if (school_code.length >= 4) {
                        $.ajax({
                            type     : "POST",
                            url      : "/ajax/home/validateSchoolCode",
                            data     : $("#signup-form").serialize(),
                            success  : function(data) { 
                                var result = JSON.parse(data);
                                if (result.success) {
                                    $('#signup-form').submit();
                                } else {
                                    alert('School code is invalid.');
                                }
                            }
                        }).fail(function( data ) {
                            $("#code_result").html(school_code + ' is not a valid school code.');
                        });
                    } else {
                        $('#signup-form').submit();
                    }
                } else {
                    windowWidth      = $(this).width();
                    windowHeight     = $(this).height();
                    signupFormHeight = signupForm.outerHeight();
                    signupFormWidth  = signupForm.outerWidth();
                    window.location.href=window.location.href+"#top";
                    if ((signupFormHeight + 160) < windowHeight) {
                        if (windowWidth < 990) {
                            signupForm.removeClass('fixed');
                            signupForm.css('width', '100%');
                        }
                    } else {
                        signupForm.removeClass('fixed');
                        signupForm.css('width', '100%');
                    }                
                }
            });

            if ($("#homeschool-video").length > 0) {
                var player = videojs("homeschool-video");
                $('#play-demo').click(function() {
                    player.play();
                });
            }

            /* Video Library */
            var $arrVideos = [
                'bTb5mFPpKWI',
                'WtbTS-GBm44',
                'WPSFpujkoes',
                'QyMXgxDExAo',
                'g4pw07ZCfb4',
                '7L3OTk9oD2M',
                '_apvuEyVPVA',
                '69pACegCXiA',
                'x5CDrXeI7co',
                'MwKN5_IfGs4',
                '5FLuAENN8jc'
            ];

            function getParameterByName(name) {
                var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
                return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
            }

            var $specifiedVideoID = getParameterByName('index');

            if ($specifiedVideoID) {
                var $id = $arrVideos[$specifiedVideoID];
                var $title = $('.video-index-'+$specifiedVideoID).attr('data-title');
                $url = "https://www.youtube.com/embed/";
                $url = $url + $id + '?autoplay=1&rel=0';
                $("#videoModal iframe").attr("src", $url);
                $title = $('a[data-video-index='+$specifiedVideoID+']').attr('data-title');
                setTimeout(function() { 
                    $('#modalVideoLabel').html($title);
                    $('#videoModal').modal('show');
                }, 500);
            }

            $('.video-thumbnail').click(function(event) {
                var $id = $(this).attr('data-id');
                var $title = $(this).attr('data-title');
                var $url = "https://www.youtube.com/embed/";
                if ($id) {
                    $url = $url + $id + '?autoplay=1&rel=0';
                    $("#videoModal iframe").attr("src", $url);
                }
                $('#modalVideoLabel').html($title);
            });

            $("#videoModal").on('hidden.bs.modal', function (e) {
                $("#videoModal iframe").attr("src", '');
            });

            $('#videoModal').on('shown.bs.modal', function () {
                $('#videoModal').removeClass('in');
            });   
            /* Video Library */
            
            $("#leftnav a.list-group-item, #leftnavDropdown a.dropdown-item").click(function(event) {
                event.preventDefault();
                var url     = $(this).attr('href').split('/');
                var content = url[3];
                $(this).parent().children('a.dropdown-item').removeClass('active');
                $(this).parent().children('a.list-group-item').removeClass('active');
                $(this).addClass('active');
                $.ajax({
                    type     : "POST",
                    url      : "/docs/teachers/load_content",
                    data     : { template: content },
                    success  : function(data) { 
                        $('#guide_content').html(data);
                        $('body,html').animate({
                            scrollTop : 0                       
                        }, 500);
                    }
                }).fail(function( data ) {
                    //
                });
            });

            // Dropdown mobile for support section
            $('#support-section-dropdown a').click(function(event) {
                event.preventDefault();
                $('#support-section-tabs li:eq('+$(this).index()+') a').tab('show');
            });

            if ($("#my-video").length) {
                var vid_obj  = videojs("my-video");
                vid_obj.on('play', function() {
                    $.getJSON('https://ipinfo.io/?token=7df9f1f08676b4', function(data){
                        data.product = 'MATHSONLINE';
                        $.ajax({
                            type     : "POST",
                            url      : "/home/log_play",
                            data     : data,
                        });    
                    });              
                });
            }

        });

    </script>
    
