            <script>
            var trimWords = function (descriptionContent,limit) {
                    var limit = limit || 150;
                    var words = descriptionContent.split(' ');
                    var ws = '', ellipse = false;
                    for (var i = 0; i < words.length; i++) {
                        if (ws.length + words[i].length < limit) {
                            ws = ws + (ws.length > 0 ? ' ' : '') + words[i];
                        }
                        else {
                            ellipse = true;
                            break;
                        }
                    }
                    return { ws: ws, ellipse: ellipse };
                };

                var AutoSuggestionMgr = {
                    hashResults: {},
                    getSuggestions: function (searchText) {                     
                            var suggestions = []; 
                            $.ajax({url:'http://connect.polycom.com/bin/solr/searchRec',data:{rows:6,coreName:$('.localeNameForCore').val() ,searchTerm:searchText},contentType:'application/x-www-form-urlencoded',method:'GET',async:true,success:function(res){
                                    console.log('response',res);
                            suggestions=JSON.parse(res);
                            console.log('sugegtsion',suggestions);
                            var val = $('.home-search-input').val();
                             var maxLines = parseInt($('.hdn-autosuggestion-maxlines').val());
                                if (isNaN(maxLines)) {
                                    maxLines = 6;
                                }
                            var autoSuggestionHtml = '', suggestionsWords = [];
                            var enableellipses = true;
                            var suggestionCharLimit =30;
                                for (var i = 0; i < suggestions.length; i++) {
                                    if (suggestions[i].toLowerCase().indexOf(val.toLowerCase()) === 0) {
                                        var rL = suggestions[i].toLowerCase();
                                    var textL = val.toLowerCase();
                                    var trimmedWordsObject = trimWords(rL,suggestionCharLimit);
                                    var autoSuggestionText = rL;
                                    if (enableellipses == true && trimmedWordsObject.ws.length > 0 ) {
                                        autoSuggestionText = trimmedWordsObject.ws + (trimmedWordsObject.ellipse ? '...' : '');
                                     }
                                        suggestionsWords.push(suggestions[i]);
                                        autoSuggestionHtml = autoSuggestionHtml + '<li class="auto-suggestion-item" data-val="' + suggestions[i] + '">' + (autoSuggestionText.substr(autoSuggestionText.indexOf(textL), textL.length) + '<span class="font-bold">' + autoSuggestionText.substr(rL.indexOf(textL) + textL.length, autoSuggestionText.length) + '</span>') + '</li>';
                                    }
                                    if (suggestionsWords.length > maxLines ) {
                                        break;
                                    }
                                }
                                 $('.search-suggestion').html('').addClass('hidden');
                                if (suggestionsWords.length > 0 && val.length > 0) {
                                    $('.search-suggestion').html(autoSuggestionHtml).removeClass('hidden');
                                    $('.auto-suggestion-item').click(function () {
                                       
                                        var autoSuggestionItem = $(this).attr('data-val');
                                         $('.search-suggestion').html('').addClass('hidden');
                                        $('.home-search-input').val(autoSuggestionItem);
                                             if(!window.wwwDigitalData){
                                                    wwwDigitalData={};
                                             }
                                    wwwDigitalData.search={          
                                           'technology': 'solr-search',
                                            'linkName': 'Search Initiated',
                                             'keyword': autoSuggestionItem
                                    }

                                 location.href = '/content/www/'+$('.localeNameForCore').val()+'/searchresults.html?q=' + encodeURIComponent(autoSuggestionItem);
                                    });
                                } 
                                else {
                                    $('.search-suggestion').html('').addClass('hidden');
                                }
                            }});
                            console.log('test4');
                             return suggestions;                       
                    }
                };

                $(document).ready(function () {
                    $('.btn-show-field-section').click(function (e) {
                        e.preventDefault();                       
                        $('.ply-global-header').addClass('active');
                        $('.home-search-input').val('');
                        $('.home-search-input').focus();
                       if(!window.wwwDigitalData){
                             wwwDigitalData={};
                        }
                       
                         wwwDigitalData.search={          
                           'technology': 'solr-search',
                            'linkName': 'Search Initiated'
                          }
                    });

                    $('.btn-search').click(function (e) {
                        e.preventDefault();
                         //$('.ply-global-header').removeClass('active');
                       var val= $('.home-search-input').val();
                        if(val.length>0){
                            if(!window.wwwDigitalData){
                                 wwwDigitalData={};
                             }
                             wwwDigitalData.search={          
                                       'technology': 'solr-search',
                                        'linkName': 'Search Initiated',
                                         'keyword': val
                              }
                              document.cookie='searchrefer='+ _satellite.getVar('PageName');
                            location.href = '/content/www/'+$('.localeNameForCore').val()+'/searchresults.html?q=' + encodeURIComponent(val);
                        }
                        else{
                           $('.ply-global-header').removeClass('active');
                               if(!window.wwwDigitalData){
                                        wwwDigitalData={};
                                }
                                wwwDigitalData.search={          
                                       'technology': 'solr-search',
                                        'linkName': 'Search aborted',
                                         'keyword': ''
                                }
                        }
                    });

                    $('body').click(function (e) {
                        var $btnShowField = $('.btn-show-field-section');
                       var $auto=$('.auto-suggestion-item');
                        if (!$auto.has(e.target).length=== 0 && !$(e.target).is($('.auto-suggestion-item')) && !$(e.target).is($('.home-search-input')) && !$btnShowField.is(e.target) && $btnShowField.has(e.target).length === 0 && !$(e.target).hasClass('auto-suggestion-item')) {
                                    $('.ply-global-header').removeClass('active');
                                    $('.search-suggestion').html('').addClass('hidden');
                        }
                    });

                    $('.home-search-input').keyup(function (e) {
                        var val = $(this).val();
                        var keyCode = e.keyCode;
                          var minKeyup= parseInt($('.hdn-autosuggestion-keyup').val());
                            if (isNaN(minKeyup)) {
                                    minKeyup= 1;
                                }
                        if (keyCode === 13) {
                            if ($(this).val() === '*') {
                        
                            } else {
                                if ($('.auto-suggestion-item.active').length > 0) {
                                    var activeText= $('.auto-suggestion-item.active').attr('data-val');
                                     $('.search-suggestion').html('').addClass('hidden');
                                    $(this).val(activeText);
                                    if(!window.wwwDigitalData){
                                         wwwDigitalData={};
                                     }
                                     wwwDigitalData.search={          
                                               'technology': 'solr-search',
                                                'linkName': 'Search Initiated',
                                                 'keyword': activeText
                                      }
                                }
                            }
                            location.href = '/content/www/'+$('.localeNameForCore').val()+'/searchresults.html?q=' + encodeURIComponent($(this).val());
                        } else if (val.length > minKeyup) {
                            if ([37, 38, 39, 40].indexOf(keyCode) >= 0) {
                                if (!$('.search-suggestion').hasClass('hidden')) {
                                    switch (keyCode) {
                                        
                                        case 38:
                                              if ($('.auto-suggestion-item.active').length > 0) {
                                            var currentSuggestion = $('.auto-suggestion-item.active');
                                               var nextSuggestion = $('.auto-suggestion-item.active').prev();
                                            currentSuggestion.removeClass('active');
                                            nextSuggestion.addClass('active');
                                        } else {
                                            $('.auto-suggestion-item').first().addClass('active');
                                        }
                                            break;
                                        
                                        case 40:
                                                 if ($('.auto-suggestion-item.active').length > 0) {
                                                var currentSuggestion = $('.auto-suggestion-item.active');
                                                var nextSuggestion = $('.auto-suggestion-item.active').next();
                                                currentSuggestion.removeClass('active');
                                                nextSuggestion.addClass('active');
                                            } else {
                                                $('.auto-suggestion-item').first().addClass('active');
                                            }
                                            break;
                                        default:
                                            break;
                                    }
                                }
                            } else {
                                var maxLines = parseInt($('.hdn-autosuggestion-maxlines').val());
                                if (isNaN(maxLines)) {
                                    maxLines = 6;
                                }
                                var suggestions = AutoSuggestionMgr.getSuggestions(val.toUpperCase());
                                
                            }
                        }
                         else {
                            $('.search-suggestion').html('').addClass('hidden');
                        }
                    });
                });
            </script>
