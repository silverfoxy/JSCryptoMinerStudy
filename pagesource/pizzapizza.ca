function findstoreid (code,type,callback) {
	var url = '/ajax/';
	var	data = {};
	data.processid = '412';
	data.mktCityList = "y";
	data.cache = "y";
	data.authCityCode		= code;
	
	if(type=='city'){
		data.authCityCode		= code;
	}
	
	jQuery.ajax({
		url:		url,
		dataType:	'json',
		type:		'post',
		data:		data,
		timeout: 10000,
		success:	function(json){
			callback(json);
		},
		error: function(jqXHR, textStatus, errorThrown){
			//console.log('ERROR 388: ',textStatus);
			setallDef();
		}
	});
};

function getPPLocation (callback) {
