$(document).ready(function($) {

  // Set initial zoom level
  var zoom_level=100;

  // Click events
  $('.er-font-increase').click(function() { zoom_page(10) });
  $('.er-font-reduce').click(function() { zoom_page(-10) });
  $('.er-font-reset').click(function() { zoom_page(0) });

  // Zoom function
  function zoom_page(step) {
    // Zoom just to steps in or out
    if(zoom_level>=120 && step>0 || zoom_level<=80 && step<0) return;

    // Set / reset zoom
    if(step==0) zoom_level=100;
    else zoom_level=zoom_level+step;

    // Set page zoom via CSS
    $('html').css({
      zoom: zoom_level/100 // set zoom
    });
  }
  
  $('#er-accessibility').keyup(function(event) {
      $('#er-accessibility').addClass('focus');
  });
  $('#er-header, .er-internal-tab, #er-page-wrapper').on('keydown', function(event) {
    $('#er-accessibility').removeClass('focus');
  });
  $('#er-breadcrumb-accessibility').click(function(event) {
    event.preventDefault();
    $('#er-accessibility').toggleClass('focus');
  });
});


