<script type="text/javascript">
$(document).ready(function(){
  var er_sayt_input          = $('#er-search-sidepanel, #er-search-sidepanel2');
  var er_search_border_tl    = er_sayt_input.css('border-top-left-radius');
  var er_search_border_tr    = er_sayt_input.css('border-top-right-radius');
  var er_search_border_reset = "18px";

  $('#er-search-sidepanel, #er-search-sidepanel2').keyup(function(){
    $(this).parents('#er-search-form').find('.er-search-sidepanel-results').hide();
    $(this).parents('form').attr('data-val', $(this).val().toLowerCase());
    $(this).parents('form').find('button').attr('data-val', $(this).val().toLowerCase());
    var filter = $(this).val().split(' ');
    var exempt = ['who', 'what', 'how', 'why', 'for', 'and', 'where', 'are'];
    var list = $('.er-menu-drilldown-1');
    var results_arr = [];
    var results_notice = [];
    var filtered = '';

    list.find('.filtered').removeClass(function(index, classname){
      return (classname.match(/filtered|key-[0-9]+|fil-[0-9]+/g).join(' '));
    });

    $.each(filter, function(idx, obj){
      if (obj.length > 2 && exempt.indexOf(obj) < 0) {
        results_arr = [];

        if (obj.toLowerCase().substring(obj.length - 1) == 's') {
          obj = obj.substring(0, obj.length - 1);
        }

        else if (obj.toLowerCase().substring(obj.length - 3) == 'ing') {
          obj = obj.substring(0, obj.length - 3);
        }

        else if (obj.toLowerCase().substring(obj.length - 2) == 'ed') {
          obj = obj.substring(0, obj.length - 2);
        }

        if (obj.match(/\d+\/\d+|\d{7}/g) != null) {
          var planning_notice = '<div class="alert-icon alert-info er-nomargintop"><span>Are you looking for a planning application?<br/>Search on our <a href="/environment/planning-and-building-control/planning-disclaimer/">planning portal</a>.</span></div>';
          results_notice.push(planning_notice);
        }

        var filtered = $(list).find('li:not(.er-menu-parent-selected, .er-nosayt) > a.er-menu-link-end').filter(function() {
          var txt = $(this).text().toLowerCase();
          var val = obj.toLowerCase();
          var pat = "^" + val + "| " + val;
          var reg = new RegExp(pat, 'gi');
          var exm = '';
          if ($(this).attr('data-exempt') != undefined) {
            exm = $(this).attr('data-exempt');
          }
          if (txt.indexOf(val) > -1 && exm.indexOf(obj) < 0) {
            return (txt.match(reg));
          }
          else {
            return false;
          }
        }).addClass('filtered').addClass('fil-' + idx);

        var filtered = $(list).find('li:not(.er-menu-parent-selected, .er-nosayt) > a.er-menu-link-end').filter(function() {
          if ($(this).attr('data-key') != null) {
            var key = $(this).attr('data-key').toLowerCase();
            var val = obj.toLowerCase();
            var pat = "^" + val + "| " + val;
            var reg = new RegExp(pat, 'gi');
            if (key.indexOf(val) > -1) {
              return (key.match(reg));
            }
            else {
              return false;
            }
          }
        }).addClass('filtered').addClass('key-' + idx);
      }
    });

    list.find('.filtered').each(function(){
      var count = 0;
      if ($(this).is('[class*="fil-"]')) {
        count = $(this).attr('class').match(/fil-/g).length;
      }
      if ($(this).attr('data-key') != null && $(this).is('[class*="key-"]')) {
        count = $(this).attr('class').match(/key-/g).length + 89;
      }

      var landing_page = $(this).parent().siblings('.er-menu-parent-selected').text().trim();

      if ($(this).parent().hasClass('er-menu-haschild') == false) {
        results_arr.push('<li data-sec="'+ landing_page + '" data-order="' + count + '">' + $(this)[0].outerHTML.replace(/tabindex=\"-1\"/,'') + '</li>');
      }
    })

    $(this).parents('#er-search-form').find('.er-search-sidepanel-results').find('li, [class*="alert-"]').remove();
    $(this).parents('#er-search-form').find('.er-search-sidepanel-results ul').html($(results_arr.join('')));

    var parent = $(this).parents('#er-search-form');

    $(this).parents('#er-search-form').find('.er-search-sidepanel-results li a').each(function(){
      $(this).wrapInner('<div><span/></div>');
    });
    $(this).parents('#er-search-form').find('.er-search-sidepanel-results li').sort(function (a, b) {
      var a_attr = a.getAttribute('data-order').match(/[0-9]/g);
      var b_attr = b.getAttribute('data-order').match(/[0-9]/g);
      return parseInt(a_attr) - parseInt(b_attr);
    }).each(function () {
      var elem = $(this);
      elem.remove();
      $(elem).prependTo(parent.find('.er-search-sidepanel-results ul'));
    });

    $(this).parents('#er-search-form').find('.er-search-sidepanel-results .er-padding').prepend(results_notice.join(''));

    $('.er-search-results-btn').attr('href', '/search/?q=&an=' + $(this).val());
    if ($(this).val().length > 2 && results_arr.length == 0 && results_notice.length > 0) {
      $(this).parents('#er-search-form').find('.er-search-results-btn ').hide();
    }
    else if ($(this).val().length > 2 && results_arr.length > 0 && results_notice.length > 0) {
      $(this).parents('#er-search-form').find('.er-search-results-btn ').show();
    }
    if ($(this).val().length > 2 && (results_arr.length > 0 || results_notice.length > 0)) {
      $(this).parents('#er-search-form').find('.er-search-sidepanel-results').show();
      er_sayt_input.css({'border-top-left-radius': er_search_border_reset, 'border-top-right-radius': er_search_border_reset, 'border-bottom-left-radius': '0', 'border-bottom-right-radius': '0', "box-shadow": "none"});
    }
    else {
      $(this).parents('#er-search-form').find('.er-search-sidepanel-results').hide();
      er_sayt_input.css({'border-radius': er_search_border_tl});
    }
  })
  $(document).mouseup(function(e) {
    var container = $('#er-search-form');
    var results   = $('.er-search-sidepanel-results');
    // if the target of the click isn't the container nor a descendant of the container
    if (!container.is(e.target) && container.has(e.target).length === 0) {
      results.hide();
      er_sayt_input.css({'border-radius': er_search_border_tl});
    }
  });
});
</script>


