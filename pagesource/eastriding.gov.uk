function er_bc_drop() {
  $('#er-breadcrumb-links-drop').remove();
  $('#er-breadcrumb-links').clone().prop('id', 'er-breadcrumb-links-drop').prependTo('#er-breadcrumbs .er-padding');

  var er_bc_width              = $('#er-breadcrumb-links-drop ul').width() - 30.56;
  var er_bc_li_length          = $('#er-breadcrumb-links-drop li').length;
  var er_bc_li_widths          = [];
  var er_bc_li_width_total     = 0;
  var er_bc_li_width_total_sub = 0;
  var er_bc_li_dropdowns       = [];

  $('#er-breadcrumb-links-drop li').each(function(idx, obj){
    er_bc_li_width_total += $(this).outerWidth();
    er_bc_li_widths.push($(this).outerWidth());
  });

  if (er_bc_li_width_total > er_bc_width) {
    for (var i = 0; i < er_bc_li_length; i++) {
      er_bc_li_width_total_sub += $('#er-breadcrumb-links-drop li').eq(i).outerWidth();
      if (er_bc_width > er_bc_li_width_total_sub) {
        $('#er-breadcrumb-links-drop li').eq(i).addClass('er-breadcrumb-visible');
      }
      else {
        var er_bc_link = $('#er-breadcrumb-links-drop li').eq(i).find('a').attr('href');
        var er_bc_txt = $('#er-breadcrumb-links-drop li').eq(i).find('a').attr('title').trim();
        $('#er-breadcrumb-links-drop li').eq(i).hide()
        er_bc_li_dropdowns.push('<li class="er-breadcrumb-hidden"><a href="' + er_bc_link + '" class="er-breadcrumb-link" title="' + er_bc_txt + '">' + er_bc_txt + '</a></li>');
      }
    }
  }

  if (er_bc_li_dropdowns.length > 0) {
    $('#er-breadcrumb-links-drop ul').append($('<li class="er-breadcrumb-dropdown"><a id="er-breadcrumb-caret" href="" aria-label="More breadcrumb items"><i class="fa fa-fw fa-caret-square-o-down"></i></a><ul>' + er_bc_li_dropdowns.join('') + '</ul>'));
  }
  $('#er-breadcrumbs').css({'height':'auto', 'visibility':'visible'});
}
