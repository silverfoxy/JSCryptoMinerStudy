<script type="text/javascript">
$(document).ready(function() {

  function parseResponses(responses) { // takes a responses array and turns it into a <ol>
    var ol = document.createElement('ol');
    for (var i=0; i < responses.length; i++) {
      ol.appendChild(parseResponse(responses[i]));
    }
    return ol;
  }

  function parseResponse(response) { // takes a response object and turns it into a <li>
    var li = document.createElement('li');
    li.className = response.category;
    if (response.category == 'output') {
      li.innerHTML = '<div class="er-dt-result">' + response.responses+ '</div>';
    }
    else if (response.category == 'function') {
      li.innerHTML = '<div class="er-dt-result"><div class="hidden">' + response.responses+ '</div><div class="replace"></div></div>';
    }
    else if (response.category == 'question') {
      li.innerHTML = '<h3 class="er-nomargintop">' + response.name + '</h3>';
    }
    else if (response.category == 'answer') {
      li.innerHTML = '<a href="" class="er-link-styled er-dt-answer">' + response.name + '</a>';
    }

    if (response.responses && response.category != 'output' && response.category != 'function') {
      li.appendChild(parseResponses(response.responses))
    }

    if (response.category != 'answer') {
      li.insertAdjacentHTML('beforeend', '<a href="" class="er-dt-back er-button-5 er-button-small er-nomarginbottom">Back</a>');
    }
    
    return li;
  }

  if ($('.er-dt-placeholder').length > 0) {
    $('.er-dt-placeholder').each(function(idx, obj){
      var file = $(this).attr('data-map');
      var $this = $(this);

      $.getScript('/EasySiteWeb/EasySite/StyleData/corporate/PageTemplate/dtrees/structure/' + file + '.js?v=1.3.3', function(data){
        var structure_name = 'structure_' + file;
        var dt_output = parseResponses(eval(structure_name));
        $this.append(dt_output);
        $this.addClass('loaded');
      })

    })
  }

  $(document).on('click', '.er-dt-answer', function(event){
    event.preventDefault();
    $(this).addClass('inactive').addClass('bc-answer');
    $(this).parents('.question:first').find('> h3, > .er-dt-back').addClass('inactive');
    $(this).parents('.question:first').find('> h3').addClass('bc-question');
    $(this).parent().siblings('.answer').addClass('inactive');
    $(this).siblings('ol').addClass('active').find('ol:first').addClass('active');

    if ($(this).siblings('ol').find('> .function').length > 0) {
      var file = $(this).parents('.er-dt-placeholder').attr('data-map');
      $(this).siblings('ol').find('.er-dt-result').addClass('active');
      $.getScript('/EasySiteWeb/EasySite/StyleData/corporate/PageTemplate/dtrees/functions/' + file + '.js?v=1.3.3');
    }
    var target_scroll = $(this).parents('.er-dt-placeholder');
    $.smoothScroll({
      offset: -30,
      scrollTarget: target_scroll[0]
    });
  });

  $(document).on('click', '.er-dt-back', function(event){
    event.preventDefault();
    if ($(this).parents('li:first').hasClass('output') == true || $(this).parents('li:first').hasClass('function') == true) {
      $(this).parents('.question:first').find('.inactive, .active, .bc-answer, .bc-question').removeClass('inactive active bc-answer bc-question');
      $(this).parents('.question:first').find(' > ol').addClass('active');
      $(this).siblings('.er-dt-result').removeClass('active');
    }
    else {
