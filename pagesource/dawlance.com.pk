<script type="text/javascript">

    $(document).ready(function () {
        var popupDelay = 1000;
        $('.gmaslider').gmaSlider();
        $('.product-opts').change(function () {
            if ($(this).is(":checked")) {
                $(this).siblings('.fCheck').fadeIn();
                return;
            }
            $(this).siblings('.fCheck').fadeOut();
        });

        setTimeout(function () {
            showPopup();
        }, popupDelay);

        $('#clicktowinnerlist').click(function () {
            showPopup(true);
        });
        function hidePopup() {
            $('.feedbackContainer').fadeOut();
            $('.cloak').fadeOut();
        }

        $(document).keyup(function (e) {
            if (e.keyCode == 27) {
                hidePopup();
            }
        });
$('.cloak, .close1').click(function () {
            hidePopup();
        });

        $('.cloak, .close').click(function () {
            hidePopup();
        });
        var showPopup = function (isForcedPopup) {
            if (window.location.hash == '' || isForcedPopup) {
                $('.cloak').slideDown(250, function () {
                    $('.feedbackContainer').fadeIn({queue: false, duration: 300});
                    //$('.feedbackContainer').animate({ top: "50px" }, 300);
                });
            }
        }
        $('.title').click(function () {
            $('.paragraph-bullets').slideToggle();
        })
    });
    $('.cooking-video-sec').fancybox({
        openEffect: 'none',
        closeEffect: 'none',
        helpers: {
            media: {}
        }
    });


</script>


