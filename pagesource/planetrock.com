    else 
      setTimeout(function() {location.href = link.href;}, 150);
    return false;
  }catch(err){}
}

// Creating custom :external selector
$.expr[':'].external = function(obj){
    return obj.href && obj.href != "" && !obj.href.match(/^mailto:/) && (obj.hostname != location.hostname) && !obj.href.match(/player/) && !obj.className.match(/preview-button/);
};

// Add onclick call to all external links for GA
$(document).ready(function() {
	// Add 'external' CSS class to all external links
	$("a:external").attr("onclick", function() {
		return "recordOutboundLink(this, 'Outbound Links', '" + this.href + "');return false;";
	});
});
