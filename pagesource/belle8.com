              there.parent().find('input[name=mobile]').focus();
              return false;
            }
            return true;
          }

          if(checkf() == true) {
            if(!code.val()) {
              there.parent().find('.form_error').html('请输入验证码');
             // code.focus();
              return false;
            } else {
              there.parent().find('.form_error').html('');
              return true;
            }
          }
          return false;
        }
      });
    });

    

    function validForm(t,m,s){
      if(!t){
        s.parent().find('.form_error').html('请输入用户名');
      //  s.parent().find('input[name=truename]').focus();
        return false;
