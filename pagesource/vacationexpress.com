            case "AL":
                SetCookie('cookie_airfromcity', $('#airfromcity').val(), 30, '/', document.domain, '');
                SetCookie('cookie_cbDepart', $('#airfromcity').val(), 30, '/', document.domain, '');
                SetCookie('cookie_cbDepartAIRONLY', $('#airfromcity').val(), 30, '/', document.domain, '');
