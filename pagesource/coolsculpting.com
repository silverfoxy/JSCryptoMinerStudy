<script type="text/javascript">
    $('.txlive-langselector').css('background', 'white !important');
    
    var getBreak = $( window ).width();
		if (getBreak < 1200) {
			window.liveSettings = {
				api_key: "b707f4ccaa0f41c488d5a429ab22fa14",
				picker: "bottom-left"
			};
		}
  Transifex.live.onReady(function(load_msec) {
    $('.txlive-langselector .txlive-langselector-current').css('color', '#0077bf').css('text-transform', 'uppercase');
	$('.txlive-langselector .txlive-langselector-marker, .txlive-langselector-topleft .txlive-langselector-marker').css('border-top', '4px solid #0077bf');
	$('.txlive-langselector.txlive-langselector-topright').css('background-color', '#FFF !important');
	$('.txlive-langselector.txlive-langselector-topright .txlive-langselector-toggle').css('background-color', '#FFF');
    
      
	//$('.txlive-langselector.txlive-langselector-topright').css('box-shadow', 'none');
	$('.txlive-langselector.txlive-langselector-topright .txlive-langselector-toggle').css('border-top', 'none').css('padding', '2px 20px');
	  
      
    $("ul#tx-live-lang-picker li[data-value='en_AU']").remove();  
	$("ul#tx-live-lang-picker li[data-value='en_GB']").remove();
	$("ul#tx-live-lang-picker li[data-value='pt_BR']").html('Portugu\u00EAs');
	$("ul#tx-live-lang-picker li[data-value='es_419']").html('Espa\u00f1ol');
    $("ul#tx-live-lang-picker li[data-value='en_US']").html('English');
    $("ul#tx-live-lang-picker li[data-value='zh_CN']").html('中文(简体)');
    $("ul#tx-live-lang-picker li[data-value='zh_TW']").html('中文(繁體)');
    $("ul#tx-live-lang-picker li[data-value='ko']").remove();  

	//SRX00116
	$('#tx-live-lang-container').removeClass('txlive-langselector-bottomleft').addClass('txlive-langselector-topright');
	
	var getBreak = $( window ).width();
	if (getBreak < 1200) {
		$('#tx-live-lang-container').removeClass('txlive-langselector-topright').addClass('txlive-langselector-bottomleft');
        
    }
	$('.txlive-langselector.txlive-langselector-bottomleft').css('background-color', 'rgba(255, 255, 255, 1)');
	
	$('.header-utility--locale-selector').css('display', 'none');
    var getLang = $('html').attr('lang');
    var getCurrent = $('#tx-live-lang-current').text();
      
	if (getLang == 'pt-br') {
        $('.txlive-langselector-current').html('Portugu\u00EAs');
	}
	if (getLang == 'es-419') {
		$('.txlive-langselector-current').html('Espa\u00f1ol');
		$('.nav-item-text').css('line-height', '20px');
	}
    if (getLang == 'de') {
		$('.nav-item-text').css('line-height', '20px');
	}  
    if (getLang == 'zh-cn') {
            $('.txlive-langselector-current').html('中文(简体)');
    }
    if (getLang == 'zh-tw') {
            $('.txlive-langselector-current').html('中文(繁體)');
    }
	if (getCurrent == 'English (US)') {
		$('#tx-live-lang-current').html('English');	
	}
	$('#tx-live-lang-picker').click(function() {
		var getLang = $('html').attr('lang');
		if (getLang == 'pt-br') {
			$('.txlive-langselector-current').html('Portugu\u00EAs');
		}
		if (getLang == 'es-419') {
			$('.txlive-langselector-current').html('Espa\u00f1ol');
			$('.nav-item-text').css('line-height', '20px');
		}
        if (getLang == 'en-us') {
			$('.txlive-langselector-current').html('English');
			$('.nav-item-text').css('line-height', '20px');
		}
        if (getLang == 'de') {
