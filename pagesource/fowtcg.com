    <script type="text/javascript">
    var do_find = function( append_params ){
      var d = {
        page: 1
      }
      if( append_params ){
          for( key in append_params ){
            d[key] = append_params[key];
          }
      }
      $.get( "/contents",
             d,
             function(html){
               $("#contents").html(html);
               if( d.page > 1 ){
                 window.location.href = "#contents";
               }
               var ops = {
                 controls: true,
                 moveSlides: 1,
                 nextText: '',
                 pager: false,
                 prevText: '',
               };
               $('.slide .slide-contents').bxSlider(ops);
               $('.bx-prev').html('<button class="slide-btn slide-btn-prev glyphicon glyphicon-menu-left"></button>');
               $('.bx-next').html('<button class="slide-btn slide-btn-next glyphicon glyphicon-menu-right"></button>');
             }
      )
    }
    $(function(){
      $("#slide_loader").hide();
      $("#home_slide").show();
      do_find();
    })
    </script>
