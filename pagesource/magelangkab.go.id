<script type="text/javascript">
	
	function refresh(){
		hal = document.getElementById('fhalaman').value;
		if(hal==''){
			//page = "http://magelangkab.go.id/utama";
			page = "http://magelangkab.go.id/v2";
		}else{
			switch(hal) {
				case 'branding':
					page =	"http://magelangkab.go.id/utama/wisata";
					break;
				case 'egov':
					page =	"http://aspirasi.magelangkab.go.id";
					break;
				case 'ekonomi':
					page =	"http://sehatimagelang.com";
					break;
				case 'skpd':
					page =	"http://rsud.magelangkab.go.id";
					break;				
				case 'kecamatan':
					page =	"http://kecamatanbandongan.magelangkab.go.id/v2/";
					break;
				case 'desa':
					page =	"http://magelangkab.go.id/utama/desasrumbung";
					break;
				case 'aplikasi':
					page =	"http://simoneva.magelangkab.go.id/";
					break;
				default:
					//page = "http://magelangkab.go.id/utama";
					page = "http://magelangkab.go.id/v2";
			}
		}

		embedframe(page);
	}
   
   function embedframe(isi){
		//html = '<iframe height="100%" allowTransparency="true" class="RightBackground" border=0 id=iframe1 name=iframe1 src="'+isi+'" width="100%" scrolling="yes" noresize frameborder="0" ></iframe>';
		
		//html =	'<embed src='+isi+" style='width:100%; height:50em;'>";
		//html = ' <iframe src="http://google.com" frameborder="0" style="overflow:hidden;overflow-x:hidden;overflow-y:hidden;height:150%;width:150%;position:absolute;top:0px;left:0px;right:0px;bottom:0px" height="150%" width="150%"></iframe>';
		html = '<iframe allowTransparency="true" style="overflow:hidden;height:100%;width:100%" height="100%" width="100%" id=iframe1 name=iframe1 src="'+isi+'" scrolling="yes" noresize frameborder="0" ></iframe>';
		//html = '<iframe allowTransparency="true" style="overflow:hidden;height:100%;width:100%" height="100%" width="100%" id=iframe1 name=iframe1 src="http://magelangkab.go.id" scrolling="yes" noresize frameborder="0" ></iframe>';
		$('#isiframe').html(html);
		
   }
   
   
