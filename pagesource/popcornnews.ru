                function getInfo() {
                    console.log('Welcome!  Fetching your information.... ');
                    FB.api('/me',{fields:'name,address,birthday,email,gender,hometown,location'}, function(response) {
//                                            console.log('Successful login for: ' + response.name);
                        var img_path = 'https://graph.facebook.com/'+response.id+'/picture?width=85&height=85';
