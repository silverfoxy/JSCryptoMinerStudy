                var completeAlert = function () {
                    $scope.notificationService = NotificationService;
                    $scope.notificationService.notify('', 'Click here to finish setting up your account details', '/Account/EditRegistration', 'top', 'right', 'truck', 'inverse', '', '');
                }
                //Check their address
                //Check if they have previously been prompted
                $scope.readCookie = readCookie('/Account/EditRegistration');
                $scope.freightAddress = '';
                if ($scope.freightAddress == null || $scope.freightAddress == '' || $scope.freightAddress == 'null') {
                    if ($scope.readCookie != 'hide') {
                        completeAlert();
                    }
                }
                
            }

            $scope.$watch('LoginSwitchService.isLoggedin', function (newVal, oldVal) {
                if (newVal) {
                    $scope.init();
                    
                    var url = '/Home/GetCurrentUser';

                    $http.get(url).then(function (response) {
                        $scope.username = response.data.user.regUserName;
                    });

                }
            });
            $scope.init = function () {
                if ($scope.LoginSwitchService.isLoggedin == true) {

                    $scope.pushHub = PushHub;
                    $scope.notificationHub = NotificationHub;
                    $scope.headerDataLoaded = false;



                    var loadPersonsData = function () {
                        var url = '/Home/GetHeaderData'
                        $http.get(url).then(function (response) {

                            $scope.headerDataLoaded = true;
                            $scope.balance = response.data.Balance;
                            $scope.cartTotal = response.data.CartTotal;
                            $scope.biddingstatus = response.data.BiddingStatus;
                            $scope.biddingstatusimgurl = '/img/myaccount' + '/' + response.data.BiddingStatus + 'Bidder.png';
                        });

                        var url = '/Home/GetHeaderWatching'
                        $http.get(url).then(function (response) {
                            $scope.watching = response.data.Watching;
                        });
                        var url = '/Home/GetHeaderWinningAuctions'
                        $http.get(url).then(function (response) {
                            $scope.winningAuctions = response.data.WinningAuctions;
                        });

                        var url = '/Home/GetHeaderOutbidAuctions'
                        $http.get(url).then(function (response) {
                            $scope.outbidAuctions = response.data.OutbidAuctions;
                        });
                    }

                    loadPersonsData();

                    $scope.$watch('notificationHub.outbidNotification', function () {
                        if (!!$scope.notificationHub && !!$scope.notificationHub.outbidNotification && !!$scope.notificationHub.outbidNotification.auc) {
                            
                            $scope.removeAuction($scope.winningAuctions,$scope.notificationHub.outbidNotification.auc);
                            $scope.winningAuctions.count--;
                            
                            $scope.outbidAuctions.auctions.push($scope.notificationHub.outbidNotification.auc);
                            $scope.outbidAuctions.count++;
                            $scope.notificationHub.outbidNotification = null;
                        }
                    });
                    $scope.$watch('notificationHub.winningNotification.winning', function () {
                        if (!!$scope.notificationHub && !!$scope.notificationHub.winningNotification && !!$scope.notificationHub.winningNotification.winning) {
                            
                            $scope.winningAuctions=$scope.notificationHub.winningNotification.winning;
                            $scope.outbidAuctions=$scope.notificationHub.winningNotification.losing;
                            $scope.notificationHub.winningNotification = null;
                        }
                    });
                    $scope.$watch('notificationHub.watchingNotification.watching', function () {
                        if (!!$scope.notificationHub && !!$scope.notificationHub.watchingNotification && !!$scope.notificationHub.watchingNotification.watching) {
                            
                            $scope.watching=$scope.notificationHub.watchingNotification.watching;
                            $scope.notificationHub.watchingNotification = null;
                        }
                    });
                    $scope.$watch('pushHub.newBeFirstInCart', function(newVal, oldVal) {

                        if (!!newVal) {
                            $scope.cartTotal = newVal;
                        }
                    });
                    $scope.notifications = [];
                    $scope.indexOfObject=function(arr, obj){
                        for(var i = 0; i < arr.length; i++){
                            if(arr[i].aucID==obj.aucID){
                                return i;
                            }
                        };
                        return -1;
                    };
                    $scope.removeAuction=function(array, auc){
                        var index=$scope.indexOfObject(array,auc.auctions);
                        if(index!==-1){
                            array.splice(index,1);
                        }
                    }
                }
            }

            $scope.init();
        });

