            function expandColumns() {
                $timeout(function () {
                    var sideSubCategory = document.getElementById('side-sub-category');
                    var ul = sideSubCategory.firstChild;
                    var counts = ul.childElementCount;
                    for (var i = 0; i < ul.childElementCount; i++) {
                        var li = ul.children[i];
                        var liSubMenu = li.querySelector('ul');
                        if (liSubMenu.style.display === 'block') {
                            counts = counts + liSubMenu.childElementCount;
                        }
                    }

                    var columnSize = Math.ceil(counts / 14);
                    var columncountprop = getsupportedprop(['columnCount', 'MozColumnCount', 'WebkitColumnCount']) //get appropriate CSS3 column-count property
