    <script type="text/javascript">
        app.controller('CarouselDemoCtrl', CarouselDemoCtrl);

        app.controller('CampaignController', HomePageCampaignController);

        app.directive('timerWrapper', function () {
            return {
                restrict: 'E',
                replace: true,
                transclude: true,
                controller: 'CampaignController',
                template: '<div ng-transclude></div>',
                scope: {
                    campaignColumnId: '='
                }
            };
        });

        HomePageCampaignController.$inject = ['$scope', '$http', '$compile', '$sce'];
        function HomePageCampaignController($scope, $http, $compile, $sce) {
            $scope.$on('timer-stopped', function (event, data) {
                var url = '/Home/GetHomePageCampaignData'
                $http.get(url).then(function (response) {
                    var campaigns = response.data.campaigns;
                    for (var j = 0; j < campaigns.length; j++) {
                        var campaign = campaigns[j];
                        for (var i = 0; i < campaign.campaignColumns.length; i++) {
                            var column = campaign.campaignColumns[i];
                            if ($scope.campaignColumnId == column.content.id) {
                                $scope.$broadcast('timer-add-cd-seconds', column.content.ClosingCountdownNext);
                            }
                        }
                    }
                });
            });
        }

        CarouselDemoCtrl.$inject = ['$scope', '$http', 'PushHub', 'AuctionWatch', '$interval'];
        function CarouselDemoCtrl($scope, $http, PushHub, AuctionWatch, $interval) {
                        $scope.indexOfObject=function(arr, obj){
                for(var i = 0; i < arr.length; i++){
                    if(arr[i].aucID==obj.aucID){
                        return i;
                    }
                };
                return -1;
            };
            $scope.pushHub=PushHub;
            $scope.$watch('pushHub.newAuction',function(){
                if(!!$scope.pushHub&&!!$scope.pushHub.newAuction){
                    $scope.auction=$scope.pushHub.newAuction;
            
                    if(!!$scope.auctions){
                        var index=$scope.indexOfObject($scope.auctions,$scope.auction);
                        if(index!==-1){
                            $scope.auctions[index]=$scope.auction;
                        }
                    }
                    if(!!$scope.bidTableParams){
                        var index=$scope.indexOfObject($scope.bidTableParams.data,$scope.auction);
                        if(index!==-1){
                            $scope.bidTableParams.data[index]=$scope.auction;
                        }
                    }
                }
            });
            $scope.auctionWatch=AuctionWatch;
            $scope.auctionWatch.url='/Account/MemorizeAuction';
              
            $scope.now=function(auction){
                $scope.servertimeUTC=new Date(auction.aucNowUtc);
                $scope.servertime = $scope.servertimeUTC;
                return new Date($scope.servertime);
            }
            $scope.closing=function(auction){
                return new Date(auction.aucCloseUtc);
            }
            $scope.isLastCall=function(auction){
            var timeToClose=$scope.closing(auction)-$scope.now;
            var timeToLastCallOffset=(1000*60*10);
            return(timeToClose<=timeToLastCallOffset);
            }

            
            $scope.countdown=function(auction) {
                   var countdownDays = countdown($scope.now(auction), $scope.closing(auction),countdown.DAYS).toString().replace(" days","").replace(" day","");
                   $scope.countdownDays = parseInt(countdownDays,10);
                    
                 var countdownHours = countdown($scope.now(auction), $scope.closing(auction),countdown.HOURS).toString().replace(" hours","").replace(" hour","");
                   $scope.countdownHours = parseInt(countdownHours,10);

                var countdownMinutes = countdown($scope.now(auction), $scope.closing(auction),countdown.MINUTES).toString().replace(" minutes","").replace(" minute","");
                   $scope.countdownMinutes = parseInt(countdownMinutes,10);


                if (countdownDays > 0)
                    {
                        return countdown($scope.now(auction), $scope.closing(auction), countdown.DAYS|countdown.HOURS).toString().replace(" days","d").replace(" day","d").replace(" hours","h").replace(" hour","h").replace(" and "," ") + " left";
                    } 
                else
                    {
                        if (countdownHours > 0){
                                return countdown($scope.now(auction), $scope.closing(auction), countdown.HOURS|countdown.MINUTES).toString().replace(" hours","h").replace(" hour","h").replace(" minutes","m").replace(" minute","m").replace(" and "," ") + " left";
                            }
                            else if (countdownMinutes > 0){
                                return countdown($scope.now(auction), $scope.closing(auction), countdown.MINUTES).toString().toString().replace(" hours","h").replace(" hour","h").replace(" minutes","m").replace(" minute","m").replace(" seconds","s").replace(" second","s").replace(" and "," ") + " left";
                            }
                            else
                        {
                            return countdown($scope.now(auction), $scope.closing(auction), countdown.SECONDS).toString().replace(" hours","h").replace(" hour","h").replace(" minutes","m").replace(" minute","m").replace(" seconds","s").replace(" second","s").replace(" and "," ") + " left";
                        }
                        
                    }
            }
            $scope.closingCountdownText=function(auction){
                if($scope.isClosed(auction)){
                    return 'Closed';
                }
                else{
                    return $scope.countdown(auction);
                }
            }
            $scope.isClosed=function(auction){
                return $scope.now(auction)>=$scope.closing(auction);
            }

            $scope.animateClosingTime=function(auction){
                closingInterval = $interval(function(){
                    
                    $scope.servertimeUTC=new Date(auction.aucNowUtc);
                    $scope.servertime = $scope.servertimeUTC;
                    $scope.computertime = new Date();
                    $scope.servertime.setSeconds($scope.servertime.getSeconds() + 1);
                    $scope.nowAnimate = new Date($scope.servertime);
                    //$scope.nowAnimate = new Date();
                    //Sync the clock if it is 5secs out

                    if ((Math.floor(($scope.computertime - $scope.servertime)/ 1000) > ($scope.clockinterval + 5)) || (Math.floor(($scope.computertime - $scope.servertime)/ 1000) < ($scope.clockinterval -5)))
                    {

                        $scope.value = 0;

                        $scope.start = function() {
                            $scope.value++;
                            $scope.timeout = $timeout(countdown, 1000);
                        };

                        $scope.stop = function() {
                            $timeout.cancel($scope.timeout);
                        };

                        $scope.getLatestTime=function(){
                            var url='/Search/getDateTimeNow';
                            $http.get(url)
                            .success(function(data){
                            $scope.stop();
                            $scope.servertimeUTC = new Date(data);
                            $scope.servertimeUTC.setSeconds($scope.servertimeUTC.getSeconds() + $scope.serverTimeUTCInterval + $scope.value)
                            $scope.servertime = $scope.servertimeUTC;
                            $scope.nowAnimate = new Date($scope.servertime);
                            });
                          
                            };
                            $scope.start();
                            $scope.getLatestTime();
                            }

                            var countdownDays = countdown($scope.nowAnimate, $scope.closing(auction),countdown.DAYS).toString().replace(" days","").replace(" day","");
                            $scope.countdownDays = parseInt(countdownDays,10);

                            var countdownSeconds = countdown($scope.nowAnimate, $scope.closing(auction),countdown.SECONDS).toString().replace(" seconds","").replace(" second","");
                            $scope.countdownSeconds = parseInt(countdownSeconds,10);
                            
                            var countdownHours = countdown($scope.nowAnimate, $scope.closing(auction),countdown.HOURS).toString().replace(" hours","").replace(" hour","");
                            $scope.countdownHours = parseInt(countdownHours,10);

                            var countdownMinutes = countdown($scope.nowAnimate, $scope.closing(auction),countdown.MINUTES).toString().replace(" minutes","").replace(" minute","");
                            $scope.countdownMinutes = parseInt(countdownMinutes,10);
                            



                            if (countdownSeconds == 0) {
                            // only reload once
                            //location.reload();
                            }

                            if($scope.nowAnimate>=$scope.closing(auction)){
                            //auction closed unless extended
                            $interval.cancel(closingInterval);
                            closingInterval=undefined;
                            }
                            var countdownText = "";
                            if (countdownDays > 0)
                    {
                        var countdownText = countdown($scope.nowAnimate, $scope.closing(auction), countdown.DAYS|countdown.HOURS).toString().replace(" days","d").replace(" day","d").replace(" hours","h").replace(" hour","h").replace(" and "," ") + " left";
                    }
                else
                    {
                        if (countdown.HOURS > 0){        
                                var countdownText = countdown($scope.nowAnimate, $scope.closing(auction), countdown.HOURS|countdown.MINUTES).toString().replace(" hours","h").replace(" hour","h").replace(" minutes","m").replace(" minute","m").replace(" and "," ") + " left";
                        }
                        else if (countdown.MINUTES > 0){

                            var countdownText = countdown($scope.nowAnimate, $scope.closing(auction), countdown.MINUTES|countdown.SECONDS).toString().replace(" hours","h").replace(" hour","h").replace(" minutes","m").replace(" minute","m").replace(" second","s").replace(" and "," ") + " left";
                        }
                        else
                        {
                            var countdownText = countdown($scope.nowAnimate, $scope.closing(auction), countdown.SECONDS).toString().replace(" hours","h").replace(" hour","h").replace(" minutes","m").replace(" minute","m").replace(" second","s").replace(" and "," ") + " left";
                        }
                        
                        

                    }



                            },1000);
                            };


            
