        app.controller('HomeStatsController', HomeStatsController);

        HomeStatsController.$inject = ['$scope', '$interval', '$http', 'StatsHub'];
        function HomeStatsController($scope, $interval, $http, StatsHub) {
            $scope.raisedForCharity = 0;
            $scope.bids24hour = 0;
            $scope.closingAuctions = 0;
            $scope.auctionClosedThisYear = 0;
            $scope.statsHub = StatsHub;

            $scope.$watch('statsHub.newCharityTotal', function () {
                if (!!$scope.statsHub && !!$scope.statsHub.newCharityTotal) {
                    $scope.raisedForCharity = $scope.statsHub.newCharityTotal;
                }
            });

            $scope.$watch('statsHub.newBidsPlacedTotal', function () {
                if (!!$scope.statsHub && !!$scope.statsHub.newBidsPlacedTotal) {
                    $scope.bids24hour = $scope.statsHub.newBidsPlacedTotal;
                }
            });

            $scope.$watch('statsHub.newAuctionClosedTotal', function () {
                if (!!$scope.statsHub && !!$scope.statsHub.newAuctionClosedTotal) {
                    $scope.auctionClosedThisYear = $scope.statsHub.newAuctionClosedTotal;
                }
            });

            $scope.raiseCharity = function () {
                var url = '/Home/IncreaseCharity';

                $http.post(url, { newValue: $scope.raisedForCharity + 10 }).then(function (response) { });
            };

            $scope.init = function () {
                var url = '/Home/GetHomeStats';
                $scope.statsReady = false;
                $http.get(url).then(function (response) {
                    $scope.statsReady = true;
                    $scope.raisedForCharity = response.data.raisedForCharity;
                    $scope.bids24hour = response.data.bids24hour;
                    $scope.closingAuctions = response.data.closingAuctions;
                    $scope.auctionClosedThisYear = response.data.auctionClosedThisYear;
                });
            };
        }

        app.factory('StatsHub', ['$rootScope', 'Hub', function ($rootScope, Hub) {
            var StatsHub = this;
            StatsHub.newCharityTotal = 0;
            StatsHub.newBidsPlacedTotal = 0;
            StatsHub.newAuctionClosedTotal = 0;
            var hub = new Hub('statsHub', {
                rootPath: "/signalr/hubs",
                listeners: {
                    'updateCharity': function (newCharityTotal) {
                        StatsHub.newCharityTotal = newCharityTotal;
                        $rootScope.$apply();
                    },
                    'updateBidsPlaced': function (newBidsPlacedTotal) {
                        StatsHub.newBidsPlacedTotal = newBidsPlacedTotal;
                        $rootScope.$apply();
                    },
                    'updateAuctionsClosed': function (newAuctionClosedTotal) {
                        StatsHub.newAuctionClosedTotal = newAuctionClosedTotal;
                        $rootScope.$apply();
                    }
                },
                errorHandler: function (error) {
                    console.error(error);
                },

                stateChanged: function (state) {
                    switch (state.newState) {
                        case $.signalR.connectionState.connecting:
                            break;
                        case $.signalR.connectionState.connected:
                            break;
                        case $.signalR.connectionState.reconnecting:
                            break;
                        case $.signalR.connectionState.disconnected:
                            break;
                    }
                }
            });
            return StatsHub;
        }]);
    </script>

        
