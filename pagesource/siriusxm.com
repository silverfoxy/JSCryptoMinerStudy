<script type="text/javascript">
jQuery(document).ready(function() {

	var category='null';
	var bTag='null';
	if( bTag ==='' || bTag ==='null')
	{
		bTag='';
	}
		
	if( category ==='' || category ==='null')
	{
		category='';
	}
	var blogURL = 'https://blog.siriusxm.com/category/siriusxm-com//feed/json/';
	var publicURL = false;
		if(blogURL.indexOf('public-api.wordpress.com') !=-1)
		{
			publicURL=true;
		}
		else
		{
			blogURL = blogURL+'?jsonp=?';
		}
		jQuery.ajax({
			url: blogURL,
			dataType: 'jsonp',
			
			data: { 
	
		        'category':category,
		        'tag':bTag,
		        'per_page':1,
		    },
			
			success: function(json) {
				
				
				var bTitle = 'null';
				var respArray = null;
				
				if(jQuery.isArray(json) || jQuery.isArray(json.posts))
				{
					if(json.length >0)
					{
						respArray=json;
					}
					else if(json.posts &&json.posts.length >0 )
					{
						respArray=json.posts;
						publicURL=true;
					}
					
					for( var i=0;i<respArray.length;i++)
					{
						 
							 
							if(publicURL)
							{
								jQuery('#12939503398791anc').css('background-image', 'url('+respArray[i].featured_image+'?w=300)');
								jQuery('#12939503398791anc').attr('href', respArray[i].URL+'?intcmp=GN_HEADER_NEW_WhatsOn_HearNowBlog');
								 jQuery('#12939503398791anc1').attr('href', respArray[i].URL);
								 jQuery('#12939503398791readmore').attr('href', respArray[i].URL+'?intcmp=GN_HEADER_NEW_WhatsOn_HearNowBlog');
							}
							else
							{
							jQuery('#12939503398791anc').css('background-image', 'url('+respArray[i].featured_image_url+'?w=300)');
							jQuery('#12939503398791anc').attr('href', respArray[i].permalink+'?intcmp=GN_HEADER_NEW_WhatsOn_HearNowBlog');
							 jQuery('#12939503398791anc1').attr('href', respArray[i].permalink);
							 jQuery('#12939503398791readmore').attr('href', respArray[i].permalink+'?intcmp=GN_HEADER_NEW_WhatsOn_HearNowBlog');
							}
							 
							
							 
							if (!bTitle || bTitle == 'null') {
	
								jQuery('#12939503398791body').html(respArray[i].title);
							} else {
	
								jQuery('#12939503398791body').html(bTitle);
							}
							var excertText = null;
							excertText = respArray[i].excerpt;						
							excertText = jQuery("<div/>").html(excertText).text();
							excertText = excertText.substr(0, 115);
							excertText = excertText.substr(0, excertText.lastIndexOf(" "));
	
							jQuery('#12939503398791excerpt').html(excertText + " &hellip;");
							
							break;
					 }
					
				}
				else
