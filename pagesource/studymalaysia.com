		// Init toggles
		$( ".toggle h5" ).click(function() {
			$(this).toggleClass( "open" );
			
			var rel = '#' + $(this).attr('rel');
			$(rel).slideToggle( "slow", function() {
				$(rel).toggleClass( "open" );
			});

			// For Articles page - instead of REL => TITLE Attribute
			// REL doesnt works due to HTML Purifier
			var rel_article = '#' + $(this).attr('title');
			var rel_var = rel_article.toLowerCase();
			$(rel_var).slideToggle( "slow", function() {
				$(rel_var).toggleClass( "open" );
			});
		});
		
		// our function that decides weather the navigation bar should have "fixed" css position or not.
	    var sticky_navigation = function(){
	        var scroll_top = $(window).scrollTop();
				
			if(scroll_top > 160 && !$('#top_wrapper').hasClass('sticky')){
				$('#top_wrapper').addClass('sticky');
				$('html, body').css({'padding-top': 48});
				$('#top_wrapper').css({top: -200});
				$('#top_wrapper').animate({top:0}, 500);
			}
			if(scroll_top == 0 && $('#top_wrapper').hasClass('sticky')){
				$('#top_wrapper').removeClass('sticky');
				$('#top_wrapper').css({top: 0});
				$('html, body').css({'padding-top': 0});
			}
	    };
	     
	    // run our function on load
	    sticky_navigation();
	     
	    // and run it again every time you scroll
	    $(window).scroll(function() {
	         sticky_navigation();
	    });

				
		var custom_menu = {label: '', duplicate: false, prependTo: '.mobile_nav', closeOnClick: true, openedSymbol: '&#8722;', closedSymbol: '+'};
		$('#mobile_menu').slicknav(custom_menu);

	});
	
	
</script>

