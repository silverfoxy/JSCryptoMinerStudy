        $(".social-share-like-subtitle").click(function () {
            $.cookie('showBanner_' + whatOpen, 'NO', {expires: 365, path: '/'});
            $('#social-share-like').removeClass("visible");
        });


    });


    function eventiPulsantiNewsletter() {
        $("#survey_button_news_no").bind("click", function () {
            $.cookie('showBanner_newsletter', 'NO', {expires: 365, path: '/'});
            $.cookie('showBanner_newsletter_step2', 'NO', {expires: 365, path: '/'});
            if (top.from_fancy == 1) {
                $.fancybox.close();
            } else {
                $('#social-share-like').removeClass("visible");
            }
        });
        $("#survey_button_news_yes").bind("click", function () {
            $.cookie('showBanner_newsletter', 'NO', {expires: 1, path: '/'});
            $.cookie('showBanner_newsletter_step2', 'NO', {expires: 1, path: '/'});
            if (top.from_fancy == 1) {
                $.fancybox.close();
            } else {
                $('#social-share-like').removeClass("visible");
            }
        });
    }
</script>
