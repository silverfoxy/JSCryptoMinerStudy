			length: "This value length is invalid. It should be between %s and %s characters long.",
			mincheck: "You must select at least %s choices.",
			maxcheck: "You must select %s choices or fewer.",
			check: "This checkbox has to be ticked",
			equalto: "Both password has to match"
		});

		function _calculateAge(birthday) { // birthday is a date
    	var ageDifMs = Date.now() - birthday.getTime();
    	var ageDate = new Date(ageDifMs); // miliseconds from epoch
    	return Math.abs(ageDate.getUTCFullYear() - 1970);
		}

		window.Parsley.addValidator('minimumage', {
		  validate: function(value) {
				// console.log(value);
				var year = parseInt($m.find("select[name='birthday_year']").val());
				var month = parseInt($m.find("select[name='birthday_month']").val());
				var day = parseInt($m.find("select[name='birthday_day']").val());

				var date = new Date(year,(month - 1),day);
				var age = _calculateAge(date);
				// console.log("age", age);
				return age >= 18 ? true : false;
			},
		  messages: {
		    en: 'The minimum age is 18'
		  }
		});

		function checkUsername(username, cb) {
			$.ajax({
				url : "/_site/ajax/validate-username",
				type: 'get',
				data: 'username='+username,
				dataType : 'json',
				success : function(response) {
					if(response.success) {
						// console.log("Free username", username);
						 cb(true);
					}
					else {
						// console.log("Taken username");
						cb(false);
					}
				},
				error : function (response) {
					// console.log("Error: ", response.responseText)
					 cb(false);
				}
			})
		}

		function checkEmail(email, cb) {
			$.ajax({
				url : "/_site/ajax/validate-email",
				type: 'get',
				data: 'email='+email,
				dataType : 'json',
				success : function(response) {
					if(response.success) {
						// console.log("Free email", email);
						 cb(true);
					}
					else {
						// console.log("Taken email");
						cb(false);
					}
				},
				error : function (response) {
					// console.log("Error: ", response.responseText)
					 cb(false);
				}
			})
		}

		window.Parsley.addValidator('usernameavailable', {
			validateString: function(value){
	    	var deferred = new $.Deferred(function(){

	        checkUsername(value, function(boolean){
	          if (boolean) {
	            deferred.resolve(boolean);
	          }
						else {
	            deferred.reject(boolean);
	          }
	        });
	    	});
    	return deferred.promise();
			},
		  messages: {
		    en: 'Username already exists'
		  }
		});

		window.Parsley.addValidator('emailavailable', {
			validateString: function(value){
				var deferred = new $.Deferred(function(){

					checkEmail(value, function(boolean){
						if (boolean) {
							deferred.resolve(boolean);
						}
						else {
							deferred.reject(boolean);
						}
					});
				});
			return deferred.promise();
			},

		  messages: {
		    en: 'There is already an account registered to this e-mail'
		  }
		});

		function popuplateSummary(){
			// console.log($m);
			var username = $m.find("#Username").val();
			var email = $m.find("#Email").val();
			var mobilecountrycode = $m.find("#mobilecountrycode").val();
			var MobilePhoneNumber = $m.find("#MobilePhoneNumber").val();
			var fullname = $m.find("#firstname").val() + " " + $m.find("#lastname").val();
			var dateofbirth = $m.find("select[name='birthday_year']").val() + "/" + $m.find("select[name='birthday_month']").val() + "/" + $m.find("select[name='birthday_day']").val();
			var gender = $m.find("select[name='Gender']").val();
			var country = $m.find("select[name='Country']").val();
			var language = $m.find("select[name='Language']").val();
			var currency = $m.find("select[name='Currency']").val();
			var fulladdress = $m.find("#address1").val() + ", " + $m.find("#zip").val() + " " + $m.find("#city").val();

			var countryString = $m.find("select[name='Country'] option[value='" + country +"']").text();
			var languageString = $m.find("select[name='Language'] option[value='" + language +"']").text();

			$m.find(".section.username .value p").text(username);
			$m.find(".section.email .value p").text(email);
			$m.find(".section.phone .value p").text("+" + mobilecountrycode + MobilePhoneNumber);
			$m.find(".section.fullname .value p").text(fullname);
			$m.find(".section.dob .value p").text(dateofbirth);
			$m.find(".section.fulladdress .value p").text(fulladdress);
			$m.find(".section.gender .value p").text(gender);
			$m.find(".section.country .value p").text(countryString);
			$m.find(".section.currency .value p").text(currency);
			$m.find(".section.language .value p").text(languageString);

			$m.find("#summary").show();
		}

		function navigateTo(index) {
			if(index == 3) popuplateSummary();
			// Mark the current section with the class 'current'
			$sections
			.removeClass('current')
			.eq(index)
			.addClass('current');
			// Show only the navigation buttons that make sense for the current section:
			$('.form-navigation .previous').toggle(index > 0);
			var atTheEnd = index >= $sections.length - 1;
			$('.form-navigation .next').toggle(!atTheEnd);
			$('.form-navigation [type=submit]').toggle(atTheEnd);
			$(".form-navigation").attr("data-step", index);
		}

		function curIndex() {
			// Return the current index by looking at which section has the class 'current'
			return $sections.index($sections.filter('.current'));
		}

		function showServerError(errorCode, translation){
			window.site.contetitor.translate("error" + errorCode, null, function(data) {
				var errorTranslated = data["error" + errorCode];
				// console.log("Show Aleacc Error", errorCode, translation);
				$("#sidebar .server-error-block h2").text(errorTranslated);
				$("#sidebar .server-error-block p").text(translation);
				$("#sidebar .server-error-block").show();

				// Email already exists in Database
				if(errorCode == "EmailExists") {
					$("#sidebar #Email").parent().removeClass("success").addClass("error");
					$("#sidebar #Email").focus();
				}

				// Username already exists in Database
				else if(errorCode == "UsernameExists") {
					$("#sidebar #Username").parent().removeClass("success").addClass("error");
					$("#sidebar #Username").focus();
				}

				navigateTo(0);

				$(".form-section.current .register-title").hide();
				$(".form-section.current .register-subtitle").hide();
			});
		}

		$(".form-navigation input[type='submit']").click(function(e) {
			var self = this;
			$(self).find('.loader').show();
			// e.preventDefault();
			// $('form.signup-form input[type="submit"]').submit();

			window.site.ajaxForm($("form.signup-form"), function(error,data,form) {
				// console.log("Ajax Form Create Account");
				$(self).find('.loader').hide();

				if (!error && data.content && !data.content.success) {
					error = true;
					// console.log("Error Creating Account. ErrorCode: ", data.content.code, "Message:", data.content.translation);
					showServerError(data.content.code, data.content.translation);
				}

				if (!error) {
					// console.log("Signed up succesfully!");
					ga('send', 'event', {eventCategory: 'registration', eventAction: 'submit', eventLabel: 'registration'});

					// console.log("Go to step activation");
					// console.log("Trigger signup");

					// Show activation part
					$("#sidebar #panel-sidebar-sign-up .activation").show();
					$("#sidebar #panel-sidebar-sign-up .signup-row").hide();

					// Set username into field
					$("#sidebar #panel-sidebar-sign-up .activation").find('input[name="Username"]').val(data.content.user.username);

					// Autofocus activation code field
					window.site.autofocus($("#sidebar #panel-sidebar-sign-up .activation").find('input[name="activationcode"]'));

					window.site.trigger('signup', [$m, data]);
				}
			});


		});

		window.site.ajaxForm($m.find('form.activateaccount'), function(error, data, $form) {
				// console.log("activateaccount ", data);
				window.site.handleActivate(error,data,$m,$form);
		});

		window.site.ajaxForm($m.find('.activationdetails form'), function(error, data, $form) {
				// console.log("activationdetails from");
				if (!error) {
						var $overview = $form.closest('.activationdetails').find('.updateaccountoverview');
						$overview.find('.placeholder.email').text($form.find('input[name="Email"]').val());
						$overview.find('.placeholder.mobile').text($form.find('input[name="Mobile"]').val());
						$form.closest('.activationdetails').addClass('showoverview');
						$form.find('.form-message').show().removeClass('error').text(data.content.translation);
						return;
				}
				else if (data && data.content) {
						// console.log("Show error?");
						$form.find('.form-message').show().addClass('error').text(data.content.translation);
				}
		});

		$('.signup-form').parsley().on('field:validated', function() {
			var parent = this.$element.parents(".status-block");
			// Null check and validation is true
			if(this.validationResult != null && this.validationResult == true) {
				// If the field is depending on other field
				if(parent.hasClass("multiple")) {
					var fieldsApproved = 0;

					// Loop thrugh all fields of this parent and check them
					for (var i = 0; i < parent.find("input").length; i++) {
						if($(parent.find("input")[i]).hasClass("parsley-success")) fieldsApproved++;
					}

					// If all fields is approved -> Show check box
					if(fieldsApproved == parent.find("input").length) {
						parent.addClass("success");
					}

				}
				// If validation is true and not depending on other
				else {
					parent.addClass("success");
				}
			}

			// If validation is null or false
			else {
				// If the element is depending on other - ignore
				// TODO: Refactor
				if(parent.hasClass("multiple")) {
					parent.removeClass("success");
				}
				else {
					parent.removeClass("success");
				}
			}

		});

		// Next button goes forward if current block validates
		$('.form-navigation .next').click(function() {
			$('.signup-form').parsley().whenValidate({
				group: 'block-' + curIndex()
			}).done(function() {
				navigateTo(curIndex() + 1);
			});
		});

		// Previous button is easy, just go back
		$('.form-navigation .previous').click(function() {
			navigateTo(curIndex() - 1);
		});


		// Prepare sections by setting the `data-parsley-group` attribute to 'block-0', 'block-1', etc.
		$sections.each(function(index, section) {
			$(section).find(':input').attr('data-parsley-group', 'block-' + index);
		});

		navigateTo(0); // Start at the beginning
	});
	</script>

