	var isMobile = "";

	var date = new Date("2025-01-01");
 	var m = 10;
 	date.setTime(date.getTime() + (m * 60 * 1000));

	var hasTakeover = false;
	var takeover1Active = "";
	var takeover2Active = "1";

	if(takeover1Active == 1) {
		hasTakeover = true;
	}
	if(takeover2Active == 1) {
		hasTakeover = true;
	}

	function getWidth() {
	  if (self.innerWidth) {
	    return self.innerWidth;
	  }

	  if (document.documentElement && document.documentElement.clientWidth) {
	    return document.documentElement.clientWidth;
	  }

	  if (document.body) {
	    return document.body.clientWidth;
	  }
	}
  function getHeight() {
    return $(".world").height();
  }


	// Counts Visits and return number of visits
	function countVisits() {
		var visits;

		// IDEA: add if loggedin check?
		if (typeof $.cookie("counter") == 'undefined') {
			visits = 1;
			$.cookie("counter", visits, { expires: date });

		}
		else {
			visits = parseInt($.cookie("counter")) + 1;
			$.cookie("counter", visits, { expires: date });

		}
		return visits;
	}

	function showTakeover() {
		//check if not mobile
		if(!isMobile) {
			if ($(".first-world-usp-section").hasClass("takeover")) {
				console.log("Showing Desktop Takeover");
				$(".default-index-container").hide();
				$(".cookies.takeover-container").show().css("opacity", 1);
			}
		}
		//check if mobile
		else if (isMobile) {
			if ($(".mobile-wrapper").hasClass("takeover")) {
				console.log("Showing Mobile Takeover");
				$(".mobile-wrapper .default-slider").hide();
				$(".mobile-wrapper.takeover .mobile-takeover").show().css("opacity", 1);
			}
		}
	}
	function showTransferTakeover() {
		$(".world.transfer").show();
		$(".transfer-takeover").show();
	}

	_$(function() {
		pageWidth = getWidth();
    worldHeight = getHeight();
		$( window ).resize(function() {
		  pageWidth = getWidth();
      worldHeight = getHeight();
		});

		// If custom Btag show Transfer Takeover
		if($("body").hasClass("transfer-active")) showTransferTakeover();

		var casinoArea = $(".casino-area");
		var sportsArea = $(".sportsbook-area");
		var drago = $("#index-drago");

		$('.flexslider').flexslider();

		if (pageWidth > 991) {
			$(".first-world-usp-section, #index-drago, #cta-areas, .cards-container, .sportsbook-area, #sports-img").css("opacity", 1);
		}

		// On load - Add Visit to VisitCookie. If you have visited 5 times > showTakeover();
		var visits = countVisits();
		if(visits >= 3) showTakeover();

		// Only use snap sports/casino when no index mode is specified
		if(indexMode == "") {
			$(".first-world-usp-section").mousemove(function(e) {
				if(!hasTakeover) {
					var percentageRight = (e.pageX / pageWidth) * 100;
					var percentageTop = (e.pageY / worldHeight) * 100;

					if(percentageTop > 18 && percentageTop < 70) {
						if(percentageRight < 35) {
							if(!$("body").hasClass("snap-left")) {
								$("body").addClass("snap-left");
							}
						}
						else {
							$("body").removeClass("snap-left");
						}

						if(percentageRight > 65){
							if(!$("body").hasClass("snap-right")) {
								$("body").addClass("snap-right");
							}
						}
						else {
							$("body").removeClass("snap-right");
						}
					}
					else {
						$("body").removeClass("snap-right");
						$("body").removeClass("snap-left");
					}
				}
			});
		}
	});
</script>

