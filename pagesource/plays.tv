        R.utils.post("/ws/track", tracking_params);
      }, 2000);

    };
      
    setTimeout(ldinsp, 500);
    
    document.readyState != "complete" 
      ? (window.attachEvent 
        ? window.attachEvent('onload', ldinsp) 
        : window.addEventListener('load', ldinsp, false)) 
      : ldinsp();
    
    })();
</script>

