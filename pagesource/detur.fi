                        jQuery("#roomtabs").attr("style", "display:none;");


                    }

                    function showroomtabOF() {

                        jQuery("#roomtabsOF").attr("style", "");


                    }
                    function hideroomtabOF() {

                        jQuery("#roomtabsOF").attr("style", "display:none;");


                    }


                    var dayString = "p&#228;iv&#228;&#228;";

                    jQuery(function () {
                        jQuery("#roomtabs").tabs();
                    });

                    jQuery.expr[':'].Contains = function (a, i, m) {
                        return jQuery(a).text().toUpperCase().indexOf(m[3].toUpperCase()) >= 0;
                    };
                    jQuery.expr[':'].contains = function (a, i, m) {
                        return jQuery(a).text().toUpperCase().indexOf(m[3].toUpperCase()) >= 0;
                    };

                    angular.module('MultiRoomSearchCtrl', [])
                    .filter('unique', function () {
                        return function (input, key) {
                            var unique = {};
                            var uniqueList = [];
                            for (var i = 0; i < input.length; i++) {
                                if (typeof unique[input[i][key]] == "undefined") {
                                    unique[input[i][key]] = "";
                                    uniqueList.push(input[i]);
                                }
                            }
                            return uniqueList;
                        };
                    })
                    .directive('repeatDone', function () {
                        return function (scope, element, attrs) {

                            if (scope.$last) { // all are rendered
                                scope.$eval(attrs.repeatDone);

                            }

                        }
                    })

                    .directive('datepicker', function () {
                        var linker = function (scope, element, attrs) {
                            element.datepicker({
                                dateFormat: 'dd.mm.yy',
                                firstDay: 1,
                                regional: "fi",
                                beforeShowDay: function (date) {
                                    var month = date.getMonth();
                                    var day = date.getDate();
                                    var year = date.getFullYear();

                                    if (attrs.id == "datepickerHF") {
                                        if ($.inArray((month + 1) + '-' + day + '-' + year, scope.EnableDatesHF) != -1) {
                                            return [true];
                                        }
                                    } else if (attrs.id == "datepickerOF1") {
                                        if ($.inArray((month + 1) + '-' + day + '-' + year, scope.EnableDatesOF1) != -1) {
                                            return [true];
                                        }
                                    } else if (attrs.id == "datepickerOF2") {
                                        if ($.inArray((month + 1) + '-' + day + '-' + year, scope.EnableDatesOF2) != -1) {
                                            return [true];
                                        }
                                    }


                                    return [false];
                                },
                                onSelect: function (dateText) {

                                    if (attrs.id == "datepickerHF") {
                                        scope.FlightSelectListHF = this.value;
                                        jQuery("#loadingContent1").toggle();

                                        scope.GetFlightNight();
                                        scope.roomsCountDone();
                                    } else if (attrs.id == "datepickerOF1") {

                                        if (Boolean(scope.RoundTrip) == true) {
                                            scope.FlightSelectListOF = this.value;
                                            jQuery("#loadingContent2").toggle();
                                            scope.GetFlightListforOF();
                                        }

                                    }



                                }
                            });
                        }
                        return {
                            restrict: 'A',
                            link: linker
                        }
                    })


                    .controller('AppCtrl', function ($scope, $http, $timeout, $filter) {


                        $scope.loading = 0;
                        $scope.selectedLocationValue = "-1_-1_-1"
                        $scope.selectedLocationValueOF = "5"
                        $scope.monastirtest = 0;



                        $scope.SendHotelSearchList = function () {
                            jQuery("#loadingContent1").show();
                            var departureId = jQuery("#departureHF").val()
                            var countryId = jQuery("#arrivalHF").val().toString().split("_")[0];
                            var arrivalId = jQuery("#arrivalHF").val().toString().split("_")[1];
                            var locationId = jQuery("#arrivalHF").val().toString().split("_")[2];
                            var date = jQuery("#datepickerHF").val()
                            var duration = jQuery("#durationHF").val()
                            var flightarrivalId = jQuery("#arrivalHF").val().toString().split("_")[1];



                            if (countryId==946) {
                                var airportcode = jQuery("#arrivalHF").val().toString().split("_")[3];

                                var roomparams="";
                                for (var i = 0; i < $scope.searchroomdataHF.rooms.length; i++) {
                                    roomparams+="?Room"+(i+1).toString()+"="+$scope.searchroomdataHF.rooms[i].AdultCount+"_"+$scope.searchroomdataHF.rooms[i].ChildCount;
                                    for (var z = 0; z < $scope.searchroomdataHF.rooms[i].ChildCount; z++) {
                                        roomparams+="_"+ $scope.searchroomdataHF.rooms[i].childs[z].age;
                                    }
                                }

                                jQuery("#loadingContent1").hide();

                                return false;
                            }else {
                                $http.get('/WebUI/GetDetailedFlightList?depCity=' + departureId + '&arrCity=' + arrivalId + '&checkIn=' + date + '&night=' + duration + '&country=' + countryId + '&_' + Math.random()).success(function (data) {

                                    data = eval('(' + JSON.parse(data) + ')');
                                    //alert(data)

                                    //console.log(date)
                                    //console.log(JSON.stringify(data))
                                    $scope.FindFlightList = data;

                                    var findflight=false;
                                    for (var z = 0; z < $scope.FindFlightList.length; z++) {

                                        //console.log(z +":" +$scope.FindFlightList[z].Active)

                                        if ($scope.FindFlightList[z].Active != null && $scope.FindFlightList[z].Active == 'active-nav' && !findflight) {

                                            for (var i = 0; i < $scope.FindFlightList[z].Data.length; i++) {

                                                if ($scope.FindFlightList[z].Data[i].Active == 'checked') {

                                                    if (arrivalId == -1 && locationId == -1 && $scope.FindFlightList[z].Data[i].Night==duration) {

                                                        flightarrivalId = $scope.FindFlightList[z].Data[i].PLArrCity
                                                        findflight=true;

                                                    }else if(arrivalId == -1 && locationId == -1) {
                                                        if ($scope.FindFlightList.length>z+1) {

                                                            $scope.FindFlightList[z+1].Active='active-nav'
                                                        }


                                                    }
                                                }
                                            }
                                        }
                                    }


                                    //alert(JSON.stringify($scope.searchroomdataHF))

                                    var roomJson;

                                    roomJson = JSON.stringify($scope.searchroomdataHF)
                                    //console.log(JSON.stringify($scope.searchroomdataHF1))
                                    if (date != '-') {

                                        var sessioncontent = departureId + "_" + countryId + "_" + arrivalId + "_" + locationId + "_" + date + "_" + duration + "_" + roomJson + "_" + flightarrivalId;
                                        //alert(sessioncontent)

                                        jQuery.ajax({
                                            type: "GET",
                                            url: "/" + "WebUI/setsessionobject",
                                            data: "sessionname=quicksearch&sessioncontent=" + sessioncontent + "&_=" + Math.random(),
                                            success: function (strHtml) {

                                                if ('FINMAR' == 'SWEMAR') {


                                                    if (locationId == 516) {

                                                        window.open('/resor/halsoresor', '_self')
                                                        return
                                                    } else if (locationId == 429) {
                                                        window.open('/resor/rundresor/kappadokien', '_self')
                                                        return
                                                    } else if (locationId == 451) {
                                                        window.open('/resor/rundresor/lykien', '_self')
                                                        return
                                                    }


