                        $scope.GetFlightNight = function () {

                            var DateSplit = $scope.FlightSelectListHF.toString().split('.');
                            var formateddate = DateSplit[2] + "-" + DateSplit[1] + "-" + DateSplit[0]

                            var nightdd;

                            if ($scope.arrivalSelectListHF.split('_')[0] == 69) {

                                $scope.durationlistHF = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, Country: parseInt($scope.arrivalSelectListHF.split('_')[0]) }, true);
                                nightdd = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, Country: parseInt($scope.arrivalSelectListHF.split('_')[0]) }, true)

                            } else if (parseInt($scope.arrivalSelectListHF.split('_')[1]) != -1) {

                                $scope.durationlistHF = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, PLArrCity: parseInt($scope.arrivalSelectListHF.split('_')[1]) }, true);
                                nightdd = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, PLArrCity: parseInt($scope.arrivalSelectListHF.split('_')[1]) }, true)

                            } else if (parseInt($scope.arrivalSelectListHF.split('_')[1]) == -1 && parseInt($scope.arrivalSelectListHF.split('_')[0]) != -1) {

                                $scope.durationlistHF = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, Country: parseInt($scope.arrivalSelectListHF.split('_')[0]) }, true);
                                nightdd = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, Country: parseInt($scope.arrivalSelectListHF.split('_')[0]) }, true)

                                if($scope.durationlistHF.length == 0) {
                                    $scope.durationlistHF = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, PLCountry: parseInt($scope.arrivalSelectListHF.split('_')[0]) }, true);
                                    nightdd = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate, PLCountry: parseInt($scope.arrivalSelectListHF.split('_')[0]) }, true)
                                }
                            }else {

                                $scope.durationlistHF = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate }, true);
                                nightdd = $filter('filter')($scope.allFlightDays, { FlyDate: formateddate }, true)
                            }
                            $scope.durationAllistHF= [];
                            $scope.durationAllistHF.push.apply($scope.durationAllistHF,$scope.durationlistHF)

                            //console.log(JSON.stringify( $scope.durationAllistHF))
                            nightdd = $filter('unique')($scope.durationlistHF, 'Night');
                            nightdd = $filter('orderBy')(nightdd, 'Night');
                            $scope.durationlistHF = nightdd;



                            $scope.FlightNightDone();

                            jQuery("#loadingContent1").toggle();

                        }

