function convertdate(mydate) {
    var splitdate;
    if (mydate.indexOf('-') != -1) {
        splitdate = mydate.split('-');
    } else if (mydate.indexOf('/') != -1) {
        splitdate = mydate.split('/');
    }

    var date1 = splitdate[1].toString() + "," + splitdate[2].toString() + "," + splitdate[0].toString();

    var dateStr = date1.replace(/,/g, '-');
    var d = dateStr.split("-");

    var myDates = new Date(d[2], (d[0] - 1), d[1], 00, 00, 00);


    return myDates;
}
function formattedDateToString(date) {
    var d = new Date(date || Date.now()),
        month = '' + (d.getMonth() + 1),
        day = '' + d.getDate(),
        year = d.getFullYear();

    if (month.length < 2) month = '0' + month;
    if (day.length < 2) day = '0' + day;

    return [day, month, year].join('.');
}



