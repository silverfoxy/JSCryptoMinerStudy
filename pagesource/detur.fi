                    function PaintDate(mydate) {
                        var splitdate;
                        if (mydate.indexOf('-') != -1) {
                            splitdate = mydate.split('-');
                        } else if (mydate.indexOf('/') != -1) {
                            splitdate = mydate.split('/');
                        }

                        return parseInt(splitdate[1].toString()) + "," + parseInt(splitdate[2].toString()) + "," + parseInt(splitdate[0].toString());
                    }


