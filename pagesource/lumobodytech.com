<div class="jquery-media-detect"></div><script>
    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }
    var exdate = new Date();
	exdate.setDate(exdate.getDate() + 45);

    if(getParameterByName("utm_source").toLowerCase()==="shareasale"){
        jQuery.cookie("ref_name","shareasale",{domain:'.lumobodytech.com',path:'/',expires:exdate,secure:false});
    }
    //used by coupon codes passed as URL parameters
    //allow customers to leave and come back within 90
    //days with whatever coupon still in effect
    if (getParameterByName("coupon")!=""){
        var customer_exdate = new Date();
        customer_exdate.setDate(exdate.getDate() + 2);
        console.log("setting discount code");
        jQuery.cookie("coupon",getParameterByName("coupon"),{domain:'.lumobodytech.com',path:'/',expires:exdate,secure:false});
    }
    //if customer arrives with a referral token in URL
    //we save it to a cookie so they can leave and come back while
    //retaining the discount
    if (getParameterByName("lumo_ref")!=""){
        var customer_exdate = new Date();
        customer_exdate.setDate(exdate.getDate() + 2);
        console.log("setting bidirectional referral cookie");
        jQuery.cookie("lumo_ref",getParameterByName("lumo_ref"),{domain:'.lumobodytech.com',path:'/',expires:exdate,secure:false});
    }
</script>
