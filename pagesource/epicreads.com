            onSliderLoad    : function(currentIndex) {
                tnSliderLinks.eq(currentIndex).addClass('active');
            },
            onSlideNext     : function(slideElement, oldIndex, newIndex) {
                tnSliderLinks.removeClass('active');
                tnSliderLinks.eq(newIndex).addClass('active');
            },
            onSlidePrev     : function(slideElement, oldIndex, newIndex) {
                tnSliderLinks.removeClass('active');
                tnSliderLinks.eq(newIndex).addClass('active');
            }
        }, maxSlides)

        tnSliderThumbs.find('li').on('click', 'a', function() {
            var this$     = $(this),
                dataIndex = this$.data('index');

            tnSliderLinks.removeClass('active');
            tnSliderLinks.eq(dataIndex).addClass('active');
            TH_SliderBx.goToSlide(dataIndex);

            return false;
        });

        // =================================================================
        // = Top Trending Books: scroll
        // =================================================================
        if(!epicReads.isMobileResolution() && scrollContainer.length && $.isFunction($.fn.mCustomScrollbar)) {
            scrollContainer.mCustomScrollbar({
                theme:'dark'
            });
        }

        // =================================================================
        // = Recent Posts Slider
        // =================================================================
        if(!epicReads.isMobileResolution()) {
            RP_SliderBx = epicReads.initBxSlider(RP_Slider, {
                moveSlides      : 1,
                minSlides       : 1,
                maxSlides       : 4,
                slideWidth      : 233,
                infiniteLoop    : true,
                pager           : false,
                hideControlOnEnd: true
            }, 4)
        } else {
            RP_SliderBx = epicReads.initBxSlider(RP_Slider, {
                moveSlides      : 1,
                minSlides       : 1,
                maxSlides       : 1,
                infiniteLoop    : true,
                pager           : false,
                hideControlOnEnd: true
            }, 1)
        }

        // =================================================================
        // = Series To Binge This Weekend Slider
        // =================================================================
        if(!epicReads.isMobileResolution()) {
            STBTW_SliderBx = epicReads.initBxSlider(STBTW_Slider, {
                moveSlides      : 1,
                minSlides       : 1,
                maxSlides       : 6,
                slideWidth      : 150,
                infiniteLoop    : true,
                pager           : false,
                hideControlOnEnd: true
            }, 6)

            sliderHeight = STBTW_Slider.height();
            STBTW_Slider.children().height(sliderHeight);

        } else {

        }

        // =================================================================
        // = Youtube player: fit width
        // =================================================================
        if($.isFunction($.fn.fitVids)) {
             videoBox.fitVids();
        }

        // =================================================================
        // = Fun blocks: fit descriptions height
        // =================================================================
        if(funBox.length && !epicReads.isMobileResolution()) {
            if($.isFunction($.fn.matchHeight)) {
                var boxContent = funBox.find('.poster-text');
                boxContent.matchHeight();
            }
        }

        if(!epicReads.isMobileResolution()) {
            FB_sliderBx = epicReads.initBxSlider(FB_slider, {
                moveSlides      : 1,
                minSlides       : 1,
                maxSlides       : 4,
                slideWidth      : 233,
                infiniteLoop    : true,
                pager           : false,
                hideControlOnEnd: true
            }, 4)
        }

        // =================================================================
        // = Social blocks: fit content height
        // =================================================================
        if(socialBox.length && !epicReads.isMobileResolution()) {
            if($.isFunction($.fn.matchHeight)) {
                var boxContent = socialBox.find('.social-eq-box');
                boxContent.matchHeight();
            }
        }

        // =================================================================
        // = Orientation change events
        // =================================================================
        epicReads.isBreakpointChanged(function() {
            // mobile
            if(typeof RP_SliderBx !== 'undefined') {
                epicReads.reloadSlider(RP_SliderBx, {
                    moveSlides      : 1,
                    minSlides       : 1,
                    maxSlides       : 1,
                    infiniteLoop    : true,
                    pager           : false,
                    hideControlOnEnd: true
                });
            }
        }, function() {
            // desktop
            if(typeof TP_SliderBx === 'undefined') {
                TP_SliderBx = epicReads.initBxSlider(TP_Slider, {
                                    moveSlides      : 1,
                                    minSlides       : 1,
                                    maxSlides       : 8,
                                    slideWidth      : 108,
                                    infiniteLoop    : true,
                                    pager           : false,
                                    hideControlOnEnd: true
                                }, 8)
            }
            if(typeof STBTW_SliderBx === 'undefined') {
                STBTW_SliderBx = epicReads.initBxSlider(STBTW_Slider, {
                                moveSlides      : 1,
                                minSlides       : 1,
                                maxSlides       : 6,
                                slideWidth      : 150,
                                infiniteLoop    : true,
                                pager           : false,
                                hideControlOnEnd: true
                            }, 6)
            }
            if(typeof RP_SliderBx !== 'undefined') {
                epicReads.reloadSlider(RP_SliderBx, {
                                     moveSlides      : 1,
                                     minSlides       : 1,
                                     maxSlides       : 4,
                                     slideWidth      : 233,
                                     infiniteLoop    : true,
                                     pager           : false,
                                     hideControlOnEnd: true
                });
            }
        })

    
});
