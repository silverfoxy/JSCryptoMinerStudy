				var visCheckElem = this;

				var alreadyReached = false;

				jQuery(window).scroll(function () {

					if (!alreadyReached && (jQuery(visCheckElem).offset().top + (jQuery(visCheckElem).height() / triggerPoint)) < (jQuery(window).scrollTop() + jQuery(window).height())) {
						callback(visCheckElem);
						alreadyReached = true;
					}
				});
			})
		}

		jQuery(document).ready(function() {

			elementOnScreen('.worth-col:nth-child(1)', function(){
				jQuery('.worth-col:nth-child(1)').css('opacity', '1');
