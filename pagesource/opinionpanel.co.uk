<script>
jQuery(window).load(function() {

    (function(){
        var fullLengthComments = {};
        var $comments = jQuery('.commentlist li');
        $comments.each(function(){
            var $commentParas = jQuery(this).find(' > div > p');
            var text = $commentParas.text();
            if(text.length > 500) {
                var commentID = $commentParas.closest('li').attr('id');
                fullLengthComments[commentID] = $commentParas;
                $commentParas.remove();
                jQuery(this).find('> div > .comment-author-and-time').after('<p>'+ text.substr(0, 490) + '[<span class="comment-expand">Read more...</span>]' + '</p>');
            }
        })

        $comments.on('click', 'div p span', function(){
            var $commentLi = jQuery(this).closest('li');
            var expandingCommentID = $commentLi.attr('id');
            //console.log(expandingCommentID);
            $commentLi.find(' > div  > p').remove();
            $commentLi.find(' > div  > .comment-author-and-time').after(fullLengthComments[expandingCommentID]);
        });
    })();

    (function(){
        var urlParams = window.location.search;
        if(urlParams.indexOf('replytocom') >= 0) {
            var $commentForm = jQuery('#commentform').remove();
            var numberRegex = /\d+/g;
            var commentToReplyToID = 'comment-' + numberRegex.exec(urlParams);
            var $commentToReplyTo = jQuery('#' + commentToReplyToID);
            $commentToReplyTo.after($commentForm[0].outerHTML);
            jQuery(window).scrollTop(($commentToReplyTo.offset()['top']));
        }
    })();

}); 
</script>
    
