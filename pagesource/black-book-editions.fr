function hp_liste_show()
{
	var html;
	$.post(xml_script_url, { a: 'hp_liste_show' }, function(data){
		eval(data);
		if (xml_process)
		{
			$("#hp_catalogue").html(html);
			if (hp_liste_content=='all') $('#hp_catalogue a.popup').magnificPopup({type:'image'});
		}
		else if (do_reporting) alertify.notify('ERROR XML : '+xml_msg, "error", 3, function(){});
		$("#hp_liste_processing").hide();
	});
}

function hp_select_content(id)
{
	$("#hp_liste_content").val(id);
	hp_liste_filtre_update();
}

function hp_liste_filtre_update()
{
	$("#hp_liste_processing").show();

	hp_liste_content 	= $("#hp_liste_content").val();
	hp_liste_tri 		= $("#hp_liste_tri").val();
	hp_liste_nb 		= $("#hp_liste_nb").val();

	$.post( xml_script_url, 
			{ a: 'hp_liste_filtre_update', 
			  hp_liste_content: hp_liste_content, 
			  hp_liste_tri: hp_liste_tri, 
			  hp_liste_nb: hp_liste_nb, 
			  hp_liste_mode: hp_liste_mode }
			, function(data) {
				eval(data);
				if (xml_process) hp_liste_show();
				else if (do_reporting) alertify.notify('ERROR XML : '+xml_msg, "error", 3, function(){});
	});
	hp_liste_show();
}

function hp_change_mode(mode)
{
	if (mode=='list')
	{
		$("#hp_liste_type_pic").removeClass('active');
		$("#hp_liste_type_list").addClass('active');
	}
	else
	{
		$("#hp_liste_type_pic").addClass('active');
		$("#hp_liste_type_list").removeClass('active');
	}
	hp_liste_mode = mode;
	hp_liste_filtre_update();
}

