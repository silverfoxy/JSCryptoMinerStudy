<div class="swym-top-widget-loop products-grid row" data-swrwidget="top-wishlisted" data-swrwidget-params='{"days":30, "n": 12, "src": "site", "pos": "default" }' data-swrwidget-ui='{"productTemplate": "swym-widget-product", "showCount": true, "showAddToCart": true}'>
  <div class="swym-widget-loading">Loading...</div>
</div>

<script id="swym-top-widget-js">
  (function(){
    var swat;
    function initTopWidgets(){
      swat = window._swat;
      window.SwymWidgets = [];
      var widgetContainers = document.querySelectorAll("[data-swrwidget]");
      for(var i = 0; i < widgetContainers.length; i++){
        var widgetContainer = widgetContainers[i];
        var w = new SwymReportWidget(widgetContainer);
        SwymWidgets.push(w);
        w.load();
      }
    }
    function SwymReportWidget(elem){
      this.container = elem;
      this.type = elem.getAttribute("data-swrwidget");
      var paramsStr = elem.getAttribute("data-swrwidget-params");
      var uiStr = elem.getAttribute("data-swrwidget-ui");
      this.params = safeJSONParse(paramsStr, {});
      this.ui = safeJSONParse(uiStr, {});
      copyDefaults(this.params, this.defaultParams);
      this.params.token = this.params.pid = window.swymJSObject.pid;
      copyDefaults(this.ui, this.defaultUi);
      this.cache = new swat.productCache.constructor(this.type, 300, swat.storage);
    };

    SwymReportWidget.prototype.defaultParams = {
      "days": 30,
      "n": 10,
      "src": "site",
      "pos": "default"
    };

    SwymReportWidget.prototype.defaultUi = {
      "productTemplate": "swym-widget-product",
      "showWishlist": true,
      "openUi": false
    };

    SwymReportWidget.prototype.load = function(){
      // get from cache if available or fetch from server
      var me = this;
      if(this.cache.isCacheEntryStale()){
        this.fetch();
      }else{
        var products = this.cache.getObject();
        setTimeout(function(){
          me.render(products);
        });
      }
    };

    SwymReportWidget.prototype.transform = function(o){
      if(o.product){
        // copy o.product props to o
        for(var k in o.product){
          o[k] = o.product[k];
        }
      }else{
        o["iu"] = o["ImageURL"];
        o["epi"] = o["productid"];
        o["pr"] = o["price"];
      }
      //o["icon"] = "swym-" + swat.retailerSettings.Wishlist.ButtonIcon;
      if(Shopify.formatMoney && Shopify.money_format){
        //debugger;
        o["pr"] = Shopify.formatMoney(o["pr"].toFixed(2));
        if(o["op"]){
          o["op"] = Shopify.formatMoney(o["op"].toFixed(2));
        }
      }else{
        o["pr"] = o["pr"].toFixed(2)
      }
      return o;
    };

    SwymReportWidget.prototype.render = function(products){
      var tmpl = loadTemplate(this.ui.productTemplate);
      var renderedHTML = "<div>";
      products.forEach(function(p){
        renderedHTML += swat.Mustache.render(tmpl, p);
      }, this);
      renderedHTML += "</div>";
      this.container.innerHTML = renderedHTML;
    };

    SwymReportWidget.prototype.fetch = function(){
      var me = this;
      // fire api
      swat.swymApi({
        endpoint: '/widgets/' + this.type,
        params: this.params,
        callbackFn: function (respJson, req) {
          // transform
          respJson.forEach(function(p){
            p = me.transform(p);
          });
          // save to cache
          me.cache.putObject(respJson);
          // render
          me.render(respJson);
        },
        errorFn: function(x){
          console.log("Error fetching top products", x, me);
        },
        sendRegId: false,
        sendSessionId: false,
        checkRegId: false,
        v2: true
      });
    };

    function copyDefaults(o, _defaults){
      for(var k in _defaults){
        if(!(k in o) ){
          o[k] = _defaults[k];
        }
      }
    }

    function safeJSONParse(str, d){
      try{
        return JSON.parse(str);
      }catch(err){
        return d;
      }
    }

    function loadTemplate(templateId) {
      var template =  document.getElementById(templateId);
      if(!template) {
        SwymUtils.warn('Error cannot find the template #' + templateId);
        return false;
      }
      var templateHtml = template.innerHTML.trim();
      swat.Mustache.parse(templateHtml);
      return templateHtml;
    }
    if(window._swat){
      initTopWidgets();
    }else{
      window.SwymCallbacks = window.SwymCallbacks || [];
      window.SwymCallbacks.push(initTopWidgets);
    }
  })();
</script>



