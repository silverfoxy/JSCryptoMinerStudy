<!-- Pinterest script -->
<script>
    $(function () {
        var scrollPositionSocialWidget;
        $('.js-social-widget-pinterest').click(function (event) {
            event.preventDefault();
            $('.js-pinterest-widget').height($(document).height());
            scrollPositionSocialWidget = $(window).scrollTop();
            window.scrollTo(0, 0);
            $('.js-pinterest-widget').show();
        });
        $('.js-pinterest-widget-close').click(function (event) {
            event.preventDefault();
            $('.js-pinterest-widget').hide();
            window.scrollTo(0, scrollPositionSocialWidget);

        });
    });
</script>
<script>
    $('.js-social-widget-pinterest').click(function (event) {
        event.preventDefault();
        var pageTitle = $(document).attr('title');
        var pageAddress = $(location).attr('href');
        var pinterestItems = "";
        $('img').each(function () {
            imgsrc = this.src;
            var ext = imgsrc.split('.').pop().toLowerCase();
            if ($.inArray(ext, ['jpg', 'jpeg']) !== -1) {
                var selectedImage = new Image();
                selectedImage.src = imgsrc;
                var imgWidth = selectedImage.naturalWidth
                var imgHeight = selectedImage.naturalHeight;
                if (imgWidth > 90 && imgHeight > 90) {
                    if (imgWidth < imgHeight) {
                        var scaleRatio = imgHeight / imgWidth;
                        var newImageHeight = 200 * scaleRatio;
                        var marginCorrection = (200 - newImageHeight) / 2;
                        pinterestItems += '<div class="social-widget-pinterest-item">' + '<img src="' + imgsrc + '" class="pinterest-item-img" height="' + newImageHeight + '" width="200' + '" style="margin-top:' + marginCorrection + 'px">' + '<span class="pinterest-widget-size">' + imgWidth + ' x ' + imgHeight + '</span>' + '<a href="http://pinterest.com/pin/create/bookmarklet/?media=' + imgsrc + '&url=' + pageAddress + '&is_video=false&description=' + pageTitle + '" target="_blank" class="pinterest-widget-button js-pinterest-pin-btn">' + '<img src="http://staticcdn.wheretoget.it/img/PinterestPinBtn.png" class="pinterest-widget-button-img" alt="pinterest pin it button">' + '</a>' +
                            '</div>';
                    } else if (imgWidth > imgHeight) {
                        var scaleRatio = imgWidth / imgHeight;
                        var newImageWidth = 200 * scaleRatio;
                        var marginCorrection = (200 - newImageWidth) / 2;
                        pinterestItems += '<div class="social-widget-pinterest-item">' + '<img src="' + imgsrc + '" class="pinterest-item-img" height="200" width="' + newImageWidth + '" style="margin-left:' + marginCorrection + 'px">' + '<span class="pinterest-widget-size">' + imgWidth + ' x ' + imgHeight + '</span>' + '<a href="http://pinterest.com/pin/create/bookmarklet/?media=' + imgsrc + '&url=' + pageAddress + '&is_video=false&description=' + pageTitle + '" target="_blank" class="pinterest-widget-button js-pinterest-pin-btn">' + '<img src="http://staticcdn.wheretoget.it/img/PinterestPinBtn.png" class="pinterest-widget-button-img" alt="pinterest pin it button">' + '</a>' +
                            '</div>';
                    } else if (imgWidth === imgHeight) {
                        pinterestItems += '<div class="social-widget-pinterest-item">' + '<img src="' + imgsrc + '" class="pinterest-item-img"  height="200" width="200">' + '<span class="pinterest-widget-size">' + imgWidth + ' x ' + imgHeight + '</span>' + '<a href="http://pinterest.com/pin/create/bookmarklet/?media=' + imgsrc + '&url=' + pageAddress + '&is_video=false&description=' + pageTitle + '" target="_blank" class="pinterest-widget-button js-pinterest-pin-btn">' + '<img src="http://staticcdn.wheretoget.it/img/PinterestPinBtn.png" class="pinterest-widget-button-img" alt="pinterest pin it button">' + '</a>' +
                            '</div>';
                    }


                }
            }
        });
        $('.social-widget-pinterest__container').html(pinterestItems);
    });
</script>

