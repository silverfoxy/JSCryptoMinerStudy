<script type='text/javascript'>
$(function(){
	var threshold = $(window).height()/2;

	$(window).scroll(function(){
		(window.innerWidth ? window.pageYOffset : document.documentElement.scrollTop) >= threshold ? $("#back-to-top").fadeIn('fast') : $("#back-to-top").fadeOut('fast');
	});

	$("#back-to-top").click(function(){
		$("html, body").animate({
			scrollTop: "0"
		}, 400);
		return false;
	});
});
</script>

