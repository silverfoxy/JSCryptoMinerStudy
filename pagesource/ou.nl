<script>
//Adds a 'selected' class on the current article button
function addSelectedClass(articleId){
	AUI().use('aui-module, aui-node', function(A){ 
		var buttons = A.all(".carousel-button");
		buttons.each(
			function(button){
				button.removeClass("selected-button");
				var buttonId = button.get('id');
				if(articleId.indexOf(buttonId) > -1){
					button.addClass("selected-button");
				}
			}	
		);
	})	
};	
//Sets a new articleId & article based on the previous set
function slideArticles(){
	AUI().use('aui-module, aui-node', function(A){
		var articles = [486678, 486698, 486685, 486709, 486720];
		if(articles.length > 1){
			var articleIdDiv = A.one("#articleIdDiv"); 
			var currentArticleId = parseInt(articleIdDiv.getHTML());
			var newArticleId;
			var currentIndex = articles.indexOf(currentArticleId);
			if(currentIndex < articles.length - 1){
				newArticleId = articles[currentIndex+1];
			}
			else{
				newArticleId = articles[0];
			}
			updateDiv(newArticleId);
		}
});};
//Updates the header every 5 seconds
var intervalTijd = 10000;
var timer = setInterval(function() {
		slideArticles();
		}, intervalTijd);

//Pauses the updating of headers when hovering on the header
$('#container').hover(function(ev){
    clearInterval(timer);
}, function(ev){
    timer = setInterval( function() {
		slideArticles();
	}, intervalTijd);
});
$('.carousel-button').hover(function(ev){
    clearInterval(timer);
}, function(ev){
    timer = setInterval( function() {
		slideArticles();
	}, intervalTijd);
});

//Resizes a div to the same height & width as the calculated header size
//Also moves sets the left style property if needed
function resizeDiv(resizableDiv, width, height, setToLeft){
	resizableDiv.setStyle("height", height);
	resizableDiv.setStyle("width", width);
	if(setToLeft){
		resizableDiv.setStyle("left", width);
	}
}

//Update method to get content from server using resource url
function updateDiv(newArticleId){
	AUI().ready('aui-base','aui-node','aui-io-request', function(A){
		//aui ajax call to get updated content
		var articleIdDiv = A.one("#articleIdDiv");
		var articleContentDiv = A.one("#articleContent");
		var newContentDiv = A.one("#newArticleContent");
		A.io.request('https://www.ou.nl/home?p_p_id=carousel_WAR_corpcarouselportlet_INSTANCE_ughk4Zwxn6MS&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_cacheability=cacheLevelPage&p_p_col_id=column-1&p_p_col_pos=1&p_p_col_count=4',{
			dataType: 'json',
			method: 'GET',
			data: { _carousel_WAR_corpcarouselportlet_INSTANCE_ughk4Zwxn6MS_articleId: newArticleId },
			on: {
				success: function() {
					var data=this.get('responseData');
						newContentDiv.setHTML(data);
						newContentDiv.show();
						$('#newArticleContent').animate({
							left: 0
						});
					
						
						
						var width = document.getElementById("articleContent").clientWidth;
						var height = document.getElementById("articleContent").clientHeight;
						
						resizeDiv(newContentDiv, width, height, false);
						
					
						articleIdDiv.setHTML(newArticleId);
						addSelectedClass(newArticleId.toString());
						setTimeout(function(){ 
							articleContentDiv.setHTML(data);
							newContentDiv.empty();
							newContentDiv.setStyle("left",width); 
						}, 1000);
				}
			}
		});
	});
}
//Checks which article button is selected, also initializes all the dynamic div sizes
AUI().ready('aui-module, aui-node', function(A){ 
	var articleIdDiv = A.one("#articleIdDiv"); 
	var currentArticleId = articleIdDiv.getHTML();
	addSelectedClass(currentArticleId);
	
	var containerDiv = A.one("#container");
	var articleContentDiv = A.one("#articleContent");
	var newContentDiv = A.one("#newArticleContent");
	var width = document.getElementById("articleContent").clientWidth;
	var height = document.getElementById("articleContent").clientHeight;
	resizeDiv(newContentDiv, width, height, true);
	resizeDiv(containerDiv, width, height, false);
	
})
</script>

	
	

					
				
			
		
	
	



