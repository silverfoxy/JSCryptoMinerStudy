<script type="text/javascript">

	var windowSize = 0;

	$(document).ready(function () {
		windowSize = $(window).width();
		configMenu();
	});

	function configMenu() {

		

		$('.navItemWrap').each(function () {

			var totalWidth = 0;
			var wrapHeight = 0;

			$('.menuContainer', this).each(function () {
				totalWidth += $(this).outerWidth(true);

				var tmpHeight = $(this).outerHeight(true);

				if (tmpHeight > wrapHeight) {
					wrapHeight = $(this).outerHeight(true);
				}
			});

			totalWidth += $('#NavCheckForService', this).outerWidth(true);
			$('#NavCheckForService > table', this).height(wrapHeight);
			$('.subMenuWrap', this).width(totalWidth + 65);


			if ($(this).find('.subMenuWrap').length > 0) {
				var m = $(this).find('.subMenuWrap');
				var mOffset = $(m).offset().left;
				var mWidth = $(m).outerWidth();

				if ((mOffset + mWidth) > windowSize) {
					$(m).css('left', '-' + ((mOffset + mWidth) - windowSize) + 'px');
				}
			}

		});
	}

	$(window).resize(function() {

