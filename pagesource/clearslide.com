            if(error == '') { error = 'There was an error processing your request'; }
	    jQuery('.result_header').text(error);
	  }
	});
}

function checkemail(email){
var re = /[A-Z0-9._%+-]+@[A-Z0-9.-]+.[A-Z]{2,4}/igm;
