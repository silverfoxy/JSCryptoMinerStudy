<script>
  
var SiteNavigation = (function() {
  var $body;
  var $navigation;
  var $navbarToggleBtn;
  var $firstLevelNavigation;
  var $secondLevelNavigation
  var $navigationTabs;
  var $navigationSubTabs;
  var isNavigationOpen = false;
  var isNavigationAligned;
  var navigationHoverTimeout;
  var isScreeResizeThrottled;


  var stopHoverEffect = function(func) {

    var later = function() {
      var $currentHoverTab = document.querySelectorAll('.siteNavigation__tab--hover')[0];
      var $currentHoverSubTabRow = document.querySelectorAll('.siteNavigation__sub-tab-row--hover')[0];
      navigationHoverTimeout = null;

      $navigation.classList.remove( 'siteNavigation--hover' );
      if ($currentHoverTab != null) {
        $currentHoverTab.classList.remove( 'siteNavigation__tab--hover' );
      }
      if ($currentHoverSubTabRow != null) {
        $currentHoverSubTabRow.classList.remove( 'siteNavigation__sub-tab-row--hover' );
      }
    };

    clearTimeout(navigationHoverTimeout);
    navigationHoverTimeout = setTimeout(later, 1000);
  };

  // Where el is the DOM element you'd like to test for visibility
  function isHidden(el) {
    return (el.offsetParent === null)
  }

  var openSecondLevelNavigation = function(e) {
    e.stopPropagation();
    e.preventDefault();

    $subMenuHeader.classList.add('siteNavigation__sub-menu-header--active');

    $subMenuHeaderTitle.innerHTML = e.target.innerHTML;
    $subMenuHeaderTitle.href = e.target.href;
    $firstLevelNavigation.classList.add('hidden');

    var subTabRow = document.querySelectorAll('.siteNavigation__sub-tab-row[data-parent="' + e.target.innerHTML + '"]')[0];

    subTabRow.classList.add('siteNavigation__sub-tab-row--sub-menu');
    
    setTimeout(function() {
      subTabRow.classList.add('in');
    }, 0);
  }

  var closeSecondLevelNavigation = function(e) {
    if (e !== undefined) {
      e.stopPropagation();
      e.preventDefault();
    }

    $subMenuHeader.classList.remove('siteNavigation__sub-menu-header--active');

    var subTabRow = document.querySelectorAll('.siteNavigation__sub-tab-row--sub-menu');

    if (subTabRow.length > 0) {
      subTabRow[0].classList.remove('siteNavigation__sub-tab-row--sub-menu');
      subTabRow[0].classList.remove('in');
    }
    
    $firstLevelNavigation.classList.remove('hidden');
    $firstLevelNavigation.classList.add('siteNavigation__level-1--return-from-sub-menu');
    setTimeout(function() {
      $firstLevelNavigation.classList.add('in');
    }, 0);

    setTimeout(function() {
      $firstLevelNavigation.classList.remove('siteNavigation__level-1--return-from-sub-menu');
      $firstLevelNavigation.classList.remove('in');
    }, 1000 * .3);
  }

  var closeNavigationInDesctop = function(e) {
    if (!isScreeResizeThrottled) {
      if (isHidden($navbarToggleBtn)) {
        $body.classList.remove('slide-visible');
        $firstLevelNavigation.removeEventListener('click', openSecondLevelNavigation);
        closeSecondLevelNavigation();
        window.removeEventListener("resize", closeNavigationInDesctop);

        if (!isNavigationAligned) {
          aligneNavigation();
        }
      }

      isScreeResizeThrottled = true;
      setTimeout(function() {
        isScreeResizeThrottled = false;
      }, 100);
    }
  }

  var aligneNavigation = function() {

    if (!isHidden($navbarToggleBtn)) {
      isNavigationAligned = false;
      return;
    }

    isNavigationAligned = true;

    for (i = 0; i < $navigationTabs.length; i++) { 
      $navigation.classList.remove('siteNavigation--css-not-fully-loaded');
      var navigationBoundingRectangle = $navigation.getBoundingClientRect();
      var elementBoundingRectangle = $navigationTabs[i].getBoundingClientRect();

      var $subTab = document.querySelectorAll('.siteNavigation__sub-tab-row[data-parent="' + $navigationTabs[i].innerHTML + '"]')[0];
      
      if ($subTab !== undefined) {
        var subTabItems = $subTab.querySelectorAll('li');
        var subTabItemsWidth = 0;
        for (var j = 0; j < subTabItems.length; j++) subTabItemsWidth += subTabItems[j].offsetWidth;

        var subTabWidthMiddle = subTabItemsWidth / 2;

        var elementMiddle = elementBoundingRectangle.width /2 + elementBoundingRectangle.left;
        elementMiddle -= navigationBoundingRectangle.left;

        if (elementMiddle - subTabWidthMiddle < 15) {
          // If element if too much to the right
          elementMiddle = subTabWidthMiddle + 15
        } else if (navigationBoundingRectangle.width < elementMiddle + subTabWidthMiddle + 15) {
          // If element if too much to the left 
          elementMiddle = navigationBoundingRectangle.width - subTabWidthMiddle - 15;
        }
        
        $subTab.style.left = elementMiddle - subTabWidthMiddle + 'px';
      }
    }
  }

  return {

    init: function() {

      $body = document.querySelectorAll('body')[0];
      $navigation = document.querySelectorAll('.siteNavigation')[0];
      $navbarToggleBtn = document.querySelectorAll('.header .navbar-toggle')[0];
      $firstLevelNavigation = document.querySelectorAll('.siteNavigation__level-1')[0];
      $secondLevelNavigation = document.querySelectorAll('.siteNavigation__level-2')[0];
      $navigationTabs = document.querySelectorAll('.siteNavigation__tab');
      $navigationSubTabs = document.querySelectorAll('.siteNavigation__sub-tab');
      $subMenuHeader = document.querySelectorAll('.siteNavigation__sub-menu-header')[0];
      $subMenuHeaderTitle = document.querySelectorAll('.siteNavigation__sub-menu-header-title')[0];

      $navbarToggleBtn.addEventListener('click', function (e) {
        e.stopPropagation();
        e.preventDefault();

        if ($body.className.indexOf('slide-visible') >= 0) {
          
          $body.classList.remove('slide-visible');
          $firstLevelNavigation.removeEventListener('click', openSecondLevelNavigation);
          closeSecondLevelNavigation();
          window.removeEventListener("resize", closeNavigationInDesctop);

        } else {
          
          $body.classList.add('slide-visible');
          $firstLevelNavigation.addEventListener('click', openSecondLevelNavigation);
          window.addEventListener("resize", closeNavigationInDesctop);

        }
      }, false);

      document.querySelectorAll('.siteNavigation__sub-menu-header-back-btn')[0]
        .addEventListener('click', closeSecondLevelNavigation, false);
      
      // Linking navingation items parents and children
      for (i = 0; i < $navigationTabs.length; i++) {
        $navigationTabs[0].data = {};
        var secondLevelNav = document.querySelectorAll('.siteNavigation__sub-tab-row[data-parent="' + $navigationTabs[i].innerHTML + '"]')[0];
        
        if (secondLevelNav !== undefined) {
          var secondLevelNavItems = secondLevelNav.querySelectorAll('a');
          secondLevelNav.data = {};
          secondLevelNav.data.parent = $navigationTabs[0];
        } else {
          var secondLevelNavItems = [];
        }
        
        $navigationTabs[0].data.child = secondLevelNav;

        $navigationTabs[i].dataset.nameTrack = 'QT1:' + $navigationTabs[i].innerHTML;
        
        for (j = 0; j < secondLevelNavItems.length; j++)
          secondLevelNavItems[j].dataset.nameTrack = 'QT1:' + $navigationTabs[i].innerHTML + ':' + secondLevelNavItems[j].innerHTML;
      }

      // This section is responsible for aligning the tabs
      aligneNavigation();
      window.addEventListener('load', aligneNavigation);
      
      window.addEventListener('load', function() {
        var links = document.querySelectorAll('.siteNavigation a[data-name-track]');

        for (j = 0; j < links.length; j++) {
          links[j].addEventListener('click', function (e) {
            s.prop21 = e.target.dataset.nameTrack;
            s.tl($(this),'o', s.prop21, {
              linkTrackVars:'prop21',
              prop21:s.prop21
            });
          })
        };
          
      });
      

      $firstLevelNavigation.addEventListener('mouseover', function(event) {
        var $currentHoverTab = document.querySelectorAll('.siteNavigation__tab--hover')[0];
        var $currentHoverSubTabRow = document.querySelectorAll('.siteNavigation__sub-tab-row--hover')[0];
        var $taretHoverSubTabRow = document.querySelectorAll('ul[data-parent="' + event.target.innerHTML + '"]')[0];
        
        clearTimeout(navigationHoverTimeout);

        $navigation.classList.add('siteNavigation--hover');

        if ($currentHoverTab != null) {
          $currentHoverTab.classList.remove('siteNavigation__tab--hover');
        }
        event.target.classList.add('siteNavigation__tab--hover');

        if ($currentHoverSubTabRow != null) {
          $currentHoverSubTabRow.classList.remove('siteNavigation__sub-tab-row--hover');
        }
        
        if ($taretHoverSubTabRow != undefined) {
          $taretHoverSubTabRow.classList.add('siteNavigation__sub-tab-row--hover');
        }
      })

      $secondLevelNavigation.addEventListener('mouseover', function() {
        clearTimeout(navigationHoverTimeout);
      }, false)

      $navigation.addEventListener('mouseleave', stopHoverEffect, false);
      $secondLevelNavigation.addEventListener('mouseleave', stopHoverEffect, false);
    }
  };

})();
  
</script>

