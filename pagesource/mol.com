<script type="text/javascript">

    $(document).ready(function () {
        $('#loading').hide();
        var index = 1;
        var temp = GetParameterByName("t");
        if (temp != '') {
            index = parseInt(temp);            
        }

        LoadContent(index);
    });
    
    var loadedTabs = [];

    function LoadContent(tabIndex) {
        
        var loaded = false;

        for (var i = 0; i < loadedTabs.length; i++) {
            if (loadedTabs[i] == tabIndex) {
                loaded = true;
                break;
            }
        }

        if (loaded == true) {
            return;
        }
        else {
            loadedTabs.push(tabIndex);
        }

        $('#loading').show();
        
        var url = $("#Tab" + tabIndex.toString()).val();
        
        $.ajax(
        {
            type: 'get',
            url: url,
            dataType: 'html',
            success: function (result) {                
                $('#t' + tabIndex.toString()).html(result);                
            },
            error: function (error) {
                $('#t' + tabIndex.toString()).load('/error.html');
                loadedTabs.pop();
            },
            complete: function () {                
                $('#loading').hide();
            }
        });
    }

    function GetParameterByName(name) {
        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
        return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }

</script>


