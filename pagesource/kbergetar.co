<script type='text/javascript'>
  /*<![CDATA[*/
    var numPages=6;
    var firstText ='First';
    var lastText ='Last';
    var prevText =' Previous';
    var nextText ='Next ';
    var urlactivepage=location.href;
    var home_page="/";
  /*]]>*/
