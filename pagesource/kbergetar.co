position: absolute;
top: 45%;
left: 45%;
opacity:0;
padding: 0;
z-index:1;
border: 2px solid #fff;
border-radius: 50%;
transition: all 0.25s ease-in-out 0s;
}
.block-image a:after {
content: "";
