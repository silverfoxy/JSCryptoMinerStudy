      html += '<div class="vs FloatL"><img src="http://static.sporttery.cn/pres/proj/2016/sporttery-2016/images/home/vs.jpg" /><br>('+ d[i].goalline +')</div>';
      html += '<div class="zhu FloatL"><span><strong>'+ d[i].h_s_cn +'</strong><br>主胜 '+ d[i].h +'</span><img src="'+ d[i].h_pic +'"></div>';
      html += '<div class="clear"></div>';
      if(d[i].news_title != undefined && d[i].news_title != ''){
        news = '<div class="FloatL foc-bg"><a href="'+ d[i].news_url +'">'+ d[i].news_title +'</a></div>';
      }else{
        news = '';
      }
      html += '<div class="bonus">'+ news +' <a href="http://info.sporttery.cn/basketball/hdc_list.php?ref=hbtn" target="_blank" class="more-btn FloatR">计算奖金</a></div>';
    }
  }else{
    html = '<div class="no_match">暂无赛程提供,点击<a href="http://info.sporttery.cn/basketball/match_interim.php" class="red" target="_blank">预览</a>暂定受注事</div>';
  }
  $('.foc-event:eq(1)').html(html);
}

