    <script type="text/javascript">
        //Enumeration for messages status
        MessageStatus = {
            Success: 1,
            Information: 2,
            Warning: 3,
            Error: 4
        };

        //Global variables
        var intervalID = 0;
        var subintervalID = 0;
        var fileUpload;
        var form;

        //Attach to the upload click event and grab a reference to the progress bar
        function pageLoad() {
            $addHandler($get('upload'), 'click', onUploadClick);
        }

        //Register the form
        function register(form, fileUpload) {
            this.form = form;
            this.fileUpload = fileUpload;
        }

        //Start upload process
        function onUploadClick() {
            if (fileUpload.value.length > 0) {
                var filename = '';
                if (filename == '') {
                    //Update the message
                    updateMessage('Please wait ... Initializing process ...', '');
                    //Submit the form containing the fileupload control
                    form.submit();
                    //Initialize progressbar
                    setProgress(0);
                    //Start polling to check on the progress ...
                    intervalID = window.setInterval(function () {
                        window.PageMethods.GetUploadStatus(function (result) {
                            if (result) {
                                setProgress(result.PercentComplete);
                                //Upadte the message every 500 milisecond
                                updateMessage(result.UploadProgressMessage, result.FileName);
                                SetElapsedTime(result.ElapsedTime);
                                if (result == 100) {
                                    //clear the interval
                                    window.clearInterval(intervalID);
                                    clearTimeout(subintervalID);
                                }
                            }
                        });
                    }, 500);
                }
                else
                    onComplete(MessageStatus.Error, '', '', '');
            }
            else
                onComplete(MessageStatus.Warning, 'Veuillez s√©lectionner un fichier PDF svp.', '', '');
        }

        //Stop progrss when file was successfully uploaded
        function onComplete(type, msg, filename, downloadBytes) {
            window.clearInterval(intervalID);
            clearTimeout(subintervalID);
            updateMessage(msg, filename);
            if (type == MessageStatus.Success) setProgress(100);
        }
    </script>


