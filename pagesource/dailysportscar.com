							} else {
								newAtts = setIsotopeFilterValues(atts, defaultFilter);
								addTermsToButton(defaultFilter);
							}

							//lets call the ajax function to go get the posts.
							getPosts(newAtts, postsFilter);
							//console.log(response);
							//loadMoreBtn.removeClass('is-loading');
						});

					});
