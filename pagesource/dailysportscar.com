							html += '</div>';
							html += '</article>';
							html += '</li>';
							count++;
						});
						return html;
					}

					/**
					 * -----------------------------------------------------------------------
					 * Add Items to list
					 * -----------------------------------------------------------------------
					 */
					function addItemsToFilterList(data, atts, element) {
						var html = postFilterCreateListItems(data, atts);
						var $resCollectionItems = $($.parseHTML(html));
						var $collection = itemList;
						var isotopeLayout = throttle(function() {
							$collection.isotope('layout');
						}, 800);

						$collection.removeClass('g1-collection-loading');
						$collection.append($resCollectionItems).isotope('appended', $resCollectionItems);
						isotopeLayout();
						$resCollectionItems.find('img').on('load', isotopeLayout);
						loadMoreBtn.removeClass('is-loading');
					}

					/**
					 * -----------------------------------------------------------------------
					 * Get Posts
					 * -----------------------------------------------------------------------
					 * This is the Ajax function that goes and gets our posts that we want.
					 * @param 	string atts
					 * -----------------------------------------------------------------------
					 */
					function getPosts(atts, element) {
						var data = {
							'action': 'postsfilter_action',
							'security': 'a85c1d01b1',
							'atts': atts,
						};

						$.post(ajaxurl, data, function(response){
							if(parseResults(response)) {
								//console.log('its true we have vaild json');
								addItemsToFilterList(response, atts, element);
							}
						})
						.fail(function(){
							console.log('error');
						});
					}

