    <script type="text/javascript">
        var autoRoloutEnabled = true;
        var $ = jQuery.noConflict();
        (function ($) {
            $(document).ready(function () {
                var stateInput = $('#StatesDropdown_txtautofromDB');
                var inmateInput = $('#inmateIDTextBox');
                inmateInput.watermark('Enter Inmate ID #', { useNative: false });
                inmateInput.val('');
                stateInput.val('');

                $(document).click(function () {
                    if (inmateInput != null)
                        inmateInput.qtip('hide');

                    if (stateInput != null)
                        stateInput.qtip('hide');
                });

                inmateInput.focus(function () {
                    inmateInput.qtip('destroy');
                    stateInput.qtip('destroy');
                });

                stateInput.focus(function () {
                    inmateInput.qtip('destroy');
                    stateInput.qtip('destroy');
                });

                $("#rolloverButtonMoney").hover(
                    function () {
                        autoRoloutEnabled = false;
                        setRollover(true, false, false, false);
                    },
                    function () {
                        setRollover(true, false, false, false);
                        autoRoloutEnabled = true;
                    }
                );
                $("#rolloverButtonMail").hover(
                    function () {
                        autoRoloutEnabled = false;
                        setRollover(false, true, false, false);
                    },
                    function () {
                        setRollover(true, false, false, false);
                        autoRoloutEnabled = true;
                    }
                );
                $("#rolloverButtonVideo").hover(
                    function () {
                        autoRoloutEnabled = false;
                        setRollover(false, false, true, false);
                    },
                    function () {
                        setRollover(true, false, false, false);
                        autoRoloutEnabled = true;
                    }
                );
                $("#rolloverButtonMusic").hover(
                    function () {
                        autoRoloutEnabled = false;
                        setRollover(false, false, false, true);
                    },
                    function () {
                        setRollover(true, false, false, false);
                        autoRoloutEnabled = true;
                    }
                );
                $("#nextImageButton").hover(
                    function () { $("#nextImageButton").attr("src", "/images/Home/next_btn_hover.gif"); },
                    function () { $("#nextImageButton").attr("src", "/images/Home/next_btn_up.gif"); }
                );

                setInterval(autoRollout, 5000);
            });
        } (jQuery));

        function setRollover(money, mail, video, music) {
            setMoneyActive(money);
            setMailActive(mail);
            setVideoActive(video);
            setMusicActive(music);
        }
        function setMoneyActive(active) {
            if (active) {
                $("#rolloverButtonMoney").attr("src", "/images/Home/money_btn_active.gif");
                $("#rolloverImage").attr("src", "/images/Home/money_hero.jpg");
            } else {
                $("#rolloverButtonMoney").attr("src", "/images/Home/money_btn_inactive.gif");
            }
        }
        function setMailActive(active) {
            if (active) {
                $("#rolloverButtonMail").attr("src", "/images/Home/mail_btn_active.gif");
                $("#rolloverImage").attr("src", "/images/Home/mail_hero.jpg");
            } else {
                $("#rolloverButtonMail").attr("src", "/images/Home/mail_btn_inactive.gif");
            }
        }
        function setVideoActive(active) {
            if (active) {
                $("#rolloverButtonVideo").attr("src", "/images/Home/video_btn_active.gif");
                $("#rolloverImage").attr("src", "/images/Home/video_hero.jpg");
            } else {
                $("#rolloverButtonVideo").attr("src", "/images/Home/video_btn_inactive.gif");
            }
        }
        function setMusicActive(active) {
            if (active) {
                $("#rolloverButtonMusic").attr("src", "/images/Home/music_btn_active.gif");
                $("#rolloverImage").attr("src", "/images/Home/music_hero.jpg");
            } else {
                $("#rolloverButtonMusic").attr("src", "/images/Home/music_btn_inactive.gif");
            }
        }
        function autoRollout() {
            if (!autoRoloutEnabled)
                return;
            if ($("#rolloverButtonMoney").attr("src") == '/images/Home/money_btn_active.gif') {
                setRollover(false, true, false, false);
            } else if ($("#rolloverButtonMail").attr("src") == '/images/Home/mail_btn_active.gif') {
                setRollover(false, false, true, false);
            } else if ($("#rolloverButtonVideo").attr("src") == '/images/Home/video_btn_active.gif') {
                setRollover(false, false, false, true);
            } else if ($("#rolloverButtonMusic").attr("src") == '/images/Home/music_btn_active.gif') {
                setRollover(true, false, false, false);
            }
        }

        /*
        BELOW THREE FUNCTIONS TO TRIM BOTH LEADING AND TRALING SPACES IN THE PASSED STRING
        */
        function trim(str, chars) {
            return ltrim(rtrim(str, chars), chars);
        }
        function ltrim(str, chars) {
            chars = chars || "\\s";
            return str.replace(new RegExp("^[" + chars + "]+", "g"), "");
        }
        function rtrim(str, chars) {
            chars = chars || "\\s";
            return str.replace(new RegExp("[" + chars + "]+$", "g"), "");
        }
    </script>
