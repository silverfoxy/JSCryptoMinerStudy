                "Ampara","Anuradhapura","Badulla","Batticaloa","Colombo","Galle","Gampaha","Hambantota","Jaffna","Kalutara","Kandy","Kegalle","Kilinochchi","Kurunegala","Mannar","Matale","Matara","Monaragala","Mullaitivu","Nuwara-Eliya","Polonnaruwa","Puttalam","Ratnapura","Trincomalee","Vavuniya"];
            function loadAutoComplete(itemName){
                jQuery( itemName ).attr('title','Please Select a Delivery district from the list.');
                jQuery( itemName ).autocomplete({
                    source: availableTags,
                    close: function( event, ui ) {
					  	jQuery( "form[name='checkout']" ).trigger('update_custom_session');
                    },
                    change: function (event, ui) {
                        if(!ui.item){
                            //http://api.jqueryui.com/autocomplete/#event-change -
                            // The item selected from the menu, if any. Otherwise the property is null
                            //so clear the item for force selection
                            var value = jQuery( itemName ).val();
                            jQuery(itemName).val("");
                            jQuery( itemName ).tooltip({show: { effect: "blind", duration: 200 },position: { my: "left+15 center", at: "right center" },open: function (event, ui) {
                                    setTimeout(function () {
                                        jQuery(ui.tooltip).hide('explode');
                                        jQuery( itemName ).tooltip('destroy');
                                    }, 2000);
			
                                }});
                            jQuery( itemName ).tooltip('open');
                        }

                    },
                    focus: function (event, ui) {
                        return false;
                    }, minLength: 0,
                    scroll: true
                });
            }
            if (jQuery("#shipping_postcode").length ) {
                loadAutoComplete("#shipping_postcode");
            }
            if (jQuery("#billing_postcode").length ) {
                //loadAutoComplete("#billing_postcode");
            }
            if (jQuery("#ship-to-different-address-checkbox").length ) {
                jQuery('#ship-to-different-address-checkbox').change(function() {
                    if(this.checked) {
                        jQuery('#billing_postcode').autocomplete('disable');
                        jQuery('#billing_postcode').val('none');
                        jQuery('#billing_postcode_field').css('display','none');
                    }else{
                        jQuery('#billing_postcode').autocomplete('enable');
                        jQuery('#billing_postcode').val('');
                        jQuery('#billing_postcode_field').css('display','block');
                    }  
	
                });
            }
			
            if (jQuery("#billing_postcode").length ) {
