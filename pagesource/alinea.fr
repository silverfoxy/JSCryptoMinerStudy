            error : function(xhr,status,error) {
                addMessageToCart('Impossible de charger le panier','fail');
                // on permet le reload si il ya une ereur
                isMiniquoteQuerySent = false;
            },
            complete: function() {
                dfd.resolve();
            }

        });
        return dfd.promise();
