	}

	.top-header .tooltip-message {
		display: none;
		position: absolute;
		top: 35px;
		z-index: 1000;
		background: #F2F2F2;
		color: #000;
		box-shadow: 2px 3px 0px #CDCDCD;
		padding: 10px;
		font: 600 12px 'Open sans', Arial, sans-serif;
		text-transform: none;
	}

	.top-header .tooltip-message::after {
		position: absolute;
		content: "";
		width: 0;
		height: 0;		
		border-left: 0.5em solid transparent;
		border-right: 0.5em solid transparent;
