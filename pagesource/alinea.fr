	.top-header ul li {
		display: inline-block;
	}

	.top-header ul li a {
		display: inline-block;
		position: relative;
		font-family: 'Open Sans', Arial, sans-serif;
		text-transform: uppercase;		
		color: #fff;
		padding: 10px;
	}

	.top-header ul li a:hover,
	.top-header ul li.active a {
		background: #fff;
		color: #2d3714;
