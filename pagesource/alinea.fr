            prepareRequestQAS(form, callbackaction);
        }



    /**
     * Replace each inputs email origin by correction
     * @returns {undefined}
     */
    function correctEmail()
    {

        jQuery.each(jQuery('.email_correction'), function () {
            if (jQuery(this).is(':checked')) {
                idEmailToCorrect = jq(jQuery(this).data('correctionid'));
                inputToCorrect = jQuery(idEmailToCorrect);
                inputToCorrect.val(jQuery(this).val());
            }
        });

        hideErrorPanel();
    }

    /**
     * Lance la validation QAS
     * @param {type} btn Slecteur JQuery sur le bouton
     * @param {type} event Evenement de navigation
     * @param {type} callbackAction Action a appeler en cas de sucess.
     * @returns {Boolean} false
     */
    function launchValidationQAS(btn, event, callbackAction) {

        var qasform = jQuery(btn).closest("form");

        // Validate the form
        if (qasform.valid()) {
            showWaitPanel();

            // checked data user input
            prepareRequestQAS(qasform, callbackAction);

            // block the post form
            if( null !==  event ){
                event.preventDefault();
                event.stopPropagation();
            }
            
            return false;
        } else {
            // block the post form
            if( null !==  event ) {
                event.preventDefault();
                event.stopPropagation();
            }
            
            return false;
        }
    }

    /**
     * Validation via QAS (Call Ajax)
     * @param  {[object json]} datatovalidate [description]
     * @param  {[formulaire]} form [description]
     * @param  {[function]} callbackAction Obligation de passer ce param pour relancer les onclick inline catché précédemment
     * @return {[type]}       [description]
     */
    function sendRequestQAS(datatovalidate, form, callbackAction) {
        var result = null;
        var url = '//www.alinea.fr/qas/process/validateData/';

        jQuery.ajax({
            url: url,
            method: "POST",
            async: true,
            data: { datatovalidate: datatovalidate },
            success: function (data) {
                hideWaitPanel();

                // récupération réponse QAS ou réponse vérification locale
                result = JSON.parse(data);

                // si erreur QAS retournées
                if (isDataValidatedByQAS(result) === false) {
                    showErrorPanel(result);
                } else {
                    if (typeof callbackAction != 'undefined' && callbackAction != null) {
                        callbackAction();
                    }

                    var btn = form.find('*[data-qas="submit"]');
                    btn.unbind('click').click();
                    btn.bind('click', function () {
                        launchValidationQAS(btn, null, callbackAction);
                    });
                    //form.submit();
                }
            }
        });
    }

    /**
     * Vérification des données du formulaire renseigné
     * @param  {[type]} form [description]
     * @return {[type]} result_email [description]
     */
    function prepareRequestQAS(form, callbackAction) {
        /**
         * Récupérer les champs emails
         */
        var email_fields = form.find('*[data-qas="email"]');
        var aDataEmails = {};

        jQuery.each(email_fields, function (key, value) {
            if (jQuery(this).is(":visible")) {
                var id = jQuery(this).attr('id');
                var oEmail = {
                    'email': jQuery(this).val()
                };
                aDataEmails[id] = oEmail;
            }
        });

        /**
         * Récupérer les champs téléphones
         */
        var phone_fields = form.find('*[data-qas="landline"],*[data-qas="mobile"]');
        var aDataPhones = {};

        jQuery.each(phone_fields, function (key, value) {
            if (jQuery(this).is(":visible")) {
                var id = jQuery(this).attr('id');
                var oPhone = {
                    'number': jQuery(this).val(),
                    'countrycode': form.find('*[data-qas="country"] option:selected').val(),
                    'type': jQuery(this).data('qas')
                };

                if (jQuery(this).val() != '') {
                    aDataPhones[id] = oPhone;
                }
            }
        });

        /**
         * Récupérer les champs adresses
         */
        var aDataAddresses = {};
        var oAddress = {
            'firstname': form.find('*[data-qas="firstname"]').val(),
            'lastname': form.find('*[data-qas="lastname"]').val(),
            'street1': form.find('*[data-qas="street1"]').val(),
            'street2': form.find('*[data-qas="street2"]').val(),
            'postcode': form.find('*[data-qas="postcode"]').val(),
            'city': form.find('*[data-qas="city"]').val(),
            'countrycode': form.find('*[data-qas="country"] option:selected').val(),
            'reliability': form.find('*[data-qas="reliability"]').val()
        };

        if (typeof oAddress.street1 != 'undefined' && oAddress.street1 != '') {
            aDataAddresses['address_id1'] = oAddress;
        }

        // Fusionner toutes les datas à envoyer au ProcessController
        var dataToInsert = {"emails": aDataEmails, "phones": aDataPhones, "addresses": aDataAddresses};

        sendRequestQAS(JSON.stringify(dataToInsert), form, callbackAction); // -> call Ajax
    }

    /**
     * Détecter si champ non valide dans la réponse QAS
     * @param  {[object]}  result [Json retourné par QAS]
     * @return {Boolean}        [true si erreur, false si pas d'erreur]
     */
    function isDataValidatedByQAS(result) {
        if (result.isValid == true) {
            return true;
        }

        return false;
    }

    /**
     * Show waiting popin
     * @return void
     */
    function showWaitPanel() {
        Element.show('popup_wait_qas');
        jQuery('body').addClass('hidemain');
        scrollToPopin('#popup_wait_qas');
    }

    /**
     * Hide waiting popin
     * @return void
     */
    function hideWaitPanel() {
        Element.hide('popup_wait_qas');
        jQuery('body').removeClass('hidemain');
    }

    /**
     * Show errors popin
     * @param  {[object]} result JSON QAS
     * @return void
     */
    function showErrorPanel(result) {
        makeContentPanel(result);
        Element.show('popup_err_qas');
        jQuery('body').addClass('hidemain');
        scrollToPopin('#popup_err_qas');
    }

    /**
     * Hide errors popin
     * @return void
     */
    function hideErrorPanel() {
        Element.hide('popup_err_qas');
        jQuery('body').removeClass('hidemain');
    }

    /**
     * Gérer l'affichage des erreurs QAS
     * @param  {[object]} result JSON QAS
     * @return void
     */
    function makeContentPanel(result) {
        var html_content = '<form id="other-emails">';
        html_content += "<div id='logerr_message'><h2>Certains champs n'ont pas pû être validés :</h2></div>";

        if (typeof result.phones != 'undefined') {
            html_content += displayPhoneResponse(result.phones);
        }

        if (typeof result.emails != 'undefined') {
            html_content += displayEmailResponse(result.emails);
        }

        if (typeof result.addresses != 'undefined') {
            html_content += displayAddressesResponse(result.addresses);
        }

        html_content += '</form>';

        jQuery('#err_qas_message').html(html_content);
        
        jQuery('.email_correction').change(function () {
            jQuery('#buttonValidateEmailCorrection').prop('disabled', false);
        });
    }

    /**
     * Display Phones Error QAS Response
     * @param  {[object]} result - JSON QAS
     * @return {[string]} Content html
     */
    function displayPhoneResponse(phones) {
        var phone_html_content = "";

        jQuery.each(phones, function (key, value) {
            if (typeof value.isValid != 'undefined' && value.isValid != true) {

                // Get phone type (mobile/landline)
                key_id = jq(key);
                phone_type = jQuery(key_id).data('qas');

                phone_html_content += "<dl class='dl-error-message'>";
                if (phone_type == 'mobile') {
                    phone_html_content += "<dt>Téléphone portable</dt>";
                } else {
                    phone_html_content += "<dt>Téléphone fixe</dt>";
                }
                phone_html_content += "<dd>" + value.message + "</dd>";
                phone_html_content += "</dl>";
            }
        });

        return phone_html_content;
    }

    /**
     * Display Mail Error QAS Response
     * @param  {[object]} result - JSON QAS
     * @return {[string]} Content html
     */
    function displayEmailResponse(emails) {
        var email_html_content = "";

        jQuery.each(emails, function (key, value) {
            jQuery('#buttonValidateEmailCorrection').hide();
            jQuery('#buttonValidateEmailCorrection').prop('disabled', true);
                
            if (typeof value.isValid != 'undefined' && value.isValid != true) {
                email_html_content += "<dl class='dl-error-message'>";
                email_html_content += "<dt>E-mail</dt>";
                email_html_content += "<dd>" + value.message + "</dd>";
                email_html_content += "</dl>";

                if (value.corrections != null && value.corrections.length > 0) {
                    jQuery('#buttonValidateEmailCorrection').show();

                    idEmailToCorrect = jq(key);
                    emailToCorrect = jQuery(idEmailToCorrect).val();

                    email_html_content += "<div><h2>Cependant nous avons trouvé des adresses se rapprochant de l'email : <span class='pink'>\"" + emailToCorrect + "\"</span>.</h2></div><br/>";
                    email_html_content += "<div><h2>Merci de sélectionner l'adresse mail correcte dans la liste ci-dessous : </h2></div>";
                    email_html_content += "<div id='other-emails'>";

                    jQuery.each(value.corrections, function (key2, value2) {
                        email_html_content += '<div class="other-emails-email"><input type="radio" name="' + key + '_correction" data-correctionid="' + key + '" id="' + key + '_' + key2 + '_correction" class="email_correction" value="' + value2 + '" /><label for="' + key + '_' + key2 + '_correction">' + value2 + '</label></div>';
                    });
                    email_html_content += "</div>";
                }
            }
        });



        return email_html_content;
    }

    /**
     * Display Addresses Error QAS Response
     * @param  {[object]} result - JSON QAS
     * @return {[string]} Content html
     */
    function displayAddressesResponse(addresses) {
        var address_html_content = "";

        jQuery.each(addresses, function (key, value) {
            if (typeof value.isValid != 'undefined' && value.isValid != true) {
                address_html_content = "<dl class='dl-error-message'>";
                address_html_content += "<dt>Adresse</dt>";
                address_html_content += "<dd>" + value.message + "</dd>";
                address_html_content += "</dl>";
            }
        });

        return address_html_content;
    }

    /**
     *  Scrolle jusque la popin
     * @param {string} popinName Nom de la popin
     * 
     */
    function scrollToPopin(popinName)
    {
        jQuery('html, body').animate({
            scrollTop: jQuery(popinName).offset().top - 100
        }, 300);
    }

    /**
     * Format id for jquery selector
     * cf. http://learn.jquery.com/using-jquery-core/faq/how-do-i-select-an-element-by-an-id-that-has-characters-used-in-css-notation/
     * @param  {string} myid ID
     * @return {string}      formatted id
     */
    function jq(myid) 
    {
        return "#" + myid.replace( /(:|\.|\[|\]|,)/g, "\\$1" );
    }

