	function _calendar_WAR_nvicalendarportlet_addItemToArray(spanid, message){
		arr.push([spanid, message]);
	}

	function _calendar_WAR_nvicalendarportlet_getArrayValue(){
		for	(index = 0; index < arr.length; index++) {
			_calendar_WAR_nvicalendarportlet_addPopoverTooltip(arr[index][0], arr[index][1]);
			_calendar_WAR_nvicalendarportlet_addEventsAccessibilityText(arr[index][1]);
		} 
		
		if(arr.length < 1){
			_calendar_WAR_nvicalendarportlet_addNoEventsAccessibilityText();
		}
	}


	function _calendar_WAR_nvicalendarportlet_hideAllPopover(){
	  	for	(index = 0; index < arrPoppover.length; index++) {
			arrPoppover[index].set('visible',false);
		}
	}
	
	function _calendar_WAR_nvicalendarportlet_clearAllPopover(){
	  	for	(index = 0; index < arrPoppover.length; index++) {
			arrPoppover[index] = null;
		}
	  	arr = [];
	  	arrPoppover=[];
	}
	
	
	function _calendar_WAR_nvicalendarportlet_addPopoverTooltip(spanid, events){
		AUI().use(
				  'aui-popover',
				  function(A) {
	  
					var itemCount = 0;
				    var trigger = A.one('#'+spanid);
				    var message = '<div><span class="nvi-calendar-tooltipClose" style="position: absolute; top: 0px; right: 2px; line-height: 0;">' + 
									'<i class="icon icon-remove"></i>' + 
									'</span></div>';
				    message += "<div><span class='nvi-calendar-tooltipDate'>"+ events.eventDate + "</span></div>";
				    message += "<div class='nvi-scrollbar'>";
				    message += "<div class='scrollbar'>";
				    message += "	<div class='track'>";
				    message += "		<div class='thumb'>";
				    message += "			<div class='end'>";
				    message += "			</div>";
				    message += "		</div>";
				    message += "	</div>";
				    message += "</div>";
				    message += "<div class='viewport'>";
				    message += "	<div class='overview'>";

					A.Array.each(events.eventDetails, function(event, idx){
						message += '<div class="nvi-calendar-tooltipWrapper">'
							message += "<div><span class='nvi-calendar-tooltipLocation'>"+ event.location + "</span></div>";
							message += "<div><span class='nvi-calendar-tooltipBody'>"+ event.eventTooltip + "</span></div>";
							message += "<a href=\'" +  event.eventUrl;
							message += "\'><span class=\'nvi-calendar-tooltipBody\'>";
							message += '\u0054\u006f\u0076\u00e1\u0062\u0062\u0069\u0020\u0069\u006e\u0066\u006f\u0072\u006d\u00e1\u0063\u0069\u00f3</a>';
						message += '</div>';
						
						itemCount++;
					});
					message += '</div>';
					message += '</div>';
					message += '</div>';
  
				    var popover = new A.Popover(
				    	{
				        align: {
				          node: trigger,
				          points:[A.WidgetPositionAlign.LC, A.WidgetPositionAlign.RC]
				        },
				        bodyContent: message,
				        //headerContent: 'Header content',
				        position: 'right',
				        visible: true,
				        zIndex: 20,
				      }
				    ).render();
				    
				    
				    resizePopover(popover);
				    
				    popover.set('visible',false);
				    arrPoppover.push(popover);
				    
				    

					if (window.innerWidth < MOBILE_BREAK_POINT) {
						
						
	 					trigger.on(
	 						      'click',
	 						      function() {

	 						    	layerWrapper = A.one('#_calendar_WAR_nvicalendarportlet_layer');
	 						    	layerWrapper.show();
	 						    	
	 						   		layerWrapperContent = A.one('#_calendar_WAR_nvicalendarportlet_layer > div.popover-content');
	 						   	
	 						   		layerCloseBtn = A.one('#_calendar_WAR_nvicalendarportlet_layer > div > span.nvi-calendar-tooltipClose');
	 						   		layerCloseBtn.on(
	 						   		  'click',
	 						   		  function() {
	 						   			  layerWrapper.hide();
	 						   		  }
	 						   		); 						    	  
	 						    	  
	 						    	var eventDate = A.one('#'+popover.get('id') + ' > div > div > div:nth-child(2)');
	 						    	var events = A.one('#'+popover.get('id') + ' > div .viewport > div.overview');
	 						    	var layerHtml = eventDate._node.innerHTML + events._node.innerHTML;
	 						    	layerWrapperContent.setHTML(layerHtml);
	 						        
	 						      }
	 						    );
						
						
					}else{
						

	 					trigger.on(
					      'mouseover',
					      function() {
					    	  
					    	 for (index = 0; index < arrPoppover.length; index++) {
	 							arrPoppover[index].set('visible',false);
	 						  }
				  				    	  
					    	  popover.set('visible',true);
					    	  resizePopover(popover);
					        
					      }
					    );
	 					
	 					var closeBtn = A.one('#'+popover.get('id') + ' > div > div > div > span.nvi-calendar-tooltipClose');
	 					closeBtn.on(
					      'click',
					      function() {
					    	  popover.set('visible',false);
					        
					      }
					    );
					}
				    
 					
 					
				    function resizePopover(popover){
				    	  var viewPortElement = A.one('#'+popover.get('id') + ' > div .viewport > div.overview');
				    	  var viewPortHeight = viewPortElement.getStyle('height');

				    	  viewPortHeight =  viewPortHeight.substring(0, viewPortHeight.length -2);
				    	  if (viewPortHeight > 220) {
				    		  viewPortHeight = '220px';
				    	  }
				    	  
				    	  var viewportNode = A.one('#'+popover.get('id') + ' > div .nvi-scrollbar > div.viewport');
					    	var scrollbarNode = A.one('#'+popover.get('id') + ' > div .nvi-scrollbar');

		            		if (scrollbarNode) {
		            			scrollbar = tinyscrollbar(scrollbarNode.getDOMNode());
		            			scrollbarViewport = scrollbarNode.one('.viewport');

			            		viewportNode.setStyle('height', viewPortHeight);
			            		scrollbarViewport.setStyle('height', viewPortHeight);
			            		scrollbarViewport.setStyle('width', '220');
			            		scrollbar.update();
		            		}
		            		popover.set('visible',false);
		            		scrollbar.update();
		            		popover.set('visible',true);
		            		scrollbar.update();
				    } 					
 					
 					
				  }
				);
	}
	
	
	function _calendar_WAR_nvicalendarportlet_addEventsAccessibilityText(events) {
		
		var message = "<span>" + events.eventDate + " , " + events.eventDetails.length + "\u0065\u0073\u0065\u006d\u00e9\u006e\u0079</span>";
		
		var A = AUI();
		
		A.Array.each(events.eventDetails, function(event, idx){
			
			message += "<span>" + event.eventTooltip + ", \u0068\u0065\u006c\u0079\u0073\u007a\u00ed\u006e: " + event.location + "</span>";
			
			if(event.eventUrl){
				
				message += "<a href='" + event.eventUrl + "'>\u0054\u006f\u0076\u00e1\u0062\u0062\u0069\u0020\u0069\u006e\u0066\u006f\u0072\u006d\u00e1\u0063\u0069\u00f3</a>";
			}
		});
		
		A.one('#_calendar_WAR_nvicalendarportlet_accessibilityEventDetails').append(message);
	}
	
	function _calendar_WAR_nvicalendarportlet_addMonthAccessibilityText(year, month) {
		
		var message = "<span>" + year  + " " + month + "</span>";
		
		var A = AUI();
		
		A.one('#_calendar_WAR_nvicalendarportlet_accessibilityMonth').setHTML(message);
		A.one('#_calendar_WAR_nvicalendarportlet_accessibilityEventDetails').setHTML('');
	}
	
	function _calendar_WAR_nvicalendarportlet_addMonthStepperAccessibilityButtons(year, month, monthName) {
		
		var nextMonthMessage = "javascript:_calendar_WAR_nvicalendarportlet_setNextMonth(" + year + "," + month + ",1)";
		var previousMonthMessage = "javascript:_calendar_WAR_nvicalendarportlet_setPrevMonth(" + year + "," + month + ",1)";
		
