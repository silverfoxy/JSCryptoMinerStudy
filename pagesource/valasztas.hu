	function _calendar_WAR_nvicalendarportlet_setNextMonth(year, month, day){
		var selectedDate = new Date(year, month, day);
		var selectedDateNextMonth = new Date( new Date(selectedDate).setMonth(selectedDate.getMonth()+1));

		_calendar_WAR_nvicalendarportlet_hideAllPopover();
		_calendar_WAR_nvicalendarportlet_clearAllPopover();
		_calendar_WAR_nvicalendarportlet_getCalendarData(selectedDateNextMonth.getFullYear() + '.' + (selectedDateNextMonth.getMonth() + 1) + '.' + selectedDateNextMonth.getDate());
	}

	function _calendar_WAR_nvicalendarportlet_setPrevMonth(year, month, day){
		var selectedDate = new Date(year, month, day);
		var selectedDatePrevMonth = new Date( new Date(selectedDate).setMonth(selectedDate.getMonth()-1));
		
		_calendar_WAR_nvicalendarportlet_hideAllPopover();
		_calendar_WAR_nvicalendarportlet_clearAllPopover();
		_calendar_WAR_nvicalendarportlet_getCalendarData(selectedDatePrevMonth.getFullYear() + '.' + (selectedDatePrevMonth.getMonth() + 1) + '.' + selectedDatePrevMonth.getDate());
	}
	
	function _calendar_WAR_nvicalendarportlet_getCalendarData(calendarDate)
	{

		var divId='calendar';
		
		AUI().use('aui-base','aui-io-request', function(A){
			A.io.request('http://www.valasztas.hu/home?p_p_id=calendar_WAR_nvicalendarportlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=resourceIdGetCalendar&p_p_cacheability=cacheLevelPage&p_p_col_id=column-1&p_p_col_pos=2&p_p_col_count=4', {
				method: 'GET',
				dataType: 'json',
				data: { _calendar_WAR_nvicalendarportlet_calendarDate: calendarDate},
				  on: {
				   success: function() {
					   var data=this.get('responseData');
					   
						if(!data.error){
						   A.one('#_calendar_WAR_nvicalendarportlet_'+divId).empty();
	
						   var controlPrev = "<i class='icon icon-chevron-left'></i>";
						   var controlNext = "<i class='icon icon-chevron-right'></i>";
						   var beginRowMonthHeader="<div class='nvi-calendar-header-month'>";
						   var beginRowDaysHeader="<div class='nvi-calendar-header-days'>";
						   var endRowHeader="</div>";
						   var endRowDayHeader="</div>";
						   var beginCellHeader = "<span class='nvi-calendar-day'>";
						   var endCellHeader = "</span>";
						   var beginCellDayHeader = "<span class='nvi-calendar-day'>";
						   var endCellDayHeader = "</span>";
						   
		        		   var calendarYear=data.year;
		        		   var calendarMonth=data.month;
		        		   var calendarMonthName=data.monthName;
		        		   		        		   
		        		   var onclickPrevMonth = 'javascript:_calendar_WAR_nvicalendarportlet_setPrevMonth(' + calendarYear + ',' + calendarMonth + ',1)';
		        		   var beginCellHeaderPrevMonthClick = "<span class='nvi-calendar-control-prev' onclick='" + onclickPrevMonth + "'>";
		        		   
		        		   var onclickNextMonth = 'javascript:_calendar_WAR_nvicalendarportlet_setNextMonth(' + calendarYear + ',' + calendarMonth + ',1)';
		        		   var beginCellHeaderNextMonthClick = "<span class='nvi-calendar-control-next' onclick='" + onclickNextMonth + "'>";
		        		   
		        		   
		        		   // header Year + Month
		        		   var headerMonth = beginRowMonthHeader + beginCellHeaderPrevMonthClick + controlPrev + endCellHeader +  beginCellHeader +  calendarYear + "&nbsp;" + calendarMonthName + endCellHeader + beginCellHeaderNextMonthClick + controlNext + endCellHeader + endRowHeader;   
		        		   A.one('#_calendar_WAR_nvicalendarportlet_'+divId).append(headerMonth);
		        		   		        		   
		        		   // header Day
		        		   var cumulateCellDayHeader = '';
		        		   A.Array.each(data.dayAbbrevs, function(obj, idx){
		        			   cumulateCellDayHeader += beginCellDayHeader + obj.dayAbbrev + endCellDayHeader;
		        		   });
							var headerDay = beginRowDaysHeader + cumulateCellDayHeader + endRowDayHeader;
							A.one('#_calendar_WAR_nvicalendarportlet_'+divId).append(headerDay);
		        		   
						   var beginRow="<div class='nvi-calendar-week'>";
						   var endCell = "</span>";
		        		   var endRow="</div>";
		        		   var row = '';
		        		   
						   A.Array.each(data.days, function(obj, idx){
						   	
			        		   var eventCount = obj.eventCount;
			        		   var events = obj.events;
			        		   
							   var cellStyle = "nvi-calendar-day";

							   if(obj.dayInMonth){
								   cellStyle = "nvi-calendar-day current-month";	   
							   }
							   
							   var year = obj.year;
							   
							   var month = new String(obj.month); 
							   var month2 = month.length == 1 ? "0" + month : month;
							   
							   var day = new String(obj.day);
							   var day2 = day.length == 1 ? "0" + day : day;
	
							   var dayInMonth = obj.dayInMonth;
							   var isBeginRow = obj.isBeginRow;
							   var isEndRow = obj.isEndRow;
							   var onmouseover ="";
							   var spanid = "";
							   var spanidEvent = "";
							   var spanEventCount = "";
							   
							   if (eventCount >0){
							   		cellStyle = 'nvi-calendar-day event-day';
									spanid="_calendar_WAR_nvicalendarportlet_tooltips" + String(year) + String(month2) + String(day2);	
									spanidEvent="id=\'" + spanid + "\'";
									
									// add event count span
									spanEventCount = "<span class=\'nvi-calendar-day event-day-count\'>" + eventCount + "</span>";
							   }else{
								   onmouseover="onmouseover=\'_calendar_WAR_nvicalendarportlet_hideAllPopover();\'";
							   }
							   
							   if (obj.isToday){
								   cellStyle += " current-day";	
							   }
							   
							   var beginCell = "<span " + spanidEvent + " " + onmouseover + "  class=\'" + cellStyle + "\'\>";
							   
							   var newRowStart = "";
							   if (isBeginRow){
								   row = "";
							   } 
							   
							   row= row + beginCell + day + spanEventCount + endCell; 
							   
							   var newRowEnd = "";

							   //add item to temp array
							   //after render item will be get form temp array add popover event wil be added
							   if (eventCount > 0){
								   _calendar_WAR_nvicalendarportlet_addItemToArray(spanid, obj.events);
							   }
							   
							   if (isEndRow){
								   A.one('#_calendar_WAR_nvicalendarportlet_'+divId).append(beginRow + row + endRow);
								   row = "";
							   } 						   
							   
						   });
						}
						
						//add accessibility text and buttons
						_calendar_WAR_nvicalendarportlet_addMonthAccessibilityText(data.year, data.monthName);
						_calendar_WAR_nvicalendarportlet_addMonthStepperAccessibilityButtons(data.year, data.month, data.monthName);
						
						//get item from temp array to add popover event 
						_calendar_WAR_nvicalendarportlet_getArrayValue();
						
						
				   }
				 }
			});
		});
		
	}
	



