<script type="text/javascript">

  	function menuUpdate() {
      if (jQuery(window).width() <= 768) {
        jQuery('#topmenu .dropdown').addClass('dropdown-disable').removeClass('dropdown');
        jQuery('#topmenu .dropdown-menu ').attr('data-style', jQuery('#topmenu .dropdown-menu ').attr('style')).removeAttr('style');
      } else {
        jQuery('#topmenu .dropdown-disable').addClass('dropdown').removeClass('dropdown-disable');
        jQuery('#topmenu .dropdown-menu ').attr('style', jQuery('#topmenu .dropdown-menu ').attr('data-style')).removeAttr('data-style');
      }
    }

    function menuFixed(){
      var z=jQuery(document.getElementById("main-nav")),
          c='has-scroll';
      var scrollHeight=(document.documentElement.scrollHeight-document.documentElement.offsetHeight);
      if((jQuery(window).scrollTop()>0)&&(jQuery(window).width()>768)){
        z.addClass(c);
      }
      else{
        z.removeClass(c);
      }
    }

    jQuery(document).ready(function(){
		menuFixed();
      	menuUpdate();
      	jQuery(window).resize(function() {
          menuUpdate();
        });

        jQuery(window).scroll(function(){
           menuFixed();
        });
    });
	jQuery(function () {
