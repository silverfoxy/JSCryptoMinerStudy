		<script>
	  function change_p_year(c_year)
	  {
             var zdate= document.getElementById('current_year').value;
             //alert(zdate );
              document.getElementById("show_calender").innerHTML='loading...';
             if(c_year <= zdate)
             {
              var xmlhttp;
              if (window.XMLHttpRequest)
              {
              xmlhttp=new XMLHttpRequest();
              }
              else
              {
              xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
              }
              xmlhttp.onreadystatechange=function()
              {
              if (xmlhttp.readyState==4 && xmlhttp.status==200)
              {
              document.getElementById("show_calender").innerHTML=xmlhttp.responseText;
              }
              }
            xmlhttp.open('GET','ajax_calender_show.php?c_year='+c_year+'&rand='+Math.random(),true);
            xmlhttp.send();
            }
            else
            {
            change_p_year(zdate);
            }
       }
	</script>
 	       	
