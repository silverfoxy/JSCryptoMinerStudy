					jq172('.go-top').fadeIn(200);
				} else {
					jq172('.go-top').fadeOut(200);
				}
			});

			// Animate the scroll to top
			jq172('.go-top').click(function(event) {
				event.preventDefault();

				jq172('html, body').animate({
					scrollTop : 0
				}, 300);
			})

		});
	</script>


		
