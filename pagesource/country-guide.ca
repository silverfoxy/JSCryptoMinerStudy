<script>
        var FBC = FBC || {};
        FBC.ajaxurl = "https://www.country-guide.ca/wp-admin/admin-ajax.php";
        FBC.browser_width = window.innerWidth || window.document.documentElement.clientWidth;
        var FBC_MBC = FBC_MBC || FBC; //backwards compat
        </script>
<script>
                jQuery(document).ready(function($) {
                    var auto_filler = new Auto_Filler();

                    var is_linked = '';
                    var is_questionnaire_updated = '0';
                    var is_logged_in = '';
                    var spinner = '<div class="lr_fade"><div style="margin-left: 40%;width: 338px;height: 338px;text-align: center;margin-top: 12%;font-size: 4em;color: #fff;"><div class="lr-ur-spinner"><div class="spinner-frame"><div class="spinner-cover"></div><div class="spinner-bar"></div></div></div></div></div>';

                    function redirect( action, form ) {
                        $.ajax({
                            url: 'https://www.country-guide.ca/wp-admin/admin-ajax.php',
                            type: 'POST',
                            data: {
                                action: 'fm_popup',
                                form_action: action,
                                form: form
                            }
                        })
                            .done(function(data) {
                                $('html, body').animate({scrollTop : 0},800);
                                $('.fm-popup-container').html(data);
                                $('.fm-popup-frame, .fm-popup-overlay').show();
                                auto_filler.fill_auto_complete();
                                hideShowElement('.fm-survey-active-land','.fm-survey-hidden');
                            });
                    }

                    function save_survey( form,  business_type) {
                        $.ajax({
                            url: 'https://www.country-guide.ca/wp-admin/admin-ajax.php',
                            type: 'POST',
                            data: {
                                action: 'fm_save_survey',
                                form: form
                            },
                        })
                            .done(function(data) {
                                $('.lr_fade').hide();
                                $('.fm-popup-overlay').hide();
                                $('.fm-popup-frame').hide();
                                if (window.location.pathname == "/settings/follows/" || window.location.pathname == "/settings/subprint/") {
                                    if (window.location.pathname == "/settings/follows/") {
                                        if (typeof business_type != "undefined" && (business_type === "active" || business_type === "manage" || business_type === "rent")) {
                                                window.location.reload();
                                        } else {
                                            $('.lrprint-only-survey').hide();
                                        }
                                    } else {
                                        window.location.reload();
                                    }
                                }
                            });
                    }

                    $( '[data-action="restricted"]' ).click( function(e){
                        e.preventDefault();
                        //console.log( $(this).data('url') );

                        //Check if user is logged in
                        if( '1' == is_logged_in ) {
                            if( '1' == is_linked ) {

                                if( '1' == is_questionnaire_updated ) {
                                    //Redirect user to restricted url
                                    window.location = $(this).data('url');
                                } else {
                                    // Show questionnaire
                                    redirect('08');
                                }

                            } else {
                                if( '1' == is_questionnaire_updated ) {
                                    //Redirect user to restricted url
                                    window.location = $(this).data('url');
                                } else {
                                    // Show link user form
                                    redirect('01');
                                }
                            }
                        } else {
                            //Open Login Popup Form
                            showPopupForm('login');
                            show_cookie_disabled_message();

                        }
                    });


                    (function(){
                        $('#fm-subscriptions-none').change( function( event ) {
                            enableDisableSubscriptions();
                        });

                        $(document).on( 'change', '.fm-survey-active-land', function( event ) {
                            $(".survey_response-error").html('');
                            $(".survey_response-error").removeClass("error lr-error-message");
                            $('.fm-survey-other-des').val('');
                            $('#crop_error').html('');
                            $(':input[type="submit"]').prop('disabled', false);
                            if( $(".fm-survey-active-land").is(':checked') ) {
                                 $(".fm-survey-hidden").show();
                                 $('.fm-survey-other-des').hide();
                            }
                        });
                        $(document).on( 'change', '.fm-survey-not-active-land', function( event ) {
                            $(".survey_response-error").html('');
                            $(".survey_response-error").removeClass("error lr-error-message");
                            $('.fm-survey-other-des').val('');
                            $('#crop_error').html('');
                            $(':input[type="submit"]').prop('disabled', false);
                            if( $(".fm-survey-not-active-land").is(':checked') ) {
                                $(".fm-survey-hidden").hide();
                                 if ($(this).val() == 'Other, Nec' ) {
                                    $('.fm-survey-other-des').show();
                                 } else {
                                    $('.fm-survey-other-des').hide(); 
                                 }
                                $(".fm-survey-hidden").find("input").each(function() {
                                      $(this).val('0');
                                });
                            }
                        });
                        // $( ".fm-survey-other-des" ).focus(function() {
                        //   $(':input[type="submit"]').prop('disabled', false);
                        //   $(".survey_response-error").removeClass("error lr-error-message");
                        //   $(".survey_response-error").html('');
                        // });
                        enableDisableSubscriptions();
                        hideShowElement('.fm-survey-active-land','.fm-survey-hidden');

                        var selected_action = '01'; //default is the first page
                        $(".fm-popup input[type='submit']").live('click', function(){
                            auto_filler.click_event($(this));
                        });

                        $(document).live('submit', '.fm-popup form', function( e ) {
                            if(typeof auto_filler.selected_action != 'undefined'){
                                e.preventDefault();

                                $("#crop_error").remove();
                                var validate = 0;
                                var business_type = $('.fm-popup form').find("input[name=business_type]:checked").val();
                                if (business_type === "active" || business_type === "manage" || business_type === "rent") {
                                   var sum_acres = 0;
                                   var total_acres = 0;
                                   var exclude = ["beef_cattle","dairy_cattle","cow_calf","feedlot","hogs","sheep_or_lambs","poultry","other_livestock"];
                                   $(".fm-survey-hidden").find("input[type=number]").each(function() {
                                        if (this.name === "farm_size") total_acres = parseInt($(this).val());
                                        else if (exclude.indexOf(this.name) ===-1) sum_acres = sum_acres + parseInt($(this).val());
                                   });
                                   if (sum_acres > total_acres) validate = 1;
                                } else if (business_type == 'Other, Nec' && $('.fm-survey-other-des').val() == '') {
                                   validate = 2;
                                }
                                if (validate === 0) {
                                    var data = $('.fm-popup form').serialize();
                                    // Handle survey response differently
                                    if( 'survey_response' == auto_filler.selected_action ) {
                                        $('.lr_fade').remove();
                                        $('.lr_fade').show();
                                        save_survey( data,  business_type);
                                    } else {
                                        auto_filler.save_auto_complete(data);
                                        redirect( auto_filler.selected_action, auto_filler.current_ajax_data );
                                    }
                                } else if (validate === 1) {
                                    var error_template = "<p id='crop_error' style='padding-top: 20px;color: red;margin-bottom: 0px;'>The sum of the Crop Acres must be less than or equal to the Total Acres</p>";
                                    $( error_template ).insertBefore($("label.lr-input-label"));
                                } else {
                                    jQuery(".survey_response-error").addClass("error lr-error-message").html('Please provide a value for Other');
                                }
                            }

                        });

                        $(document).on('click','.skip-to-survey',function(e) {
                            redirect('02', 'subscriptions%5Bnone%5D=none');
                        });

                    })($);



                });



            </script>
