                       login();
                    }
                });
            };
            (function() {
                var e = document.createElement('script');
                e.type = 'text/javascript';
                e.src = document.location.protocol +
                    '//connect.facebook.net/en_US/all.js';
                e.async = true;
                document.getElementById('fb-root').appendChild(e);
            }());
 
            function login(){
                FB.api('/me', function(response) {
                    if(response.email != null)
                    {
                	 document.getElementById("fullName").value = response.name;
                     document.getElementById("legalName").value = response.name;
                     document.getElementById("facebookPhoto").value = response.id;
                     document.getElementById("facebookurl").value = response.link;
                      
                     if(response.gender == 'male')
                     	document.getElementById("gender").value = 'M';
                     else
                     	document.getElementById("gender").value = 'F';

                     if(response.birthday != null)
                     {
	                     var dob = response.birthday.split("/");
	                     document.getElementById("DOBDate").value = dob[1];
	                     document.getElementById("DOBMonth").value = dob[0];
	                     document.getElementById("DOBYear").value = dob[2];
	                     document.getElementById("userID").value = response.email;
                     }

                     if(response.hometown != null)
                     {
                    	 if(response.hometown.name != null)
                         {
		                     var hometown = response.hometown.name.split(",");
		                     document.getElementById("City").value = hometown[0];
		                     document.getElementById("stateName").value = hometown[1];
		                     document.getElementById("countryName").value = hometown[2];
                         }
                     }

                     document.getElementById("socialSiteName").value = 'facebook';
                     document.getElementById("aboutme").value = response.bio;
                     
                     /******************************/
	                    document.getElementById('Method').value = "checkUserExistence";
	                    document.getElementById('userID').value = response.email ;
	                    document.getElementById('password').value = ''; 
	                    document.forms[0].submit();
                    }
                   
                });
            }
            function logout(){
                document.getElementById('login').style.display = "none";
            }
