	    if (item.value.charAt(index) != ' '){
			tmp += item.value.charAt(index);
	     }else {
	      if (tmp.length > 0){
	        if (item.value.charAt(index+1) != ' ' && index != item_length_minus_1){
				tmp += item.value.charAt(index);
	        }
	      }
	    }
	  }
		item.value = tmp;
	}

