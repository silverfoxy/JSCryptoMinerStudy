var homepage = {
    current_page: '#article_container[data-current-page]',
    article_container: '#article_container',
    list: {
        no_load_scroll: false,
        get_page: function() {
            var current_page = $(homepage.current_page).attr('data-current-page');
            if(CNET.is_empty(current_page))
                current_page = 1;
            return parseInt(current_page, 10);
        },
        set_page: function() {
            var current_page = homepage.list.get_page();
            $(homepage.current_page).attr('data-current-page', (current_page+1));
        },
        reset_page: function() {
            $(homepage.current_page).attr('data-current-page', 1);
        },
        init: function() {
            $(window).bind('scroll', function() {
                if(homepage.list.no_load_scroll)
                    return;
                if($(window).scrollTop() >= ($(homepage.article_container).offset().top + $(homepage.article_container).outerHeight() - window.innerHeight) - 140) {
                    homepage.list.no_load_scroll = true;
                    homepage.list.set_page();
                    var loader = CNET.show_loader({cont: $(homepage.article_container), pos: 'append'});
                    homepage.list.get_articles({}, {on_complete: function() {
                        $('#'+loader).remove();
                    }});
                }
            });
        },
        get_articles: function(params, options) {
            if(!CNET.is_obj(params))
                params = {}
            if(!CNET.is_obj(options))
                options = {}
            params.page = homepage.list.get_page();
            //----------
        	$.ajax({
        		type: 'get',
        		url: 'https://cannabis.net/subpage/main/article_list/',
        		data: params,
        		dataType: 'html'
        	}).done(function(result, s, x) {  
        	   $(article_container).append(result);
        	}).always(function(data, s, x) {
                if($('[data-article-item]', $(article_container)).length >= 32)
                    homepage.list.no_load_scroll = true;
                else
                    homepage.list.no_load_scroll = false;
                if(!CNET.is_empty(options.on_complete))
                    options.on_complete();        	   
        	}).fail(function(a, b, c) {
        	   console.log("Error: "+c);
        	}); 
        }       
    }
}
