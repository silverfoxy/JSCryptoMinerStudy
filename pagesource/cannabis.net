    $('#map-search-form').submit(function(event) {
        event.preventDefault();
        if(!$('#search-input').val()) {
            alert('To search, please enter zip code or city');
            return false;
        }
        $("#search-btn").children("span").replaceWith('<span class="fa fa-spinner faa-spin animated"></span>');
        $(".map-container").append('<div class="searching-map"></div>');
    	$.ajax({
    		type: 'post',
    		url: 'https://cannabis.net/subpage/map-v2/map-search',
    		data: $('#map-search-form').serialize(),
    		dataType: 'json',
            cache: false,
    		success: function(result){
                //console.log(result);
                if(result.list != undefined) {
                    var res_loader = CNET.show_loader({cont: $('#search-result-items')});
                    $("#search-result-items").css({'opacity':'0.4'});
                    var companies = []
                    var lats = []
                    var lngs = []
                    var index_ct = result.list.length;
                    map.removeMarkers();
                    $.each(result.list, function(index, value){
                        companies.push(value.id);
                        
                        if(value.address_lat == undefined || value.address_lat == '')
                            return true;
                        if(value.address_lon == undefined || value.address_lon == '')
                            return true;
                        if(jQuery.inArray(value.address_lat, lats) !== -1 && jQuery.inArray(value.address_lon, lngs) !== -1)
                            return true;
                        if(typeof value === "object") {
                            //console.log(value);
                            var marker_size = 76;
                            if(value.membership_type_id == 1)
                                marker_size = 42;
                            marker = map.addMarker({
                                lat: value.address_lat,
                                lng: value.address_lon,
                                title: JSON.stringify(value.name),
                                icon: {
                                    url: value.marker_url,
                                    scaledSize: {
                                        height: marker_size,
                                        width: marker_size                        
                                    }
                                },
                                zIndex: index_ct--,
                                infoWindow: {
                                    content: value.info_window,
                                    maxWidth: 200
                                }
                            });
                            if(index == 0) {
                                map.setCenter(
                                    parseFloat(value.address_lat), 
                                    parseFloat(value.address_lon)
                                );
                            }
                            lats.push(value.address_lat);
                            lngs.push(value.address_lon);
                        }
                    });
                    $('#map-search-form').attr('data-scroll-to-result', '1');
                    map.fitZoom();
                    var result_params = { id: companies }
                                        if(companies.length > 0) {
                    	$.ajax({
                    		type: 'post',
                    		url: 'https://cannabis.net/subpage/map_v2/results',
                    		data: result_params,
                    		dataType: 'html',
                    		success: function(result){
                                $("#search-result-items").replaceWith(result);
                            },
                            complete: function() {
                                $('#'+res_loader).remove();
                                $("#search-result-items").css({'opacity':'1'});
                            },
                            error: function(a, b, c) {
                                console.log("Error: "+c);
                            }
                    	});
                    } else {
                        $('#'+res_loader).remove();
                        $("#search-result-items").css({'opacity':'1'});
                    }
                } else {
                    $('#map-search-form').attr('data-scroll-to-result', '0');
                    $('#map_msg').html(map_msg.nores);                    
                    $("#noresults_map").show();
                    $("#noresults_map a").focus();
                    $("#noresults_map a").on("click", function(event){
                        event.preventDefault();
	                   $("#noresults_map").hide(); 
                    });
                }
    		},
            complete: function(r, s) {
                $(".searching-map").remove();
                $("#search-btn").children("span").replaceWith('<span class="fa fa-search"></span>');
                $("#search-results").show();
                if($('#map-search-form').attr('data-scroll-to-result') == '1') {
                    $("html, body").animate({ scrollTop: $('#search-results').offset().top }, 800);
                }
                setTimeout(function() {
                    $("[data-toggle='tooltip']").tooltip();
                }, 1000);
                $('#search-input').autocomplete("close");
                $('input[data-autocomplete]', $('#map-search-form')).remove();
            },
            error: function(a, b, c) {
                console.log("Error: "+c);
            }
    	});
    });
    $('input[name="search_filters[]"], #search-btn').click(function(event) {
        $('#map-search-form').trigger('submit');
    });
    
    $("#search-input").val('Ashburn, VA');	$("[data-toggle='tooltip']").tooltip();
    //-------------------------
    var company_types = []
    if($('input[name="company_type_id[]"]', $('#map-search-form'))) {
         $('input[name="company_type_id[]"]', $('#map-search-form')).each(function() {
            company_types.push($(this).val());
         });
    }
    $('#search-input').autocomplete({
        source: function(request, response) {
            $.ajax({
                url: "https://cannabis.net/process/map/search/",
                dataType: "json",
                data: {
                    term : request.term,
                    'search_type': $('#search_type', $('#map-search-form')).val(),
                    'company_type_id': company_types
                },
                success: function(data) {
                    response(data);
                }
            });
        },
        minLength: 1,
        delay: 1,
        select: function(event, ui) {
            for (var key in ui.item.result) {
                $('#'+key, $('#map-search-form')).remove();
                var field = $('<input />').attr({
                    'type': 'hidden',
                    'id': key,
                    'name': key,
                    'value': ui.item.result[key],
                    'data-autocomplete': 1
                });
                $('#map-search-form').append(field);
            }
            setTimeout(function() {
                $('#map-search-form').trigger('submit');
            }, 300);
        }
    });
    //-------------------------
});
