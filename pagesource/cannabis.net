    var owl_do = function(selector, opts) {
        var $owl = $(selector);
    	$owl.owlCarousel(opts);
    }
    var owl_kill = function(selector) {
        var $owl = $(selector);
    	$owl.trigger('destroy.owl.carousel');
    	$owl.html($owl.find('.owl-stage-outer').html()).removeClass('owl-loaded');
    }
    var owl_redo = function(selector, opts) {
        owl_do(selector, opts);
        setTimeout(function() {
            var $owl = $(selector);
    		$owl.trigger('destroy.owl.carousel');
    		$owl.html($owl.find('.owl-stage-outer').html()).removeClass('owl-loaded');
            setTimeout(function() {
                $owl.owlCarousel(opts);
            }, 1);
        }, 1);
    }
