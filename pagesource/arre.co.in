        jQuery('body').append(mainDiv).append(bgDiv);
        document.getElementById('the_champ_sharing_popup_close').onclick = function () {
            mainDiv.parentNode.removeChild(mainDiv);
            bgDiv.parentNode.removeChild(bgDiv);
        }
