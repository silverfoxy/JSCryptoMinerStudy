                locale: "en-US",
                redirect_url: "mailto:?subject=" + postTitle + "&body=Link: " + postUrl,
            }
        }
        var theChampMoreSharingServicesHtml = '<h3 class="title ui-drag-handle">Share this!</h3><button id="the_champ_sharing_popup_close" class="close-button separated"><img src="' + theChampCloseIconPath + '" /></button><div id="the_champ_sharing_more_content"><div class="all-services"><ul class="mini">';
        var arr = ['Facebook', 'Google plus', 'Twitter', 'Reddit', 'Email'];

        for (var i in theChampMoreSharingServices) {
            if (jQuery.inArray(theChampMoreSharingServices[i].title, arr) >= 0) {

                if (theChampMoreSharingServices[i].title == 'Facebook' || theChampMoreSharingServices[i].title == 'Twitter' || theChampMoreSharingServices[i].title == 'Reddit') {
                    theChampMoreSharingServicesHtml += '<li></li>';
                } else {
                    theChampMoreSharingServicesHtml += '<li><a ';
                    if (theChampMoreSharingServices[i].bookmarklet_url) {
                        theChampMoreSharingServicesHtml += 'href="' + theChampMoreSharingServices[i].bookmarklet_url + '" ';
                    } else {
                        theChampMoreSharingServicesHtml += 'onclick="theChampPopup(\'' + theChampMoreSharingServices[i].redirect_url + '\')" href="javascript:void(0)" ';
                    }
                    theChampMoreSharingServicesHtml += '"><i title="' + theChampMoreSharingServices[i].title + '" class="the_champ_sharing_service the_champ_sharing_service_' + theChampMoreSharingServices[i].title.toLowerCase().replace('_', '-') + '"></i>' + theChampMoreSharingServices[i].title + '</a></li>';
                }
            }
        }
        if (window.innerWidth <= 800 && window.innerHeight <= 600) {
            theChampMoreSharingServicesHtml += '<li><a href="whatsapp://send?text=ARRE' + postUrl + '"><i title="Whatsapp" class="icon icon__whats-appc" ></i>Whats App</a></li>';
