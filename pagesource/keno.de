    <script type="text/javascript">
      $(function () {
        var jetzt  = new Date();
        var tomorrow = new Date( jetzt.getTime() + (24 * 60 * 60 * 1000));
        if (  jetzt.getHours() < 19 ) {
          if( jetzt.getHours() == 19 && jetzt.getMinutes() < 10  ) {
            NextKenoZiehung = new Date(  jetzt.getFullYear(),  jetzt.getMonth(),    jetzt.getDate(),19,10,00);
          } else {
            NextKenoZiehung = new Date(  tomorrow.getFullYear(),  tomorrow.getMonth(),  tomorrow.getDate(),19,10,00); 
          }
        } else {
          NextKenoZiehung = new Date(  tomorrow.getFullYear(),  tomorrow.getMonth(),  tomorrow.getDate(),19,10,00);
        }
        $('.counter').countdown({until:   NextKenoZiehung, layout: '<div><p>{h10}{h1}<span> Stunden</span></p><div>&nbsp;</div></div><div><p>{m10}{m1}<span>Minuten</span></p><div>&nbsp;</div></div><div><p>{s10}{s1}<span>Sekunden</span></p><div>&nbsp;</div></div>' });
