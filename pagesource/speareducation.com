        var $uploadCrop;

        $('#upload').on('change', function () {
            if (readFile(this)) {
                $('button.upload-result').removeAttr('disabled');
            } else {
                $('button.upload-result').atrr('disabled', 'disabled');
            }
        });

        function readFile(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();

                reader.onload = function (e) {
                    $('.upload-demo').addClass('ready');
                    $uploadCrop.croppie('bind', {
                        url: e.target.result
                    }).then(function () {
                        console.log('jQuery bind complete');
                    });
                }

                reader.readAsDataURL(input.files[0]);

                return true;
            } else {
                if ($('#upload').val()) {
                    alert('Sorry, but your browser doesn\'t support the FileReader API');
                }

                return false;
            }
        }

        $uploadCrop = $('#upload-demo').croppie({
            viewport  : {
                width : 200,
                height: 200,
                type  : 'square'
            },
            boundary  : {
                height: 300
            },
            enableExif: true
        });

        $('.upload-result').on('click', function (e) {
            $uploadCrop.croppie('result', {
                type  : 'base64',
                size  : 'viewport',
                format: 'png'
            }).then(function (result) {
                $.post(
                    '/web-services/add-user-image',
                    {
                        user_id: 0,
                        img_src: result
                    }
                ).done(function (data) {
                    $('.user-avatar').attr('style', 'background:url(' + result + ');background-size: cover;-moz-background-size:cover;background-position:center;');
                    $('#avatar, .avatar, .avatar-thm img, .avatar img').attr('src', result);
                    $('#modal-logo').modal('hide');
                });
            });
        });
    });

