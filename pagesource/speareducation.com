                    alert(data.message);
                    return false;
                }
            });
        });

        showSelectedInputOnload('#selectAppt', '.appt1', 'Office Manager');
        showSelectedInputOnload('#selectAppt', '.appt2', 'Text');
        showInputElementAfterSelect('#selectCountry', '.selectTimeZone', 'United States');
        showInputElementAfterSelect('#selectAppt', '.appt1', 'Office Manager');
        showInputElementAfterSelect('#selectAppt', '.appt2', 'Text');
    });

    function showSelectedInputOnload(obj, objTarget, strValue) {
        if ($(obj).val() === strValue) {
            $(objTarget).show();
        } else {
            $(objTarget).hide();
        }
    }

    function showInputElementAfterSelect(obj, objTarget, strValue) {
        $(obj).on('change', function(){
            if ($(this).val() === strValue) {
                $(objTarget).show();
            } else {
                $(objTarget).hide();
            }
        });
    }

        //]]>
</script>
