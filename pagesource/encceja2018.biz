<script type="text/javascript">
	var $equal = jQuery.noConflict();

	$equal.fn.extend({
		equalHeights: function(){
			var top=0;
			var row=[];
			var classname=('equalHeights'+Math.random()).replace('.','');
			$equal(this).each(function(){
				var thistop=$equal(this).offset().top;
				if (thistop>top) {
					$equal('.'+classname).removeClass(classname);
					top=thistop;
				}
				$equal(this).addClass(classname);
				$equal(this).height('auto');
				var h=(Math.max.apply(null, $equal('.'+classname).map(function(){ return $equal(this).height(); }).get())); // height or outerHeight
				$equal('.'+classname).height(h);
			}).removeClass(classname);
		}
	});

	$equal(window).load(function(){
		// Popular
		$equal('.equalize article a').equalHeights();
	}).trigger('resize');
	$equal(window).resize(function(){
		// Popular
		$equal('.equalize article a').equalHeights();
	}).trigger('resize');
</script>
