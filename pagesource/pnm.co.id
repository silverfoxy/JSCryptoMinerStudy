    <script>$(document).ready(function(){$("#example-basic").steps({headerTag:"h3",bodyTag:"section",transitionEffect:"slideLeft",autoFocus:true,onFinishing:function(event,currentIndex){submitBusinessData();}});});function searchUlamm(){var province=$("#search_province").val();window.location='http://pnm.co.id/ulamm/cabang/?province='+province;}
function submitBusinessData(){$("#name_error").html("");$("#province_error").html("");$("#email_error").html("");$("#phone_error").html("");$("#business_error").html("");$.post("http://pnm.co.id/proc/insertdatabusiness/",$("#form_business_data").serialize(),function(data){var result=$.parseJSON(data);if(result.response_code==1){$("#name").val("");$("#province").val("");$("#phone").val("");$("#email").val("");$("#business").val("");alert("Data anda sudah kami terima. Terima kasih");window.location="http://pnm.co.id/ulamm/cabang/";}else{$("#name_error").html(result.message.name);$("#province_error").html(result.message.province);$("#email_error").html(result.message.email);$("#phone_error").html(result.message.phone);$("#business_error").html(result.message.business);$("#example-basic").steps('previous');}});}</script>

