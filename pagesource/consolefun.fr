<script type="text/javascript">
    function rgb2hex(rgb)
    {
        rgb = rgb.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/);
        return "#" +
        ("0" + parseInt(rgb[1],10).toString(16)).slice(-2) +
        ("0" + parseInt(rgb[2],10).toString(16)).slice(-2) +
        ("0" + parseInt(rgb[3],10).toString(16)).slice(-2);
    }

    // Anime le choix d'une news
    function animate(obj)
    {
        var news = $('.news', '.news_wrapper');
        var newsMenu = $('.bandeau_news_menu li').find('li');
        var newsMenuSelected = $('.bandeau_news_menu li li.selected');
        var categorieSelected = $('.bandeau_news_menu li.selected').eq(0);
        var categorie = obj.parents('li').eq(0);
        obj.unbind('click');
        newsMenuSelected.bind('click', function(){
            selectAnimate($(this));
        });

        // D√©sactiver la news d√©j√† selectionn√©e.
        news.eq(newsMenu.index(newsMenuSelected)).fadeOut(400);
        newsMenuSelected.removeClass('selected');

        var marginLeft = categorie.css('marginLeft');
        var paddingLeft = categorie.css('paddingLeft');
        var borderTopLeftRadius = categorie.css('borderTopLeftRadius');
        var borderBottomLeftRadius = categorie.css('borderBottomLeftRadius');

        // Si la news selectionn√©e n'est pas dans la m√™me cat√©gorie que la pr√©c√©dente news, on d√©selectionne la cat√©gorie
        // puis on selection la nouvelle cat√©gorie
        if(!categorieSelected.is(categorie))
        {
            categorieSelected.removeClass('selected');
            categorieSelected.animate(
                {marginLeft: marginLeft,
                paddingLeft: paddingLeft,
                borderTopLeftRadius: borderTopLeftRadius,
                borderBottomLeftRadius: borderBottomLeftRadius},
                400
            );

            categorie.animate(
                {marginLeft: marginLeftSelected,
                paddingLeft: paddingLeftSelected,
                borderTopLeftRadius: borderTopLeftRadiusSelected,
                borderBottomLeftRadius: borderBottomLeftRadiusSelected},
                400,
                function(){
                    categorie.addClass('selected');
                }
            );
        }

        // element avec classe selected
        obj.addClass('selected');
        news.eq(newsMenu.index(obj)).delay(300).fadeIn(400);
    }

    // Fonction ex√©cut√©e lors du choix manuel d'une news
    function selectAnimate(obj)
    {
        // Arret de l'animation automatique du bandeau
        clearTimeout(timeout);
        // On relance l'animation √† partir de la news selectionn√©e
        runAnimate(obj);
    }

    // Lance l'animation automatique du bandeau
    function runAnimate(obj)
    {
        var newsMenu = $('.bandeau_news_menu li').find('li');
        var objIndex = newsMenu.index(obj);
        if(objIndex + 1 == newsMenu.length)
        {
            var nextObj = newsMenu.eq(0);
        }
        else
        {
            var nextObj = newsMenu.eq(objIndex + 1);
        }
        animate(obj);
        timeout = setTimeout(function(){
            runAnimate(nextObj);
        }, 5000);
    }

    $(document).ready(function(){
        $('.bandeau_news_menu li li').bind('click', function(){
            selectAnimate($(this));
        });
        $('.news', '.news_wrapper').eq(0).fadeIn(800);

        // On r√©cup√®re ici en tant que global les infos css des cat√©gories doubles selectionn√©es, pour √™tre utilis√©es lors de l'animation de selection.
        // Cela ne marche que si la premi√®re cat√©gorie selectionn√©e au d√©part est double.
        var categorieSelected = $('.bandeau_news_menu li.news_double.selected').eq(0);
        marginLeftSelected = categorieSelected.css('marginLeft');
        paddingLeftSelected = categorieSelected.css('paddingLeft');
        borderTopLeftRadiusSelected = categorieSelected.css('borderTopLeftRadius');
        borderBottomLeftRadiusSelected = categorieSelected.css('borderBottomLeftRadius');

        runAnimate($('.bandeau_news_menu li li').eq(0));
        $('#bandeau_news_loading').css('display', 'none');
        $('#bandeau_news').css('display', 'block');
    });
</script>

