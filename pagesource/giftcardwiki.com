    <script language="JavaScript">
        function populateCompactCards(title, url, section) {
            $.getJSON("/api/v1/gift-cards/" + url + "/", function (data) {
                var cards = $("#" + section);
                cards.empty();
                if (data.length > 0) {
                    cards.append('<div class="row" style="margin-top: 10px; margin-bottom: 0px">' +
                        '<div class="col-md-1"></div>' +
                        '<div class="col-md-10">' +
                        '<h3><span class="label label-default">' + title + '</span></h3>' +
                        '</div>' +
                        '<div class="col-md-1"></div>' +
                        '</div>');
                }
                var content = '';
                for (var i = 0; i < data.length; ++i) {
                    var item = data[i];
                    if (i % 5 == 0) {
                        content += '<div class="row">';
                        content += '<div class="col-md-1"></div>';
                    }
                    content += getCardContentCompact(item);
                    if ((i + 1) % 5 == 0) {
                        content += '<div class="col-md-1"></div>';
                        content += '</div>';
                    }
                }
                if (data.length % 5 != 0) {
                    content += '</div>';
                }
                cards.append(content)
            });
        }

        populateCompactCards("Popular brands", "top-gift-cards", "topCards");
        populateCompactCards("Recently visited", "recently-visited", "recentlyVisited");
    </script>
