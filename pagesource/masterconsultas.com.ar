	<script>
		$(document).ready(function(){
			
			$('.legales').click(function () {
				if (!$(this).hasClass('animado')) {
					$(this).addClass('animado');
                    $('#cboxLoadedContent').css('overflow', 'hidden');
					$('.desplegable').animate({'height':200},{
						duration: 1000,
						step: function(){
							$.colorbox.resize();
						},
						complete: function() {
							$('.desplegable').animate({'opacity':1},200)
						}
					});
				}
			});

            loadBanner("principal");

            $.getJSON("data/accesos-index.json").done(function(data){
                var itemsHTML = '';
                $.each(data.top, function(n, novedad){
					if(novedad.link[0] != "@"){
                        novedad.blank = true;
                    } else {                        
						novedad.link = novedad.link.slice(1);
                    }

                    itemsHTML += '<div class="acces">' +
                                     '<h6' + (novedad.destacado === 0 ? '' : ' class="red"') + '>' + novedad.supra_titulo + '</h6>' +
                                     '<h1>' + novedad.title + '</h1>' +
                                     '<div class="img_acc"><img src="assets/' + novedad.image + '" width="158" height="73"></div>' +
                                     '<div class="txt_acces">' + novedad.text + '</div>' +
                                     '<a ' + ((novedad.blank)? 'target="blank"' : '') + ' href="' + novedad.link + '"><div class="vermas ' + (novedad.destacado === 0 ? '' : 'red') + '"></div></a>' +
                                 '</div>';
                });

                $('.cont_acces').html(itemsHTML);

                itemsHTML = '';
                $.each(data.bottom, function(n, novedad){
                    if(novedad.link[0] != "@"){
                        novedad.blank = true;
                    } else {
                        novedad.link = novedad.link.slice(1);
                    }

                    itemsHTML += '<div style="background-image:url(assets/' + novedad.image + ')" class="ban_ch uno_s">' +
                                     '<div class="frases_banner primero">' + novedad.title +  '<span class="frases_banner_dest"></span></div>' +
                                     '<div class="frases_banner_red primero">'+novedad.text+'</div>' +
                                     '<a ' + ((novedad.blank)? 'target="blank"' : '') + ' href="' + novedad.link + '"><div class="ingresar primero"></div></a>' +
                                 '</div>';
                });

                $('.cont_ban_ch').html(itemsHTML);
            });

          /*$('#head').load('header.html');*/



			
		});
	</script>
  
