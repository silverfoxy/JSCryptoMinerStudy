    <script>
    var timeout;
    
    function initHomepageNews() {
        hp_news = $(".whats-happening > li");
     
        hp_news.hover(function() {
            clearTimeout(timeout);
            hp_news.removeClass("selected");
            $(this).addClass("selected");
        });
    }

    var featuredTimer = function() {
        var next = $(".selected").removeClass("selected").next();
    
        if (next.length) {
            $(next).addClass("selected");
            timeout = setTimeout(featuredTimer, 3000);
        }
        else {
            $(".whats-happening li:first-child").addClass('selected');
            clearTimeout(timeout);
        }
    }
    
    function shareLinks() {
        $(document).on("click", function () {
            $(".share-links-popup").slideUp(100);
            sharelink.parent().removeClass("active");
        });        
        
        sharelink = $(".share-links li:not(:first-child) i");
        
        sharelink.click(function(event) {
            event.stopPropagation();
            
            if ( $(this).parent().hasClass("active") ) {
                sharelink.parent().removeClass("active");
                $(".share-links-popup").slideUp(100);
            }
            else {
                sharelink.parent().removeClass("active");
                $(".share-links-popup").slideUp(100);
                $(this).parent().addClass("active");
                $(this).parent().find(".share-links-popup").slideDown(250);
            }
        });
    }

    $(document).ready(function(){
        initHomepageNews();
        setTimeout(featuredTimer, 3000);
        shareLinks();
    });
</script>
