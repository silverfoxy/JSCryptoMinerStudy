    function reSizes(){
    	var vpw = $(window).width(),
           	bar = $('#gb-channel-list-outer').offset(),
            bar2 = $('#gb-global-icons-outer').offset(),
            bH = $('#gb-channel-list-outer').height(),
            bH2 = $('#gb-global-icons-outer').height();
        	
        $(window).on('scroll', function(){
        	var wS = $(window).scrollTop();
            if($('.hp #hp-content-upper').length > 0){
                if(wS >= bar.top && vpw >= 1024){
                    $('#gb-header-outer').css('margin-bottom', bH + 'px'); //-- Fills in the space of the Channel Bar
                    $('#gb-channel-list-outer').addClass('fixed');
                } else if(wS < bar.top && vpw >= 1024){
                    $('#gb-header-outer').css('margin-bottom', '0px'); 
                    $('#gb-channel-list-outer').removeClass('fixed');
                }
                if(wS >= (bar2.top - bH - 2) && vpw >= 1024){
                    $('#gb-global-icons-outer').css({
                        position : 'fixed',
                        top : bH + 'px',
                        width : '100%',
                        background : '#FFF',
                        'z-index' : '8900'
                    }).addClass('fixed');
                    if($('html.ff').size() != 0){
                        $('#hp-content-outer').css('padding-top', (bH + bH2 - 12) + 'px');
                    } else {
                    	if(bH >= 100){
                        	$('#hp-content-outer').css('padding-top', (bH + bH2 - 57) + 'px');
                        } else {
                        	$('#hp-content-outer').css('padding-top', (bH + bH2) + 'px');
                        }
                    }
                } else if(wS < (bar2.top - bH - 2) && vpw >= 1024) {
                    $('#gb-global-icons-outer').css({
                        position : 'relative',
                        top : '0px',
                        width : '100%',
                        'z-index' : '900'
                    }).removeClass('fixed');
                    $('#hp-content-outer').css('padding-top', '0px');
                    if($('.sp').size() > 0){
                        $('#sp-content-outer').css('padding-top', '0px');
                    } else if($('.spn').size() > 0){
                        $('#spn-content-outer').css('padding-top', '0px');
                    }
                }
             } else {
                if(wS >= bar.top && vpw >= 1024){
                    $('#gb-header-outer').css('margin-bottom', bH + 'px'); //-- Fills in the space of the Channel Bar
                    $('#gb-channel-list-outer').addClass('fixed');
                } else if(wS < bar.top && vpw >= 1024){
                    $('#gb-header-outer').css('margin-bottom', '0px'); 
                    $('#gb-channel-list-outer').removeClass('fixed');
                }
                
             }
        });
        if(vpw < 1024){
        	$('#hp-content-outer').prepend($('#hp-tabbed-apps'));
            $('.gb-global-icons .max-width, .gb-footer-btns').removeClass('cs-jc');
            $('#sw-content-container1').removeClass('cs-fw');
            if($('#gb-header-right #rs-menu-btn').size() == 0){
                $('#gb-header-right').append($('#rs-menu-btn'));
            }
            $('#gb-header-left').addClass('cs-jfs').removeClass('cs-jc');
            if(vpw < 768){
            	$('.gb-global-icons .max-width, .gb-footer-btns').removeClass('cs-jc');
            	if(vpw < 640){
                    $('.gb-social-media .max-width, .gb-footer-btns').addClass('cs-jc');
                	if($('#sw-mystart-right #rs-menu-btn').size() == 0){
                    	$('#sw-mystart-right').append($('#rs-menu-btn'));
                    }
                    if(vpw < 480){
                    	$('#gb-header-left').removeClass('cs-jfs').addClass('cs-jc');
                        $('#sw-content-container1').addClass('cs-fw');
                    }
                }
            }
        } else {
            $('.gb-global-icons .max-width, .gb-footer-btns').removeClass('cs-jc');
            $('#hp-slideshow-outer .max-width').append($('#hp-tabbed-apps'));
        }
    }
	
