	function calendarListView() {
		// AUTO CLICK FUNCTION FOR SCHOOLWIRES CALENDAR LIST VIEW FOR SMALL MOBILE DEVICES
		if(!$(".ui-widget.app.calendar #calendarlist-pnl-specialview.loaded").length){
			$("a.ui-btn-toggle.list").click();
			$("a.ui-btn-toggle.month").click();
			$(".ui-widget.app.calendar #calendarlist-pnl-specialview").addClass("loaded");
		}
		
		switch(csGetBreakPoint()) {
			case "desktop": case "768": case "640":
				$(".ui-widget.app.calendar #calendar").removeClass("ui-helper-hidden");
				$(".ui-widget.app.calendar #calendarlist-pnl-specialview").addClass("ui-helper-hidden");
			break;
			case "480": case "320":
				$(".ui-widget.app.calendar #calendar").addClass("ui-helper-hidden");
				$(".ui-widget.app.calendar #calendarlist-pnl-specialview").removeClass("ui-helper-hidden");
				$(".ui-btn-toggle.month").removeClass("active");
				$(".ui-btn-toggle.list").addClass("active");
			break;
		}
	}
	
	function csGetBreakPoint() {
		return window.getComputedStyle(document.querySelector("body"), ":before").getPropertyValue("content").replace(/"|'/g, "");/*"*/
	}
	
	function hexToRgb(h){
	        var r = parseInt((cutHex(h)).substring(0,2),16);
	        var g = parseInt((cutHex(h)).substring(2,4),16);
	        var b = parseInt((cutHex(h)).substring(4,6),16);
	        return r+', '+g+', '+b;
	};
	 
	function cutHex(h) {
	        return (h.charAt(0)=="#") ? h.substring(1,7):h;
	};
    
    
	function sortSchools(){
		var csTheSchools = [];
			$('.sw-mystart-dropdown.schoollist .sw-dropdown-list li').each(function() {
				var csSchoolName = $("a", this).text(), csSchoolURL = $("a", this).attr("href");
				var csPackedSchool = [csSchoolName, csSchoolURL];
	
				// Push the Schools SCHOOL
                csTheSchools.push(csPackedSchool);
				
			});//End each
			
			// BUILD ELEMENTARY SCHOOL LIST
			var elementarySchools = "";
			for(var e in csTheSchools) {
				elementarySchools += "1," + csTheSchools[e][0] + "," + csTheSchools[e][1] + ",_blank;";
			}
			
		function buildSchoolList(csTheSchools) {
			// BUILD SCHOOL LIST
			var fullSchoolList = '<div class="gb-school-dropdown" aria-hidden="true"><div class="gb-school-list-outer max-width cs-fc cs-fw">',
            	columnOne = '<ul class="gb-school-list-column first cs-fcc">',
                columnTwo = '<ul class="gb-school-list-column second cs-fcc">',
                columnThree = '<ul class="gb-school-list-column third cs-fcc">',
                columnFour = '<ul class="gb-school-list-column forth cs-fcc">';
			
			var p = 1,
            	c1 = 1,
                c2 = 1,
                c3 = 1,
                c4 = 1;
			for(var i = 0; i < csTheSchools.length; i++) {
				if(p == 1){
                	columnOne += '<li><a href="' + csTheSchools[i][1] + '" data-school-column="1" data-school-link="'+c1+'">' + csTheSchools[i][0] + '</a></li>';
					p++;
                    c1++;
                } else if(p == 2){
                	columnTwo += '<li><a href="' + csTheSchools[i][1] + '" data-school-column="2" data-school-link="'+c2+'">' + csTheSchools[i][0] + '</a></li>';
					p++;
                    c2++;
                } else if(p == 3){
                	columnThree += '<li><a href="' + csTheSchools[i][1] + '" data-school-column="3" data-school-link="'+c3+'">' + csTheSchools[i][0] + '</a></li>';
                	p++;
                    c3++;
                } else {
                	columnFour += '<li><a href="' + csTheSchools[i][1] + '" data-school-column="4" data-school-link="'+c4+'">' + csTheSchools[i][0] + '</a></li>';
					p = 1;
                    c4++;
                } 
			}
			columnOne += '</ul>';
			columnTwo += '</ul>';
			columnThree += '</ul>';
			columnFour += '</ul>';
			fullSchoolList += columnOne + columnTwo + columnThree + columnFour + '</div></div>';
            
            //-- Custom School Dropdown Functionality
            $('.gb-custom-dropdown.schoollist').click(function(){
            	var school = $(this).offset();
            	if($('.gb-school-dropdown').hasClass('open')) {
                	$(this).removeClass('open').attr('aria-lebel', 'Open School List');
                	$('.gb-school-dropdown').removeClass('open').attr('aria-hidden', 'true').slideUp();
                    $('.gb-school-dropdown a').attr('tabindex', '-1');
                } else {
                	$(this).addClass('open').attr('aria-label', 'Close School List');
                	$('.gb-school-dropdown').css('left', '-' + (school.left + 1) + 'px').addClass('open').attr('aria-hidden', 'false').slideDown();
                    $('.gb-school-dropdown a').attr('tabindex', '-1');
                }
            });
            
            //-- Close School List On Blur
            $('#gb-district a, #gb-search-button').focus(function(){
                  $('.gb-custom-dropdown.schoollist').removeClass('open').attr('aria-lebel', 'Open School List');
                  $('.gb-school-dropdown').removeClass('open').attr('aria-hidden', 'true').slideUp();
            });
            
			//-- ADD EVERYTHING
			$(".gb-custom-dropdown.schoollist").append(fullSchoolList);
            
            
			$('.gb-school-list-outer li a').keydown(function(e){
            	var code = e.which;
                if((code == kDown) || (code == kUp) || (code == kLeft) || (code == kRight)){
                	e.preventDefault();
                    if(code == kUp && $(this).data('school-link') == '1' && $(this).data('school-column') == '1'){
                        $('.gb-custom-dropdown.schoollist').focus();
                    } else if((code == kUp || code == kLeft ) && $('*[data-school-link="'+ ($(this).data('school-link') - 1) +'"][data-school-column="'+ $(this).data('school-column') +'"]').html() == undefined) {
                        $('*[data-school-link="'+ $(this).closest('.gb-school-list-column').prev().children('li').length +'"][data-school-column="'+ ($(this).data('school-column') - 1) +'"]').focus();
                    } else if(code == kUp || code == kLeft){
                        $('*[data-school-link="'+ ($(this).data('school-link') - 1) +'"][data-school-column="'+ $(this).data('school-column') +'"]').focus();
                    } else if((code == kDown || code == kRight )&& $('*[data-school-link="'+ ($(this).data('school-link') + 1) +'"][data-school-column="'+ $(this).data('school-column') +'"]').html() == undefined) {
                        $('*[data-school-link="1"][data-school-column="'+ ($(this).data('school-column') + 1) +'"]').focus();
                    } else if(code == kDown || code == kRight){
                        $('*[data-school-link="'+ ($(this).data('school-link') + 1) +'"][data-school-column="'+ $(this).data('school-column') +'"]').focus();
                    } 
                    
                    /*else if(code == kRight){
                        $('*[data-school-link="'+ ($(this).data('school-link')) +'"][data-school-column="'+ ($(this).data('school-column') + 1) +'"]').focus();
                    } else if(code == kLeft){
                        $('*[data-school-link="'+ ($(this).data('school-link')) +'"][data-school-column="'+ ($(this).data('school-column') - 1) +'"]').focus();
                    }*/
                }
            });
            
            //-- DROPDOWN FUNCTIONALITY AND NAVIGATION
            $('.gb-custom-dropdown.schoollist').keydown(function(e){
            	var code = e.which;
                if(code == kDown || code == kEnter || code == kSpace){
                	if(code == kDown || code == kSpace){
                    	e.preventDefault();
                    }
                    var school = $(this).offset();
                    if($('.gb-school-dropdown').hasClass('open') && code == kEnter || $('.gb-school-dropdown').hasClass('open') && code == kSpace) {
                   		$(this).removeClass('open');
                        $('.gb-school-dropdown').removeClass('open').attr('aria-hidden', 'true').slideUp();
                    } else if(!($('.gb-school-dropdown').hasClass('open'))) {
                    	$(this).addClass('open');
                        $('.gb-school-dropdown').css('left', '-' + (school.left + 1) + 'px').addClass('open').attr('aria-hidden', 'false').slideDown();
                    	$('li a', this).first().focus();
                    }
                }
            });
				
		}
		buildSchoolList(csTheSchools);
        
	}
    
	
