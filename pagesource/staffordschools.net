	function appView() {
		if(!appViewSet) {
			// MAKE HOMEPAGE APPS COLLAPSIBLE FOR SMALL MOBILE DEVICES
			//if(!$("#hp-content-lower div.ui-widget.app.active").length){
			//	$("#hp-content-lower div.ui-widget.app:first").addClass("active");
			//}
			
			// DO NOT ADD APP INTO ACCORDION IF IT DOES NOT HAVE A HEADER
			$("#hp-content-lower div.ui-widget.app").each(function() {
				if($(".ui-widget-header", this).hasClass("ui-helper-hidden") || jQuery.trim($(".ui-widget-header h1", this).text()) == "") {
					$(this).addClass("no-header");
				}
                if($('.ui-widget-header .plus-icon', this).size() == 0){
                	$('.ui-widget-header', this).append(plusSVG);
                }
			});
			
			// ACCORDION HEADER CLICK
			$("#hp-content-lower div.ui-widget-header, #hp-tabbed-apps div.ui-widget-header").click(function(){
				var clickedApp = $(this).parent().parent().attr("id");
				
                if($(this).parent().hasClass('active')){
                	$(this).parent().removeClass('active');
                } else {
                    $("#hp-content-lower div.ui-widget.app, #hp-tabbed-apps div.ui-widget.app").removeClass("active");
                    $(this).parent().addClass("active");

                    $("html, body").animate({
                        scrollTop: $("#" + clickedApp).offset().top
                    }, 500, function(){
                        $("html, body").stop(true, false);
                    });
                }
			});
		
			appViewSet = 1;
		}
	}
	
	function appViewReset() {
		if(appViewSet) {
