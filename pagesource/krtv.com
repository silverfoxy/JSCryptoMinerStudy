<script type="text/javascript">
	PLATFORM.EventMan.registerToEvent(function() {

		var videoIcon = "http://ftpcontent.worldnow.com/wncustom/custom/cordillera/2015/video.png";
		var defaultImage = "http://KRTV.images.worldnow.com/images/6431244_G.jpg";
		var summaryLinkVar = "";
		var botTemplate = '<div class="csStory">' + '{if $graphicSrc}' + '<a href="{$headlineHref}"><img class="csImage" src="{$graphicSrc}"></a>' + '{/if}' + '<h4>' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}' + '<a href="{$headlineHref}"><img src="' + videoIcon + '" /></a>' + '{/if}' + '{if $custDate}<span class="badge new">NEW</span>{/if}' + '{$sectionText}' + '</h4>' + '</div>';

		// Load story data
		var storyTop = $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		var featureStory = storyTop[0];
		var storyBottom = $wn('#WNCol2 .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		var storyCol2 = $wn('#WNCol2 .wnDS-20 .wnDVSummary h4');

		function setBadge(stObj) {

			var badgeInsert = '';
			if (stObj.toLowerCase().indexOf("breaking") != -1) {
				badgeInsert = '<span class="badge breaking">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("developing") != -1) {
				badgeInsert = '<span class="badge developing">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("update") != -1) {
				badgeInsert = '<span class="badge update">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("stream") != -1) {
				badgeInsert = '<span class="badge stream">' + stObj + '</span>';
			} else {
				badgeInsert = '<span class="badge">' + stObj + '</span>';
			}
			return badgeInsert;
		}

		// Build top
		featureStory.storyId = (function(href) {
			var matches = href.match(/^.*\/story\/(\d+)\/.*$/i) || [];
			return matches[1] || '';
		})(featureStory.headlineHref);

		if ((navigator.userAgent.toLowerCase().indexOf('firefox') > -1 || navigator.userAgent.toLowerCase().indexOf('chrome') > -1 || navigator.userAgent.toLowerCase().indexOf('trident') > -1 || navigator.userAgent.indexOf('Safari') != -1) && /DT/.test(featureStory.dateText)) {
			var mDate = new Date(featureStory.dateText);
			mDate = (mDate.setHours(mDate.getHours() + 1));
			featureStory.dateText = mDate;
		}

		if (CSHELPERS.secondsSince(featureStory.dateText) < 3600) {
			if (featureStory.sectionText.length > 0) {
				featureStory.custDate = false;
			} else {
				featureStory.custDate = true;
			}
		}

		if (featureStory.sectionText.length > 0) {
			featureStory.sectionText = setBadge(featureStory.sectionText);
		}

		if (!featureStory.graphicSrc) {
			featureStory.graphicSrc = defaultImage;
		}
		featureStory.hasVideo = (featureStory.isClip || featureStory.isVideo);
		if (featureStory.hasVideo) {
			summaryLinkVar = 'Watch Video';
		} else {
			summaryLinkVar = 'Read More';
		}
		var topTemplate = '<div class="csFeatureStory">' + '{if $graphicSrc}' + '<a href="{$headlineHref}"><img class="csFeatureImage" src="{$graphicSrc}"></a>' + '{/if}' + '</div>' + '<div class="csTopRight">' + '<h4>' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}' + '<a href="{$headlineHref}"><img src="' + videoIcon + '" /></a>' + '{/if}' + '{if $custDate}<span class="badge new">NEW</span>{/if}' + '{$sectionText}' + '</h4>' + '<p>{$summaryHtml}</p>' + '<a class="wnSummaryLink" href="{$headlineHref}">' + summaryLinkVar + ' &gt;</a>' + '</div>';

		$wn('#csTsr #csTop').csFormatterAppend(topTemplate, featureStory);

		/* PRSV-236 - Fixing Abstract/Summary, Jit */
		$('#csTsr .csTopRight p').ellipsis({
			row : 4,
			onlyFullWords : true
		});
		var customImageDiv = "";
		// Build bottom
		$wn.each(storyBottom, function(i) {
			if (this.dateText.length > 0) {
				if ((navigator.userAgent.toLowerCase().indexOf('firefox') > -1 || navigator.userAgent.toLowerCase().indexOf('chrome') > -1 || navigator.userAgent.toLowerCase().indexOf('trident') > -1 || navigator.userAgent.indexOf('Safari') != -1) && /DT/.test(this.dateText)) {
					var mDate = new Date(this.dateText);
					//timestamp = this.dateText;
					mDate = (mDate.setHours(mDate.getHours() + 1));
					this.dateText = mDate;
				}
