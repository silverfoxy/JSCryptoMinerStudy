      function chkLogin1()
      {
        var sno =  document.getElementById('Username').value;
        var pwd =  document.getElementById('Password').value;
        
        //alert('tommy' + sno + 'REYES');
        if(sno == '')
        {
          alert('Student No. is required for login.');
          document.getElementById('Username').focus();
          return false
        }else if(pwd == '')
        {
          alert('Password is required for login.');
          document.getElementById('Password').focus();
          return false
        }else{  
          return true
        }
      }
      
      
