<script type="text/javascript">
    hidetabs = function() {
        var objtabs = document.getElementsByClassName("evtHide");
        for (var i = 0; i < objtabs.length; i++) {
            objtabs[i].style.display = 'none'
        }

        var objtabscurrent = document.getElementsByClassName("stActiveLink");
        for (var i = 0; i < objtabscurrent.length; i++) {
            objtabscurrent[i].className= "";
        }
    };


    clicktab = function(id,currentId) {
        hidetabs();                        
        document.getElementById(id).style.display = 'block';
        currenttab = document.getElementById(currentId);
        currenttab.setAttribute('class', 'stActiveLink');
        fade00();
        fade01();                        
        fade02();
        fade03();

    };

    fade00 = function(){
        var i = 0;
        var cont = document.getElementById("WHResults-0");
        var el = cont;
        fadeIn(el,i);
    }                    
    fade01 = function(){
        var i = 0;
        var cont = document.getElementById("WHResults-1");
        var el = cont;
        fadeIn(el,i);
    }
    fade02 = function(){
        var i = 0;
        var cont = document.getElementById("WHResults-2");
        var el = cont;
        fadeIn(el,i);
    }
    fade03 = function(){
        var i = 0;
        var cont = document.getElementById("WHResults-3");
        var el = cont;
        fadeIn(el,i);
    }

    function fadeIn(el,i) {
        i = i + 0.01;
        seto(el,i);
        if (i<1){setTimeout(function(){fadeIn(el,i);}, 1);}
    }

    function seto(el,i)
    {
        el.style.opacity = i;
    } 
</script>
                
                
