		viewport:		$('meta[name="viewport"]'),
		handle:			$('#viewport-options'),
		storage:		(typeof (Storage) !== "undefined") ? window.localStorage : {},
		setResponsive: function ()
		{
			this.storage.isResponsive = (this.storage.isResponsive === "undefined") ? "true" : this.storage.isResponsive;
			this.handle.html (this.isResponsive () ? 'Desktop Version' : 'Optimized Version');
			if (!this.isResponsive ())		{ this.viewport.attr ('content', 'width=' + this.widthFull); }
		},
		getResponsive: function ()		{ return this.storage.isResponsive; },
		isResponsive: function ()		{ return (this.getResponsive () == "true"); },
		toggle: function (value, width, display) {
			this.storage.isResponsive = value;
			this.viewport.attr ('content', 'width=' + width);
			this.handle.html (display);
		},
		showFull:		function ()	{ this.toggle ("false", this.widthFull,	'Optimized Version');	},
		showOptimized:	function ()	{ this.toggle ("true",  this.widthDevice, 'Desktop Version');	},
		run: function ()
		{
			if (this.isResponsive ())	{ this.showFull (); }
			else						{ this.showOptimized (); }
		},
		init: function ()
		{
			var self = this;
			self.setResponsive ();
			self.handle.on ('click', function () {
				self.run ();
				return false;
			})
		}
	};

	Viewport.init ();
});
</script>
