    <script type="text/javascript">
        var herobanner = document.getElementById("heroBanner");
        if (typeof (herobanner) != 'undefined' && herobanner != null) {
            var win = Math.min(document.documentElement.clientWidth, window.innerWidth || 0)
            var bgw = '';
            var setm = '';
            if (win < 2000 && win > 640) {
                bgw = 2000;
            } else {
                bgw = 640;
            }
            if (win > 2000) {
                setm = 'auto';
            } else {
                if (win < bgw) {
                    setm = (win - bgw) / 2;
                }
                else if (bgw < win) {
                    setm = (win - bgw) / 2;
                } else {
                    setm = 'auto';
                }
            }
            var imgs = herobanner.getElementsByTagName("img");
            if (win > 640) {
                for (var i = 0; i < imgs.length; i++) {
                    imgs[i].setAttribute("style", "visibility:visible;margin-left:" + setm + "px;");
                }
            } else {
                for (var i = 0; i < imgs.length; i++) {
                    imgs[i].setAttribute("style", "visibility:visible;margin-left:auto;");
                }
            }
        }
    </script>
    
