                        increaseArea: '20%' // optional
                    });

                    var desktop = true;
                    var mobile = false;
                    enquire.register("screen and (min-width: 641px) and (orientation: landscape)", {
                        match: function () {
                            desktop = true;
                            mobile = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            desktop = false;
                        }
                    });
                    enquire.register("screen and (min-width: 641px) and (orientation: portrait)", {
                        match: function () {
                            desktop = true;
                            mobile = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            desktop = false;
                        }
                    });
                    enquire.register("screen and (max-width: 640px) and (orientation: portrait)", {
                        match: function () {
                            mobile = true;
                            desktop = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            mobile = false;
                        }
                    });
                    enquire.register("screen and (max-width: 640px) and (orientation: landscape)", {
                        match: function () {
                            mobile = true;
                            desktop = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            mobile = false;
                        }
                    });

                }

                function lazyLoad(desktop, mobile) {
                    $('img[data-src]').each(function () {//images that are not made responsive
                        var imgSrc = $(this).attr('data-src');
                        $(this).attr('src', imgSrc);

                    });
                    $('iframe[data-src]').each(function () {//iframes that are not made responsive
                        var imgSrc = $(this).attr('data-src');
                        $(this).attr('src', imgSrc);

                    });

                    if (desktop == true) {
                        $('img[data-src-desktop]').each(function () {
                            var imgSrc = $(this).attr('data-src-desktop');
                            $(this).attr('src', imgSrc);
                            bgw();
                        });
                    }
                    if (mobile == true) {
                        $('img[data-src-mobile]').each(function () {
                            var imgSrc = $(this).attr('data-src-mobile');
                            $(this).attr('src', imgSrc);
                            bgw();
                        });
                    }
                    //for home page banner only
                    var imageUrl = $('#HomeRotatorBg').attr('src');
                    $('#mediaBanner').css('background-image', 'url(' + imageUrl + ')');
                }


                if (document.all && !window.atob) {//for IE9
                    $('img[data-src-desktop]').each(function () {
                        var imgSrc = $(this).attr('data-src-desktop');
                        $(this).attr('src', imgSrc);
                    });
                    //for home page banner only
                    var imageUrl = $('#HomeRotatorBg').attr('src');
                    $('#mediaBanner').css('background-image', 'url(' + imageUrl + ')');
                }
