    <script>
        $(window).load(function () {
            var intervalId;

            var latestNewsInterval = {
                start: function () {
                    intervalId = setInterval(getLatestNews, 10000);
                },
                stop: function () {
                    clearInterval(intervalId);
                }
            };

            latestNewsInterval.start();

            $('.js-get-latest-news, .js-get-latest-titles, .js-get-latest-as-is-news').hover(function () {
                latestNewsInterval.stop();
            }, function () {
                latestNewsInterval.start();
            });

            function getLatestNews() {
                $.ajax({
                    url: "/get-latest-news.php",
                    success: function (data) {
                        var content = $(data);
                        $('.js-get-featured-title').html(content.filter('.js-get-featured-title').html());
                        $('.js-get-featured-image').html(content.filter('.js-get-featured-image').html());
                        $('.js-get-latest-titles').html(content.filter('.js-get-latest-titles').html());
                        $('.js-get-latest-news').html(content.filter('.js-get-latest-news').html());
                        $('.js-get-latest-as-is-news').html(content.filter('.js-get-latest-as-is-news').html());
                        $('.js-get-latest-news .widget-content, .js-get-latest-as-is-news .widget-content').slick({
                            dots: false,
                            infinite: true,
                            autoplay: true,
                            autoplaySpeed: 5000,
                            speed: 300,
                            slidesToShow: 1,
                            rtl: true,
                            nextArrow: '<div class="next"></div>',
                            prevArrow: '<div class="prev"></div>'
                        });
                        $('.js-get-latest-titles .equal-height').matchHeight({
                            byRow: true,
                            property: 'height',
                            target: null,
                            remove: false
                        });
                    }
                });
            }
        });

        (function () {
            var titleCycling = {
                start: function () {},
                stop: function () {}
            };
            $('body').on('mouseenter', '.js-latest-title', function () {
                titleCycling.stop();
                var image = $(this).data('image');
                var title = $(this).data('title');
                var url = $(this).find('a').prop('href');
                $(this).closest('.top-featured-section').find('.image-wrap').hide();
                $(this).closest('.top-featured-section').find('.image-title').hide();
                $(this).closest('.top-featured-section').find('.image-wrap--hover').css({'display': 'block', 'background-image': 'url("' + image + '")'});
                $(this).closest('.top-featured-section').find('.image-title--hover').show();
                $(this).closest('.top-featured-section').find('.image-title--hover h6 a').text(title);
                if (!url) {
                    url = 'javascript:void(0)';
                }
                $(this).closest('.top-featured-section').find('.image-wrap--hover').prop('href', url);
                $(this).closest('.top-featured-section').find('.image-title--hover h6 a').prop('href', url);
            }).on('mouseleave', '.js-latest-title', function () {
                titleCycling.start();
                $(this).closest('.top-featured-section').find('.image-wrap').show();
                $(this).closest('.top-featured-section').find('.image-title').show();
                $(this).closest('.top-featured-section').find('.image-wrap--hover').hide();
                $(this).closest('.top-featured-section').find('.image-title--hover').hide();
                $(this).closest('.top-featured-section').find('.image-wrap--hover').prop('href', 'javascript:void(0)');
                $(this).closest('.top-featured-section').find('.image-title--hover h6 a').prop('href', 'javascript:void(0)');
            });


            (function findNextTitle(counter) {
                var latestTitles = $('.js-latest-title');
                if (latestTitles[counter]) {
                    var latestTitle = latestTitles[counter];
                    $(latestTitle).closest('.top-featured-section').find('.image-wrap').hide();
                    $(latestTitle).closest('.top-featured-section').find('.image-title').hide();
                    $(latestTitle).closest('.top-featured-section').find('.image-wrap--hover').css({'display': 'block', 'background-image': 'url("' + $(latestTitle).data('image') + '")'});
                    $(latestTitle).closest('.top-featured-section').find('.image-title--hover').show();
                    $(latestTitle).closest('.top-featured-section').find('.image-title--hover h6 a').text($(latestTitle).data('title'));
                    $(latestTitle).closest('.top-featured-section').find('.image-wrap--hover').prop('href', $(latestTitle).find('a').prop('href') || 'javascript:void(0)');
                    $(latestTitle).closest('.top-featured-section').find('.image-title--hover h6 a').prop('href', $(latestTitle).find('a').prop('href') || 'javascript:void(0)');
                } else {
                    counter = -1;
                    $('.top-featured-section').find('.image-wrap').show();
                    $('.top-featured-section').find('.image-title').show();
                    $('.top-featured-section').find('.image-wrap--hover').hide();
                    $('.top-featured-section').find('.image-title--hover').hide();
                    $(latestTitle).closest('.top-featured-section').find('.image-wrap--hover').prop('href', 'javascript:void(0)');
                    $(latestTitle).closest('.top-featured-section').find('.image-title--hover h6 a').prop('href', 'javascript:void(0)');
                }

                var timeoutId = setTimeout(function() {
                    findNextTitle(counter + 1);
                }, 5000);

                titleCycling = {
                    start: function () {
                        timeoutId = setTimeout(function() {
                            findNextTitle(counter + 1);
                        }, 5000);
                    },
                    stop: function () {
                        clearTimeout(timeoutId);
                    }
                };
            } ());
        }(0));
    </script>

