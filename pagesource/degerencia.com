var linkSel;
function cambiaColor(actual) {
	if (document.getElementById(linkSel)) document.getElementById(linkSel).style.background = "#ffffff";
	actual.style.background = "#cccccc";
	linkSel = actual.id;
}
function cargar_contenido(target,id) 
{ 
	
	if (document.getElementById(target).style.display == "none") {
		var myConn = new XHConn(); 
			if (!myConn) alert("XMLHTTP no esta disponible. Intétalo con un navegador más actual."); 
			var peticion = function (oXML) {  document.getElementById(target).innerHTML = oXML.responseText; }; 
			myConn.connect("arbol_ajax.php?menu=no&opc="+id+"&id="+id+"&bas=", "GET", "", peticion); 
		document.getElementById(target).style.display = "block";
	}
	else {
		document.getElementById(target).innerHTML = "";
		document.getElementById(target).style.display = "none";
	}
} 
function cambiarImagen(mostrar,ocultar) {
	if (document.getElementById(mostrar)) document.getElementById(mostrar).style.display = "block";
	if (document.getElementById(ocultar)) document.getElementById(ocultar).style.display = "none";	
}		
