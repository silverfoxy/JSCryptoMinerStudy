          $arrAreaCodeSelect.next( '.customSelect' ).find( '.customSelectInner' ).text( selectEmptyText );

          var group_code = $(this).val();
          if( group_code ){
              // 値が選択された
              var area_codes = options[group_code];
              applySelect( $arrAreaCodeSelect, area_codes, selectEmptyText );     // 目的地方面コードの設定
          }

      } );

      // 目的地方面グループの更新
      function updateArrAreaGroup(){
          var group_id = $depAreaGroupIdSelect.val();
          if( !group_id ) return false;

          var count_opts = areaGroupCounts[group_id];
          if( !count_opts ) return false;

          // 件数が0件のものを間引く
          var set_opts = {};
          for( var code in areaGroups ){
              var count = count_opts[code];
              if( typeof count !== 'undefined' && count > 0 ){
                  set_opts[code] = areaGroups[code];
              }
          }
          // _d(areaGroups);
          // _d(count_opts);
          // _d(set_opts);

          applySelect( $arrAreaGroupCodeSelect, set_opts, selectEmptyText );
      }
      updateArrAreaGroup();

    });



    </script>
</body>
</html>