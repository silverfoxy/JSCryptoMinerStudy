				srcfix='src';
				$('#'+a).html('<img '+srcfix+'="'+img194+'" alt=""/><span class="marque">'+b+'</span><span class="description">' + intitule + '</span><span class="prix">'+data.d.Product.DisplaySalePriceAfter+'</span>');
				$('#'+a).attr('href','javascript:pdp('+a+');');
			}else{$('#'+a).remove()
			};
		}else{$('#'+a).remove()}
	}
})};
	
