html,
body {
  overflow-x: hidden;
}

body {
  background: #ffb10e;
  color: #333;
  padding-top: 140px;
  letter-spacing: 1px;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/*main*/
main {
  width: 100%;
  max-width: 1600px;
  margin: auto;
}

.centerRow {
  max-width: 1200px;
  padding: 0 10px;
  margin: auto;
}

p {
  font-family: "Pedigree Light";
}

header {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: #ffb10e;
  z-index: 999;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
}

header .container {
  position: static;
  height: 140px;
  padding: 0;
  display: table;
  vertical-align: middle;
}

header .plogo {
  width: 160px;
  display: table-cell;
  vertical-align: middle;
}

header .plogo img {
  max-width: 118px;
  width: 100%;
}

.navScroll header .container {
  height: 75px;
}

.navScroll header .container .plogo img {
  width: 72px;
}

.navScroll .menuSub {
  top: 75px;
}

.navMenu {
  width: auto;
  z-index: 999;
  display: table-cell;
  vertical-align: middle;
}

.navMenu a {
  color: #fff;
  font-size: 14px;
  text-transform: uppercase;
  text-decoration: none;
  padding: 10px 0;
  display: inline-block;
}

.navMenu li {
  vertical-align: middle;
  padding: 0 8px;
}

.navMenu li > a {
  border-bottom: 3px solid transparent;
}

.navMenu li > a:hover, .navMenu li > a.active {
  color: #ba0e0d;
  border-bottom-color: #fff;
}

.navMenu .searchBtn:hover {
  border-bottom-color: transparent;
}

.menuSub {
  display: none;
  position: absolute;
  left: 0;
  top: 140px;
  width: 100%;
  background: #ccc;
  background: rgba(0, 0, 0, 0.6);
  text-align: center;
  padding: 0;
  list-style: none;
  z-index: 100;
}

.menuSub li {
  display: inline-block;
}

.menuSub li a {
  padding: 18px 7px;
  border: 0;
}

.menuSub li a.active {
  background: #000;
  color: #ffb10e;
}

.menuSub li a:hover {
  color: #ffb10e;
}

.menuSubBtnArrow {
  position: absolute;
  height: 60px;
  padding: 10px 15px;
  right: 0;
  top: 0;
  border-left: 1px solid #fff;
  font-size: 20px;
  color: #fff;
  z-index: 100;
  display: none;
  font: normal normal normal 14px/1 FontAwesome;
}

.menuSubBtnArrow:before {
  content: '\f107';
}

.menuSubShow .menuSub {
  display: block;
}

.menuSubShow .menuSubBtnArrow:before {
  content: '\f106';
}

.mobileMenuBtn {
  position: absolute;
  left: 1px;
  top: 1px;
  color: #fff;
  font-size: 24px;
  padding: 10px 20px 10px 10px;
  z-index: 1000;
}

.mobileMenuBtn .fa:before {
  content: '\f0c9';
}

.mobileSearchBtn {
  position: absolute;
  right: 1px;
  top: 1px;
  color: #fff;
  font-size: 24px;
  padding: 10px;
  z-index: 998;
}

.searchField {
  background: #fff;
  display: none;
}

.searchField .container {
  height: 70px !important;
  padding: 15px 60px 15px 0;
  display: block;
  position: relative;
  z-index: 1;
}

.searchField input[type="search"] {
  border: none;
  color: #333;
  text-align: right;
  padding-right: 5px;
  background: #fff;
}

.searchField input[type="search"]::-webkit-input-placeholder {
  color: #333;
  opacity: 1;
}

.searchField input[type="search"]::-moz-placeholder {
  color: #333;
  opacity: 1;
}

.searchField input[type="search"]:-moz-placeholder {
  color: #333;
  opacity: 1;
}

.searchField input[type="search"]:-ms-input-placeholder {
  color: #333;
  opacity: 1;
}

.searchField .searchSubmitBtn {
  position: absolute;
  color: #ba0e0d;
  right: 0;
  top: 0;
  font-size: 30px;
  padding: 20px;
  z-index: 1;
}

