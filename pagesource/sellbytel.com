<script>
    $(document).ready(function(){
	var viewportAbroad=$("#careerViewportAbroad");
	var viewportSimple=$("#careerViewportSimple");

        var request = $.ajax({
            url:'/get/translateCountry',
            type: "get",
            dataType: "json",
            contentType: "application/x-www-form-urlencoded; charset=UTF-8"
        });

        request.success(function (response, textStatus, jqXHR){
            var country=response;
            var countryName = country.country_name;
            if(!$(country)) return;
            var ajaxUrl= '/get/localjobs?location='+country.country_code+'&limit=1&locale=en_US&filterRighthead=1';

            var request = $.ajax({
              url:ajaxUrl,
              type: "get",
              dataType: "json",
              contentType: "application/x-www-form-urlencoded; charset=UTF-8"
            });

            request.success(function (response, textStatus, jqXHR){
                var jobs=response;

                if(!$(jobs) || jobs.length!=1) return;

                var localizedJobs='';

                $.each(jobs, function(key, job) {
                    var teaserHTML='';
                    teaserHTML+='   <h1>Job offers nearby.</h1>';
                    teaserHTML+='    <div class="line"></div>';
                    teaserHTML+='    <a href="'+job.Link+'" class="clickable">';
                    teaserHTML+='       <h2>'+job.work_location+'</h2>';
                    teaserHTML+='       <p>'+job.Title+'</p>';
                    teaserHTML+='    </a>';
                    teaserHTML+='    <div class="text-right">';
                    teaserHTML+='        <a href="'+job.Link+'" class="button-yellow applybutton">APPLY NOW</a>';
                    teaserHTML+='    </div>';
                    teaserHTML+='    <a href="/en/career/open-positions/Search?job-search='+countryName+'" class="arrow-link" >Further vancancies in your country</a>';
                    $("<div>").html(teaserHTML).addClass("jobTeaser left").appendTo($(".teasers",viewportAbroad));
                });

                var ajaxUrlInvers= '/get/localjobs?location='+country.country_code+'&limit=1&invers=1&locale=en_US&filterRighthead=1';

                var requestInvers = $.ajax({
                    url:ajaxUrlInvers,
                    type: "get",
                    dataType: "json",
                    contentType: "application/x-www-form-urlencoded; charset=UTF-8"
                });

                requestInvers.success(function (response, textStatus, jqXHR){
                    var jobs=response;

                    if(!$(jobs)) return;

                    $.each(jobs, function(key, job) {
                        var teaserHTML='';
                        teaserHTML+='  <h1>Job offers abroad.</h1>';
                        teaserHTML+='  <div class="line"></div>';
                        teaserHTML+='  <a href="'+job.Link+'"  class="clickable">';
                        teaserHTML+='      <h2>'+job.work_location+'</h2>';
                        teaserHTML+='       <p>'+job.Title+'</p>';
                        teaserHTML+='  </a>';
                        teaserHTML+='  <div class="text-right">';
                        teaserHTML+='      <a href="'+job.Link+'" class="button-yellow applybutton">APPLY NOW</a>';
                        teaserHTML+='  </div>';
                        teaserHTML+='  <a href="/en/career/open-positions/Search?job-search=&functions=All&level=All&places=All&language=EN%2CDE%2CES" class="arrow-link" >Further open positions</a>';
                        $("<div>").html(teaserHTML).addClass("jobTeaser right").appendTo($(".teasers",viewportAbroad));
                    });
                    viewportAbroad.show();
                    viewportSimple.hide();
                    sellbytel.view.careerAbroadView = SellByTel.View.HomeCareerAbroadView($('#careerViewportAbroad'));
                    $(document).trigger("elementSnap.complete", { scrolledUp: true, activeElement: $('#careerViewportAbroad') });
                });

            });
        });

        request.error(function (jqXHR, textStatus, errorThrown){
            //console.log('error_tech');
        });

    });
</script>
    



