      if($('#'+type+'Checkbox').prop('checked')){
        reset_type(type);
        if(type == 'reiseart'){
          reset_type('reiseart');
          reset_type('unterkunft');
        }
        if(type == 'region'){
          reset_type('land');
          reset_type('region');
          reset_type('ort');
        }
        $('#'+type+'Checkbox').prop('checked', true);
      } else {
        $('#'+type+'Checkbox').prop('checked', true);
      }
  }

  function reset_type(type){
    $.get('//'+window.location.host+'/suchen/fastsearch_ajax.php?action=reset&type='+type)
     .success(function(data) {
      load_sessionVars(type);
//      load_gefundene_pakete();// schon unten... + schon in load_sessionVars() drin
      $('#fprModal').dialog('close');
      $('#fprModalCal').dialog('close');
      if(type == 'reisedatum'){
        $('#'+type+'Checkbox').prop('checked', true);
      }
      if(type == 'region'){
          regionList.splice(0,regionList.length);
          regionListText = '';
          landList.splice(0,landList.length);
          landListText = '';
          $('#regionCheckbox').prop('checked', true);
      }
      if(type == 'land'){
          regionList.splice(0,regionList.length);
          regionListText = '';
          landList.splice(0,landList.length);
          landListText = '';
