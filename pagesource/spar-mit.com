  $('table.fprueberergebnis td.rechts').remove();
 });

  function unload(type){
    switch(type) {
      case 'region':
          landList.splice(0,landList.length);
          landListText = '';
          send_selection('land', landListText);
          regionList.splice(0,regionList.length);
          regionListText = '';
          send_selection('region', regionListText);
          break;
      case 'reisedatum':
          //have to come latr
          break;
      case 'reiseart':
          reiseartList.splice(0,reiseartList.length);
          reiseartListText = '';
          unterkunftList.splice(0, unterkunftList.length);
          unterkunftListText = '';
          send_selection('reiseart', reiseartListText );
          send_selection('unterkunft', unterkunftListText);
          break;
      case 'spezial':
          spezialList.splice(0,reiseartList.length);
          spezialListText = '';
          send_selection('spezial', spezialListText);
          break;
      default:
          //none
    }

  }
</script>


