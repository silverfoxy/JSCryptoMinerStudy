  function select_unterkunft(unterkunftID){
  	if(unterkunftList == ""){
  		unterkunftList = [];
  	}
    var checkArray = unterkunftList.indexOf(unterkunftID);
    if(checkArray >= 0){
      unterkunftList.splice(checkArray, 1);
    } else {
      unterkunftList.push(unterkunftID);
    }
    var tempText = '';
    for (index = 0; index < unterkunftList.length; index++) {
      tempText += unterkunftList[index]+",";
    }
    unterkunftListText = tempText.substring(0, tempText.length - 1);
    if (unterkunftList.length == 0){
      unterkunftList.splice(0,unterkunftList.length);
      unterkunftListText = '';
     }
      if(unterkunftList.length == 0 && reiseartList.length == 0){
        $('#reiseartCheckbox').prop('checked', true);
      }  else {
        $('#reiseartCheckbox').prop('checked', false);
      }
  }

  function select_spezial(spezialID){
    var checkArray = spezialList.indexOf(spezialID);
    if(checkArray >= 0){
      spezialList.splice(checkArray, 1);
    } else {
      spezialList.push(spezialID);
    }
    var tempText = '';
    for (index = 0; index < spezialList.length; index++) {
      tempText += spezialList[index]+",";
    }
    spezialListText = tempText.substring(0, tempText.length - 1);
    if (spezialList.length == 0){
      spezialList.splice(0,spezialList.length);
      spezialListText = '';
      $('#spezialCheckbox').prop('checked', true);
    }  else{
       $('#spezialCheckbox').prop('checked', true);
      }
  }

  function send_selection(type, varSet){
  var postvars = "action=set&type="+type+"&vars="+varSet;
  if(varSet){
    set_sessions(postvars, type);
    $('#'+type+'Checkbox').prop('checked', false);
  } else {
    $.get('//'+window.location.host+'/suchen/fastsearch_ajax.php?action=reset&type='+type)
     .success(function(data) {
          $('#'+type+'Display').hide();
          load_sessionVars(type);
          $('#'+type+'Display').show();
//          load_sessionVars(type);//warum denn nochmal?
//          load_gefundene_pakete();//schon in load_sessionVars() drin
          $('#fprModal').dialog('close');
          $('#fprModalCal').dialog('close');
          $('#fprModalWide').dialog('close');
    });
  }
  }
