  function load_sessionVars(type){
    $.get('//'+window.location.host+'/suchen/fastsearch_ajax.php?action=get&type='+type)
     .success(function(data) {
      $('#'+type+'Display').html(data);
      load_gefundene_pakete();
    });
  }
  function load_sessionVars2(types){
    $.getJSON('//'+window.location.host+'/suchen/fastsearch_ajax.php?action=get&types='+types)
     .success(function(data) {
		if (data.types != undefined){
			for (d in data.types){
				$('#'+d+'Display').html(data.types[d]);
			}
		}
		load_gefundene_pakete();
    });
  }

  function load_gefundene_pakete(){
    if($('#regionCheckbox').prop('checked') && $('#reisedatumCheckbox').prop('checked') && $('#spezialCheckbox').prop('checked') && $('#reiseartCheckbox').prop('checked')){
      $('#getResults').hide();
    } else {
       $.get('//'+window.location.host+'/suchen/fastsearch_ajax.php?action=count')
        .success(function(data) {
         if(data == '1'){
           $('#getResults').html(data+' Angebot anzeigen');
           $('#getResults').show();
         } else if(data != '0'){
           $('#getResults').html(data+' Angebote anzeigen');
           $('#getResults').show();
         } else {
           $('#getResults').hide();
         }
       });
    }
 }

  function set_sessions(postvars, type){
    $.ajax({
            type: 'POST',
            url: '//'+window.location.host+'/suchen/fastsearch_ajax.php',
            data: postvars,
            dataType: 'json text'
        });
    $('#fprModal').dialog('close');
    $('#fprModalCal').dialog('close');
	$('#fprModalWide').dialog('close');
    $('#'+type+'Display').hide();
    load_sessionVars(type);
    $('#'+type+'Display').show();
  }

