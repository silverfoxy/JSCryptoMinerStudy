<script type="text/javascript">
	
	function addPromo(){
		var promoArray = [];
		var mulPromoArray = [];
		if(document.getElementById("promoCatIDs") != null){
			var promoCatIDs = document.getElementById("promoCatIDs").value;
			if(promoCatIDs != null)
				promoArray = promoCatIDs.split(',');		
		}
		for(var i=0; promoArray!=null && i<promoArray.length; i++) {
			var catentId = promoArray[i];
			if(document.getElementById("promo_div_"+catentId) != null){
				document.getElementById("promo_div_"+catentId).style.display = "block";
			}
		}
			
		if(document.getElementById("mulPromoCatIDs") != null){
			var mulPromoCatIDs = document.getElementById("mulPromoCatIDs").value;
			if(mulPromoCatIDs != null)
				mulPromoArray = mulPromoCatIDs.split(',');		
		}
		
		for(var i=0; mulPromoArray!=null && i<mulPromoArray.length; i++) {
			var catentId = mulPromoArray[i];
			if(document.getElementById("promo_div_"+catentId) != null){
				document.getElementById("promo_div_"+catentId).className = "product_discount_mul";
			}
		}
	}
	
	function scriptsToRunAtPageChange() {
		try {
			var i = 0;
	  		dojo.query(".dojotooltip").forEach(function(node) {
	     		if(!node.title) {
	        		return; 
	        	}
	      		if(!node.id) {
					node.id = "dojoToolTip-"+i;
					i++;
				}
				var tt = new dijit.Tooltip({label:node.title, connectId:[node.id]});
				node.title = ""; // remove the Browsers standard title tag
			});
		} catch (err){}
		try {
			jQuery(".energylabel_close_button").click(hideEnergyLabelLarge);
			jQuery(".energylabel").click(showEnergyLabelLarge);
		} catch (err) {}
		
		
		try {
			jQuery(".custombutton").each(function() {
				var newButtonClass = jQuery(this).attr('class').replace('custombutton_', 'button_').replace('custombutton', '');
				var newButtonImage = jQuery(this).attr('image');
				if (typeof newButtonImage === "undefined") {
					var newButtonImageDiv = jQuery('<div />');
				} else {
					var newButtonImageDiv = jQuery('<div />', {
						'class': 'button_image ' + newButtonImage
					});
				}

				jQuery(this).after(
					jQuery('<a />', {
						'role': 'button',
						'class': newButtonClass,
						'id': jQuery(this).attr('id'),
						'onclick': jQuery(this).attr('onclick'),
						'onblur': 'JavaScript: this.blur();',
						'onmouseout': 'JavaScript: this.blur()'
					}).append(
						jQuery('<div />', {
							'class': 'left_border'
						})
					).append(
						newButtonImageDiv
					).append(
						jQuery('<div />', {
							'class': 'button_text',
							'text': jQuery(this).html()
						})
					).append(
						jQuery('<div />', {
							'class': 'right_border'
						})
					)
				);
				jQuery(this).attr('id', jQuery(this).attr('id') + "_oldbutton");
				jQuery(this).removeClass('custombutton');
				jQuery(this).attr('style', 'left: -1000px; position: fixed; top: -1000px;');
			});
		} catch (err) {}
		
		
		addPromo();
		
		
		try{
			
			if(jQuery('#bz_productIds').html()){
			
				var bvproductIds = jQuery('#bz_productIds').html();
				var tmpArray = bvproductIds.split(",");
			
				$BV.ui( 'rr', 'inline_ratings', {
					productIds : tmpArray,
			 		containerPrefix : 'BVRRInlineRating'
				});
			}
		}catch(err){
			console.log(err);
		}
	}
	// Added to ditect browser type and apply appropriate css
	function msieversion(){
      var ua = window.navigator.userAgent;
      if ( (ua.indexOf ( "MSIE" ) > 0 || ua.indexOf ( "rv:") > 0 ) && !(ua.indexOf ( "Firefox" ) > 0) )      // If Internet Explorer, return 1
         return 1;
      else if ( ua.indexOf ( "Chrome" ) > 0)	// If Chrome, return -1
      	 return -1;
      else                 // If another browser, return 0
         return 0;
   	}
   	
   	function changePriceDiv(){
	   	var timeRun = 0; 
		var interval = setInterval(function(){
			var isIE = "";
			if ( msieversion() > 0 )
				isIE = "_ie";
			if (msieversion() < 0)
				isIE = "_chrome";
				
			if(jQuery(".igoRec").length != 0){
				if(document.getElementById("product0") != null){
					var priceDiv = jQuery("#product0 .price")[0];
					if(priceDiv != null){
						var wid = (jQuery('#product0 span.price').css('width') );
						jQuery('#product0 span.newPrice'+isIE).width(wid);
					}
				}
				if(document.getElementById("product1") != null){
					var priceDiv = jQuery("#product1 .price")[0];
					if(priceDiv != null){
						var wid = (jQuery('#product1 span.price').css('width') );
						jQuery('#product1 span.newPrice'+isIE).width(wid);
					}
				}
				if(document.getElementById("product2") != null){
					var priceDiv = jQuery("#product2 .price")[0];
					if(priceDiv != null){
						var wid = (jQuery('#product2 span.price').css('width') );
						jQuery('#product2 span.newPrice'+isIE).width(wid);
					}
				}
				if(document.getElementById("product3") != null){
					var priceDiv = jQuery("#product3 .price")[0];
					if(priceDiv != null){
						var wid = (jQuery('#product3 span.price').css('width') );
						jQuery('#product3 span.newPrice'+isIE).width(wid);
					}
				}
			}
			timeRun += 1;
			if(timeRun === 15){
				clearInterval(interval);
			}
		}, 500);
	}
	jQuery(document).ready(function() {
		scriptsToRunAtPageChange();
		changePriceDiv();
	});
</script>


