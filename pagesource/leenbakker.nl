<script type="text/javascript">

	// Added function for email validation prior reservation submit for mantis #1102
	function prepareSubmit(form){
		reWhiteSpace = new RegExp(/^\s+$/);
		var email = form['email'];
		email.value = trim(email.value);
		var emailval = email.value;
		
		/** NR-802: Commented the name field in the reservation box as requested in the ticket:
		var name = form['name'];
		var nameVal = trim(name.value);
		if(nameVal == "" || reWhiteSpace.test(nameVal)){
			MessageHelper.formErrorHandleClient(name.id, MessageHelper.messages["ERROR_NameEmpty"]);
			return;
		}		
		**/	
		if(emailval == "" || reWhiteSpace.test(emailval)){
			MessageHelper.formErrorHandleClient(email.id, MessageHelper.messages["ERROR_EmailEmpty"]);
			return;
		}
		if(!MessageHelper.isValidUTF8length(emailval, 256)){ 
			MessageHelper.formErrorHandleClient(email.id, MessageHelper.messages["ERROR_EmailTooLong"]);
			return;
		}
		if(!MessageHelper.isValidEmail(emailval)){
			MessageHelper.formErrorHandleClient(email.id, MessageHelper.messages["ERROR_INVALIDEMAILFORMAT"]);
			return;
		}
		shoppingActionsJS.sendPreOrderConfEmail();
		submitReservationForm();
	}

	function submitReservationForm() {
		jQuery("#reservation_form").submit();
	}

	jQuery("#reservation_form").submit(function(event) {
		/* stop form from submitting normally */
		event.preventDefault();

		

		/* get some values from elements on the page: */
		var $form = jQuery( this ),
			_r_name = $form.find( 'input[name="name"]' ).val(),
			_r_email = $form.find( 'input[name="email"]' ).val(),
			_r_phone = $form.find( 'input[name="phone"]' ).val(),
			_r_erp = $form.find( 'input[name="erp"]' ).val(),
			_r_catentryid = $form.find( 'input[name="catentryid"]' ).val(),
			_r_releasedate = $form.find( 'input[name="releasedate"]' ).val(),
			_r_storeid = $form.find('input[name="storeId"]').val(),
			_r_langid = $form.find('input[name="langId"]').val(),
			_r_productname = $form.find('input[name="productname"]').val(),
			url = $form.attr( 'action' );

		var _r_ok = true;
			
		jQuery("#reservation_name_validation").hide();
		jQuery("#reservation_email_validation").hide();
        /** NR-802: No need for name field in the reservation pop up:
		if (_r_name == '') {
			jQuery("#reservation_name_validation").show();
			_r_ok = false;
		}	
		**/	
		if (_r_email == '') {
			jQuery("#reservation_email_validation").show();
			_r_ok = false;
		}	
		
		if (_r_ok) {
			/* Send the data using post */
			var posting = jQuery.post( url, { 
				'name': _r_name, 
				'email': _r_email, 
				'phone': _r_phone, 
				'erp': _r_erp, 
				'catentryid': _r_catentryid, 
				'releasedate': _r_releasedate,
				'storeId': _r_storeid, 
				'langId':_r_langid,
				'productname': _r_productname 
			} );
			
			/* Put the results in a div */
			posting.done(function( data ) {
				jQuery("#reservation_element").hide();
				jQuery("#reservation_ok").show();
				
				// Send data to Relay42
				if ('true' == 'true' && typeof (reservationProductData) != 'undefined') {
					_st('publishEvent', 'Reserveer', reservationCompleteEvent, reservationProductData);
				}
			});
		}
	});

	function closeReservation() {
		dijit.byId('reservation_container').hide(); 
	}
	
	var reservationCompleteEvent;
	var reservationProductData;
	
	function showReservation(erp, catentryid, releasedate, productname, source) {
		jQuery("#reservation_erp").val(erp);
		jQuery("#reservation_catentryid").val(catentryid);
		jQuery("#reservation_releasedate").val(releasedate);
		jQuery("#reservation_productname").val(productname);

		jQuery("#reservation_name_validation").hide();
		jQuery("#reservation_email_validation").hide();
		jQuery("#reservation_element").show();
		jQuery("#reservation_ok").hide();
		
		dijit.byId('reservation_container').show();

		// Send data to Relay42
		if ('true' == 'true' && document.getElementById("productsUA_"+catentryid)) {
			var tempProductStr = document.getElementById("productsUA_"+catentryid).value;
			var tempProduct = tempProductStr.split("-||-");
			var productData = {'name': tempProduct[0].trim(), 'id': tempProduct[1], 'price': tempProduct[2],
			   	   			   'brand': tempProduct[3], 'category': tempProduct[4]};
			reservationProductData = productData;

			var src = "";
			if (document.getElementById('listViewAdd2Reserve_'+catentryid)) {
				src = document.getElementById('listViewAdd2Reserve_'+catentryid).getAttribute("data-list");
			}
			
			if (source == 'Productdetailpage') {
				_st('publishEvent', 'Reserveer', 'Start Productdetail Page', productData);
				reservationCompleteEvent = 'Complete Productdetail Page';
			} else if (source == 'Categorie' || src == 'Categorie' || tempProduct[4].indexOf('Categorie') !== -1) {
				_st('publishEvent', 'Reserveer', 'Start Category Page', productData);
				reservationCompleteEvent = 'Complete Category Page';
			} else if (source == 'Zoek resultaat' || src == 'Zoek resultaat' || tempProduct[4].indexOf('Zoek resultaat') !== -1) {
				_st('publishEvent', 'Reserveer', 'Start Search Page', productData);
				reservationCompleteEvent = 'Complete Search Page';
			} else {
				_st('publishEvent', 'Reserveer', 'Start eSpot', productData);
				reservationCompleteEvent = 'Complete eSpot';
			}
		}
	}

</script>
