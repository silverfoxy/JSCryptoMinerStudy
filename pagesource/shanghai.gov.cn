<script>
$("#owl-homeSlider").owlCarousel({
      navigation : true,
      slideSpeed : 300,
      paginationSpeed : 400,
      autoPlay: 5000,
      singleItem : true
});

        function clearcollapse($obj, $this) {
            if ($this == null) {
                $obj.each(function () {
                    var $target = $($(this).attr("data-target"));
                    if ($target.hasClass("in")) {
                        var $this0 = $(this);
                        outcollapse($this0);
                    }
                });
            }
            else {
                var _target=$this.attr("data-target");
                $obj.not($("[data-target="+_target+"]")).each(function () {
                    var $target = $($(this).attr("data-target"));
                    if ($target.hasClass("in")) {
                        var $this0 = $(this);
                        outcollapse($this0);
                    }
                });
            }
        }
        function incollapse($this) {
            var href
      , target = $this.attr('data-target')
        || (href = $this.attr('href')) && href.replace(/.*(?=#[^\s]+$)/, '') //strip for ie7
            $this['removeClass']('collapsed')
            $(target).collapse("show")
            $this.parent().addClass("active");
            if(target!='#collapseOne')
              $(".friendLink").addClass("IE7Hidden");
            else
              $(".friendLink").removeClass("IE7Hidden");
        }
        function outcollapse($this) {
           var href
      , target = $this.attr('data-target')
        || (href = $this.attr('href')) && href.replace(/.*(?=#[^\s]+$)/, '') //strip for ie7
            $this['addClass']('collapsed')
            $(target).collapse("hide")
            $this.parent().removeClass("active");
        }
        function bindcollapseover($obj) {
            $obj.hover(function () {
                var $this = $(this);
                clearcollapse($obj, $this);
                incollapse($this);
            }, function () {
            });
        }
        $(function () {
            var $obj0 = $(".accordion-toggle[data-parent='#accordion2']");
            bindcollapseover($obj0);
            $("li.accordion-group>a").focus(function () {
                var $this = $(this).find("div").first();
                clearcollapse($obj0, $this);
                incollapse($this);
            });
            $("ul#accordion2").mouseleave(function () {
                clearcollapse($(".accordion-toggle[data-parent='#accordion2']"), null);
                $(".friendLink").removeClass("IE7Hidden");
            });
        });
</script>
