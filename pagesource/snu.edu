<script type="text/javascript" defer="defer">
//<![CDATA[
var SlideShow6346188initilized = false;
var SlideShow6346188 = new function() { };
LCMS.jq.extend(SlideShow6346188, slideShowClass);
//initialize the containers to that hold the previous, current and next images.
SlideShow6346188.autoTransitionWait = 10;
SlideShow6346188.container = LCMS.jq("#feature_7_7_ctl00_SlideShowImageContent");
SlideShow6346188.currentContentContainer = LCMS.jq("#feature_7_7_ctl00_current");
SlideShow6346188.nextContentContainer = LCMS.jq("#feature_7_7_ctl00_next");
SlideShow6346188.previousContentContainer = LCMS.jq("#feature_7_7_ctl00_previous");
SlideShow6346188.selectedContentContainer = LCMS.jq("#feature_7_7_ctl00_selected");
//initialization function to set up the slideshow.  Once initialized the js will start the timer.

//set the height and the width of the container that contains the image and the caption so that the caption appears attached to the image
SlideShow6346188.resizeCaptionContainer = function() {
    var $img = LCMS.jq(this);
	$img.parents('.captionContainer').eq(0).width($img[0].width).height($img[0].height);
};

SlideShow6346188.setContainers = function(current_container) {
	//set up the containers for the next transition
	if (current_container === undefined) current_container = 'index';
	
	switch (current_container) {
		case 'next':
			SlideShow6346188.currentContentContainer.html(SlideShow6346188.nextContentContainer.html());
		break;
		case 'prev':
			SlideShow6346188.currentContentContainer.html(SlideShow6346188.previousContentContainer.html());
		break;
        case 'selected':
			SlideShow6346188.currentContentContainer.html(SlideShow6346188.selectedContentContainer.html());
		break;
		default:
			SlideShow6346188.currentContentContainer.html(SlideShow6346188.contents[SlideShow6346188.currentContentIndex]);
	}
    SlideShow6346188.nextContentContainer.html(SlideShow6346188.contents[SlideShow6346188.NextContentIndex(SlideShow6346188)]);
    SlideShow6346188.previousContentContainer.html(SlideShow6346188.contents[SlideShow6346188.PreviousContentIndex(SlideShow6346188)]);
};

SlideShow6346188.updateIndexes = function () {
    var currentIndex = SlideShow6346188.currentContentIndex;
    var photoCount = SlideShow6346188.container.parent().find('#SlideShow6346188IndexNavigation').data('photo-count');

    SlideShow6346188.container.parent().find('.sldw_photo_index.selected').removeClass('selected');
    SlideShow6346188.container.parent().find('.sldw_photo_index[data-index=' + currentIndex + ']').addClass('selected');
   
    if (photoCount > 7) {
        var maxIndex = SlideShow6346188.CalcMaxIndex(photoCount);
        var minIndex = SlideShow6346188.CalcMinIndex(photoCount);
        SlideShow6346188.container.parent().find('.sldw_photo_index').each(function() {
            var $li = LCMS.jq(this);
            var photoIndex = parseInt($li.data('index'));
            if ((photoIndex < minIndex) || (photoIndex > maxIndex)) {
                $li.hide();
            }
            else {
                $li.show();
            }
            
            if (photoIndex === maxIndex && maxIndex !== (photoCount - 1)) {
                $li.find('a').text('...');
            }
            else if (photoIndex === minIndex && minIndex !== 0) {
                $li.find('a').text('...');
            }
            else {
                $li.find('a').text((photoIndex + 1));
            }
        });
    }
};

SlideShow6346188.InitializeTransition = function()
{
    if (SlideShow6346188initilized) return;
    SlideShow6346188initilized = true;
	//load the containers based on the currentContentIndex
    SlideShow6346188.setContainers();
    
    SlideShow6346188.container.append(SlideShow6346188.currentContentContainer.html());
    
    
    	//set the slideshow container to twice the width of the parent so that it can hold the hidden transition slides
        SlideShow6346188.container.width(SlideShow6346188.container.parent().parent().width() * 2);
        SlideShow6346188.container.children('.SlideShowImage').width(SlideShow6346188.container.parent().parent().width()).css('float', 'left');
    
    	
    	var current_caption_container = SlideShow6346188.container.children('.SlideShowImage').first().children('.captionContainer')

        //set the captionbox to resize on the image load event
        if (current_caption_container.children('img').length > 0) {
			current_caption_container.children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6346188.resizeCaptionContainer);
        } else {
        	current_caption_container.children().children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6346188.resizeCaptionContainer);
        }
}

// Perform transition is run after the timer expires or when the next/prev arrows are clicke
SlideShow6346188.PerformTransition = function(trans_dir) 
{
	if (SlideShow6346188.transitioning == true) return;    
    var is_fwd = (trans_dir === 'next');
    var is_selected_index = (trans_dir === 'selected');
    var next_img = '';

    if (!is_selected_index) {
        // next_img only references the correct element before setContainers is called!
        next_img = is_fwd ? SlideShow6346188.nextContentContainer : SlideShow6346188.previousContentContainer;
    } else {
        SlideShow6346188.selectedContentContainer.html(SlideShow6346188.contents[SlideShow6346188.currentContentIndex]);
        next_img = SlideShow6346188.selectedContentContainer;
    }

    var next_caption_container = next_img.find('.captionContainer');
	if (next_caption_container.children('img').length > 0) {
		next_caption_container.children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6346188.resizeCaptionContainer);
	} else {
		next_caption_container.children().children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6346188.resizeCaptionContainer);
	}

    SlideShow6346188.updateIndexes();
    
    
    	
    	
    	SlideShow6346188.transitioning = true;
    	
    	if (is_fwd || is_selected_index) {
	    	//Set the sliding transition. append the next slide to the current container
	    	SlideShow6346188.container.append(next_img.children(":first").clone(true));	
    	} else {
	    	//Set the margin of the previous contents to negative of the parent container width so it can slide in from the right.
	    	next_img.children('.SlideShowImage').css('margin-left', -SlideShow6346188.container.parent().parent().width())
	    	//prepend the next image in front of the current image
	    	SlideShow6346188.container.prepend(next_img.children(":first").clone(true));
    	}
    	
        
        //make sure the next slide is visible
        SlideShow6346188.container.children('.SlideShowImage').width(SlideShow6346188.container.parent().parent().width()).css('float', 'left');    
        
        if (!is_fwd && !is_selected_index) {
	        //since we have now prepended the image we want to the current container we set the margin of the previous container back to 0
	        next_img.children('.SlideShowImage').css('margin-left', '0px')
        }
        
        // first_div is the current slide for 'next' and the new slide for 'prev'
        var first_div = SlideShow6346188.container.children('.SlideShowImage').eq(0), 
        	second_div = SlideShow6346188.container.children('.SlideShowImage').eq(1);
        
        
        // if 'next' transition, animate the transition by setting the left margin to the negative width of the parent container and animating it
        first_div.animate(
            {marginLeft: (is_fwd || is_selected_index ? -SlideShow6346188.container.parent().parent().width() + 'px' : '0px')}, 
            (0.8 * 1000), 
            'easeInOutQuint',
            function() {
				// Remove the old div which had the current image. It is no longer displayed.
                if (is_fwd || is_selected_index) {
	            	first_div.remove();    
                } else {
	                second_div.remove();
                }
                
                SlideShow6346188.transitioning = false;
            });
        SlideShow6346188.setContainers(trans_dir);
          
            
    
}

function nextSlide6346188() {
	SlideShow6346188.MoveToNextSlide(SlideShow6346188, 'SlideShow6346188');
}
function previousSlide6346188() {
	SlideShow6346188.MoveToPreviousSlide(SlideShow6346188, 'SlideShow6346188');
}
function goToSlide6346188(index) {
    SlideShow6346188.MoveToSlide(SlideShow6346188, index);
}

