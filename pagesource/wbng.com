var titan_url = encodeURI("http://webservices.titantvguide.titantv.com/contentblocks.asmx/TonightOn?registrationKey=R6SC%2FvlaDV3b0YPbOm%2F21pRwAnRBezJS");

function titanInit(){
	titanParse(this);
}

function titanParse(xml){
	var fullFeed = xml.response.responseXML;
	var numShows = $(fullFeed).find("Show").length;
	var showHTML = "";
	var zTooltipText = "";
	for(var ii=0; ii < numShows; ii++){
		var $el = $(fullFeed).find("Show:eq("+ ii +")");
		var sT = $el.attr("adjustedStartTime").split(":");
		var amPm = "AM"
		if(sT[0] > 12){
			sT[0] = sT[0] - 12;
			amPm = "PM";
		}else if(sT[0] == 12){
			amPm = "PM";
		}else if(sT[0] == 0){
			sT[0] = 12;
		}
		var desc = $el.find("Description").text();
		if (desc == ""){
			desc = "No description.";
		}
		showHTML += "<div class='titantv_row'><div class='titantv_time'><div class='titantv_time_text'>"+ sT[0]+":"+sT[1]+amPm +"</div></div><div class='titantv_show'><div class='titantv_show_text' rel='"+ desc +"'>"+ $el.find("Title").text() +"</div></div><div style='clear:both;'></div><div class='zText'>" + desc + "</div></div>";
	}
	$(".titantv_bg").append(showHTML);
	titanRollover();
}

function titanRollover(){
	var itemNum = $(".titantv_show").length;
	
	for(cc=0;cc<itemNum;cc++){
		$(".titantv_show:eq("+cc+")").hover(
			function(e){
				//$(this).css("cursor","pointer");
				var pos = $(this).offset();
				$("#tooltip").html($(this).parent().find(".zText").html()).css({
					"display" : "block",
					"top" : (pos.top-200)+"px",
					"left" : "90px"
				});				
			},
			function(e){
				//$(this).css("cursor","default");
				$("#tooltip").css("display","none");
			}
		);
	}
}

function titanError(){
}

$(document).ready(function(){
	WNHttpRequestManager.makeRequest(titan_url, {
		onSuccess: titanInit,
		onError: titanError
	});
});
