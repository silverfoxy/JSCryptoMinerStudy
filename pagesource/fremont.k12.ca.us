	function quickLinksMenu() {
		// IF A SITE SHORTCUTS APP EXISTS
		if( $(".sp").length > 0 ){
			//we direct the ajax call to the sites homepage and if the server finds our request valid it will return the data to our success callback function so we can process it as we wish.
			$.ajax("//" + window.location.hostname + "/fusd", {
				success: function(data){
					//we then pass that data into a jQuery object so it will become searchable.
						//next we search through that jQuery object to find the app area we want to grab the HTML from
							//after we have the HTML we append it to the quicklinks bar on the subpage 
					$('#gb-header-outer .gb-quick-links').html($(data).find("#gb-header-outer .gb-quick-links").html()); 
					
					//now that we have the HTML in place and on the page we call our initializing function to setup click events and do all of its magic.
					quickLinksInit();
				}
			});
		}else{
			quickLinksInit();
		}
	}
	
	function quickLinksInit(){
		if($(".gb-quick-links-outer .ui-widget.app.siteshortcuts").length) {
			// BIND THE TAB CLICK EVENT
			$(".gb-quick-links-tab").click(function(e) {
				e.preventDefault();
				
				// IF QUICKLINKS MENU IS CLOSED
				if($(".gb-quick-links-outer").hasClass("closed")) {

					$("#gb-slide").animate({
					  right: "-=245px"
					}, function() {
						// REMOVE CLOSED CLASS AND ADD OPEN CLASS
						$(this).removeClass("closed").addClass("open");
					});

					// ANIMATE THE QUICKLINKS MENU OPEN
					$(".gb-quick-links-outer").animate({
						left: "0px"
					}, function() {
						// REMOVE CLOSED CLASS AND ADD OPEN CLASS
						$(this).removeClass("closed").addClass("open");
					});
				}
				
				// IF QUICKLINKS MENU IS OPEN
				else {
					$("#gb-slide").animate({
						right: "+=245px"
					}, function() {
						// REMOVE OPEN CLASS AND ADD CLOSED CLASS
						$(this).removeClass("open").addClass("closed");
					});
					
					// ANIMATE THE QUICKLINKS MENU CLOSED
					$(".gb-quick-links-outer").animate({
						left: "-245px"
					}, function() {
						// REMOVE OPEN CLASS AND ADD CLOSED CLASS
						$(this).removeClass("open").addClass("closed");
					});
				}
			});
			
			// SHOW THE MENU
			$(".gb-quick-links-outer").removeClass("hidden");
		}
	}
	
	
