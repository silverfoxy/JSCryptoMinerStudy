	function homepageFeatureHeights() {
		var featureItemH = 0;
		
		// GET THE HEIGHT OF THE TALLEST ITEM
		$(".hp.section.one .hp-feature-item").each(function() {
			if($(this).outerHeight(true) > featureItemH) {
				featureItemH = $(this).outerHeight(true);
			}
		});
		
		// APPLY SAME HEIGHT TO ALL ITEMS
		$(".hp.section.one .hp-feature-item").css("height", featureItemH + "px");
		$(".ui-widget.app.cs-rs-multimedia-rotator").parent().css({
			"background" : "#17438E",
			"color" : "#FFF"
			});
		
		// BIND RESIZE EVENT
		$(".hp.section.one .hp-feature-item").resize(function() {
			var screenWidth = $(window).width();
			if (screenWidth >= 640) { // 960 - 640
				$(".hp.section.one .hp-feature-item").css("height", "auto");
				featureItemH = 0;
				
				// GET THE HEIGHT OF THE TALLEST ITEM
				$(".hp.section.one .hp-feature-item").each(function() {
					if($(this).outerHeight(true) > featureItemH) {
						featureItemH = $(this).outerHeight(true);
					}
				});
				
				// APPLY SAME HEIGHT TO ALL ITEMS
				$(".hp.section.one .hp-feature-item").css("height", featureItemH + "px");
				
				featureItemHeightRemove = 0;
			}
			
			if (screenWidth < 640) { // 480-
				if(!featureItemHeightRemove) {
					$(".hp.section.one .hp-feature-item").css("height", "auto");
					featureItemHeightRemove = 1;
				}
			}
		});
	}
	
