  function TrackAddToCart() {
    
    $('.ajax_addtocart').click(function() {
      	var line_items = [];
      	var product_ids;
      	var product_names;
      	var pricevalue = 0;
      
  		jQuery.getJSON('/cart.js', function(cart) {
