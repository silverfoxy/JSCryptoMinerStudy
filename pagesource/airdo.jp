<script type="text/javascript">
    var date_after_75 = moment().add(75, 'days').format('YYYY-MM-DD');
    var date_after_3 = moment().add(3, 'days').format('YYYY-MM-DD');
    
    function hrefAddDepartureDate() {
        $('.rt-airdospecial75, .rt-dovalue3').each(function(){
            var date;
            var href;
            if ($(this).hasClass('rt-airdospecial75')) {
                date = date_after_75;
            } else if ($(this).hasClass('rt-dovalue3')) {
                date = date_after_3;
            }
            var href = $(this).attr('href');
            if (href.indexOf('departureDate') == -1) {
                $(this).attr('href', href + '&departureDate=' + date);
            }
        });
    }
    hrefAddDepartureDate();
    
    function ahpPlanAttention() {
        $('#main .slider-ticket p.text-center').eq(0).prepend('<span class="fs-sm">※2018年2月15日現在、2018年5月ご搭乗分の運賃例です。</span><br>');
        $('#main .slider-ticket p.text-center').eq(1).prepend('<span class="fs-sm">※2018年2月15日現在、2018年3月ご搭乗分の運賃例です。</span><br>');
    }
    
    if (typeof(Rtoaster) === 'object') {
        Rtoaster.callback = function(e) {
            //console.log(e);
            if (e === 'recommend-ahp-plan') {
                if (windw <= 767) {
                    if(!$('#main .slider-ticket').hasClass('slick-initialized')) {
                        $('#main .slider-ticket').slick({
                            infinite: false,
                            dots: true,
                            centerMode: true,
                            centerPadding: '50px',
                            slidesToShow: 1,
                            slidesToScroll: 1
                        });
                    }
                }
                hrefAddDepartureDate();
                ahpPlanAttention();
            }
        }
        Rtoaster.recommendNow('recommend-ahp-content', 'recommend-ahp-plan');
    }
    
    $('#main .slider-ticket').each(function(){
        if(!$('#main .slider-ticket p.text-center span.fs-sm').length) {
            ahpPlanAttention();
        }
    });
    

</script>



