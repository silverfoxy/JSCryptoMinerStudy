				$(this).siblings(cut).find('.cut').show();
				$(this).siblings(cut).find('.tridot').hide();
				$(this).siblings(cut).find('.preview').hide();
				$(this).css('display', 'none');	
			});

			$('.ts-prev, .ts-next, .steps a').click(function(){
				cut.find('.cut').hide();
				cut.find('.tridot').show();
				cut.find('.preview').show();
				$('.js_expand').css('display', 'block');
			}); 

			//prev & next opacity change
			var timeout = null;

			$('.ts-prev, .ts-next').click(function(){
				$(this).css('opacity', '0.6');

				clearTimeout(timeout);
				timeout = setTimeout(function(){ $('.ts-prev, .ts-next').css('opacity', ''); }, 200);
			});
		});
	</script>
