	if ( window.universal_variable.page.type == "checkout" ) {

		document.addEventListener('DOMContentLoaded', function() {

			var empEmailListGBG 	= ["globalbrandsgroup.com", "bcbg.com"];
			var couponListGBG 		= ["bf71b3e1a8bf442d073cab6cbe89e455", "41f5a39c90a3abee38b4ae81daca5f2a"];

			var empEmailListMARQEE 	= ["marqueebrands.com", "bodyglove.com"];
			var couponListMARQEE 	= ["3423ad54d668d7387f86c68cf3e75a3c", "d09d0ae13d8d8a6720f2c444d67331db"];

			// when billing page refreshed
			if (window.location.hash == '#billing') {
				getEmailCoupon();
			}

			// when email onchanged
			$(document).on('change', '#dwfrm_billing_billingAddress_email_emailAddress', function() {
				getEmailCoupon();
			});

			// when page loaded by ajax call
			$(document).ajaxSuccess(function(e, xhr) {

				// when coupon code updated
				if (xhr.responseJSON && xhr.responseJSON.CouponCode) {
					$(document).ajaxStop(function() {
						getEmailCoupon();
					});
				}

				// when coupon code removed
				if (xhr.responseJSON && xhr.responseJSON.message && !xhr.responseJSON.CouponCode) {
					$(document).ajaxStop(function() {
						getEmailCoupon();
					});
				}

				// when ajax called billing page
				if (xhr.responseJSON && xhr.responseJSON.step) {

					if ( xhr.responseJSON.step.indexOf('billing') > -1 ) {
						getEmailCoupon();
					}

				}


			});


			function getEmailCoupon() {
				$("#dwfrm_billing_billingAddress_email_GBG_emailAddress-error").remove();
				$("div.form-row.email").after("<div id='dwfrm_billing_billingAddress_email_GBG_emailAddress-error' class='error' style='float: left; display:none;'>You must use a valid email address to use this promo code.</div>");

				var couponCode = "";

				if ( window.universal_variable.user.ship_to != "United States" ) {
					couponCode = window.universal_variable.basket.vouchers[0];
				}else{
					couponCode = $(".appliedCoupon div.discount span.label").text();
				}

				var emailAddr = $("#dwfrm_billing_billingAddress_email_emailAddress").val();

				if (emailAddr && couponCode) {
					validEmailCoupon(emailAddr, couponCode);
				}else{
					$('#dwfrm_billing_billingAddress_email_GBG_emailAddress-error').css("display", "none");
					$('.billing-buttons button.checkout-submit').removeAttr('disabled');
					$('.payment-method-options input, .payment-method input, .payment-method select').removeAttr('disabled');
				}

			}


			function validEmailCoupon(userEmail, codeUsed) {

				var empEmailList = "";
				var couponList = "";
				var empEmail = userEmail.split('@').pop().trim();
				var couponUsed = md5(codeUsed.trim().toUpperCase());

				if ( empEmailListGBG.indexOf(empEmail) > -1 ) {
					empEmailList = empEmailListGBG;
					couponList = couponListGBG;
				}else if ( empEmailListMARQEE.indexOf(empEmail) > -1 ) {
					empEmailList = empEmailListMARQEE;
					couponList = couponListMARQEE;
				}

				if ( couponListGBG.indexOf(couponUsed) > -1 || couponListMARQEE.indexOf(couponUsed) > -1 ) {

					if ( couponList.indexOf(couponUsed) > -1 && empEmailList.indexOf(empEmail) > -1 ) {
						$('#dwfrm_billing_billingAddress_email_GBG_emailAddress-error').css("display", "none");
						$('.billing-buttons button.checkout-submit').removeAttr('disabled');
						$('.payment-method-options input, .payment-method input, .payment-method select').removeAttr('disabled');
					}else{
						$('#dwfrm_billing_billingAddress_email_GBG_emailAddress-error').css("display", "block");
						$('.billing-buttons button.checkout-submit').attr('disabled','disabled');
						$('.payment-method-options input, .payment-method input, .payment-method select').attr('disabled','disabled');
					}
				}
				
			}

		}, false);

	}

</script>
