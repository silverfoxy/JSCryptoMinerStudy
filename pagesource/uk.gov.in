                                            <script type="text/javascript">
                                                 $(document).ready(function() {
                                                     $("#marquee1").marquee();
                                                 });
                                                 function pause()
                                                 {
                                                     $('#marquee1').marquee('pause');
                                                     $('#marquee_stop').hide();
                                                     $('#marquee_resume').show();
                                                     $('#marquee1 li').addClass('stoped');
                                                     document.getElementById("marquee1").style.overflow = "scroll";
                                                     document.getElementById("marquee1").style.padding = '0 0 12px 0';
                                                 }
                                                 function play()
                                                 {
                                                     $('#marquee1').marquee('resume');
                                                     $('#marquee_resume').hide();
                                                     $('#marquee_stop').show();
                                                     $('#marquee1 li').removeClass('stoped');
                                                     document.getElementById("marquee1").style.overflow = "hidden";
                                                     document.getElementById("marquee1").style.padding = '0 0';
                                                 }

                                                 $(function() {
                                                     $("ul.allul li:nth-child(even)").addClass("striped");
                                                     $("#leftColumn li:nth-child(even)").addClass("striped");
                                                     $("#rightColumn li:nth-child(even)").addClass("striped");
                                                 });
                                            </script>
                                            <script>
                                                $('.fadeinmain img:gt(0)').hide();
                                                setTimeout(Rotate, 5000);

                                                function Rotate() {
                                                    $('.fadeinmain :first-child').fadeOut()
                                                            .next('img').fadeIn()
                                                            .end().appendTo('.fadeinmain');
                                                    setTimeout(Rotate, 5000);
                                                }
                                            </script>


