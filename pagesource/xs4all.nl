                            <form action="/reportabug/" method="post" enctype="multipart/form-data" id="reportabug-form" novalidate="novalidate">
    <fieldset>
        <legend><span class="sr-only">XS4ALL Website - Bug report</span></legend>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="field1" class="is-required">
                    Uw naam
                </label>
                <input id="field1" name="name" type="text" class="form-control" tabindex="1">
            </div>
            <div class="form-group col-md-6">
                <label for="field2" class="is-required">
                    Uw e-mailadres
                </label>
                <input id="field2" name="email" type="text" class="form-control" tabindex="2">
            </div>
        </div>

        <div class="form-group">
            <label for="field3">Samenvatting</label>
            <input id="field3" name="samenvatting" type="text" class="form-control" tabindex="3">
        </div>

        <div class="form-group">
            <label for="field4" class="is-required">Bug type</label>
            <div class="selectstyle">
                <select tabindex="4" name="bugtype" id="bugtype" class="custom-select form-control">
                    <option value="">Selecteer een bug categorie</option>
                    <option value="typefout">Typefout(en)</option>
                    <option value="doodlopende link">Doodlopende links (404's)</option>
                    <option value="productinformatie">Foutieve productinformatie</option>
                    <option value="klantinstructies">Foutieve klantinstructies</option>
                </select>
            </div>
        </div>

        <div class="form-group">
            <label for="field5">URL waar de bug zich voordoet</label>
            <input id="field5" name="bugurl" type="text" class="form-control" tabindex="5">
        </div>

        <div class="form-group">
            <label for="field6" class="is-required">Wat is het probleem?</label>
            <textarea id="field6" name="watishetprobleem" class="form-control large" tabindex="6" placeholder="voorbeeld: foutieve prijzen Zenderpakketten"></textarea>
        </div>

        <div class="form-group">
            <label for="field7" class="is-required">Waarom is het een probleem?</label>
            <textarea id="field7" name="waaromishetprobleem" class="form-control large" tabindex="7" placeholder="voorbeeld: levert 10 klantenservice telefoontjes per dag"></textarea>
        </div>

