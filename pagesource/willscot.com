            if (event.keyCode == 13) {
                if ($('#txtProductSearch').val() !== "" && $('#txtProductSearch').val() !== "Search Site") {
                    Search();
                    return false;
                }
            }
        });

        $('#btnProductSearch').click(function () {
            if ($('#txtProductSearch').val() !== "" && $('#txtProductSearch').val() !== "Search Site") {
                Search();

            }

        });
        ModifyMobileCartVisibility();   
        
    });

    function Search() {
        //window.location.href = updateQueryStringParameter("/Product-List", "search", $('#txtProductSearch').val());
        window.location.href = updateQueryStringParameter("/Search-Results", "search", $('#txtProductSearch').val());
    }

    function updateQueryStringParameter(uri, key, value) {
        var re = new RegExp("([?|&])" + key + "=.*?(&|$)", "i");
        separator = uri.indexOf('?') !== -1 ? "&" : "?";

        if (uri.match(re)) {
            return uri.replace(re, '$1' + key + "=" + value + '$2');
        }
        else {
            return uri + separator + key + "=" + value;
        }
    }

</script>
