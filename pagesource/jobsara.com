   function valform(arg)
{ 
with(arg)
	{ 
         if(name.value=='')
		{
				alert('Please Enter Name');
				name.focus();
				return false;
		}
                if(email.value=='')
		{
				alert('Please Enter Email Address');
				email.focus();
				return false;
		}
                if(email.value != '') { 
		var emailRegex = new RegExp(/^([\w\.\-]+)@([\w\-]+)((\.(\w){2,3})+)$/i);
		var valid = emailRegex.test(email.value);
		if(!valid){
		alert('Please Enter Valid Email Address');
				email.focus();
				return false;
                }
            }
            
            if(mobile.value=='')
		{
				alert('Please Enter Mobile Number');
				mobile.focus();
				return false;
		}
                var phoneno = /^\d{6,12}$/; 
             if(!mobile.value.match(phoneno)) {
				alert('Please Enter Mobile Number');
                                mobile.focus();
				return false;
			}   
         
          if(comment.value=='')
		{
				alert('Please Enter Message');
				comment.focus();
				return false;
		}
           
            
            return true;
          }
  
    }
    
    function submitdata(){
      
    
       if(valform(document.contactform)){ 

	$.ajax({

			  url : 'Contact/insert',

			  type : 'post',

			  data : $('#contactform').serialize(),

			  dataType : 'html',

			  success : function (data) {
                              
                                if(data == 1){  $('#showmsg').addClass('showsucces');
                                $('#showmsg').html('Message Send Sucessfully');
                                $("#contactform")[0].reset();
                                }else{ $('#showmsg').addClass('showerror');
                                   $('#showmsg').html('Error Please try Again!');  
                                } 

			  },
        
           }); 
       }
    };

