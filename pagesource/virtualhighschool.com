.video-js {
	width: 100%;
	height: 100%;
	box-sizing: border-box;
	background-color: lightgrey;
	padding: 0;
	position: absolute;
	top: 0px;
	left: 0px;
	bottom: 0px;
	right: 0px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	vertical-align: middle;
	display: inline-flex;
	display: -webkit-inline-flex;
	border: 1px solid lightgrey;
}
.IE .video-js {
	height: 99.7%;
}
.video-js .vjs-tech {
	position: absolute;
	top: 0;
	bottom: 0;
	right: 0;
	left: 0;
	height: 100%;
	width: 100%;
	cursor: pointer;
	box-sizing: border-box;
}
/* Full Screen Stuff  ------------------------------------------------------------------  */

.vjs-fullscreen {
	width: 100%;
	height: 100%;
	background-color: black;
	display: -webkit-flex;
	display: flex;
	position: relative;
	border: none;
}
.video-js.vjs-fullscreen.vjs-user-inactive {
	cursor: none;
}
.vjs-fullscreen.video-js {
	margin: auto;
}
.vjs-fullscreen .vjs-tech {
	width: 100%;
	height: auto;
	max-width: 1280px;
	vertical-align: middle;
	margin: auto;
	border: none;
}
body.vjs-full-window {
	height: 100%;
	margin: 0;
	overflow-y: auto;
	padding: 0;
	background-color: black;
}
.vjs-poster {
	background-repeat: no-repeat;
	background-size: cover;
	cursor: pointer;
	height: 100%;
	margin: 0px;
	padding: 0px;
	position: absolute;
	width: 100%;
	top: 0px;
	left: 0px;
}
.vjs-playing .vjs-poster {
	display: none !important;
}
.vjs-playing .vjs-big-play-button {
	display: none;
}
.vjs-waiting .vjs-tech, .vjs-seeking .vjs-tech {
	visibility: hidden;
}
.vjs-waiting.video-js {
	background: black;
}
.vjs-poster img {
	display: block;
	margin: 0 auto;
	max-height: 100%;
	padding: 0;
	width: 100%;
}
.video-js.vjs-using-native-controls .vjs-poster {
	display: none;
}
/* VJS Player in pop-up window*/

.video-window {
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: .85em;
	width: 100%;
	height: 100%;
	display: inline-flex;
	margin: auto;
}
.video-window .video-js {
	height: 100%;
	background-color: black;
	border: none;
}
.video-window .vjs-tech {
	max-width: 1280px;
	min-height: 720px;
	display: block;
	margin: auto;
	min-width: 640px;
	min-height: 360px;
	border: none;
}
/* VJS player in Colorbox*/

#colorbox .vjs-tech {
	border: none;
}
#cboxContent {
	background: black;
}
#cboxLoadedContent {
	margin: 0px !important;
}
/* YouTube Videos*/

.youtube {
	background: none;
}
.video .yt-player {
	width: 100%;
	height: 100%;
}
.yt-container {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 99%;
	border: 1px solid lightgrey;
}
/* VJS in Tabbed Pannels*/

.TabbedPanelsContentVisible .video-js {
	height: 100%;
}
.Firefox .TabbedPanelsContentVisible .video-js {
	height: 100.1%;
}
/* LOADING SPINNER*/


.spinner {
	position: absolute;
	left: 0px;
	right: 0px;
	top: 0px;
	bottom: 0px;
	margin: auto;
	width: 45px;
	height: 60px;
}
.spinner > div {
	background-color: grey;
	height: 100%;
	width: 6px;
	display: inline-block;
	margin: 0px 3px 0px 0px;
	-webkit-animation: stretchdelay 1s infinite ease-in-out;
	animation: stretchdelay 1s infinite ease-in-out;
}
.video-js + .spinner {
	display: none;
}
.video-list .video .spinner:only-child {
	display: block !important
}
.video-list .video .spinner:first-child {
	display: none
}
.spinner .rect2 {
	-webkit-animation-delay: -0.9s;
	animation-delay: -0.9s;
}
.spinner .rect3 {
	-webkit-animation-delay: -0.8s;
	animation-delay: -0.8s;
}
.spinner .rect4 {
	-webkit-animation-delay: -0.7s;
	animation-delay: -0.7s;
}
.spinner .rect5 {
	-webkit-animation-delay: -0.6s;
	animation-delay: -0.6s;
	margin-right: 0px;
}
