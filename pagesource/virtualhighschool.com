<!-- Track Logins --> 

<script>
var count = 0;

setupLoginListener(count);

function setupLoginListener(x){
	
	if (x < 10){
		x++;
	} else {
		//console.log("x is > 10");
		return;	
	}
	
	//get the form
	var form = document.getElementById('processLogonForm');
	
	if (typeof form == 'undefined' || form == null){
		//console.log("form is undefined or null " + x);	
		setTimeout(function(){setupLoginListener(x);}, 1000); return;
	} else {
		//console.log("form is defined");
		//console.log(form);	
	}
	
	//add event listener to the submit
	form.addEventListener('submit', function(event){
		
		//console.log("submit hit!")
		
	  // Creates a timeout to call `submitForm` after one second.
	  setTimeout(submitForm, 1000);
	
	  // Keeps track of whether or not the form has been submitted.
	  // This prevents the form from being submitted twice in cases
	  // where `hitCallback` fires normally.
	  var formSubmitted = false;
	
	  function submitForm() {
		if (!formSubmitted) {
			//console.log("form has not been submitted yet, will submit now");
		  formSubmitted = true;
		  //alert("will submit next");
		  form.submit();
		} else {
			//console.log("form was already submitted");	
		}
	  }	
		
		
		event.preventDefault();
		//console.log("prevented default of the event");
		if (typeof ga != 'undefined'){
			//console.log("ga is defined");
			ga('send', 'event', 'Login-Tracking', 'Login-Submit', {
				hitCallback: submitForm
			});
		} else {
			//console.log("ga is undefined");	
		}
	});
}
</script> 

