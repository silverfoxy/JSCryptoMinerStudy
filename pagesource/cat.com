<script>
	head.ready(function() {
		$(".globalSelectorDialog a").click(function(e) {
			e.preventDefault();
			var _href = $(this).attr("href");
			if (window.location.hostname == this.hostname) {
				var urlPath = "?localePageUrl=" + _href;
				if(redirectPath != ""){
					urlPath = urlPath + "&redirectURI=" + redirectPath;
				}
				window.location.href = urlPath;
			}
			else {
				window.location.href = _href;
			}
		});
	});
</script><!-- BEGIN Footer Component -->

