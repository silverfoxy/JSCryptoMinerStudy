	<script type="text/javascript">



		window.addEventListener('beforeunload', function(event){
			showLoading();
		})
		window.addEventListener('submit', function(event){
			showLoading();
		})

		

		function showLoading() {


			var i = 0;
			var element = $("#preloading .image");
			setInterval(function() {
				i++;
				element.css('background-position', '0px -' + (i * 42) % (42 * 12) + 'px');
			}, 50);

			$("#preloading")
				.css({'visibility': 'visible',
						'opacity': '0.7'})
				.fadeIn();
		}

		function shake() {
			var p = new Array(15, 30, 15, 0, -15, -30, -15, 0);
			p = p.concat(p.concat(p));
			loop();
			function loop(){
				if(!p.length) return;
				var c = p.shift();
				console.log('here', c);
				$("#imei-form").css("left", c + "px");
				setTimeout(loop, 20);
			}
		}

		$(document).ready(function() {

			setTimeout(function() {
				$("#over-all").fadeOut("slow");
			}, 300);

			$("#imei-input").focus();

			$(".show_email_info").click(function() {
				$("#special_info_div").fadeIn("fast");
			});



			$("#imei-form").css("position", "relative");

			

			$(".div-center").prepend('<div class="backlogo" ><div class="rotate-backlogo" ></div></div>');

			$("body").mousemove(function(event) {
				var rot_deg = (-10 + ((((event.clientX / window.innerWidth) + (event.clientY / window.innerHeight)) / 2) * 20));
				$(".rotate-backlogo").css("-webkit-transform", "rotate(" + rot_deg + "deg)")
					.css("-moz-transform", "rotate(" + rot_deg + "deg)")
					.css("-o-transform", "rotate(" + rot_deg + "deg)")
					.css("transform", "rotate(" + rot_deg + "deg)");
			});

			$("#imei-input").keypress(function(event) {
				if (event.which == 13) return;

				var char = String.fromCharCode(event.which)
				if (!char.match(/^[0-9\s]$/)) event.preventDefault();
			});

		});
	</script>
