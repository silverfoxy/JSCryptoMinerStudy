$(function(){	
	var DOM = {
			J_taishan_close : $("#taishan-close"),
			J_taishan : $("#taishan"),
			J_taishan_zhai : $("#taishan-zhai")
		},
		$_flag = true;
	
	// 点击关闭
	$("#taishan-close").on("click", function() {		
		taiShanYaDingClose(DOM.J_taishan, DOM.J_taishan_zhai)
	})
			
		
	function taiShanYaDing($_delay_time, el, el2) {
		el.stop().animate({
			"height" : "405px"
		}, 1000).delay($_delay_time).animate({
			"height": "0"
		}, 1000, function() {
			taiShanYaDingSmall(el2)
		})
	}
	
	function taiShanYaDingSmall(el) {
		el.stop().animate({
			"height":"48px"
		}, 500);
		$_flag = false;
	}
	
	
	function taiShanYaDingClose(el, el2) {
		DOM.J_taishan.stop().animate({
			"height" : "0"
		}, 1000, function() {
			taiShanYaDingSmall(DOM.J_taishan_zhai)
		})
	}
	
	$(window).on("scroll", function() {		
		var $_scrollTop = $(window).scrollTop()
		if( $_scrollTop > 0 ) {
			taiShanYaDingClose(DOM.J_taishan, DOM.J_taishan_zhai)
		} else if($_flag){
			taiShanYaDing(5000, DOM.J_taishan, DOM.J_taishan_zhai)
		}
	})
	
	$(window).trigger("scroll")

	
	
 });

</script>

