        function setFormat(dataType, val) {
            var format;
            switch (dataType) {
                case "Integer":
                   // console.log(data);
                    return Highcharts.numberFormat(val, 0, '', ',')
                    break;
                case "Percent":
                    //console.log(data);
                    return val.toFixed(2) + "%";
                    break;
                case "Decimal":
                    return Highcharts.numberFormat(val, 2, '.', ',');
                    break;
                case "Currency":
                    return "$" + Highcharts.numberFormat(val, 0, '', ',');
                    break;
                case "Textstring":
                    return Highcharts.numberFormat(val, 0, '', ',')
                    break;
                case "Time":
                    return moment(val).format("mm:ss")
                    break;
                case "Decimal - Tax":
                    return "$" + val
                    break;
            }
        
        }
