        var txtUname = jQuery("#txtStudentUsername");
        var txtPass = jQuery("#txtStudentPassword");

        //var res = grecaptcha.getResponse(widIdStudent);
        var res = grecaptcha.getResponse(widIdMember);
        var hasCaptchaStudent = true;
        if (res == "" || res == undefined || res.length == 0) {
            hasCaptchaStudent = false;
        }

        if (txtUname.val().length > 0 && txtPass.val().length > 0 && hasCaptchaStudent == true) {
            //$(".overlay").show();
            return true;
        }
        else {
