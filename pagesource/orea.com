                jQuery("#lblMessageStudent").text("Click the Captcha check box below to proceed");
            }
            else {
                jQuery("#lblMessageStudent").text("You have entered an incorrect ID or password");

                flashPassword();
                //$(".overlay").hide();
            }
            return false;
        }
    }

