{
nm = window.frames[i].name;
}
catch (e)
{
continue;
}
if (nm == arrayofframes[f]) {
xframe = window.frames[i];
break;
}
}
if (xframe)
{
innerFrame = null;
for (var i = 0; i < xframe.frames.length; i++) {
