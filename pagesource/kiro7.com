 <script type="text/javascript">
 (function($, win, A){
  var container = $('#carousel-breaking-newsM-HOME-BREAKING-NEWS-AND-AL');
  var carousel;
  var model ;
  var enableAjaxCall = 'true';
  var hasBindSendAjaxCall = false;
  /**Note:carousel component init is a asynchronous method**/
  var breakingNewsCarouselComponentInit = function(){
   carousel = new EP.Components.Carousel({
    container: container.get(0),
    autoPlay: true
   });
   model = carousel.model();
   container.find('.slides-wrapper').hover( model.pause, model.play );
   /* reset dotdotdot */
   $(window).trigger('refresh.dotdotdot');
   /**Need to call breaking news ajax call after carousel have finished init**/
   if (!hasBindSendAjaxCall) {
    sendBreakingNewsAjaxCall();
   }
  };
  
  if( EP.Loader.doesNamespaceExist('EP.Components.Carousel') ){
   breakingNewsCarouselComponentInit();
  } else {
   A.use('ep-carousel', breakingNewsCarouselComponentInit);
  }
  function sendBreakingNewsAjaxCall() {
   /*juge the enable ajax call flag **/
   if (enableAjaxCall == 'true') {
    /**Get ajax url**/
    var ajaxUrl =$('#carousel-breaking-newsM-HOME-BREAKING-NEWS-AND-AL').attr('ajaxurl');
    /**Get ajax interval time from module configuration data**/
    var internalTime = "60000";
    /**When first load, send ajax call**/
    getLatestJournalArticleInformation();
    /**Call interval function to send ajax call and parse the response data**/
    setInterval(getLatestJournalArticleInformation,internalTime);
    hasBindSendAjaxCall = true;
    function getLatestJournalArticleInformation () {
     $(window).unbind('refresh.dotdotdot');
     $.ajax({
      type: "GET",
      url: ajaxUrl,
     }).done(function(data) {
      /**Get old carousel model information, like current index and total page numbers**/
      var selectedIndex = carousel.model().selectedIndex();
      var hasNextPageFlag = carousel.model().hasNext();
      var oldTotalPagesCount = carousel.model().totalPages();
      var gotoPageIndex = 1;
      /**Need to pause the carousel component before send ajax call**/
      carousel.model().pause();
      /**Get latest story total numbers from ajax call response**/
      if(data == null || data.trim() == "") {
       $('#carousel-breaking-newsM-HOME-BREAKING-NEWS-AND-AL').html("");
       $('#break_news_section_'+'M-HOME-BREAKING-NEWS-AND-AL').hide();
       return;
      }
      if($('#break_news_section_'+'M-HOME-BREAKING-NEWS-AND-AL').is(':hidden')) {
       $('#break_news_section_'+'M-HOME-BREAKING-NEWS-AND-AL').show();
      }
      var newTotalPagesCount = parseBreakingStoryCount(data);
      /**Replace the content with new data**/
      $('#carousel-breaking-newsM-HOME-BREAKING-NEWS-AND-AL').html(data.trim())
      /**Need to init the carousel component to render new data on component**/
      breakingNewsCarouselComponentInit();
      
      /**The logic for page index after ajax call finish and new carousel component init**/
      if (newTotalPagesCount < oldTotalPagesCount) {
       if (hasNextPageFlag) {
        if (selectedIndex < newTotalPagesCount) {
         gotoPageIndex = selectedIndex+1 ;
        } else {
         gotoPageIndex = 1 ;
        }
       } else {
        gotoPageIndex = 1 ;
       }
      } else {
       if (hasNextPageFlag) {
        gotoPageIndex = selectedIndex+1 ;
       } else {
        if (newTotalPagesCount > oldTotalPagesCount) {
         gotoPageIndex = selectedIndex+1 ;
        } else {
         gotoPageIndex = 1 ;
        }
       }
      }
      carousel.gotoPage(gotoPageIndex);
     }).fail(function() {
      console.info("Fail to get Ajax content by url:" + ajaxUrl)
     });
    }
   
    /**The function to parse the new total story numbers**/
    function parseBreakingStoryCount(arg) {
     if(arg != null && "" != arg) {
      var jqueryObject = $(arg.trim());
      var newCountSpanContent = jqueryObject.children(".count").html().trim();
      var newCountArray = newCountSpanContent.split("/");
      var newCount = newCountArray[1];
      return newCount
     }
    }
   }
  }
 })( jQuery, window, AUI());
