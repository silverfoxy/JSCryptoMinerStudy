    var formName = form_name.replace("_form", "");
    var form  = $("#"+form_name);
    var errorID = $("#"+form_name+"_error");
    var messageDiv = form.attr("data-message");
    var webPlatform = form.attr("data-pl");
    var formValidate = pbFormValidate(form);
    var self = $(this);

    if (formValidate) {
        var formData = {
            formName: formName,
            Data : form.find(":input:visible").serializeArray(),
            webPlatform: webPlatform,
            recaptcha: grecaptcha.getResponse()
        };

        self.progress(true, "Submitting...");

        $.post(
            base + "_staff/ajax_form_submit_pdo.php",
            formData,
            function(data){
                self.progress(false);
                if (data.success) {

                    if (data.goal.length > 0) {
                        // goal

                                                    ga('send', 'event', 'Form', 'Submit', data.goal);
