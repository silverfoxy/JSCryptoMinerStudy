<script type="text/javascript">
var game_arr_click = [];
$(document).ready(function(){
	// Masonry
	$('#game-main').masonry({ columnWidth: 21 });
	$('#game-main').masonry({ columnHeight: 21 });
	$('#game-main').masonry({ singleMode: false });
	// Infinite Scroll
	var n = 2;
	var container = $('#game-main');
	container.imagesLoaded(function(){
		container.masonry({
			itemSelector: '.big-game-enery',
			columnWidth: 20
		});
	});
	container.infinitescroll(
		{
			navSelector: '#page-nav',
			nextSelector: '#next_page',
			animate: false,
			itemSelector: '.big-game-enery',
			loading: {
				finishedMsg: 'No more games to load.',
				img: '/assets/images/loading.gif'
			}
		},
		// trigger Masonry as a callback
		function( newElements )
		{
			// hide new items while they are loading
			var newElems = $(newElements).css({opacity: 0});
			// ensure that images load before adding to masonry layout
			newElems.imagesLoaded(function(){
				// show elems now they're ready
				newElems.animate({opacity: 1});
				container.masonry('appended', newElems, true);
				n++;
				$('#next_page').attr('href', '/index.php/game/index/?section=getdata&sort=gameid-decrescent&page=' + n);
			});
		}
	);
	// Click like
	$('.game-like').live('click', (function(){
		var str = $(this).children().text();
		var num = parseInt(str);
		var game_href = $(this).parent().children('a').attr('href');
		var g_index = $.inArray('game', game_href.split('/')) + 1;
		var gameId = game_href.split('/')[g_index];
		var gid_arr_click = gameId;
		if ( game_arr_click[gid_arr_click] == undefined )
		{
			$.post('/index.php/game/entry/', {section: 'like', gameId: gameId}, function(data){
				$(this).children().text(data);
			});
			$(this).children().text(++num).css('color', '#ccc');
			$(this).css('background-position', '-415px -20px');
			game_arr_click[gid_arr_click] = 1;
		}
		else
		{
			return false;
		}
	}));
});
</script>
