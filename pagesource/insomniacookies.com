<script>
$(function(){
	$('#mailing-list-email').bind('keyup', function(){
		localStorage.setItem('email', this.value);
	});

    $("body").on("submit", "#mailing-list-form", function() {
        var emailRegex = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;

        if( $("#mailing-list-email").val().length == 0 || !emailRegex.test ( $("#mailing-list-email").val() ) ) {
            $.growl.error({ message : "You must provide a valid email address!" });
            return false;
        }


    });

  // Set deal of the week
    $(".set-deal-of-the-week").on("click", function(e){
        e.preventDefault();
        var dodProductID = $(this).attr('data-productID');
        var req = {
            dod_product_id: dodProductID
        };
        $.ajax({
            url: BASE_URL + "/locations/setDealOfTheWeek/json",
            method: "post",
            data: req,
            dataType: "json",
            success: function(data) {
                console.log("data: ", data);
                if(data.success === 1){
                    window.location = BASE_URL + "/order#deliver-to-me";
                } else {
                    $.growl.error({"message": data.message});
                }
            }
        });
    });

    $(".slider-redirect-url").on("click", function(e){
        e.preventDefault();
        var url = $(this).attr('data-url');

        window.location.href = url;
    });
    $('.carousel-inner').each(function() {
        if ($(this).children('div').length === 1) $(this).siblings('.carousel-control, .carousel-indicators').hide();
    });
});
</script>


