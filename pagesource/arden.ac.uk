                        $('#apply_now_course').val('');
                        var country_id = parseInt($(this).val());
                        $('select[name="apply_now_qualification"] > option').show();
                        if (country_id != 231) {
                            $('select[name="apply_now_qualification"] > option[value="Access to HE Diploma"]').hide();
                        }
                    });

                    $('#apply_now_qualification').on('change', function() {
                        applyNowForm.onQualificationChange(this);
                    })

                    $('#apply_now_subject').on('change', function() {
                        applyNowForm.onSubjectChange(this);
                    })

                    $('#applynowform').on('submit', function() {

                        var courseId = $('#apply_now_course').val();
                        var qual     = $('#apply_now_qualification').val();
                        var subject  = $('#apply_now_subject').val();
                        var country  = $('#apply_now_country').val();

                        if (!courseId) {
                            $('#apply_now_course_error').show();
                        }

                        if (!qual) {
                            $('#apply_now_qualification_error').show();
                        }

                        if (!subject) {
                            $('#apply_now_subject_error').show();
                        }

                        if (!country) {
                            $('#apply_now_country_error').show();
                        }

                        if (!courseId || !qual || !subject) {
                            return false;
                        }

                        return true;
                    })

                });

                function enquiryform_serialize(form, options) {
                                            $('#course_id_popup').val(0);
                                        return true;
                }

                // pre-submit callback
                function enquiryform_validation(arr, form, options) {

                    var name                = $('#enquiry_name_popup').val();
                    var email               = $('#enquiry_email_popup').val();
                    var phone               = $('#enquiry_phone_popup').val();
                    var country             = $('#enquiry_country_popup').val();
                    var campus              = $('#enquiry_campus_popup').val();
                    var commence_studies    = $('#enquiry_commence_studies_popup').val();
                    var cv                  = $('#enquiry_cv_popup').val();
                    var google              = parseInt($('input[name="google_recaptcha"]').val());
                    var interested_course   = $('#enquiry_interested_course_popup').val();

                    if (!name || !email || !phone || !country) {
                        alert('Please make sure you enter all required fields');
                        return false;
                    }
                    if ($('input[name="course_type"]').val() == 'on-campus') {
                        if (!campus || !commence_studies) {
                            alert('Please make sure you enter all required fields');
                            return false;
                        }
                    }
                    if (interested_course == 'p') {
                        alert('Please make sure you enter all required fields');
                        return false;
                    }

                    // If the user has selected a file/cv to upload...
                    if (cv != '') {

                        // Here are the file types allowed
                        var fileExtension = ['pdf', 'doc', 'docx'];

                        // If the cv is not in one of these file formats...
                        if ($.inArray(cv.split('.').pop().toLowerCase(), fileExtension) == -1) {
                            // ...inform the user that they must upload a different file type
                            alert('Your CV must be in one of the following file formats: '+fileExtension.join(', '));
                            return false;
                        }

                        // Attempt to work out the size of the file the user wants to upload
                        var cv_file_size = $('#enquiry_cv_popup')[0].files[0].size;

                        // Check we have a file size (some browsers won't return a value)
                        if (cv_file_size > 0) {

                            // The file size gets returned in "bytes" so lets divide it by 1000 to get it in "kb"
                            cv_file_size = cv_file_size / 1000;

                            // If the cv is greater than 5MB...
                            if (cv_file_size > 5000) {
                                // ...inform the user they must upload a smaller file size
                                alert('Your CV file size is too big. The limit on file size is 5MB. Please upload a smaller file.');
                                return false;
                            }
                        }

                    }

                    if (google !== 1) {
                        alert('Please tick the box to confirm you are not a robot');
                        return false;
                    }

                    if (!$('#confirm_popup').is(':checked')) {
                        alert('Please make sure you accept the Terms & Conditions.');
                        return false;
                    }

                    // Prevent the user from submitting the enquiry twice
                    $('form[name="enquiryform"] input[type="submit"]').attr('disabled', true);

                    if (cv != '') {
                        // Make the upload progress bar visible (could be quite useful for users on slow connection!)
                        $('#cv_upload_progress_popup').show();

                        var percentVal = '0%';
                        $('#cv_upload_progress_popup .bar').width(percentVal);
                        $('#cv_upload_progress_popup .percent').html(percentVal);
                    }
                    else {
                        // Ensure the CV upload progress bar is hidden
                        $('#cv_upload_progress_popup').hide();
                    }

                    // If we get this far then validation has passed so return true
                    return true;
                }

                function enquiryform_progress(event, position, total, percentComplete) {
                    // Update the upload status to keep the user informed
                    var percentVal = percentComplete + '%';
                    $('#cv_upload_progress_popup .bar').width(percentVal);
                    $('#cv_upload_progress_popup .percent').html(percentVal);
                }

                // post-submit callback
                function enquiryform_success(response, status, xhr, form)  {

                    if (response.success == true) {

                        // Set the upload status to 100% (albeit briefly as the user is about to be redirected)
                        var percentVal = '100%';
                        $('#cv_upload_progress_popup .bar').width(percentVal);
                        $('#cv_upload_progress_popup .percent').html(percentVal);

                        $(window).trigger("form-submitted-with-user-id", [response.form_submit_user_id, function() {
                            popups.hide(popups.$currentpop);
                            var cv = $('#enquiry_cv_popup').val();
                            if (cv != '') {
                                window.location = '/enquiry-thank-you-cv';
                            }
                            else {
                                window.location = '/enquiry-thank-you';
                            }
                        }]);
                    }
                    else {
                        $('form[name="enquiryform"] input[type="submit"]').attr('disabled', false);
                        alert('An error has occurred submitting your enquiry. Please try again in a few minutes.');
                    }
                }

                function googleCaptchaTicked() {
                    $('input[name="google_recaptcha"]').val(1);
                }

            </script>
