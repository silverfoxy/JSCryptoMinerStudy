<script>

    



    $(document).delegate('#distance_and_blended_banner > .row > div', 'click', function() {
       window.location = $(this).find('a').attr('href');
    });

    $('.popup').on('submit', 'form[name=prospectusform]', function() {

        var $form = $(this);

        var email   = $('#prospectus_email_popup').val();

        $('#prospectus_email_error_popup').hide();
        if (!email) {
            $('#prospectus_email_error_popup').show();
        }

        $('#confirm_error_popup').hide();
        if (!$('#confirm_popup').is(':checked')) {
            $('#confirm_error_popup').show();
        }

        if (!email || !$('#confirm_popup').is(':checked')) {
            popups.heightcheck(popups.$currentpop);
            scrollpane.setheight(popups.$currentpop);
            popups.position(popups.$currentpop);
            popups.$currentpop.show();
            return false;
        } else {
            $.ajax({
                type: "POST",
                url: $form.attr('action'),
                data: {
                    email: email
                },
                success: function(){
                    $form.closest(".form").hide();
                    $('#mailing_list_thank_you_popup').show();
                    popups.heightcheck(popups.$currentpop);
                    scrollpane.setheight(popups.$currentpop);
                    popups.position(popups.$currentpop);
                    popups.$currentpop.show();
                }
            });
        }

        return false;

    });

    var $ajaxNewsletterSignup = $('.ajaxNewsletterSignup');

    if($ajaxNewsletterSignup.length > 0) {

        $ajaxNewsletterSignup.on('submit', function(e) {

            var $form = $(this),
                $mailinglistmsg = $ajaxNewsletterSignup.siblings(".mailing-list-msg"),
                $newsletteremail = $ajaxNewsletterSignup.find(".newsletteremail");

            $form.find("input").removeClass("error");

            $.ajax({

                type: 'POST',
                url: $form.attr('action'),
                data: $form.serialize(),

                success: function(data){
                    if (data.success == true) {
                        // Display success message
                        $mailinglistmsg.html("Thanks for signing up and well be in contact soon");
                        $newsletteremail.val("");
                    } else {
                        var html = "<div class=\"alert alert-danger align-center\"><ul>";

                        var count = 0;

                        $.each(data.responseJSON, function(fieldName, errorMessage){

                            console.log(errorMessage);

                            // Loop over error fields and display response

                            $form.find("input[name=" + fieldName + "]").addClass("error");

                            html += "<li>" + errorMessage + "</li>";

                            count++;

                        });

                        html += "</ul></div>"

                        $mailinglistmsg.html(html);
                    }
                },

                error: function(data){

                    var html = "<div class=\"alert alert-danger align-center\"><ul>";

                    var count = 0;

                    $.each(data.responseJSON, function(fieldName, errorMessage){

                        console.log(errorMessage);

                        // Loop over error fields and display response

                        $form.find("input[name=" + fieldName + "]").addClass("error");

                        html += "<li>" + errorMessage + "</li>";

                        count++;

                    });

                    html += "</ul></div>"

                    $mailinglistmsg.html(html);

                }
            });

            return false;

        });

    }

</script>
<script>

                var applyNowForm = {

                    onQualificationChange : function(el) {

                        var qual    = $(el).val();
                        var country = $('#apply_now_country').val();

                        $('#apply_now_subject').val('');
                        $('#apply_now_course').val('');

                        $.getJSON('/api/subjectsforqualification?qualification=' + encodeURIComponent(qual) + '&country=' + encodeURIComponent(country),
                            function(data){

                                $('#apply_now_subject').empty().append($('<option value="">Please select ...</option>'));

                                if (data.subjects) {
                                    for (var i = 0; i < data.subjects.length; i++) {
                                        var n = $('<option/>', {
                                            value: data.subjects[i].id,
                                            text: data.subjects[i].title
                                        });
                                        $('#apply_now_subject').append(n);
                                    }
                                }
                            }
                        );

                    },

                    onSubjectChange : function(el) {

                        var val           = $(el).val();
                        var qualification = $('#apply_now_qualification').val();
                        var country       = $('#apply_now_country').val();

                        $.getJSON('/api/coursesforqualificationandsubject?qualification=' + encodeURIComponent(qualification) + '&subject=' + encodeURIComponent(val) + '&country=' + encodeURIComponent(country),
                            function(data){

                                $('#apply_now_course').empty().append($('<option value="">Please select ...</option>'));

                                if (data.courses) {
                                    for (var i = 0; i < data.courses.length; i++) {
                                        var n = $('<option/>', {
                                            value: data.courses[i].id,
                                            text: data.courses[i].course_name + ' (' + data.courses[i].delivery_description + ')'
                                        });
                                        $('#apply_now_course').append(n);
                                    }
                                }

                            }
                        );


                    }

                };
