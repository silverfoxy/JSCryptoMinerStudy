<script>
    //Fix for yamm mega menu
    $(document).on('click', '.custom-drop', function(e) {
        e.stopPropagation()
    })
    function getTemplate(newObj, i, j, parent){
        var level1 = 0;
        if(parent=='accordion'){
            var level1dropdown = newObj.find('.col-sm-6:eq(0) ul:first, .col-sm-6:eq(1) ul:first');
        }else{
            var level1dropdown = newObj.find('ul');
        }
        if(newObj.find('ul').length){
            level1 = 1;
        }
        var temp = '';
        temp += '<div class="panel panel-default">';
        temp += '<div class="panel-heading" role="tab">';
        temp += '<h4 class="panel-title">';
        var level1a = newObj.find('a:first');
        var linkHref = parent+level1a.text().trim().replace(/\s/g,'-')+i;
        if(level1){
            if(level1a.attr('onclick')){
                temp += '<a class="collapsed" data-toggle="collapse" data-parent="#'+parent+'" href="#collapse-'+linkHref+'" aria-expanded="false" aria-controls="#collapse-'+linkHref+'">'+level1a.text()+' <em class="fa fa-angle-down"></em></a>';
            }else{
                
                var newStr = newObj.html();
                newStr = newStr.split('<ul>');
                var label = newStr[0].trim();
                temp += '<a class="collapsed" data-toggle="collapse" data-parent="#'+parent+'" href="#collapse-'+linkHref+'" aria-expanded="false" aria-controls="#collapse-'+label.replace(' ','-')+i+'">'+label+' <em class="fa fa-angle-down"></em></a>';
            }
        }else{
            level1a = newObj.find('a:first');
            temp += '<a class="collapsed" href="'+level1a.attr('href')+'">'+level1a.text()+'</a>';
        }
        
        temp += '</h4>';
        temp += '</div>';
        temp += '<div id="collapse-'+linkHref+'" class="panel-collapse collapse" role="tabpanel">';
        temp += '<div class="panel-body">';	
        
        if(level1){
            var tempNew = '';
            
            if(level1a.attr('onclick')){
                tempNew += '<div class="panel-group" id="'+level1a.text().trim().replace(/\s/g,'-')+'" role="tablist" aria-multiselectable="true">';
                $(level1dropdown).children('li').each(function(key, value){
                    var obj = $(value);
                    tempNew += getTemplate(obj, key, value,level1a.text().trim().replace(/\s/g,'-').trim());
                });
            }else{
                if(label){
                    tempNew += '<div class="panel-group" id="'+label.replace(/\s/g,'-')+'" role="tablist" aria-multiselectable="true">';
                    $(level1dropdown).children('li').each(function(key, value){
                        var obj = $(value);
                        tempNew += getTemplate(obj, key, label.trim().replace(/\s/g,'-'));
                    });
                }
            }
            tempNew += '</div>';
            temp += tempNew;	
            
            
        }else{
            
            
            
        }
        
        temp += '</div>';
        temp += '</div>';
        temp += '</div>';
        
        return temp;
    }
    
    $(document).ready(function(){
        var str = '';
        $('.site-nav:first ul li.left, .site-nav:first ul li.right').each(function(key, value){
            var obj = $(value);
            var temp ='';
            temp += getTemplate(obj, key, value, 'accordion');
            
            str += temp;
            
        });
        $('#accordion').html(str);
        var top = $(window).height();
        var ht = $('#slides').height();
        var str = ht + 'px !important';
        
        $('.navigtion-mobile').css({top: top-200});
        if(!$('.page').hasClass('about-template')){
            $('#slides').css({'max-height':ht});
            
        }else{
            $('.local-main-image').css({'height':top});
            $('#slides').css({'max-height':top});
            if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
                if($(window).width() > 450){
                    $('.local-main-image').css({'min-height':.60*top});
                    $('.local-main-image').css({'height':.60*top});
                    $('#slides').css({'max-height':.6*top});
                    $('.top-crest').css({'width':'80%'});
                    $('.navigtion-mobile').css({top: .6*top-60});
                }else{
                    $('.local-main-image').css({'min-height':.40*top});
                    $('.local-main-image').css({'height':.40*top});
                    $('#slides').css({'max-height':.4*top});
                    $('.top-crest').css({'width':'80%'});
                    $('.navigtion-mobile').css({top: .4*top-50});
                }
            }
        }
        
        //$('.parallax-overlay').css({'top': top-$('.parallax-overlay').height()});
        //$('.slogan-box').css({'top': top-$('.slogan-box').height()-20});
        
    });
    
    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
        var url= window.location.href;
        if(url.split("/").length<5){
            var title=$('#homepage-title h1').text();
        }	
        else
        {	
            var title=$('.section-title h1').text();
        }
        $('.show-mobile h1').text(title);
        $('#back-top').show();
        if(deviceType == "tablet"){
            $('#slides .top-crest').addClass('tablet');
        }
    }
    /***********************RESIZE***********************/
    var windowHeight = $(window).height();
    var windowWidth = $(window).width();
    $(window).resize( function(){
        window.offSetTopSiteNav = $(".nav-wrap").offset().top;
        $(".sticky-nav").fadeOut();
        var height = $(window).height();
        var sht = $('#slides').height();     
        if(!$('.page').hasClass('about-template')){
            $('#slides').css({'max-height':height});	
        }else{
            if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                if(windowWidth > 450){
                    if($(window).width() > 450){
                        $('.local-main-image').css({'min-height':.60*windowHeight});
                        $('.local-main-image').css({'height':.60*windowHeight});
                        $('#slides').css({'max-height':.6*windowHeight});
                        
                        $('.navigtion-mobile').css({top: .6*windowHeight-60});
                    }else{
                        $('.local-main-image').css({'min-height':.40*windowWidth});
                        $('.local-main-image').css({'height':.40*windowWidth});
                        $('#slides').css({'max-height':.4*windowWidth});
                        
                        $('.navigtion-mobile').css({top: .4*windowWidth-50});
                    }
                }else{
                    if($(window).width() > 450){
                        $('.local-main-image').css({'min-height':.60*windowWidth});
                        $('.local-main-image').css({'height':.60*windowWidth});
                        $('#slides').css({'max-height':.6*windowWidth});
                        
                        $('.navigtion-mobile').css({top: .6*windowWidth-60});
                    }else{
                        $('.local-main-image').css({'min-height':.40*windowHeight});
                        $('.local-main-image').css({'height':.40*windowHeight});
                        $('#slides').css({'max-height':.4*windowHeight});
                        
                        $('.navigtion-mobile').css({top: .4*windowHeight-50});
                    }
                }
            }
            
        }
        
        
    });
</script>
