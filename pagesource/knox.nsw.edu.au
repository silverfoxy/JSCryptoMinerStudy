<script type="text/javascript">
    var arrMonth = { JAN : '01', FEB : '02' ,MAR : '03', APR : '04' ,MAY : '05', JUN : '06' ,JUL : '07', AUG : '08',SEP : '09', OCT : '10' ,NOV : '11', DEC : '12' };
    $(".icalendar_date").each(function(){
        $(this).find("a").attr("href",$(this).parent().next().next().find("a").attr("href"));
        $(this).addClass("addthisevent");
        //add date from webapp to icalendar sample: 10-05-2012 00:00:00
        var eventDate = $(this).find(".eventDate").text().split("-")//23-Apr-2014
        var eventDateFinish = $(this).find(".eventDateFinish").text().split("-")//23-Apr-2014
        
        try {
            $(this).find("._start").text(""+eventDate[0]+"-"+arrMonth[eventDate[1].toUpperCase()]+"-"+eventDate[2]+" "+$(this).find(".eventTime").text());
            $(this).find("._end").text(""+eventDateFinish[0]+"-"+arrMonth[eventDateFinish[1].toUpperCase()]+"-"+eventDateFinish[2]+" "+$(this).find(".eventTimeFinish").text());
        }
        catch(err) {
            
        }
    });
    
    
    addthisevent.settings({
        license   : "aao8iuet5zp9iqw5sm9z",
        mouse     : false,
        css       : false,
        outlook   : {show:true, text:"Outlook Calendar"},
        google    : {show:true, text:"Google Calendar"},
        yahoo     : {show:true, text:"Yahoo Calendar"},
        hotmail   : {show:true, text:"Hotmail Calendar"},
        ical      : {show:true, text:"iCal Calendar"},
        facebook  : {show:true, text:"Facebook Event"},
        dropdown  : {order:"outlook,google,ical"},
        callback  : ""
    });
    
    $( document ).ready(function() {
        var currentCategory = window.location.hash;
        if(currentCategory){
            var catString = "a[href='"+currentCategory +"']";
            var catLink = $(catString);
            filterBusinessListing(catLink);
        }
        $('.categories-dropdown a').on('click',function(e){
            filterBusinessListing($(this));
        });
        
    });   
    
    function filterBusinessListing(catLinkPara){
        var text = catLinkPara.html();
        $('.categories span').html(text);
        $('.business-category-title').html(text);
        var currentCategory = catLinkPara.attr('href');
        currentCategory = currentCategory.replace('#','');
        $('.business-list').hide();
        $('div[data-category="'+ currentCategory +'"]').show();
        $('.categories a').html('<em class="fa fa-chevron-down"></em>');
        $('.categories-dropdown').hide();
        
    }
</script>
