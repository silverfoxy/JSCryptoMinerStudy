<script type="text/javascript">
    $(function() {

            //main carousel
            var rotate = true;
            var hpt = $('#hp_tabs').tabs({ fx: { opacity: 'toggle'} });
            hpt.tabs('rotate', 10000);
            
            //play-pause toggle
            $('.cNav .cToggle').click(function() {
                if (rotate) {
                    hpt.tabs('rotate', 0);
                    rotate = false;
                } else {
                    hpt.tabs('rotate', 6000);
                    rotate = true;
                }
                return false;
            });

            //next slide
            var panels = $('#hp_tabs .ui-tabs-panel').length;
            $('.cNav .cFwd').click(function() {
                hpt.tabs('rotate', 0);
                rotate = false;
                var st = hpt.data('selected.tabs') + 1;
                if (st < panels) {
                    hpt.tabs('select', st);
                } else {
                    hpt.tabs('select', 0);
                }
                return false;
            });

            //prev slide
            $('.cNav .cBack').click(function() {
                hpt.tabs('rotate', 0);
                rotate = false;
                var st = hpt.data('selected.tabs') - 1;
                if (st < 0) {
                    hpt.tabs('select', panels - 1);
                } else {
                    hpt.tabs('select', st);
                }
                return false;
            });

            //news headline carousel
            var nt = $('#news_tabs').tabs({ fx: { opacity: 'toggle'} });

            //next slide
            var n_panels = $('#news_tabs .ui-tabs-panel').length;
            $('.newsNav .newsFwd').click(function() {
                nt.tabs('rotate', 0);
                var st = nt.data('selected.tabs') + 1;
                if (st < n_panels) {
                    nt.tabs('select', st);
                } else {
                    nt.tabs('select', 0);
                }
                return false;
            });

            //prev slide
            $('.newsNav .newsBack').click(function() {
                nt.tabs('rotate', 0);
                var st = nt.data('selected.tabs') - 1;
                if (st < 0) {
                    nt.tabs('select', n_panels - 1);
                } else {
                    nt.tabs('select', st);
                }
                return false;
            });

            var at = $('#as_tabs').tabs({ fx: { opacity: 'toggle'} });
            at.tabs();

            //next slide
            var a_panels = $('#as_tabs .ui-tabs-panel').length;
            $('.nav .aNext').click(function() {
                var st = at.data('selected.tabs') + 1;
                if (st < a_panels) {
                    at.tabs('select', st);
                } else {
                    at.tabs('select', 0);
                }
                return false;
            });
            //prev slide
            $('.nav .aPrev').click(function() {
                var st = at.data('selected.tabs') - 1;
                if (st < 0) {
                    at.tabs('select', a_panels - 1);
                } else {
                    at.tabs('select', st);
                }
                return false;
            });
