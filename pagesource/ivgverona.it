<script type="text/javascript">
    //<![CDATA[
    function myCategory() {
        var attr = $( '#category option[value=' + $( this ).val() + ']'  ).attr( 'class' );
        if ( attr == 'mobili' ) {
            $( '#ricerca' ).attr( 'action','/beni_mobili/ricerca/index.html' ); 
        } 

        if ( attr == 'immobili'  ) {
            $( '#ricerca' ).attr( 'action', '/beni_immobili/ricerca/index.html' );
        }

        if ( attr == 'crediti'  ) {
            $( '#ricerca' ).attr( 'action', '/beni_crediti/ricerca/index.html' );
        }

        if ( attr == 'aziende'  ) {
            $( '#ricerca' ).attr( 'action', '/beni_aziende/ricerca/index.html' );
        }

        if ( attr == 'altro'  ) {
            $( '#ricerca' ).attr( 'action', '/beni_altro/ricerca/index.html' );
        }
        return;

    }
    
    $( document).ready(function(){
        $( '#category' ).bind( 'change', myCategory );

    });
    
    function hideSearchOptions(){
        $(".seachOptions").toggle();
        var value = "";
        var exdays = 31;
        var c_name = "";
        setTimeout(function(){
            //alert($("#seachOptions").is(":visible"));
            if( $("#seachOptions").is(":visible") ) {
                value = "visible";
            } else {
                value = "hidden";
            }
            var exdate=new Date();
            exdate.setDate(exdate.getDate() + exdays);
            var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
            document.cookie=c_name + "=" + c_value;
        },200);
    }
    
    function redirect() {
        if ( $( '#category' ).val() > 0 ) {
            $( '#ricerca' ).submit();
        } else {
            alert( 'Occorre selezionare una categoria' );
        }
        return false;
    }

    function regioneSelector( myval ) {
        val = myval;
        var val = $( this ).val();
        var text = $( '#regione option[value="' + val + '"]' ).text();


        if ( val ) {
            $.get( '/ajax/get_regione_istituti/' +val + '/index.html', {}, function( data ){
                var istitutiObject = $.parseJSON( data );
                var len = istitutiObject.length;

                if ( len ) {
                    options = '';
                    options += '<option value="">Tutti gli IVG in ' + text +'</option>';				
                    for ( i = 0; i < len; i++ ) {
                        options += '<option value="' + istitutiObject[i].id + '">' + istitutiObject[i].denominazione_breve + '</option>';					
                    }

                    $( '#institutes' ).selectBox( 'options' , options);				
                    $( '#institutes' ).selectBox( 'enable' );
                } else {
                    options = '<option>QUALSIASI</option>';
                    $( '#institutes' ).selectBox( 'options', options );
                    $( '#institutes' ).selectBox( 'disable' );
                }			
            } );
        } else {
            options = '<option>QUALSIASI</option>';
            $( '#institutes' ).selectBox( 'options', options );
            $( '#institutes' ).selectBox( 'disable' );
        }
    }
    
    $( document ).ready( function(){
        $( '#regione' ).bind( 'change', regioneSelector );
    });
    //]]>
</script>
