<script>
    // start timer of top menu
    ZeitAnzeigen();

    $(document).ready(function () {

        if ($('.alert-success').is(':visible')) {
            window.setTimeout(function () {
                $(".alert-success").fadeTo(1500, 0).slideUp(500, function () {
                    $(this).hide();
                });
            }, 5000);
        }

        if ($('.alert-danger').is(':visible')) {
            window.setTimeout(function () {
                $(".alert-danger").fadeTo(1500, 0).slideUp(500, function () {
                    $(this).hide();
                });
            }, 5000);
        }
        
        $(".super-menu ul li").on('mouseenter mouseleave', function (e) {

            var elm = $('.sfHover ul').offset();

            var l = elm.left;
            var w = $('.sfHover ul').width();
            var ww = w + 180;
            var docH = $("body").height();
            var docW = $("body").width();

            var isEntirelyVisible = (l + ww <= docW);
          //  $('.super-menu ul li').removeClass('product-menu-format');

            if (!isEntirelyVisible) {
                $(this).addClass('product-menu-format');
            } else {
                $(this).removeClass('product-menu-format');
            }
        });


        setTimeout(function () {
            $(".skip-intro").show();
        }, 5000);

        // Jquery draggable
        $('.modal-dialog').draggable({
            handle: ".modal-header , .modal-footer"
        });

        $('input[name="accordion-level-1"]').click(function () {
            var status = $(this).prop('checked');
            $('input[name="accordion-level-1"]').each(function () {
                $(this).prop('checked', false);
            });
            $(this).prop('checked', status);
        });

        $(".list-all-sub-category-top").parents('ul').parents('li').children('a').children('span').addClass('second-label-menu-top');
        $(".list-all-sub-category").parents('ul').parents('li').children('span').addClass('second-label-menu-sub-plus');
        $(".list-all-sub-category").parents('ul').parents('li').addClass('sub-menu-member');

            });

    function showSubCategoryList(productTypeId, url, shop_checkout) {

        if ($('#product_type_' + productTypeId).closest('li').has('ul').has('li').length) {
            $('.expand-all-sub-category').children('span').removeClass('second-label-menu-sub-minus');
            $('.expand-all-sub-category').children('span').addClass('second-label-menu-sub-plus');

            $('#product_type_' + productTypeId).addClass('change-backgroup');
            if ($('#subCategory' + productTypeId).css('display') == 'block') {

                $("#product_type_" + productTypeId).children('span').removeClass('second-label-menu-sub-minus');
                $("#product_type_" + productTypeId).children('span').addClass('second-label-menu-sub-plus');

                $('.all-list-category').hide('show');
                $('#subCategory' + productTypeId).hide('show');
            } else {
                if (shop_checkout) {
                    $('.all-list-category').hide('show');

                    $("#product_type_" + productTypeId).children('span').removeClass('second-label-menu-sub-plus');
                    $("#product_type_" + productTypeId).children('span').addClass('second-label-menu-sub-minus');
                    $('#subCategory' + productTypeId).show('show');
                } else {
                    window.location = url;
                }
            }
        }
    }

</script>



