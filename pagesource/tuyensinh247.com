                    }, opts);
            var MOUSE_LOCS_TRACKED = 3,
                    DELAY = options.delay;
            var mousemoveDocument = function (e) {
                mouseLocs.push({
                    x: e.pageX,
                    y: e.pageY
                });
                if (mouseLocs.length > MOUSE_LOCS_TRACKED) {
                    mouseLocs.shift();
                }
            };
            var mouseleaveMenu = function () {
                if (timeoutId) {
