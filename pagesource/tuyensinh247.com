                        }
                    }, 50);
                    var count_youtube = setInterval(function () {
                        if (count_youtube_start < count_youtube_finish) {
                            count_youtube_start = count_youtube_start + count_youtube_step;
                            if (count_youtube_start < count_youtube_finish) {
                                $("#traffic_count_youtube_sub").html(addCommas(count_youtube_start));
                            } else {
                                $("#traffic_count_youtube_sub").html(addCommas(count_youtube_finish));
                            }
                        } else {
                            clearInterval(count_youtube);
                        }
                    }, 50);
                    $("#traffic_count_fanpage").html('1.555.816');
                    $("#traffic_count_youtube_sub").html('126.000');
                }
            }
        });
        function addCommas(nStr) {
            nStr += '';
            x = nStr.split('.');
            x1 = x[0];
            x2 = x.length > 1 ? '.' + x[1] : '';
            var rgx = /(\d+)(\d{3})/;
            while (rgx.test(x1)) {
                x1 = x1.replace(rgx, '$1' + ',' + '$2');
            }
            return x1 + x2;
        }

        function buildBoxCourse(list_course, max_item) {
            var total_item = list_course.length;
            var html = '';
            for (var i = 0; i < total_item; i++) {
                var teacher_info = '';
                for (var j = 0; j < list_course[i].teachers.length; j++) {
                    teacher_info += ' | ' + list_course[i].teachers[j].title;
                }
                teacher_info = trim(teacher_info);
                teacher_info = teacher_info.replace(/^\|+|\|+$/g, '');
                teacher_info = trim(teacher_info);
                if (i < max_item) {
                    html += '<div class="td-box-course">';
                } else {
                    html += '<div class="td-box-course" style="display:none">';
                }
                html += '<div class="avt-course bg-avt1 fl"><a href="' + list_course[i].link_course + '"><img src="' + list_course[i].origin_avatar + '" style="max-width:100%"></a></div>';
                html += '<div class="info-course fl">';
                html += '<a href="' + list_course[i].link_course + '">' + list_course[i].title + '</a>';
                if (teacher_info !== '') {
                    html += '<p>' + teacher_info + '</p>';
                }

                html += '</div>';
                html += '</div>';
            }
            return html;
        }
        function loadCourse(course_box_div_id, list_cat_id) {
            $.ajax({
                url: '/eHome/loadCourse',
                type: "GET",
                data: ({
                    list_cat_ids: list_cat_id
                }),
                success: function (resp) {
                    var result = JSON.parse(resp);
                    if (result.success) {
                        var content = buildBoxCourse(result.data, 4);
                        $("#" + course_box_div_id + " .list_course_in_cat").html(content);
                        $("#" + course_box_div_id + " .td-box-sub .td-next-sub").removeClass("color-sub");
                        $("#" + course_box_div_id + " .td-box-sub .td-next-sub[item-id='" + list_cat_id + "']").addClass("color-sub");
                        $("#" + course_box_div_id + " .current_page_idx").val(1);
                    } else {
                        console.log('Can not load course data!');
                    }
                }
            });
        }

        function loadCourseByFavourite(course_box_div_id, list_cat_id, classId) {
            $.ajax({
                url: '/eHome/loadCourseByFavourite',
                type: "GET",
                data: ({
                    class_id: classId
                }),
                success: function (resp) {
                    var result = JSON.parse(resp);
                    if (result.success) {
                        var content = buildBoxCourse(result.data, 4);
                        $("#" + course_box_div_id + " .list_course_in_cat").html(content);
                        $("#" + course_box_div_id + " .td-box-sub .td-next-sub").removeClass("color-sub");
                        $("#" + course_box_div_id + " .td-box-sub .td-next-sub[item-id='" + list_cat_id + "']").addClass("color-sub");
                        $("#" + course_box_div_id + " .current_page_idx").val(1);
                    } else {
                        console.log('Can not load course data!');
                    }
                }
            });
        }

        function nextItem(course_box_div_id, max_item) {
            var total_item_in_box = $("#" + course_box_div_id + " .list_course_in_cat .td-box-course").length;
            var curr_page = $("#" + course_box_div_id + " .current_page_idx").val();
            curr_page = Number(curr_page);
            //Check if is last page >> do nothing;
            if (total_item_in_box <= curr_page * max_item) {
                return;
