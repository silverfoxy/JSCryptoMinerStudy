        function init(opts) {
            var $menu = $(this),
                    activeRow = null,
                    mouseLocs = [],
                    lastDelayLoc = null,
                    timeoutId = null,
                    options = $.extend({
                        rowSelector: "> li",
                        submenuSelector: "*",
                        submenuDirection: "right",
                        tolerance: 1500,
                        enter: $.noop,
