        <script type="text/javascript">
        var has_init = false;
        $(window).scroll(function () {
            var hT = $('#traffic_board').offset().top,
                    hH = $('#traffic_board').outerHeight(),
                    wH = $(window).height(),
                    wS = $(this).scrollTop();
            var count_user_finish = 652085, count_user_step = 50428, count_user_start = 9;
            var count_fanpage_finish = 1382080, count_fanpage_step = 55468, count_fanpage_start = 9;
            var count_youtube_finish = 186394, count_youtube_step = 3025, count_youtube_start = 9;
            if (wS > (hT + hH - wH)) {
                if (!has_init) {
                    has_init = true;

                    var count_user = setInterval(function () {
                        if (count_user_start < count_user_finish) {
                            count_user_start = count_user_start + count_user_step;
                            if (count_user_start < count_user_finish) {
                                $("#traffic_count_user").html(addCommas(count_user_start));
                            } else {
                                $("#traffic_count_user").html(addCommas(count_user_finish));
                            }
                        } else {
                            clearInterval(count_user);
                        }
                    }, 50);
                    var count_fanpage = setInterval(function () {
                        if (count_fanpage_start < count_fanpage_finish) {
                            count_fanpage_start = count_fanpage_start + count_fanpage_step;
                            if (count_fanpage_start < count_fanpage_finish) {
