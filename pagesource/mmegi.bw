	});
	currentPage.animate({ marginLeft: -400 }, 800,function(){
	currentPage.hide();
	$('#homepage_movies_wrapper .button').bind('click',handler);
	});
	}
	}
	$('#homepage_movies_wrapper .button').click(handler);
	});
