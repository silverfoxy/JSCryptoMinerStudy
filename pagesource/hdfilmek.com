    <script>
        var page = 1;
        
        $(document).ready(function () {
            $(".movie-holder .cover-surface").flowtype({
                fontRatio: 11
            });

            $(document).on('click', 'a#featured_movies_previous', function (e) {
                e.preventDefault();
                if(page > 1) {
                    page--;
                    getFeatured(page);
                }
            });
            
            $(document).on('click', 'a#featured_movies_next', function (e) {
                e.preventDefault();
                if(getFeatured(page+1)) page++;
            });
        });
        
        function getFeatured(page) {
            var response = false;
            $.ajax({
                url : 'featured?page=' + page,
                dataType: 'json',
                async: false
            }).done(function (data) {
                if(data.length > 0) {
                    $('#featured_movies').html(data);
                    response = true;
                }
            });
            $('.ajax-loader .fillImage').resizeToParent({
                delay: 0
            });
            
            general.enableHoverLink();
            
            return response;
        }
    </script>
        <script>
    var slug = function(str) {
        var $slug = '';
        var trimmed = $.trim(str);
        $slug = trimmed.replace(/-+/g, '-').replace(/^-|-$/g, '');
        return $slug;
    }
    
    var checkTimeout;
    
    $(document).ready(function() {
        $('#search_term').keyup(function() {
           $('.search_loader').show();
           checkTimeout && clearTimeout(checkTimeout);
           checkTimeout = setTimeout(checkResult, 500);
        });
                
        $("body").click(function(e){
            if(!$(e.target).is("#search_term")) {
                $('.search-filter-result').css("display","none");
                $('.search-filter-result').html('');
                $('.search_loader').hide();
            }
        });
    });
    
    function checkResult() {
        filter = $('#search_term').val();
        if(filter.length >= 3) {
            filter = slug(filter);
            $.ajax({
                url : "http://hdfilmek.com/filter-search/" + filter,
                async: true
            }).done(function (data) {
                $('.search-filter-result').css("display","block");
                $('.search-filter-result').html(data);
                $('.search_loader').hide();
            });
        } else {
            $('.search-filter-result').css("display","none");
            $('.search-filter-result').html('');
            $('.search_loader').hide();
        }
    }
