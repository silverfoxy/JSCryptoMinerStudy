<script id="shopByCategory" type="x-handlebars-template">
    <div class="shop-category-section">
        <div class="hk-pagecontainer">
            <div class="shop-category-container homeWidget shopWidget">
                <h2>Shop by Category</h2>
                <div class="shop-category-tabs">
                    <ul class="tabs-ul">
                        {{#each menuData}}
                            {{#customIf @index "==" 0}}
                                 <li class="tab-link current shopByCategoryhomeTab_{{@index}}" data-page="tab-page-{{@index}}" data-tab="shop-tab-{{@index}}">{{name}}</li>
                            {{/customIf}}
                            {{#customIf @index ">" 0}}
                                 <li class="tab-link shopByCategoryhomeTab_{{@index}}" data-page="tab-page-{{@index}}" data-tab="shop-tab-{{@index}}">{{name}}</li>
                            {{/customIf}}
                        {{/each}}
                    </ul>
                    {{#each menuData}}
                        {{#customIf @index "==" 0}}
                           <div id="shop-tab-{{@index}}" class="shop-tab-content current">
                                <div class="shop-products-list">
                                    {{#each childNodes}}
                                        <div class="shop-product-block1">
                                            {{#customIf image "neq" null}}
                                            <div class="shop-product-image">
                                                    <a href="{{url}}">
                                                     <img src="{{image.oLink}}" alt="{{image.altText}}" />
                                                    </a>
                                            </div>
                                            {{/customIf}}
                                            <h3><a href="{{replace url}}">{{name}}</a> </h3>
                                            <ul>
                                                {{#each childNodes}}
                                                    {{#customIf @index "<=" 4}}
                                                          <li><a href="{{replace url}}">{{name}}</a></li>
                                                    {{/customIf}}
                                                    {{#customIf @index "==" 5}}
                                                        <li class="view-all"><a href="{{replace ../url}}">See all</a> </li>
                                                    {{/customIf}}
                                                {{/each }}
                                            </ul>
                                        </div>
                                    {{/each }}
                                </div>
                            </div>
                        {{/customIf}}
                        {{#customIf @index ">" 0}}
                             {{#customIf type "==" 1}}
                                <div id="shop-tab-{{@index}}" class="shop-tab-content">
                                    <div class="shop-products-list">
                                        {{#each childNodes}}
                                        <div class="shop-product-block1">
                                            {{#customIf image "neq" null}}
                                            <div class="shop-product-image">
                                                <a href="{{url}}">
                                                     <img class="lazy" data-original="{{image.oLink}}">
                                                </a>
                                            </div>
                                            {{/customIf}}
                                            <h3><a href="{{replace url}}">{{name}}</a> </h3>
                                            <ul>
                                                {{#each childNodes}}
                                                    {{#customIf @index "<=" 4}}
                                                       <li><a href="{{replace url}}">{{name}}</a></li>
                                                    {{/customIf}}
                                                    {{#customIf @index "==" 5}}
                                                       <li class="view-all"><a href="{{replace ../url}}">See all</a> </li>
                                                    {{/customIf}}
                                                {{/each }}
                                            </ul>
                                        </div>
                                        {{/each }}
                                    </div>
                                </div>
                                {{/customIf}}
                                {{#customIf type "==" 11}}
                                    <div id="shop-tab-{{@index}}" class="shop-tab-content top-Brands">
                                        <div class="shop-products-list">
                                            {{#each childNodes}}
                                                {{#customIf @index "==" 0}}
                                                    <div class="top-brands-top-wrap">
                                                        <a href="{{replace url}}" class="topBrandFullClick"></a>
                                                        {{#customIf image 'neq' null}}
                                                            <div class="top-brands-image">
                                                                <img class="lazy" data-original="{{image.oLink}}" alt="{{image.altText}}" />
                                                            </div>
                                                        {{/customIf}}
                                                        <h3>{{name}}</h3>
                                                        <p>{{higlighter}}</p>
                                                        <a href="{{replace url}}">Explore &#x2192;</a>
                                                    </div>
                                                {{/customIf}}
                                                {{#customIf @index "==" 1}}
                                                    <div class="top-brands-bottom-wrap">
                                                {{/customIf}}
                                                {{#customIf @index ">" 0}}
                                                    <div class="top-brands-block">
                                                        <a href="{{replace url}}" class="topBrandFullClick"></a>
                                                        {{#customIf image 'neq' null}}
                                                            <div class="top-brands-image">
                                                                    <img class="lazy" data-original="{{image.oLink}}" alt="{{image.altText}}" />
                                                            </div>
                                                        {{/customIf}}
                                                        <h3>{{name}}</h3>
                                                        <p>{{higlighter}}</p>
                                                        <a href="{{replace url}}">Explore &#x2192;</a>
                                                    </div>
                                                {{/customIf}}
                                                {{#customIf @index "==" childNodes.length}}
                                                    </div>
                                                {{/customIf}}
                                            {{/each }}
                                        </div>
                                    </div>
                                {{/customIf}}
                        {{/customIf}}
                    {{/each}}
                    <div class="shop-products-pagination">
                        <span class="shop-nav-prev hide shopbycategory_home_previous">&#x2190; &nbsp; Previous</span>
                        <span class="shop-nav-next shopbycategory_home_next">Next &nbsp; &#x2192; </span>
                        <span class="shop-pag-dots">
                            {{#each menuData}}
                                {{#customIf @index "==" 0}}
                                  <i class="dot-selected tab-page-{{@index}}" id="tab-page-{{@index}}"></i>
                                {{/customIf}}
                                {{#customIf @index ">" 0}}
                                    <i class="tab-page-{{@index}}" id="tab-page-{{@index}}"></i>
                                {{/customIf}}
                            {{/each}}
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>
