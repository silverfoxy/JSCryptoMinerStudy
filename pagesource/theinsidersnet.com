<script type="text/javascript">

	function switchModal(e, t) {
		$('#loginForm')
			.modal('hide')
			.find('button.close').trigger('click');
	}

	function redirectToProfile(responseText) {
		var outputDiv = $("#output");
		if (responseText.status && parseInt(responseText.status) === 1) {
			outputDiv.removeClass().addClass('alert alert-success');
		} else {
			outputDiv.removeClass().addClass('alert alert-danger');
		}
		outputDiv.html(responseText.message);
		var registrationButton = $("#submitRegistration");
		registrationButton.hide(1);
		if (responseText.status && parseInt(responseText.status) === 1) {
			if (responseText.pixel != '' && typeof responseText.pixel !== 'undefined') {
				$.ajax({
					type: "POST",
					dataType: 'JSON',
					url: "https://www.theinsidersnet.com/ajax/postdata/",
					data: {actie: 'runPixel', pixel: responseText.pixel},
					success: function (response) {
						$("#response-div").html(response);
					}
				});
			}

			// RUN GERMAN PIXEL. PLEASE REMOVE THIS AFTER A FEW MONTHS (placed on 8-3-2018)
												                                                                                                                                                                                                                                                                                                                                                                                                                                                        																
																																															
			if (responseText.redirect == 'entrysurvey') {
				setTimeout(2000)
				location.href = responseText.redirectUrl;
			} else if (responseText.redirect == 'refresh') {
				setTimeout(2000)
				location.reload();
			} else if (responseText.redirect == 'none') {
				$('#registerForm').modal('hide');
				$('#collapeRegister').addClass('collapse');
			} else if (responseText.redirect == 'custom') {
				setTimeout(2000)
				location.href = responseText.redirectUrl;
			}else{
											}
		} else {
			registrationButton.show(1000);
		}
		grecaptcha.reset();
	}
</script>
