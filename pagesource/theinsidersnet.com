        ss.media = 'only x';

        head.appendChild(ss);

        setTimeout( function(){
            // set media back to `all` so that the stylesheet applies once it loads
            ss.media = 'all';
        },0);
    }

    var loadJS = function(url, implementationCode){

        var scriptTag = document.createElement('script');
        scriptTag.src = url;

        if (implementationCode!==undefined) {
            scriptTag.onload = implementationCode;
            scriptTag.onreadystatechange = implementationCode;
        }

        document.body.appendChild(scriptTag);
    };



    (function(funcName, baseObj) {
        baseObj[funcName] = function(callback, context) {
            //alert(callback);
            //setTimeout(callback,1000);
            var fn = window[funcName];
            if (readyForLoading) {
                if (callback!==undefined) {
                    callback.apply();
                }
                return void(0);
            }

            setTimeout(function() {
                fn.apply(this,[callback,context]);
            },50);
        };

    })("IsReadyForLoading", window);

    function loadFinal()
    {
        readyForLoading=true;
        if (typeof readyStateFunctionHelper!=="undefined") {
            setTimeout(function() {
                readyStateFunctionHelper();
            },10);
        }
        initAjaxFormValidation();
    }
    function loadGoogleJs()
    {
        loadJS('//www.google.com/recaptcha/api.js?hl=en',loadFinal);
