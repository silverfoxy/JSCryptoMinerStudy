<script type="text/javascript">

    function fb_login(inviteFriends) {
        if (inviteFriends === undefined) {
            inviteFriends = false;
        }

        FB.login(function (response) {
            if (response.status === 'connected') {
                if (true === inviteFriends) {
                    sessionStorage.setItem('onReload', 'openFriends');
                }

                // Logged into your app and Facebook.
                var callbackUrl = "https://www.theinsidersnet.com/home/authenticate/";
                callbackUrl += (document.getElementById('uri') ? '?returnUrl=' + document.getElementById('uri').value : '');
                window.location.replace(callbackUrl);
            } else if (response.status === 'not_authorized') {
                // The person is logged into Facebook, but not your app.
                //error message not authorized
            } else {
                // The person is not logged into Facebook, so we're not sure if
                // they are logged into this app or not.
            }
        }, {perms: 'public_profile,email'});
    }

    window.fbAsyncInit = function() {
        FB.init({
            appId      : 420966981282692,
            xfbml      : true,
            cookie     : true,
            version    : 'v2.8'
        });
        FB.AppEvents.logPageView();
        FB.getLoginStatus(function(response) {
            if (response.status === 'connected') {
                if (response.authResponse.userID) {
                    var fbUid = response.authResponse.userID;
                    //console.log(fbUid);
                    $.ajax({
                        type: "POST",
                        dataType: 'JSON',
                        url: "/ajax/fbcheck",
                        data: 'fbUid=' + fbUid + '&action=fbcheck',
                        success: function(html){
                            if (html.status && html.status=="nok") {
                                $("#fb_warning")
                                    .removeClass('hidden')
                                    .html('<div class="alert alert-danger">Facebook account is related to Insider profile ' + html.nickname + '.</div>');
                            }
                        }
                    });
                }
                if(document.getElementById('fb_login')){
                    $('#fb_login').hide();
                }
            } else if (response.status === 'not_authorized') {
                // the user is logged in to Facebook,
                // but has not authenticated your app
            } else {
                // the user isn't logged in to Facebook.
            }
        });
    };
    (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_GB/sdk.js#xfbml=1&version=v2.8";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
</script>

