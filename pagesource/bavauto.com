<script>
(function($, undefined){

    $('select.my-vehicles').on('change', function() {

    	
    	if(this.value == "addVehicle"){
			addVehicle();
			return;
    	}
    	
        if(!this.value) {
		    window.location = 'https://www.bavauto.com/customer/account/edit/';
	        return;
        }

        var values = this.value.split('-');

        var form = $(this).parents('form').first();
        $('select[name^="finder["]', form).each(function() {
            var input = $('<input />', { type: 'hidden', name: this.name, value: values.shift()});

            $(this).replaceWith(input);
        });

        form.submit();
    });


	function addVehicle(){
	   
	   var make_value = $("#finder-1--1").val();
	   var year_value = $("#finder-1--2").val();
	   var model_value = $("#finder-1--3").val();
	
	   var make_text = $("#finder-1--1 option:selected").text().trim();
	   var year_text = $("#finder-1--2 option:selected").text().trim();
	   var model_text = $("#finder-1--3 option:selected").text().trim();
	
	   if(make_value != "" && year_value != "" && model_value != ""){
		   $.ajax({
				url:'https://www.bavauto.com/amfinder/index/addVehicle/',
				type: "POST",
				dataType: "json",
				data:{
			   		'car_make_value': make_value,
			   		'car_year_value': year_value,
			   		'car_model_value': model_value,
			   		'car_make_text': make_text,
			   		'car_year_text': year_text,
			   		'car_model_text': model_text,
			   		'form_key': 'WSxashBv8EPSnjrR'
			   		},
				success:function(data){
					var garage_found = data.found_open_garage;
					var updateAcctInfoDD = false;
					var vehicleDD = null;
					if(garage_found > 0){
						//add to dropdown
						theValue = data.make_value+"-"+data.year_value+"-"+data.model_value;
						theText = data.make_text+"-"+data.year_text+"-"+data.model_text;
						$(".select_box.my-vehicles option:nth-last-child(2)").before($("<option></option>").attr("value",theValue).attr("selected","selected").text(theText));
						$(".select_box.my-vehicles").trigger("chosen:updated"); //update nice dropdown too
	
						//update account information if garage with vehicles found
						switch(garage_found){
							case 1:
								if(typeof customerVehicle1 !== "undefined"){
									updateAcctInfoDD = true;
									vehicleDD = jQuery(customerVehicle1.selects);
								}
								
								break;
							case 2:
								if(typeof customerVehicle2 !== "undefined"){
									updateAcctInfoDD = true;
									vehicleDD = jQuery(customerVehicle2.selects);
								}
						        break;
						    case 3:
						    	if(typeof customerVehicle3 !== "undefined"){
									updateAcctInfoDD = true;
									vehicleDD = jQuery(customerVehicle3.selects);
								}
						        break;
						    default:
						    	updateAcctInfoDD = false;
						}
						
						
						if(updateAcctInfoDD){
							var dfd = jQuery.Deferred();
							function dd1() {
								
								vehicleDD.eq(0).val(data.make_value).get(0).fire('amFinder:change');
								vehicleDD.eq(0).val(data.make_value).trigger('change');
							}
							function dd2() {
								
								vehicleDD.eq(1).val(data.year_value).get(0).fire('amFinder:change');
								vehicleDD.eq(1).val(data.year_value).trigger('change');
							  	setTimeout(dd3,4000);
							}
							function dd3() {
								
								vehicleDD.eq(2).val(data.model_value);
								vehicleDD.eq(2).val(data.model_value).trigger('change');
							}
		
							dfd.done(dd1,setTimeout(dd2,3000));
							dfd.resolve();
						}
										
					}
					else{
						console.log("found no garage open");
					}
				}
			});
	   }
	   
	}
    

})(jQuery);
</script>                <div class="amfinder-buttons">
