        var love_url = "http://love.bobo.com/recommend/channel/rooms?channel=jy_163.blog&callback=loveData2";
        window.loveData2  =  function(data){
            loveitem = data.rooms[0];
        };
        function loadLoveData(){
            NTES.ajax.importJs(love_url,function(){
                var list = document.getElementById("js-bobo-content");
                if(loveitem){
                    var newItem = document.createElement("li");
                    var a = document.createElement("a");
                    a.className = "hover-show-box";
                    a.href = encodeURI(loveitem.url) + '?f=jy_163.blog';
                    var img = document.createElement("img");
                    img.src = encodeURI(loveitem.coverUrl);
                    var em = document.createElement("em");
                    em.className = "png24 live-tip";
                    em.innerHTML = escapeHTML(loveitem.platform != "love" ? "直播" : "交友");
                    var span = document.createElement("span");
                    span.className = "full-hover-show semi-trans";
                    var span2 = document.createElement("span");
                    span2.className = "icon-play png24";
                    var span3 = document.createElement("span");
