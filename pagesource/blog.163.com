        // http://blog.163.com/loginGate.do?target="+_target+"&blogActivation=true&from=login
        console.log("refresh")
        var pInfo = String(NTES.cookie.get("P_INFO")),
            sInfo = String(NTES.cookie.get("S_INFO"));
        if(pInfo && sInfo){
            NTES.ajax.importJs('http://blog.163.com/loginMyInfo.do', function(){BlogIndexLogin.getUserInfo(pInfo.split("|")[0])}, 'gbk');
        }else{
            BlogIndexLogin.login()
        }
    }
