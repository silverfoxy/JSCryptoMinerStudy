                    var em2 = document.createElement("em");
                    em2.className = "png24 icon-fans";
                    var i2 = document.createElement("i");
                    i2.innerHTML = escapeHTML(loveitem.onLineCount);
                    span3.appendChild(em2);
                    span3.appendChild(i2);
                    
                    a.appendChild(img);
                    a.appendChild(em);
                    a.appendChild(span);
                    a.appendChild(span2);
                    a.appendChild(span3);
                    var p = document.createElement("p");
                    p.className = "anchor-name";
                    var a2 = document.createElement("a");
                    a2.className = "left";
                    a2.href = encodeURI(loveitem.url) + '?f=jy_163.blog';
                    a2.innerHTML = escapeHTML(loveitem.name);
                    p.appendChild(a2);
                    newItem.appendChild(a);
                    newItem.appendChild(p);
                    list.insertBefore(newItem,list.childNodes[1]);
                }
            }, 'utf-8');
        }
        function escapeHTML(str) {
            return String(str)
                .replace(/&/g, '&amp;')
                .replace(/"/g, '&quot;')
                .replace(/'/g, '&#39;')
                .replace(/</g, '&lt;')
                .replace(/>/g, '&gt;');
        }
    })();
