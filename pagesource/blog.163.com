/* 娱乐视觉 */
new NTES.ui.Carousel( $("#fn_focus .focus_ctrls li") ,$("#focus_show .focus_panel") , "current" , { prev: $("#fn_focus_prev_next .prev") , next :$("#fn_focus_prev_next .next"),event: "click" , interval : 5000,duration: 2000,auto: true });
/*切换*/
var Slide = NTES.ui.Slide;
new Slide($("#title-bar-tab li"), $("#col02-box .col02-content"), "on", "mouseover");
new Slide($("#tab-box-1 .tab-ctrls-2 > .tab-ctrl-2"), $("#tab-box-1 .tab-con-box-1 > .tab-con-1"), "current", "mouseover", 5000);
new Slide($("#tab-box-2 .tab-ctrls-2 > .tab-ctrl-2"), $("#tab-box-2 .tab-con-box-1 > .tab-con-1"), "current", "mouseover", 5000);
new Slide($("#tab-box-3 .tab-ctrls-2 > .tab-ctrl-2"), $("#tab-box-3 .tab-con-box-1 > .tab-con-1"), "current", "mouseover", 5000);
new Slide($("#tab-box-4 .tab-ctrls-1  .tab-ctrl-1"), $("#tab-box-4 .tab-con-box-1 > .tab-con-1"), "current", "mouseover", 5000);
new Slide($("#red-bg-tabs-1 .red-bg-tab"), $("#red-bg-tab-con-1 .tab-con-1"), "current", "mouseover", 5000);
new Slide($("#red-bg-tabs-2 .red-bg-tab"), $("#red-bg-tab-con-2 .tab-con-1"), "current", "mouseover", 5000);
var    isScrolling = false,  invokeFunction;
/**
 * 到页面某处
 * @param acceleration 加速度
 * @param time 时间间隔 (毫秒)
 **/
function goTop(acceleration, time) {
    acceleration = acceleration || 0.1;
    time = time || 16;
    var x1 = 0;
    var y1 = 0;
    var x2 = 0;
    var y2 = 0;
    var x3 = 0;
    var y3 = 0;
    if (document.documentElement) {
        x1 = document.documentElement.scrollLeft || 0;
        y1 = document.documentElement.scrollTop || 0;
    }
    if (document.body) {
        x2 = document.body.scrollLeft || 0;
        y2 = document.body.scrollTop || 0;
    }
    x3 = window.scrollX || 0;
    y3 = window.scrollY || 0;
    // 滚动条到页面顶部的水平距离
    var x = Math.max(x1, Math.max(x2, x3));
    // 滚动条到滚动到部位的垂直距离
    var y = Math.max(y1, Math.max(y2, y3));
    // 滚动距离 = 目前距离 / 速度, 因为距离原来越小, 速度是大于 1 的数, 所以滚动距离会越来越小
    var speed = 1 + acceleration;
    window.scrollTo(Math.floor(x / speed), Math.floor(y  / speed));
    // 如果距离不为零, 继续调用迭代本函数
    if(x > 0 || y > 0) {isScrolling = true;
        invokeFunction = "goTop(" + acceleration + ", " + time + ")";
        window.setTimeout(invokeFunction, time);
    }else{isScrolling = false;}
}
/* 滚动条控制 */
var scrollArr = [], scrollArrLen = 0, listNav = NTES("#list-nav"), listNavItemArr = NTES.all("a", listNav);
var getElementPosition = function(e) {
    var x = 0, y = 0;
    while (e != null) {
        x += e.offsetLeft;
        y += e.offsetTop;
        e = e.offsetParent;
    }
    return { x: x, y: y };
};
var getAnchorTarget = function(hrefStr){
    var anchorArr = hrefStr.split("#"), anchorArrLen = anchorArr.length, anchorTargetId = anchorArr[anchorArrLen - 1];
    return document.getElementById(anchorTargetId);
};
function initScrollPositions(){
    var titleHeight = 55,//标题的高度，减轻误差
            windowHeight = window.innerHeight || document.documentElement.clientHeight;
    for(i = 0, len = listNavItemArr.length; i < len; i++){
        if(!i){scrollArr.push(getElementPosition(getAnchorTarget(listNavItemArr[i].href)).y - getAnchorTarget(listNavItemArr[i].href).clientHeight);}
        scrollArr.push(getElementPosition(getAnchorTarget(listNavItemArr[i].href)).y - titleHeight);
        if(i + 1 === len){
            scrollArr.push(NTES("#footer").offsetTop - windowHeight);
        }
    }
    scrollArrLen = scrollArr.length;
}
initScrollPositions();
addEvent(window, "resize", initScrollPositions);
addEvent(window, "scroll",function(){
    if(isScrolling){clearTimeout(invokeFunction);}
    if (document.documentElement) {
        y1 = document.documentElement.scrollTop || 0;
    }
    if (document.body) {
        y2 = document.body.scrollTop || 0;
    }
    var y3 = window.scrollY || 0;
    var y = Math.max(y1, Math.max(y2, y3));
    if(y >= scrollArr[0]){
        if(y >= scrollArr[scrollArrLen - 1]){NTES(".current", listNav).removeCss("current");}else{
            for(i = 1; i < scrollArrLen; i++){
                if(y < scrollArr[i + 1]){
                    try{NTES(".current", listNav).removeCss("current");}catch(e){}
                    NTES(listNavItemArr[i - 1]).addCss("current");
                    break;
                }
            }}}else{NTES(".current", listNav).removeCss("current");}
});
//悬浮导航
window.onload = function() {
    var btn = document.getElementById("float-menu");
    var d = document.documentElement;
    var h = d.clientHeight * 2;
    addEvent(window, "scroll", function() {
        var scrollTopValue = document.documentElement.scrollTop+document.body.scrollTop;
        btn.style.display = (scrollTopValue  > 400 && scrollTopValue < scrollArr[scrollArrLen - 1]) ? 'block' : "none";
    });
};
/* 日历 */
// 显示 隐藏图层
function PopUpLayer(ctrl, content) {
    var timerId;
    var hide = function () {
        content.style.display = "none";
    };
    this.hide = function (event) {
        timerId = setTimeout(hide, 300);
    };
    ctrl.addEvent("mouseover", function () {
        content.style.display = "block";
    });
    ctrl.addEvent("mouseout", this.hide);
    content.addEvent("mouseout", this.hide);
    content.addEvent("mouseover", function () {
        clearTimeout(timerId);
    });
}
var calendarLayer = new PopUpLayer(NTES("#getCalendar"), NTES("#calendarBox"));
var topCalendarLayer = new PopUpLayer(NTES("#getTopCalendar"), NTES("#topCalendarBox"));
var PrintDay = function () {
    var d = new Date();
    this.getW = function (d) {
        switch (d) {
            case 0:
                return "星期日";
            case 1:
                return "星期一";
            case 2:
                return "星期二";
            case 3:
                return "星期三";
            case 4:
                return "星期四";
            case 5:
                return "星期五";
            case 6:
                return "星期六";
        }
    }
    //$("#todayDom").innerHTML = (d.getMonth() + 1) + "月" + d.getDate() + "日 " + this.getW(d.getDay());
}
PrintDay();
var getLoadData = function (xhr) {
    var data = xhr.responseText;
    if (data) {
        $("#loadXmlData").innerHTML = data;
    }
    else {
        $("#loadXmlData").innerHTML = "没有会议！";
    }
}
var Calendar = function () {
}
Calendar.cDate = new Date();
Calendar.year = Calendar.cDate.getFullYear();
Calendar.month = Calendar.cDate.getMonth();
Calendar.date = Calendar.cDate.getDate();
Calendar.init = function (y, m, fun, loadData) {
    var month;
    if (Calendar.month + 1 == 12) {
        month = 12;
    }
    else {
        month = Calendar.month + 1;
    }
    $(y).innerHTML = Calendar.year;
    $(m).innerHTML = month;
    if (loadData) {
        var tm;
        if (month < 10) {
            tm = "0" + month.toString();
        }
        else {
            tm = month;
        }
        NTES.ajax.send("http://blog.163.com/datemonth/" + Calendar.year + "/" + tm, "get", null, {
            onSuccess: getLoadData
        })
    }
    Calendar.draw(fun);
}
//获的月的天数
Calendar.monthDayNum = function (year, month) {
    var tdate = new Date(year, month + 1, 0);
    return tdate.getDate();
}
//获的指定1号是星期几
Calendar.dayInWeek = function (year, month, date) {
    var tdate = new Date(year, month, date);
    return tdate.getDay();
}
//打印日历
Calendar.draw = function (callback) {
    var year = Calendar.year;
    var month = Calendar.month;
    var currentDay = Calendar.date;
    var firstDayInWeek = Calendar.dayInWeek(year, month, 1);
    var monthDayNum = Calendar.monthDayNum(year, month, 0);
    callback(year, month, currentDay, firstDayInWeek, monthDayNum);
}
//上一月 下一月
Calendar.NP = function (yearID, monthID, state, fun, loadData) {
    var yDom = $(yearID);
    var mDom = $(monthID);
    var year = parseInt(yDom.innerHTML);
    var month = parseInt(mDom.innerHTML);
    if (state == "next") {
        month = month + 1;
        if (month > 12) {
            month = 1;
            year = year + 1;
        }
    }
    if (state == "previous") {
        month = month - 1;
        if (month == 0) {
            month = 12;
            year = year - 1;
        }
    }
    Calendar.month = month - 1;
    Calendar.year = year;
    yDom.innerHTML = year;
    mDom.innerHTML = month;
    if (loadData) {
        var tm;
        if (month < 10) {
            tm = "0" + month.toString();
        }
        else {
            tm = month;
        }
        NTES.ajax.send("http://blog.163.com/datemonth/" + year + "/" + tm, "get", null, {
            onSuccess: getLoadData
        })
    }
    Calendar.draw(fun)
}
//显示日历
Calendar.print = function (year, month, currentDay, firstDayInWeek, monthDayNum) {
    var year = year;
    var month = month;
    var currentDay = currentDay;
    var firstDayInWeek = firstDayInWeek;
    var monthDayNum = monthDayNum;
    var displayMonth, displayDay;
    var liarray = [];
    if (month < 9) {
        month = month + 1
        displayMonth = "0" + month;
    }
    else {
        displayMonth = month + 1;
    }
    for (var i = 0; i < firstDayInWeek; i++) {
        liarray.push("<li></li>");
    }
    for (var j = 1; j <= monthDayNum; j++) {
        if (j < 10) {
            displayDay = "0" + j;
        }
        else {
            displayDay = j;
        }
        if (j == currentDay) {
            liarray.push("<li><a class=\"current\" href=\"http://snapshot.news.163.com/wgethtml/http+!!blog.163.com!/" + year + "-" + displayMonth + "/" + displayDay + "/12.html\">" + j + "</a></li>");
        }
        else {
            liarray.push("<li><a href=\"http://snapshot.news.163.com/wgethtml/http+!!blog.163.com!/" + year + "-" + displayMonth + "/" + displayDay + "/12.html\">" + j + "</a></li>");
        }
    }
    var str = liarray.join("");
    $("#calendarList").innerHTML = str;
    $("#topCalendarList").innerHTML = str;
}
//初始化
Calendar.init("#setYear", "#setMonth", Calendar.print);
$("#preMonth").addEvent("click", function (e) {
    Calendar.NP("#setYear", "#setMonth", "previous", Calendar.print);
    e.preventDefault();
});
$("#nextMonth").addEvent("click", function (e) {
    Calendar.NP("#setYear", "#setMonth", "next", Calendar.print);
    e.preventDefault();
});
Calendar.init("#topSetYear", "#topSetMonth", Calendar.print);
$("#topPreMonth").addEvent("click", function (e) {
    Calendar.NP("#topSetYear", "#topSetMonth", "previous", Calendar.print);
    e.preventDefault();
});
$("#topNextMonth").addEvent("click", function (e) {
    Calendar.NP("#topSetYear", "#topSetMonth", "next", Calendar.print);
    e.preventDefault();
});
/* 日历 end */
</script>
