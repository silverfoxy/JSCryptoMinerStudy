<script type="text/javascript">
    $(document).ready(function () {
        var app_id = '215251915272795';       
        $.ajaxSetup({ cache: true });
        $.getScript('//connect.facebook.net/en_US/sdk.js', function () {
            FB.init({
                appId: app_id,
                xfbml: true,
                version: 'v2.5'
            });
            update_like_count();
            FB.Event.subscribe('edge.create', function (href, widget) {
                update_like_count();
            });
            FB.Event.subscribe('edge.remove', function (href, widget) {
                update_like_count();
            });
        });

        var update_like_count = function () {
            var ajax_url = $.smart_site.base_url + '/smart-includes/ajax/fb-ajax.php';
            $.ajax(ajax_url).done(function (data) {
                $('#fb_likes_count').html(nFormatter(data) + ' likes');
            });
        }

        var nFormatter = function (num) {
            if (num >= 1000000000) {
                return (num / 1000000000).toFixed(2).replace(/\.0$/, '') + 'G';
            }
            if (num >= 1000000) {
                return (num / 1000000).toFixed(2).replace(/\.0$/, '') + 'M';
            }
            if (num >= 1000) {
                return (num / 1000).toFixed(2).replace(/\.0$/, '') + 'K';
            }
            return num;
        }
    });
</script>

