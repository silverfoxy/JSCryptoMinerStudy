		$("#ticker li:first").slideUp("slow", function () {
		$(this).css("visibility", "hidden");
		$(this).appendTo($("#ticker"));
	    
		
		var ticker_height=$("#ticker").height();
		var elemts_height_sum=0;

		$("#ticker li").show();
		$("#ticker li").each(function( index ) {
			elemts_height_sum+=$(this).outerHeight(true);
				if(elemts_height_sum>ticker_height){
					if($(this).css("visibility") == "visible")
						$(this).css("visibility", "hidden").show().fadeOut("slow");
					}else{
					if($(this).css("visibility") == "hidden")
						$(this).css("visibility", "visible").hide().fadeIn();
					
				}
			});
		});

	}
