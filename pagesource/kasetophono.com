<div id='persoff'></div>
<script type='text/javascript'>
var per = 0;
$(document).ready(function(){
  $("#persoff").css("height", $(document).height()).hide();
  $(document).click(function(e) {
	if(!$(e.target).hasClass('switch') && per == 1) {
	  $("#persoff").fadeToggle();
	  per = 0;
	}
  });
  $(".switch").click(function(){
    $("#persoff").fadeToggle();
	per += 1;
    if (per == 2) {
	  per = 0;
	}
  });
});
</script>
