		function AcceptWarning(cookieName, expiresSeconds) {
			var expires = new Date();
			expires.setMilliseconds(expiresSeconds * 1000);
			expires = expires.toGMTString();

			$('#'+cookieName).fadeToggle( "slow", "linear" );

			setCookie(cookieName, 1, expires, '/', false, false);

			return false;
		}

