    <script>
        (function (window, $) {
            // Main script
            var $emailInput = $('.ck-landing__email-input');
            var $emailSend = $('.ck-landing__email-button');
            var $autoLoginForm = $('.js-landing-autologin-form');
            var captchaIds = {};

            var submissionApproved = false;
            var currentForm = null;

            var handleButtonClick = function (location, $form) {
                var $emailInput = $form.find('.ck-landing__email-input');
                var $emailSend = $form.find('.ck-landing__email-button');

                logEvent('LandingColorButtonClicked', {
                    LandingEmail: $emailInput.val(),
                    Email: window.USER.email || $emailInput.val(),
                    LandingType: getLandingType(),
                    Location: location,
                });

                if ($emailInput.val() && !$emailSend.attr('disabled')) {
                    currentForm = $form;
                    grecaptcha.execute(captchaIds[location]);

                } else {
                    $emailInput.focus();
                    $emailInput.toggleClass('ck-landing__email-input_error', true);
                }
            };

            $emailInput.on('input', function (evt) {
                var value = $(this).val();
                var $emailSend = $(this).parent()
                    .siblings('.ck-landing__email-button');

                $emailInput.toggleClass('ck-landing__email-input_error', false);

                if (/^[^@]+@([^@]\.?)+[a-z]*[^\.]$/i.test(value)) {
                    $emailSend.removeAttr('disabled');

                    $emailSend.toggleClass('ck-button_mask-disabled', false);

                } else {
                    $emailSend.attr('disabled', !!value);

                    $emailSend.toggleClass('ck-button_mask-disabled', !value);
                }

                if (value) {
                    $(this).next().show();
                } else {
                    $(this).next().hide();
                }
            });

            $('.ck-landing__email-button-mask', $emailSend)
                .on('click', function (evt) {
                    if (submissionApproved) return;

                    evt.preventDefault();

                    var $form = $(this).closest('.js-landing-autologin-form');
                    var formLocation = $form.attr('data-location');

                    handleButtonClick(formLocation, $form);
                });

            $autoLoginForm
                .on('submit', function (evt) {
                    if (submissionApproved) return;

                    evt.preventDefault();

                    var formLocation = $(this).attr('data-location');

                    handleButtonClick(formLocation, $(this));
                });

            window.autoLoginFormSubmit = function () {
                submissionApproved = true;
                currentForm.submit();
            };

            window.CaptchaCallback = function () {
                var recaptchaParams = {
                    sitekey: '6LfUxhwUAAAAAIgOwmlS_XszJFVjiAIN8BVvT9Xv',
                    callback: function () {
                        submissionApproved = true;
                        currentForm.submit();
                    },
                    size: 'invisible'
                };

                captchaIds['top'] = grecaptcha.render('emailForm1', recaptchaParams);
                captchaIds['bottom'] = grecaptcha.render('emailForm2', recaptchaParams);
            };

            logEvent('LandingShown', {
                ExperimentName: 'Landing',
                ExperimentValue: getLandingType(),
                UserAgent: window.navigator.userAgent,
                ScreenResolution: window.screen.availWidth + 'x' + window.screen.availHeight
            });
        }(window, jQuery));
    </script>
