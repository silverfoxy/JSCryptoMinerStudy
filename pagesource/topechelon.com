<script type="text/javascript"> 

function appendQueryParam(param, val){
  jQuery("a").each(function(i,v){
      var link = jQuery(this);
      if (link.attr("href") != null && link.attr("href").indexOf(param + "=") == -1){     
        if (link.attr("href").indexOf("?") < 0){
          link.attr("href", link.attr("href") + "?" + param + "=" + val);
        }else{
          link.attr("href", link.attr("href") + "&" + param + "=" + val);
        }
      }
    });
}

