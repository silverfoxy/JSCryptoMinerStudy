<script> //Fixed Sidebar on Scroll
 var distance = 550;
 var maxTop, y, sidebarTop, newtop;

 function setPageValues(){
  if (jQuery('.sidebar-primary').length == 0 || jQuery(".footer-top").length == 0) return;
  sidebarTop = jQuery('.sidebar-primary').offset().top - parseFloat(jQuery('.sidebar-primary').css('marginTop').replace(/auto/, 0));
    maxTop = jQuery(".footer-top").position().top - jQuery(".sidebar-primary").outerHeight();
    distance = 550;
    newtop = maxTop - distance;
 }

jQuery(window).load(function() {
    setPageValues();

    jQuery(window).scroll(function(event) {
        y = jQuery(this).scrollTop();
        jQuery('.sidebar-primary, #guide-gloss-menu, #guide-gloss-mobile-menu').css({
            position: '',
            top: ''
        });
        if (y >= maxTop) {
            jQuery('.sidebar-primary, #guide-gloss-menu, #guide-gloss-mobile-menu').css({
                position: 'absolute',
                top: (newtop) + 'px'
            });
        } else if (y + 94 >= sidebarTop) {
            jQuery('.sidebar-primary, #guide-gloss-menu, #guide-gloss-mobile-menu').addClass('fixed');
            jQuery('.cta-fixed-bar').fadeIn();
        } else {
            jQuery('.sidebar-primary, #guide-gloss-menu, #guide-gloss-mobile-menu').removeClass('fixed');
            jQuery('.cta-fixed-bar').fadeOut();
        }
    });
});
</script>
<script type="text/javascript"> // Smooth Scrolling
jQuery(function() {
  jQuery('a[href*="#"]:not([href="#"])').click(function() {
    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
      var target = jQuery(this.hash);
      target = target.length ? target : jQuery('[name=' + this.hash.slice(1) +']');
      if (target.length) {
        jQuery('html,body').animate({
          scrollTop: target.offset().top
        }, 1000);
        return false;
      }
    }
  });
});
</script>
