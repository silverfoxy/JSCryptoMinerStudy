<script type="text/javascript">
	function reposfloat(){
		var pos = $('#oohan').position();
		$('#float').css({
			left:(pos.left-3)+'px', top:pos.top+'px'
		});
	}
	var tm;
	$('#oohan').mouseover(function(){
		var pos = $(this).position();
		$('#float').css({
			left:(pos.left-3)+'px',
			top:pos.top+'px',
			width : '90px'
		}).show();
		tm = setInterval(function(){ reposfloat(); },100);
		$('#float').mouseleave(function(){
			$(this).hide();
			clearInterval(tm);
		});
	});
	$('#ht_phone_icon').mouseover(function(){
		var pos = $(this).position();
		ewm_item=$('#ewm_m_area img');
		if (ewm_item.attr("src") === undefined || ewm_item.attr("src") === false){
			ewm_item.attr("src",ewm_item.attr('rel'));
		}
		$('#ewm_m_area').css({
			left:(pos.left-22)+'px',
			top:(pos.top+30)+'px'
		}).show();
		$('#ewm_m_area').mouseleave(function(){
			$(this).hide();
		});
	}).mouseout(function(){
		$('#ewm_m_area').hide();
	});
</script>
