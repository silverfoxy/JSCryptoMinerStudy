<script type="text/javascript">
var Cookie = {};
Cookie.set = function(name,value,exptime,path,domain){
    var exp  = new Date();
    exp.setTime(exp.getTime() + exptime*1000);
	if(!path) path='/';
	cookiestr = name + "="+ escape (value) + ";expires=" + exp.toGMTString();
	cookiestr+=';path='+path;
	if(domain) cookiestr+=';domain='+domain;
    document.cookie = cookiestr;
}
Cookie.get = function(name){
    var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
     if(arr != null) return unescape(arr[2]); return null;
}
function close_huiyi_ad(id){	
	$('#mask').hide();
    $('#'+id).hide();
    Cookie.set('ad_'+id, 'close',86400);

}
var al = Cookie.get('ad_ad_huiyi');
if(!al){
	$('#mask').show();
	$('#ad_huiyi').show();
}
</script>
