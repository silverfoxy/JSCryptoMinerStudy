				{
						saintHtml += "<li>없음</li>";			
						saintHtml2 += "<li>없음</li>";			
				}
				else
				{
					for (var i = 0; i < data.length; i++) {

							if (i > 0 && parseInt(i%3) == 0)
							{
								saintHtml += "</ul>";
								saintHtml += "<ul class='saint_in'>";
							}	
							
							saintHtml += "<li class='saintinfo pointer' saint-id='"+ data[i]['saintid'] +"' >" + data[i]['fullname'] + "</li>";		
							if (i < 10)
							{
							saintHtml2 += "<li class='saintinfo pointer' saint-id='"+ data[i]['saintid'] +"' >" + data[i]['fullname'] +" <strong>"+data[i]['name2']+ "</strong></li>";
