	function biblePersonalRank()
	{

		var currentdate = new Date(); 
		var datetime = currentdate.getFullYear() + "-"
						+ (currentdate.getMonth()+1)  + "-" 
						+ currentdate.getDate();
		
		var rank = 3;
			
		$.ajax({ 
			type: "POST", 
			url: "json/bibleGate.asp", 
			data: "type=write_rank&rank="+rank+"&date=" + datetime, 
			dataType: "json", 
			success: function(data){ 
				var userhtml = "", ratehtml = "", dayhtml = "";
				var user  = "" , rate = "", day = "";
			   for (var i = 0; i < data.length; i++) {
					user = data[i]['USERNAME'] + '(' + data[i]['USERID'] + ')';
					userhtml = userhtml +"<p>" + user + "</p>";
				}  

			   for (var i = data.length-1; i >= 0  ; i--) {
					day = data[i]['WRITECOUNT'];
					dayhtml = dayhtml +"<p>" + day + "절</p>";

					rate = ( data[i]['WRITECOUNT'] / 35567 ) * 100;
					rate = round(rate, 2);
					ratehtml = ratehtml +"<p>" + rate + "%</p>";

				}  
					$('#user_user').html(userhtml);
					$('#user_rate').html(ratehtml);
					$('#user_day').html(dayhtml);

			},
			error: function(request,status,error){
					//$('#friend_num').text("오류");
					//alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
			}

		}); 
	}

