            imgMoveDownOn.src = "/Images/button-move-down-on.gif";
            imgMoveDownOff = new Image();
            imgMoveDownOff.src = "/Images/button-move-down.gif";
            imgMoveUpOn = new Image();
            imgMoveUpOn.src = "/Images/button-move-up-on.gif";
            imgMoveUpOff = new Image();
            imgMoveUpOff.src = "/Images/button-move-up.gif";
        }

        $(document.body).getElements('a.Minimize').each(function(item) {
            item.addEvent('mouseenter', function() { item.getChildren('img')[0].set('src', imgMinOn.src); });
            item.addEvent('mouseleave', function() { item.getChildren('img')[0].set('src', imgMinOff.src); });
            item.addEvent('click', function() { min(item) });
        });

        $(document.body).getElements('a.MoveUp').each(function(item) {
            item.addEvents({ 'mouseover': function() { item.getChildren('img')[0].set('src', imgMoveUpOn.src) }, 'mouseout': function() { item.getChildren('img')[0].set('src', imgMoveUpOff.src) }, 'click': function() { moveUp(item) } });
        });

        $(document.body).getElements('a.MoveDown').each(function(item) {
            item.addEvents({ 'mouseover': function() { item.getChildren('img')[0].set('src', imgMoveDownOn.src) }, 'mouseout': function() { item.getChildren('img')[0].set('src', imgMoveDownOff.src) }, 'click': function() { moveDown(item) } });
        });

        attachTips();

        if ($chk($('divMostActiveBoards')) && Cookie.read('strdivMostActiveBoardsMin') == 'Min') $('divMostActiveBoards').setStyle('height', 0);
        if ($chk($('divMoreToDo')) && Cookie.read('strdivMoreToDoMin') == 'Min') $('divMoreToDo').setStyle('height', 0);

        $(document.body).getElements('div.SideModule').each(function(item) {
            if ($chk(Cookie.read('str' + item.get('id') + 'Pos'))) item.inject($(Cookie.read('str' + item.get('id') + 'Pos')));
        });
    }

    function changeTab(strTab, strNum) {
        $('divTop' + selTab).set('class', 'tabs_sprite-green-main_top');
        $('divBottom' + selTab).set('clpass', 'tabs' + selNum + ' tabs_sprite-green-main_bottom');
        if ($chk($('divTopSep' + selTab))) $('divTopSep' + selTab).set('class', 'tabs_sep tabs_sprite-green-sep_top');
        if ($chk($('divBottomSep' + selTab))) $('divBottomSep' + selTab).set('class', 'tabs_sep tabs_sprite-green-sep_bottom');

        if (selNum == '1') {
            $('divMenuLeftTop').set('class', 'tabs_sprite-green-left_top');
            $('divMenuLeftBottom').set('class', 'tabs_end tabs_sprite-green-left_bottom');
        }
        if (selNum == '2') {
            $('divMenuRightTop').set('class', 'tabs_sprite-green-right_top');
            $('divMenuRightBottom').set('class', 'tabs_end tabs_sprite-green-right_bottom');
        }

        $('divTop' + strTab).set('class', 'tabs_sprite-blue-main_top');
        $('divBottom' + strTab).set('class', 'tabs' + strNum + ' tabs_sprite-blue-main_bottom');
        if ($chk($('divTopSep' + strTab))) $('divTopSep' + strTab).set('class', 'tabs_sep tabs_sprite-blue-sep_top');
        if ($chk($('divBottomSep' + strTab))) $('divBottomSep' + strTab).set('class', 'tabs_sep tabs_sprite-blue-sep_bottom');

        if (strNum == '1') {
            $('divMenuLeftTop').set('class', 'tabs_sprite-blue-left_top');
            $('divMenuLeftBottom').set('class', 'tabs_end tabs_sprite-blue-left_bottom');
        }
        if (strNum == '2') {
            $('divMenuRightTop').set('class', 'tabs_sprite-blue-right_top');
            $('divMenuRightBottom').set('class', 'tabs_end tabs_sprite-blue-right_bottom');
        }

        ajaxRequest('ajaxManagers/BoardAjaxManager.asp', 'action=getPopPosts&type=' + strTab, 'getPopPosts');

        selTab = strTab;
        selNum = strNum;
    }

    function doTopBoardsFilter(strFilter) {
        newFilter = (strFilter == 'ByPosts' ? 'ByVisits' : 'ByPosts');
        $('spnTopBoards' + strFilter).replaces($('aTopBoards' + strFilter));
        var aTopBoards = new Element('a', { 'id': 'aTopBoards' + newFilter, 'href': 'javascript:void(1);' }).addEvent('click', function() { doTopBoardsFilter(newFilter); }).wraps($('spnTopBoards' + newFilter));
        ajaxRequest('ajaxManagers/BoardAjaxManager.asp', 'action=getTopBoards&filter=' + strFilter, 'getTopBoards');
    }

    function ajaxCallback(strActionType, responseTree) {
        if ($chk(responseTree) && responseTree != 'False') {
            switch (strActionType) {
                case 'getPopPosts':
                    var objPopPosts = JSON.decode(responseTree);
                    intCount = 0;
                    objPopPosts.message.each(function(item) {
                        intCount += 1;

                        $('aSubject' + intCount).set('href', 'smbd.asp?mb=' + item.mbId + '&mn=' + item.messageNumber + '&pt=msg&mid=' + item.msgId);
                        $('aSubject' + intCount).set('text', item.subject);
                        $('aAuthor' + intCount).set('href', 'viewprofile.asp?Alias=' + item.alias);
                        $('aAuthor' + intCount).set('text', item.alias);
                        $('aBoardName' + intCount).set('href', 'smbd.asp?mb=' + item.mbId);
                        $('aBoardName' + intCount).set('text', item.name);
                        $('spnDate' + intCount).set('text', new Date(item.datePosted).format('%x'));
                        $('spnTime' + intCount).set('text', new Date(item.datePosted).format('%X'));
                        $('spnMessageText' + intCount).set('html', shortenString(stripBrs(stripHtml(stripStyles(item.messageText))), '650'));
                        $('aContinue' + intCount).set('href', 'smbd.asp?mb=' + item.mbId + '&mn=' + item.messageNumber + '&pt=msg&mid=' + item.msgId);
                        if (item.messageText.length > 650) {
                            $('aContinue' + intCount).removeClass('class', 'dn');
                        } else {
                            $('aContinue' + intCount).addClass('class', 'dn');
                        }
                    });
                    break;
                case 'getTopBoards':
                    $('divMostActiveBoards').getFirst().empty();
                    var rsTopBoards = JSON.decode(responseTree);
                    intCount = 0;
                    rsTopBoards.Fields.each(function(item) {
                        intCount += 1;
                        var spnNew = new Element('span', { 'class': 'fwb' }).set('text', intCount + '. ').adopt(new Element('a', { 'href': 'smbd.asp?mb=' + item.MbId }).set('text', item.Name + (item.Description != '' ? ' - ' : '') + item.Description), new Element('br'));
                        spnNew.inject($('divMostActiveBoards').getFirst(), 'bottom');
                    });
                    break;
            }
        }
    }
</script>
