<script type="text/javascript">
function initPage() {


  if ($(window).width() > 706) {
             $('h3.link-trigger a').removeClass("unchanged changed");
            $('ul.link-list').show(); //We want them to see the list
            $('h3.link-trigger').unbind(); //We don't want people playing with the menu on a big screen
        }

       else {
            $('h3.link-trigger a').addClass("unchanged");
            $('ul.link-list').hide(); //Auto hide our link lists
            $('h3.link-trigger').unbind();
            $('h3.link-trigger').click(function (event) {
                event.preventDefault(); //We want to be able to toggle the lists
                //This checks to see if the css "display" is block
                if ($(this).parent().children('.link-list').css('display') != 'none') {
                    //If it wasn't that mean we just hid the menu
                    $(this).parent().find(".link-trigger a").removeClass("changed");
                }
                else {
                    $(this).parent().find(".link-trigger a").addClass("changed");
                }
                //Slide our menu out
                $(this).next().slideToggle("fast");
            });
        }

}


