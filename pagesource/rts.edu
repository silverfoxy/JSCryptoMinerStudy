<script type="text/javascript">
    function tieEnterKey(e) {
        if (window.event) { e = window.event; }
        if (e.keyCode == 13) {
            event.preventDefault();
            NewsSignup();
        }
    }


    function NewsSignup() {
        var email = document.getElementById('txtEmailSignup').value;
        var lists = document.getElementById('ctl00_ctl00_OCM1_NewsSignUp1_hiLists').value;
        var fltrEmail = /^\w+([-.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/i;
        if (email.length == 0) {
            alert("Email is required!");
            return false;
        }

        if (!fltrEmail.test(email)) {
            alert("Invalid Email Address!");
            return false;
        }

        $.ajax({
            type: "POST",
            url: "/AjaxService.asmx/SubscribeUser",
            data: '{"email":"' + email + '","lists":"' + lists + '"}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: true,
            cache: false,
            error: function (xhr, status, error) {
                alert(error);
            },
            success: function (msg) {
                var data = msg.d;
                window.location = data;
            }
        });
    }
</script>         
