	<script type="text/javascript"> 
		var t; 
		var speed = 5;//图片切换速度 
		var nowlan=0;//图片开始时间 
		function changepic() { 
			var imglen = $("#rdzl_list").find("li").length; 
			$("#rdzl_list").find("li").hide(); 
			$("#rdzl_list").find("li").eq(nowlan).show(); 
			nowlan = nowlan+1 ==imglen ?0:nowlan + 1; 
			t = setTimeout("changepic()", speed * 1000); 
		} 
		onload = function () { changepic(); } 
		$(document).ready(function () { 
			//鼠标在图片上悬停让切换暂停 
			$("#rdzl_list").hover(function () { clearInterval(t); }); 
			//鼠标离开图片切换继续 
			$("#rdzl_list").mouseleave(function () { changepic(); }); 
		}); 
	</script>
    
<script language=JavaScript> 
<!-- 热点专栏图片直播 -->
var bannerAD=new Array(); 
var bannerADlink=new Array(); 
var adNum=0; 


bannerAD[0]="/images/lhzt2017.jpg"; 
bannerADlink[0]="/ztzl/lhzt/lhzt2017/index.htm";

bannerAD[1]="/images/kjhdz2017.jpg"; 
bannerADlink[1]="http://www.stdaily.com/zhuanti01/2017kejizhou/qgkjhdz.shtml";





var preloadedimages=new Array(); 
for (i=1;i<bannerAD.length;i++){ 
preloadedimages[i]=new Image(); 
preloadedimages[i].src=bannerAD[i]; 
} 

function setTransition(){ 
var TransitionArr=new Array(); 
TransitionArr[0]=5;
TransitionArr[1]=6;



if (document.all){ 
bannerADrotator.filters.revealTrans.Transition=TransitionArr[Math.floor(Math.random()*3)];  
bannerADrotator.filters.revealTrans.apply(); 
} 
} 

function playTransition(){ 
if (document.all) 
bannerADrotator.filters.revealTrans.play() 
} 

function nextAd(){ 
if(adNum<bannerAD.length-1)
{
    adNum++ ; 
	if (bannerAD[adNum] =="") adNum=0;
}
else adNum=0; 
setTransition(); 
document.images.bannerADrotator.src=bannerAD[adNum]; 
playTransition(); 
theTimer=setTimeout("nextAd()", 6000); 
} 

function jump2url(){ 
jumpUrl=bannerADlink[adNum]; 
jumpTarget='_blank'; 
if (jumpUrl != ''){ 
if (jumpTarget != '')window.open(jumpUrl,jumpTarget); 
else location.href=jumpUrl; 
} 
} 
function displayStatusMsg() { 
status=bannerADlink[adNum]; 
document.returnValue = true; 
} 


function handle_select(select_obj)
 {
  var URL = select_obj.options[select_obj.selectedIndex].value;
select_obj.selectedIndex = 0;
  if(URL == '') {
   return;
  }
  else {
   self.open(URL,"_blank");
   return;
  }
 }
