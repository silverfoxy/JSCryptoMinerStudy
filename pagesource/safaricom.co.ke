          $(".mobile-menu").removeClass("menu-push-left");
          $('html, body').addClass("isoFixed");
          $(".skip-mobile__header").removeClass("move-menu-bottom");
          $(this).removeClass('mobile-menuopen').addClass('mobile-menuclose');
        }
        return false;
    });

