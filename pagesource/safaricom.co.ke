<script type="text/javascript">
    $(function(){
        $('a[href="#"]').on('click', function(e){
            e.preventDefault();
        });

        //$('#main-navigation > li ul li:first a').addClass('active');
        $('#main-navigation > li').on('mouseover', function(e){
            $(this).find("ul:first").show();
            $(this).find("ul li ul:first").show();
            $(this).children().addClass('expanded');
            $(this).find('> a').addClass('active');
        }).on('mouseout', function(e){
            $(this).find("ul:first").hide();
        });

        $('#main-navigation li li').on('mouseover',function(e){
            if($(this).has('ul').length) {
                $(this).parent().addClass('expanded');
            }
            $('ul:first',this).parent().find('> a').addClass('active');
            $('ul:first',this).show();
        }).on('mouseout',function(e){
            $(this).parent().removeClass('expanded');
            $('ul:first',this).parent().find('> a').removeClass('active');
            $('ul:first', this).hide();
        });

        var listItemII = document.getElementById( "main-navigation" );

        $("#main-navigation li ul li ul li a.active-nav-child").closest( "li", listItemII ).addClass("active-nav-menu");
    });

    $(".skip-mobile__header").click(function(){
        if($('.skip-mobile__header').hasClass('js-close')) {
            $('.content-nav__skip').addClass("content-nav-added");
            $('.content-nav').show();
            $('.getting-started__menu').show("fast");
            $(".js-contentnav-mobile").addClass("js-contentmobile");
            $(this).removeClass('js-close').addClass('js-open');
        } else if($('.skip-mobile__header').hasClass('js-open')) {
            $('.content-nav__skip').removeClass("content-nav-added");
