    <script id="UserScripts" class="editor-removable">


        $(function ($) {
            var homepageSlider;
            var promoSlider;

            $(".royalSlider").royalSlider({
                imageScalePadding: 0,
                slidesSpacing: 0,
                loop: true,
                controlNavigation: 'none',
                autoPlay: {
                    enabled: false
                }
            });

            // HERO CAROUSEL
            $('.carousel .slide-copy').first().addClass('is-active');
            $('.carousel .slide-copy').first().find('.slide-tile').first().addClass('is-active');

            homepageSlider = $(".royalSlider").data('royalSlider');

            homepageSlider.ev.on('rsAfterSlideChange', function () {
                $('.carousel .slide-copy').removeClass('is-active');
                $('.carousel .slide-copy').find('.slide-tile').removeClass('is-active');
                $('.carousel .slide-copy').eq(homepageSlider.currSlideId).addClass('is-active');
                $('.carousel .slide-copy').eq(homepageSlider.currSlideId).find('.slide-tile').eq(homepageSlider.currSlideId).addClass('is-active');
            });

            $('.carousel-next').click(function () {
                homepageSlider.next();
            });

            $('.carousel-previous').click(function () {
                homepageSlider.prev();
            });

            // PROMO CAROUSEL
            $('.carousel.carousel-promo .slide-caption').first().addClass('is-active');
            $('.carousel.carousel-promo .slide-caption').first().find('.slide-tile').first().addClass('is-active');

            promoSlider = $(".carousel-promo .royalSlider").data('royalSlider');

            promoSlider.ev.on('rsAfterSlideChange', function () {
                $('.carousel.carousel-promo .slide-caption').removeClass('is-active');
                $('.carousel.carousel-promo .slide-caption').find('.slide-tile').removeClass('is-active');
                $('.carousel.carousel-promo .slide-caption').eq(promoSlider.currSlideId).addClass('is-active');
                $('.carousel.carousel-promo .slide-caption').eq(promoSlider.currSlideId).find('.slide-tile').eq(promoSlider.currSlideId).addClass('is-active');
            });

        });
    </script>


        

