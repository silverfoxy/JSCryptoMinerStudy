    <script>
        jQuery(document).ready(function ($) {
            $('#ru').html('Updating...');
            $('#cu').html('Updating...');
            $('#apc').html('Updating...');
            $.ajax({
                type: "GET",
                url: "totalUsersFa.aspx",
                dataType: "text",
                success: function (d) {
                    var apc_f = d - Math.round(d / 3);
                    $('#ru').countTo({
                        from: apc_f,
                        to: d,
                        speed: 2000,
                        formatter: function (value, options) {
                            return (parseInt(value.toFixed(options.decimals))).toLocaleString();
                        }
                    });
                },
                error: function (data) {
                    $('#ru').html("undefined");
                }
            });

            $.ajax({
                type: "GET",
                url: "graphXml.aspx",
                dataType: "text",
                success: function (d) {
                    var ru_f = d - Math.round(d / 3);
                    $('#cu').countTo({
                        from: ru_f,
                        to: d,
                        speed: 2000,
                        formatter: function (value, options) {
                            return (parseInt(value.toFixed(options.decimals))).toLocaleString();
                        }
                    });
                },
                error: function (data) {
                    $('#cu').html("undefined");
                }
            });

            $.ajax({
                type: "GET",
                url: "LocationTotCount.aspx",
                dataType: "text",
                success: function (d) {
                    var cu_f = d - Math.round((parseFloat(d) + 10015) / 3);
                    $('#apc').countTo({
                        from: cu_f,
                        to: (parseFloat(d) + 10015),
                        speed: 2000,
                        formatter: function (value, options) {
                            return (parseInt(value.toFixed(options.decimals))).toLocaleString();
                        }
                    });
                },
                error: function (data) {
                    $('#apc').html("undefined");
                }
            });
        });
    </script>    <script type="text/javascript">    try {            var pageTracker = _gat._getTracker("UA-8153678-1");            pageTracker._trackPageview();        } catch (err) { }    </script>     <script type="text/javascript" src="js/indexLang_new.js"></script></body></html>