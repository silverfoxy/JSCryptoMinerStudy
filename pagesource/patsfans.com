<script>

$(document).ready(function(){
$('#search_box').keyup(function(e) { 
	if(e.which == 13) {
			$.ajax({
            type: 'POST',
    	    url: '/new-england-patriots/links/pflsearch.php', 
            data: $(this).serialize()
        })      
        .done(function(data){
             
            // show the response
            $('#response').html(data);
        })
	return false; 
	}
});
	
	
	
// End Search Function	
	
	
$( "#search_box" ).keypress(function() {
 		$('#response').empty();
});
	
    $('.search_block_toggle').on('click', function(event){
    	event.preventDefault();
    	// create menu variables
		
    	var slideoutMenu = $('#search_block');
    	var slideoutMenuWidth = $('#search_block').width();
    	
    	// toggle open class
    	slideoutMenu.toggleClass("open");
    	
    	// slide menu
    	if (slideoutMenu.hasClass("open")) {
	   	// $('#link_result').hide();
		//$('#tag_from_container').show();
			slideoutMenu.animate({
		    	right: "0px"
	    	});	
    	} else {
	    	slideoutMenu.animate({
		    	right: -slideoutMenuWidth
	    	}, 320);	
    	}
    });
});
</script>
<div id="search_block">
<div id="search_window">
