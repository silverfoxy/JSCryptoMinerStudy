		if(appHeight > 210) {
			var appNum = $(".hp-column-top-region div.ui-widget.app li").length;
			for(i=0;i<=Math.floor(appNum/2);i++) {
				$(".hp-column-top-region div.ui-widget.app li").eq(i).appendTo($(".split-app.one ul"));
				//alert(appNum);
			}
			for(i=Math.round(appNum/2);i<=appNum;i++) {
				$(".hp-column-top-region div.ui-widget.app li").eq(i).appendTo($(".split-app.two ul"));
				//alert(appNum);
			}
		}
    }
