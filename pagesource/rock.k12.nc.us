    function splitAnnouncements() {
        $(".hp-column-top-region div.ui-widget.app div.ui-widget-detail").prepend("<div class='split-app one'><ul class='ui-articles'></ul></div><div class='split-app two'><ul class='ui-articles'></ul></div>");
