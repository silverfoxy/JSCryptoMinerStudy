<!-- start Simple Custom CSS and JS -->
<style type="text/css">
/* Animations */
@keyframes showPlan {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}

@keyframes hidePlan {
  0% {
    opacity: 1;
  }
  80% {
    opacity: 0;
  }
  100% {
    display: none;
  }
}


/* yeahhh nahh WP injects <br/> for some reason  */
.c-plan_selection br {
  display: none;
  visibility: hidden;
}
/* Color States
 *
 */
.size--s h3,
.size--s .step_item--button {
  background-color: #f5b636;
}
.size--s .step_item--button:hover,
.size--s .step_item--button:focus {
  border-color: #f5b636;
}

.size--m h3,
.size--m .step_item--button {
  background-color: #5398D3;
}
.size--m .step_item--button:hover,
.size--m .step_item--button:focus {
  border-color: #5398D3;
}

.size--l h3,
.size--l .step_item--button {
  background-color: #7A75B7;
}
.size--l .step_item--button:hover,
.size--l .step_item--button:focus {
  border-color: #7A75B7;
}

.size--xl h3,
.size--xl .step_item--button {
  background-color: #ff6927;
}
.size--xl .step_item--button:hover,
.size--xl .step_item--button:focus {
  border-color: #ff6927;
}

/* 'c' Represents a 'c'omponent that is self enclosed,
 * can be reused (copy past ftw) */

/* c-step 
 * Step selection 
 */
.c-step {
  overflow: auto;
}

.c-step h2 {
  margin: 24px 0 12px 0;
}

.c-step h3 {
  
}

.c-step > p {
  margin-bottom: 12px;
}

/* Custom Step 1 stuff */
.step--1 .size--l .step_item--size {
  background-color: #cfcfe2;
}
.step--1 .size--xl .step_item--size {
  background-color: #f0ccbe;
}
.step--1 .size--m .step_item--size {
  background-color: #c6d8e9;
}
.step--1 .size--s .step_item--size {
  background-color: #eee1c2;
}

.step--2 .size--l .step_item--size {
  background-color: #cfcfe2;
}
.step--2 .size--xl .step_item--size {
  background-color: #f0ccbe;
}
.step--2 .size--m .step_item--size {
  background-color: #c6d8e9;
}
.step--2 .size--s .step_item--size {
  background-color: #eee1c2;
}

.step--1 .step_item--cta .step_item--button {
  position: relative;
  padding-right: 32px;
}
.step--1 .step_item--cta .step_item--button:after {
  position: absolute;
  right: 10%;
  top: 5px;
  content: '^';
  display: inline-block;
  font-family: Verdana, Verdana, Geneva, 'sans-serif';
  transform-origin: center;
  transform: rotate(-180deg);
}
.step--1 .step_item--price .price.promo {
  min-height: 74px;
}
.step--1 .step_item--price .price.promo:before {
  content: 'SALE';
  bottom: -12px;
  right: 50%;
  top: unset;
  left: 58%;
  width: 80px;
  margin-left: -50px;
}
.step--1 .step_item--price .price.promo ~ .was {
  visibility: hidden;
  color: transparent;
}

/* Custom Step 2 tings */
.step--2 {
  width: 100%;
  text-align: center;
}
.step--2 .c-step_item {
  padding-bottom: 24px;
}
.step--2 .step_item--price {
  padding: 24px 0 0px 0;
}
.step--2 .step_item--price .price:before {
  content: '';
}
.step--2[data-active-plan],
.step--2[data-active-plan] .c-step_item {
  display: none;
  opacity: 0;
  animation: hidePlan 1s forwards;
}
/* Which plan Types to show */
.step--2[data-active-plan="small"],
.step--2[data-active-plan="small"] .c-step_item.size--s,
.step--2[data-active-plan="medium"],
.step--2[data-active-plan="medium"] .c-step_item.size--m,
.step--2[data-active-plan="large"],
.step--2[data-active-plan="large"] .c-step_item.size--l,
.step--2[data-active-plan="extaLarge"],
.step--2[data-active-plan="extaLarge"] .c-step_item.size--xl {
  display: inline-block;
  animation: showPlan 1s forwards;
}
.back-to-step--1 {
  display: block;
  font-size: 24px;
  text-decoration: none;
  color: #8d87c1;
  margin: 12px 0;
}
.back-to-step--1:before {
  content: '\2190';
  display: inline-block;
  padding-right: 8px;
  font-family: Verdana, Verdana, Geneva, 'sans-serif';
}


/* c-step_item
 * NBN Plan items inside each step
 */
.c-step_item {
  background-color: #ececec;
  border-bottom: 20px solid #fff;
  width: 100%;
}
.c-step_item h3 {
  text-align: center;
  text-transform: uppercase;
  padding: 8px 0;
  color: #fff;
  margin: 0;
}
.c-step_item span {
  display: block;
  text-align: center;
  line-height: 1;
}
.c-step_item small {
  font-size: 50%;
}


/* Price */
.step_item--price {
  font-size: 32px;
  padding: 24px 0;
  position: relative;
  min-height: 86px;
}
.step_item--price sup {
  font-size: 18px;
}
.step_item--price.sale {
  color: #d11130;
}
.step_item--price .price.promo {
  position: relative;
}
.step_item--price .price.promo:before {
  content: 'PROMO';
  font-size: 11px;
  display: inline-block;
  background-color: #d11130;
  padding: 5px 10px;
  position: absolute;
  color: #fff;
  border-radius: 5px;
  top: 5px;
  left: 58%;
  right: 50%;
  margin-left: 15px;
  width: 50px;
}
.step_item--price .from {
  font-size: 12px;
  padding-right: 8px;
  color: #9d9ea0;
}
.step_item--price .was {
  font-size: 12px;
  text-decoration: line-through;
  color: #888;
}
.step_item--price .rate {
  font-size: 12px;
  text-align: left;
  display: inline-block;
  color: #888;
  line-height: 1em;
  margin-left: -12px;
  position: relative;
}
.step_item--price .rate:after {
  content: '^';
  display: inline-block;
  font-size: 22px;
  padding: 0;
  line-height: 1;
  position: absolute;
  top: 0;
  right: -12px;
}


/* Data Size */
.step_item--size {
  padding: 12px 0;
}
.step_item--size span:first-child {
  font-weight: bold;
  font-size: 35px;
}
.step_item--size em {
  font-size: 25px;
  color: #333;
}
.step_item--size sup {
  position: relative;
  top: -0.5em; 
  font-size: 30%;
}
.step_item--size span:first-child + span {
  font-size: 14px;
}

/* CTA */
.step_item--cta {
  overflow: auto;
  position: relative;
  padding: 24px 0 8px 0;
  text-align: center;
}
.step_item--cta p {
  text-align: center;
  font-size: 16px;
  text-transform: uppercase;
}
.step_item--cta em {
  display: block;
  font-style: normal;
  font-weight: bold;
}
.step_item--cta .step_item--button {
  color: white;
  font-size: 14px;
  text-transform: uppercase;
  padding: 8px 24px;
  display: inline-block;
  border: 1px solid white;
  margin: 12px auto 32px auto;
  outline: none;
  transition: border-color 0.25s;
}
.step_item--cta .step_item--button:hover,
.step_item--cta .step_item--button:focus {
  cursor: pointer;
}
.step_item--cta span {
  display: block;
  text-align: center;
  font-size: 12px;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  color: #888;
  line-height: 1.5em;
  padding: 0 12px;
  box-sizing: border-box;
  line-height: 1.2em;
}
.step_item--cta span:before {
  content: '^';
  padding-right: 4px;
  font-size: 10px;
}

/* Responsive Media Queries (Mobile First)
 * Base style is always mobile, then we optimise as screen gets bigger
 */

/* Tablet and up */
@media (min-width: 640px) {
  .c-step {
    padding: 24px;
    box-sizing: border-box;
  }
  .c-step h2,
  .back-to-step--1 {
    padding-left: 12px;
    text-align: center;
  }
  .c-step_item {
    box-sizing: border-box;
    border-left: 10px solid #fff;
    width: 50%;
    float: left;
  }
}

/* Tablet and up */
@media (min-width: 980px) {
  .c-step_item {
    width: 25%;
  }
  .step--2 .c-step_item {
    float: none;
  }
}
</style>
<!-- end Simple Custom CSS and JS -->
