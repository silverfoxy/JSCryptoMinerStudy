<script type="text/javascript">
document.addEventListener("DOMContentLoaded", function() {
  /* Global vars */
  var planViewOptionsHandler = document.getElementsByClassName('opts-trigger'),
      planOptionsView = document.getElementById('step--1'),
      stepOptionsView = document.getElementById('step--2'),
      backToStepOne = document.getElementById('back-to-step--1'),
      hasJquery = typeof jQuery === 'undefined' ? false : true,
      offsetHeaderheight = 50;

  /* Loops through all our view option buttons, grabs the type of data plan then adds as a attribute to the options container */
  for(var i = 0; i < planViewOptionsHandler.length; i++) {
    planViewOptionsHandler.item(i).addEventListener('click', function(event) {
      event.preventDefault()
      stepOptionsView.setAttribute('data-active-plan', this.getAttribute('data-plan'));
      if (hasJquery) {
        jQuery('html, body').animate({
          scrollTop: (jQuery(stepOptionsView).first().offset().top - offsetHeaderheight)
        },500);
      }
    }, false);
  }
  
  /* Gotta remove the active data plan from options container*/
  backToStepOne.addEventListener('click', function() {
    event.preventDefault();
    stepOptionsView.setAttribute('data-active-plan', '');
    if (hasJquery) {
      jQuery('html, body').animate({
        scrollTop: (jQuery(planOptionsView).first().offset().top - offsetHeaderheight)
      },500);
    }
  }, false);
});</script>
<!-- end Simple Custom CSS and JS -->
