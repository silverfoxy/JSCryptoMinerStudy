@font-face {
  font-family: "Arial Black";
  src: local('Arial Black');
  font-weight: 900;
}
body {
  font-family: 'Lato', Arial, sans-serif;
  font-size: 16px;
}
.page-body {
	margin-top: 0;
}
h3 {
  border-bottom: 0;
}
section {
  padding: 60px 0;
}
section:first-of-type {
  padding: 0;
}
.homepage-hero-module {
  max-height: 700px;
}
#hero .header-wrapper {
  position: absolute;
  z-index: 0;
  width: 100%;
  display: block;
  margin-top: 100px;
}
#hero .header-background {
  background: rgba(0, 0, 0, 0.7);
	width: 100%;
	margin: auto;
	padding: 30px;
}
#hero .home-header {
  margin: 0 auto 0 auto;
  text-align: center;
}
#hero-search .input-group {
  margin-top: 30px;
}
#hero-search input {
  height: 76px;
  border: 0;
  padding: 15px;
  width: 100%;
  font-size: 26px;
  background: rgba(0, 0, 0, 0.7);
  color: #fff;
}
