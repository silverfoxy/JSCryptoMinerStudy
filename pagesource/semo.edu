  color: #fff;
  opacity: 0.9;
}
#hero-search button {
  height: 76px;
  width: 76px;
  margin: 0;
  font-size: 22px;
  color: #fff;
  border-radius: 0;
}
#hero .glyphicon-chevron-down {
  text-align: center;
  display: block;
  position: relative;
  bottom: 30vh;
  top: unset;
  font-size: 40px;
  color: rgba(255,255,255, .7);
}
#admissions h2 {
  margin-top: 0;
}
#academics, #campus-life {
  background-size: cover;
  background-position: center;
  position: static;
}
#academics {
  background-image: url('../_resources/img/AcademicsBackground.jpg');
}
#campus-life {
  background-image: url('../_resources/img/CampusLife.jpg');
}
.white-arial {
  color: #fff;
  font-family: 'Arial Black', 'Archivo Black', Arial, sans-serif;
  font-weight: 900;
  text-transform: uppercase;
}
.white-arial.trending {
  font-size: 24px;
  padding: 30px 0;
}
.black-arial {
  color: #000;
  font-family: 'Arial Black', 'Archivo Black', Arial, sans-serif;
  font-weight: 900;
  text-transform: uppercase;
}
.red-arial {
  color: #c8102e;
  font-family: 'Arial Black', 'Archivo Black', Arial, sans-serif;
  font-weight: 900;
  text-transform: uppercase;
}
.text-white {
  color: #fff;
}
.hero-banner-wrapper {
  background-image: url('http://placehold.it/1920x800');
  background-position: center center;
  height: 700px;
}
.hero-banner h2 {
  text-align: left;
}
.home-header {
  margin: 60px 0;
  font-size: 40px;
  letter-spacing: -2px;
  line-height: 50px;
  text-transform: uppercase;
}

.home-header span {
    color: #c8102e;
}
#queries .item a {
  display: block;
  text-align: center;
  color: #fff;
  background-color: #9d2235;
  padding: 15px 0;
}
.home-form-wrap {
  background-color: #000;
}
