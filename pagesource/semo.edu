.home-form {
  background-color: #003b5a;
  padding: 20px; 
}
.home-form input, .home-form textarea {
  width: 100%;
  margin-bottom: 10px;
  padding: 10px;
  background-color: #fafafa;
  border: none;
  border-radius: 0;
}
.home-form label, .contactinfo {
	display: none !important;
}
.home-form button[type='submit'] {
  width: 100%;
  color: #fff;
  background-color: #279989;
  border: none;
  border-radius: 0;
  margin: 0;
}
.home-form button[type='submit']:hover {
  color: #279989;
  background-color: #fff;
}
.home-form button[type='reset'] {
  display: none;
}

.form-heading {
  font-size: 24px;
}
.btn-home {
  padding: 15px 10px;
  border-radius: 0px;
  background-color: #c8102e;
  font-size: 20px;
  text-align: center;
  text-transform: uppercase;
  color: #fff;
  font-family: 'Lato', Arial, sans-serif;
  font-weight: 900;
  white-space: normal;
}
.btn-home.red {
  color: #fff;
  background-color: #c8102e;
}
.btn-home.red:hover {
  background-color: #9d2235;
}
.btn-home.orange {
  color: #fff;
  background-color: #f15c3b;
}
.btn-home.orange:hover {
  color: #f15c3b;
  background-color: #fff;
}

.demographics a h3{
  margin-top: 0;
  text-align: center;
  float: right;
  position: relative;
  bottom: 31px;
  background: #c8102e;
  text-transform: uppercase;
  width: 175px;
  padding: 8px 0 8px 10px;
  -webkit-clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
  clip-path: polygon(25px 0%, 100% 0%, 100% 100%, 0% 100%);
  font-size: 14px;
  font-family: 'Lato', Arial, sans-serif;
  font-weight: 900;
  color: #fff;
  line-height: 15px;
  border: 0;
}
.home-testimonial {
  padding: 0;
  background-image: url('../_resources/img/lines-texture2.png');
  background-repeat: repeat;
  height: initial;
}
.home-testimonial .image img{
  position: absolute;
  top: -11px;
  right: 15px;
  -webkit-clip-path: polygon(33% 0%, 100% 0%, 67% 100%, 0% 100%);
  clip-path: polygon(33% 0%, 100% 0%, 67% 100%, 0% 100%);
}
.home-testimonial .blurb {
  padding-top: 30px;
  padding-left: 35px;
  background-image: url('../_resources/img/quote.png');
  background-repeat: no-repeat;
  color: #000;
  font-size: 25px
}
.home-testimonial .blurb p {
  line-height: 27px;
}
.home-testimonial .blurb p strong {
  font-size: 22px;
  color: #fff;
}

#academics .program-card {
  display: block;
  width: 100%;
  background-color: #fff;
  text-align: center;
  padding: 25px 10px 10px 10px;
  margin-bottom: 20px;
  color: #000;
}
#academics .glyphicons-lab {
  color: #003b5a;
}
#academics .glyphicons-flower {
  color: #279989;
}
#academics .glyphicons-music {
  color: #f15c3b;
}
#academics .glyphicons-stethoscope {
  color: #71c5e8;
}
#academics .glyphicons-education {
  color: #871b55;
}
#academics .glyphicons-stats {
  color: #5c6670;
}
#academics .program-card h3 {
  font-family: 'Lato', Arial, sans-serif;
  font-weight: 900;
  font-size: 18px;
  color: #c8102e;
  text-transform: uppercase;
}

#academics .program-card:hover {
  text-decoration: none;
  background-color: #c8102e;
  color: #fff;
  cursor: pointer
}

#academics .program-card:hover h3, #academics .program-card:hover .glyphicons{
  color: #fff;
}

#academics .program-card .red-arial {
  letter-spacing: -1px;

}

#academics .program-more {
  display: block;
  width: 100%;
  background-color: #fff;
  text-align: center;
  padding: 10px;
  margin-bottom: 20px;
  color: #000;
}

#campus-life .activity-card {
  padding: 15px;
  background: #fff;
  text-align: center;
}
#campus-life .activity-card .social {
  max-width: 40px !important;
  font-size: 40px;
  float: none;
  margin: 0 5px;
}
#feeds {
  background-color: #1f1f1f;
}
#owl-news .item .date {
  position: absolute;
  right: 10px;
  top: 0;
  padding: 17px 13px;
  background: #fff;
  width: 70px;
  height: 70px;
  color: #c8102e;
  font-size: 20px;
  font-weight: 900;
  text-align: center;
  text-transform: uppercase;
  line-height: 0.9;
}
#owl-news .item .date span {
  color: #000;
  font-size: 24px;
}
#owl-news .item img {
  height: 220px;
  object-fit: cover;
  object-position: center;
}
#owl-news .item .news-headline {
  font-weight: 900;
  font-size: 18px;
  color: #fff;
}
#owl-news .item p {
  margin-top: 15px;
  color: #fff;
}
.news {
  border: none;
}
.media {
  width: 33%;
  display: inline-block;
  vertical-align: top;
  padding-right: 20px;
  border: none;
}
.news li {
  border: none;
  padding-right: 15px;
  margin-bottom: 20px;
  max-height: 330px;
}
.media:first-child {
  margin-top: 15px;
}
.calibre {
  text-transform: uppercase;
}
.more {
  line-height: 3;
  text-transform: uppercase;
  font-family: 'Arial Black', 'Archivo Black', 'Arial', sans-serif;
  font-weight: 900;
}
.more a {
  border-top: solid 5px #c8102e;
}
		

/* Animations */

/* Responsive */

@media (min-width: 576px) {
  #academics .program-card {
    height: 280px;
  }
}
@media (min-width: 768px) {
	#hero .header-background {
		width: 600px;
	}
	#hero .header-wrapper {
		margin-top: 35vh;
	}
}

@media (min-width: 992px) {
  .demographics div {
    max-height: 125px;
    margin-bottom: 20px;
  }
  .home-testimonial {
    height: 175px;
  }
  .home-header {
    font-size: 46px;
  }
  #academics, #campus-life {
	background-attachment: fixed;
  }
  #hero .glyphicon-chevron-down {
	bottom: 20vh;
  }
}
@media (min-width: 1200px) {
  .demographics div {
    max-height: 150px;
    margin-bottom: 30px;
  }
}
@media screen and (max-width: 991px) {
  .media {
    width: 49%;
  }
}
@media (max-width: 767px) { 
  #search .home-header {
    margin-bottom: 0;
  }
  .home-testimonial .blurb {
	padding: 30px 20px 20px 20px;
  }
  .homepage-hero-module {
	max-height: 400px;
  }
  .media {
    width: 100%;
  }
}
/* Prevent demographic button text from over-extending */
@media (max-width: 420px) {
  .demographics a h3 {
    width: 100%;
    -webkit-clip-path: none;
    clip-path: none;
  }
  #hero .header-background {
	padding: 10px;
  }
  .btn-home {
	font-size: 14px;
  }
  .demographics a h3 {
	bottom: 5px;
  }
  #hero-search input {
	font-size: 20px;
  }
}
@media (max-width: 350px) {
  .home-header {
	font-size: 38px;
	line-height: 38px;
  }
}
