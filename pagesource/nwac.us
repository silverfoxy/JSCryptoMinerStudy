$(function() {
    
      var show_snowball = false;
      var show_support = true;
    
    var support_selector = '#modal-support';
    var support_cookie_name = 'support_popup_2017_09_26';
    var snowball_selector = '#modal-snowball';
    var snowball_cookie_name = 'shown_snowball_popup_new';
    var TEN_YEARS = 3652;  
    var ONE_DAY = 1;
    var SIX_MONTHS = 182;
    var TWO_YEARS = 730;
    var END_OF_SESSION = 'Session';

    var do_show_support_popup = function () {
        if (!show_support) {
            return false;
        }
        // If this cookie is set, under no circumstances should we show another
        // popup.

        // removed the following line 9-20-16.  not sure why it is here.
        // if ($.cookie('no_more_popups')) {
        //     return false;
        // }

        // If the Snowball popup is active and has not been show yet, the
        // support popup should not be shown
        if (show_snowball && ! $.cookie(snowball_cookie_name)) {
            return false;
        }

        // Otherwise, show the support popup if it has not already been
        // shown and dismissed.
        var cookie_shown = $.cookie(support_cookie_name) ? true : false;
        return ! cookie_shown;
    }   //  var do_show_support_popup = function () {...


    var popup_handler = function(cookie_name, modal_selector, expiration, location) {
        if (isNaN(expiration)) {
            $.cookie(cookie_name, 'true');
        } else {
            $.cookie(cookie_name, 'true', {expires: expiration});
        }
        // removed the following line 9-20-16.  not sure why it is here.
        // $.cookie('no_more_popups', 'true');
        if (location) {
            if (location.substring(0, 4) == "http") {
                window.open(location, '_blank');
            } else {
                window.location.href = location;
            }
        }
        $(modal_selector).modal('hide');
    };

    
    // $('#button-donate-offsite').click(function(event) {
    //     event.preventDefault();
    //     popup_handler(
    //         support_cookie_name,
    //         support_selector,
    //         SIX_MONTHS,
    //         'https://givebig.seattlefoundation.org/npo/northwest-avalanche-center-1'
    //     );
    // });

    $('#button-support-join').click(function(event) {
        event.preventDefault();
        popup_handler(
            support_cookie_name,
            support_selector,
            TWO_YEARS,  // this is ok because we change message anually.
            '/support/membership/'
        );
    });

    $('#button-support-already').click(function(event) {
        event.preventDefault();
        popup_handler(
            support_cookie_name,
            support_selector,
            SIX_MONTHS
        );
    });

    $('#button-support-remind').click(function(event) {
        event.preventDefault();
        popup_handler(
            support_cookie_name,
            support_selector,
            ONE_DAY
        );
    });

    
    $('#button-snowball-thanks').click(function(event) {
        event.preventDefault();
        popup_handler(
            snowball_cookie_name,
            snowball_selector,
            SIX_MONTHS
        );
    });

    $('#button-snowball-remind').click(function(event) {
        event.preventDefault();
        popup_handler(
            snowball_cookie_name,
            snowball_selector,
            ONE_DAY
        );
    });

    $('#button-snowball-buy').click(function(event) {
        event.preventDefault();
        popup_handler(
            snowball_cookie_name,
            snowball_selector,
            SIX_MONTHS,
            'http://northwestavalanchecenter.schoolauction.net/snowball2016/homepages/show'
        );
    });

    $('#button-snowball-info').click(function(event) {
        event.preventDefault();
        popup_handler(
            snowball_cookie_name,
            snowball_selector,
            SIX_MONTHS,
            'http://northwestavalanchecenter.schoolauction.net/snowball2016/homepages/show'
        );
    });

    $('#button-snowball-donate').click(function(event) {
        event.preventDefault();
        popup_handler(
            snowball_cookie_name,
            snowball_selector,
            SIX_MONTHS,
            'http://www.nwac.us/support/donate/'
        );
    });

    if (navigator.userAgent.indexOf('Safari') != -1 && navigator.userAgent.indexOf('Chrome') == -1) {
        if ($.cookie("safari_reload")==null) {
            $.cookie("safari_reload", 'true');
            location.reload();
        }
        else {
            $.cookie("safari_reload", null);
        }
    }

    if (do_show_support_popup()) {
        $(support_selector).modal('show');
    }
    if (show_snowball && ! $.cookie(snowball_cookie_name)) {
        $(snowball_selector).modal('show');
    }


}); //  $(function() {...

$(function() {

    $('iframe#iframe-calendar').attr('src', 'https://www.google.com/calendar/embed?showTitle=0&showNav=0&showDate=0&showPrint=0&showTabs=0&showCalendars=0&showTz=0&mode=AGENDA&height=450&wkst=1&bgcolor=%23FFFFFF&src=nwac.us_66e453uanfrtaiel81ius5qmlc%40group.calendar.google.com&color=%23875509&ctz=America%2FLos_Angeles');

