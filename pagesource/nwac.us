<script>

function initMap(danger_map_info) {

    // Set values to lowercase
    $.each(danger_map_info, function(id){
        if (id !== "errors") {
            danger_map_info[id].danger_elev_high = danger_map_info[id].danger_elev_high.toLowerCase();
            danger_map_info[id].danger_elev_middle = danger_map_info[id].danger_elev_middle.toLowerCase();
            danger_map_info[id].danger_elev_low = danger_map_info[id].danger_elev_low.toLowerCase();
        }
    })

	var mapStyle = [{
			elementType : 'labels',
			stylers : [{
					visibility : 'off'
				}
			]
		}
	];

	var labelStyle = [{
			featureType : 'all',
			stylers : [{
					visibility : 'off'
				}
			]
		}, {
			elementType : 'labels',
			stylers : [{
					visibility : 'on'
				}
			]
		}, {
			"featureType" : "administrative.province",
			"elementType" : "labels.text",
			"stylers" : [{
					"visibility" : "off"
				}
			]
		},{
            "featureType": "poi",
            "elementType": "labels.text",
            "stylers": [
              { "visibility": "off" }
            ]
          }, {
			"featureType" : "road.highway",
			"elementType" : "labels.icon",
			"stylers" : [{
					"visibility" : "on"
				}, {
					"lightness" : -11
				}, {
					"saturation" : -87
				}
			]
		}
	];

	var dangerColors = {}
	dangerColors["no-data"] = "#808080";
    dangerColors["no-rating"] = "#808080";
	dangerColors.low = "#4db848";
	dangerColors.moderate = "#fcf200";
	dangerColors.considerable = "#f7941e";
	dangerColors.high = "#cd1c24";
	dangerColors.extreme = "#231f20";

	var dangerText = {}
	dangerText["no-data"] = "No Data";
    dangerText["no-rating"] = "No Rating";
	dangerText.low = "Low";
	dangerText.moderate = "Moderate";
	dangerText.considerable = "Considerable";
	dangerText.high = "High";
	dangerText.extreme = "Extreme";

	// Create a map object and specify the DOM element for display.
	var map = new google.maps.Map(document.getElementById('map'), {
		center: {lat: 47.127, lng: -121.8},
		zoomControl: false,
		panControl: false,
		draggable: false,
		scrollwheel: false,
		disableDoubleClickZoom: true,
		mapTypeControl: false,
		streetViewControl: false,
		zoom: 7,
        mapTypeId: google.maps.MapTypeId.TERRAIN,
		styles: mapStyle
	});

	var regionLayer = new google.maps.Data();

	regionLayer.loadGeoJson("/static/home/data/regions.json");



    regionLayer.setStyle(function(feature){
        var id = feature.getProperty("id");
        var forecast = danger_map_info[id];
        var style = {
            fillColor: dangerColors["no-data"],
            fillOpacity: 0.7,
            strokeWeight: "0.8"
        }

        if (forecast) {

            if (forecast.danger_elev_low === "extreme" || forecast.danger_elev_middle === "extreme" || forecast.danger_elev_high === "extreme") {
                style.fillColor = dangerColors.extreme;
                feature.setProperty('regionDanger', 'extreme')
            } else if (forecast.danger_elev_low === "high" || forecast.danger_elev_middle === "high" || forecast.danger_elev_high === "high") {
                style.fillColor = dangerColors.high;
                feature.setProperty('regionDanger', 'high')
            } else if (forecast.danger_elev_low === "considerable" || forecast.danger_elev_middle === "considerable" || forecast.danger_elev_high === "considerable") {
                style.fillColor = dangerColors.considerable;
                feature.setProperty('regionDanger', 'considerable')
            } else if (forecast.danger_elev_low === "moderate" || forecast.danger_elev_middle === "moderate" || forecast.danger_elev_high === "moderate") {
                style.fillColor = dangerColors.moderate;
                feature.setProperty('regionDanger', 'moderate')
            } else if (forecast.danger_elev_low === "low" || forecast.danger_elev_middle === "low" || forecast.danger_elev_high === "low") {
                style.fillColor = dangerColors.low;
                feature.setProperty('regionDanger', 'low')
            }
        };

        return style;
    });

	// Oscillate colors for extreme danger regions
    var currentExtremeColor = dangerColors.high;
    setInterval(function(){
        regionLayer.forEach(function(feature){
            if (feature.getProperty('regionDanger') === 'extreme') {
                regionLayer.overrideStyle(feature, {fillColor: currentExtremeColor})
            }
        })
        if (currentExtremeColor === dangerColors.extreme) {
            currentExtremeColor = dangerColors.high
        } else {
            currentExtremeColor = dangerColors.extreme
        }
    }, 1000);

    var legend = $("#legend");
	regionLayer.addListener('mouseover', function(e) {
		var regionid = e.feature.getProperty('id')
		var danger_elev_high = danger_map_info[regionid] ? danger_map_info[regionid].danger_elev_high : "no-data";
		var danger_elev_middle = danger_map_info[regionid] ? danger_map_info[regionid].danger_elev_middle : "no-data";
		var danger_elev_low = danger_map_info[regionid] ? danger_map_info[regionid].danger_elev_low : "no-data";
        var danger_forecast = danger_map_info[regionid] ? danger_map_info[regionid].bottom_line_summary : "no-data";

        // Set region name
        $("#legend h2").html(danger_map_info[regionid].zone_abbreviated_name);

		$("#danger_elev_high_text").text(dangerText[danger_elev_high]);
		$("#danger_elev_middle_text").text(dangerText[danger_elev_middle]);
		$("#danger_elev_low_text").text(dangerText[danger_elev_low]);
        $("#legend-forecast").html(danger_forecast);

		// Change text to red if danger level is extreme
		if (danger_elev_high === "extreme") {
			$("#danger_elev_high_text").attr('fill',dangerColors.high);
			$("#danger_elev_high_text_label").attr('fill',dangerColors.high);
		} else {
			$("#danger_elev_high_text").attr('fill',"#000");
			$("#danger_elev_high_text_label").attr('fill',"#000");
		}

		if (danger_elev_middle === "extreme") {
			$("#danger_elev_middle_text").attr('fill',dangerColors.high);
			$("#danger_elev_middle_text_label").attr('fill',dangerColors.high);
		} else {
			$("#danger_elev_middle_text").attr('fill',"#000");
			$("#danger_elev_middle_text_label").attr('fill',"#000");
		}

		if (danger_elev_low === "extreme") {
			$("#danger_elev_low_text").attr('fill',dangerColors.high);
			$("#danger_elev_low_text_label").attr('fill',dangerColors.high);
		} else {
			$("#danger_elev_low_text").attr('fill',"#000");
			$("#danger_elev_low_text_label").attr('fill',"#000");
		}

		// Set polygon color based on danger value
		$(".danger_elev_high_poly").css({fill: dangerColors[danger_elev_high]})
		$(".danger_elev_middle_poly").css({fill: dangerColors[danger_elev_middle]})
		$(".danger_elev_low_poly").css({fill: dangerColors[danger_elev_low]})

		// Change icons
		$("#danger_elev_low_image").attr('xlink:href',"/static/images/danger-levels/" + danger_elev_low + ".png");
		$("#danger_elev_middle_image").attr('xlink:href',"/static/images/danger-levels/" + danger_elev_middle + ".png");
		$("#danger_elev_high_image").attr('xlink:href',"/static/images/danger-levels/" + danger_elev_high + ".png");

		if (danger_elev_high === "high" || danger_elev_high === "extreme" || danger_elev_high === "considerable") {
			$("#danger_elev_high_image").attr('width',65);
		} else {
			$("#danger_elev_high_image").attr('width',48);
		}

		if (danger_elev_middle === "high" || danger_elev_middle === "extreme" || danger_elev_middle === "considerable") {
			$("#danger_elev_middle_image").attr('width',65);
		} else {
			$("#danger_elev_middle_image").attr('width',48);
		}

		if (danger_elev_low === "high" || danger_elev_low === "extreme" || danger_elev_low === "considerable") {
			$("#danger_elev_low_image").attr('width',65);
		} else {
			$("#danger_elev_low_image").attr('width',48);
		}

        legend.show();

        regionLayer.overrideStyle(e.feature, {strokeWeight: "3.5"});
	});

    google.maps.event.addDomListener(map.getDiv(), 'mousemove', function(e){
        legend.css({ top: (e.offsetY + 10) + "px" });
        legend.css({ left: (e.offsetX + 10) + "px" });
    })

    regionLayer.addListener('mouseout', function(e) {
		var legend = $("#legend");
		legend.hide();
		regionLayer.revertStyle();
	})

    regionLayer.addListener('click', function(e) {
        window.location.href = e.feature.getProperty('zone_url');
    })

	var labelLayer = new google.maps.StyledMapType(labelStyle, { name: 'labels' });
	regionLayer.setMap(map);
	map.overlayMapTypes.push(labelLayer);

}

$.ajax({
  // url: "/danger_map_info/",
  url: "/api/v4/danger_map_data/",
}).done(initMap);

</script>



