        function InitDDL() {
            var yrChanged = function () {
                var fm = $(this).closest('form'), year = this.value, price = $('[name="compare1.Price"]', fm).val();
                var make = $('[name="compare1.Make"]', fm).dropdownlist({ url: '/Valuation/GetMakes', data: { 'year': year, 'price': price }, defaultValue: true }).val();
                if (make)
                    $('[name="compare1.Model"]', fm).dropdownlist({ url: '/Valuation/GetModels', data: { 'make': make, 'year': year, 'price': price }, defaultValue: true });
                else
                    $('[name="compare1.Model"]', fm).dropdownlist({ defaultValue: true });
            };
            var yr = $('[name="compare1.Year"]').change(yrChanged);
            var priceFrom = $('[name="compare1.PriceFrom"]').change(function () {
                var fm = $(this).closest('form'), priceFrom = this.value || 0, priceTo = $('[name="compare1.PriceTo"]', fm).val(), year = $('[name="compare1.Year"]', fm).val();
                var make = $('[name="compare1.Make"]', fm).dropdownlist({ url: '/Valuation/GetMakes', data: { 'year': year, 'priceFrom': priceFrom, 'priceTo': priceTo }, defaultValue: true }).val();
                if (make)
                    $('[name="compare1.Model"]', fm).dropdownlist({ url: '/Valuation/GetModels', data: { 'make': make, 'year': year, 'priceFrom': priceFrom, 'priceTo': priceTo }, defaultValue: true });
                else
                    $('[name="compare1.Model"]', fm).dropdownlist({ defaultValue: true });

                $('[name="compare1.PriceTo"] option').each(function () {
                    var val = $.parseNum($(this).val());
                    if (val) {
                        if (val <= $.parseNum(priceFrom))
                            $(this).hide();
                        else
                            $(this).show();
                    }
                });
            });
            var priceTo = $('[name="compare1.PriceTo"]').change(function () {
                var fm = $(this).closest('form'), priceTo = this.value || 0, priceFrom = $('[name="compare1.PriceFrom"]', fm).val(), year = $('[name="compare1.Year"]', fm).val();
                var make = $('[name="compare1.Make"]', fm).dropdownlist({ url: '/Valuation/GetMakes', data: { 'year': year, 'priceFrom': priceFrom, 'priceTo': priceTo }, defaultValue: true }).val();
                if (make)
                    $('[name="compare1.Model"]', fm).dropdownlist({ url: '/Valuation/GetModels', data: { 'make': make, 'year': year, 'priceFrom': priceFrom, 'priceTo': priceTo }, defaultValue: true });
                else
                    $('[name="compare1.Model"]', fm).dropdownlist({ defaultValue: true });

                $('[name="compare1.PriceFrom"] option').each(function () {
                    var val = $.parseNum($(this).val());
                    if (val) {
                        if (val <= $.parseNum(priceTo))
                            $(this).hide();
                        else
                            $(this).show();
                    }
                });
            });
            var mk = $('[name="compare1.Make"]').change(function () {
                var fm = $(this).closest('form');
                var year = $('[name="compare1.Year"]', fm), priceFrom = $('[name="compare1.PriceFrom"]', fm), priceTo = $('[name="compare1.PriceTo"]', fm);

                year.dropdownlist({ url: '/Valuation/GetYears', data: { make: this.value, 'priceFrom': priceFrom.val(), 'priceTo': priceTo.val() }, defaultValue: true });
                if (year.length)
                    year = year.val() || 0;
                else
                    year = 0;

                priceFrom.dropdownlist({ url: '/Valuation/GetPrices', data: { make: this.value, 'year': year, 'priceTo': priceTo.val() || 0 }, defaultValue: true });
                priceTo.dropdownlist({ url: '/Valuation/GetPrices', data: { make: this.value, 'year': year, 'priceFrom': priceFrom.val() || 0 }, defaultValue: true });

                var make = this.value;
                if (make)
                    $('[name="compare1.Model"]', fm).dropdownlist({ url: '/Valuation/GetModels', data: { 'make': make, 'year': year, 'priceFrom': priceFrom.val(), 'priceTo': priceTo.val() }, defaultValue: true });
                else
                    $('[name="compare1.Model"]', fm).dropdownlist({ defaultValue: true });
            });
            //            if (yr.val()) yr.change();
            yrChanged.apply($('select[name="compare1.Year"]')[0]);
            yrChanged.apply($('input[name="compare1.Year"]')[0]);
            if (priceFrom.val()) priceFrom.change();
            if (priceTo.val()) priceTo.change();
            if (mk.val()) mk.change();
        }

        function sliderChange() {
            $($('.banner-item').removeClass('active')[$('.flexslider').data('flexslider').animatingTo]).addClass('active');
        }
        function slideTo(i) {
            $('.flexslider').flexslider($.parseNum(i));
        }

        function ValidateForm(fm, ignoreYear) {
            var item = $(fm).serializeObject();
            var isValid = false;
            for (var i in item) {
                if (i != 'version' && i != 'ssX' && item[i] && !(ignoreYear && i == 'compare1.Year')) {
                    isValid = true;
                    break;
                }
            }
            if (isValid)
                return true;
            else {
                $('<div>Please select at least one criteria to continue.</div>').message();
                return false;
            }
        }

        function InitFeeds() {
            $("#divFeeds").rss("https://news.mycarinfo.com.my/feed", {
                ssl: true,
                limit: 3,
                layoutTemplate: $('#hdnFeedPanel').html().trim(), //"<div class='feed-container'>{entries}</div>",
                entryTemplate: $('#hdnFeed').html().trim(),
                tokens: {
                    refineBody: function (entry, tokens) {
                        var text, content = $('<div>{0}</div>'.format(entry.content));
                        $('p', content).each(function () {
                            text = $(this).text().trim();
                            if (text) return false;
                        });
                        if (!text || (text && text.length < 100))
                            text = content.text().trim();
                        if (text && text.length > 500)
                            text = text.substring(0, 500) + '...';
                        return text;
                    }
                },

                dateFormatFunction: function (date) {
                    return new Date(date).toString('MMMM dd, yyyy');
                },
                // valid values: 'show', 'slide', 'slideFast', 'slideSynced', 'slideFastSynced'
                effect: 'slideFastSynced',
                error: function () { },
                success: function () { },
                onData: function () { }
            });
        }
    </script>
