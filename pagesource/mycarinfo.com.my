        function CountDownLogOff() {
            setTimeout(function () {
                var sec = $('.ui-dialog #logoffsec');
                if (counting) {
                    sec.html(sec.html().trim() - 1);
                    if (sec.html().trim() <= 0)
                        LogOff();
                    else
                        CountDownLogOff(sec);
                }
            }, 1000);
        }
        function LogOff() {
            $('#logoffLink').parent()[0].submit();
