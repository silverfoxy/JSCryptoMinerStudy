          ET_Event.eventStart(encodeURI(tEventCategory),encodeURI(tEventObject),encodeURI(tEventAction));
        }
      });
      // selection of GENAU ticketcount
      $('#genau-ticket .ticket-optional-tickets .check-radio-list LABEL').click(function(){
        if(typeof ET_Event !== 'undefined'){
          var tEventCategory    = 'gluecksspirale';
          var tEventObject      = 'gluecksspirale - ' + $(this).find('INPUT').val() + ' Lose auswhlen';
          var tEventAction      = 'Klick';
          ET_Event.eventStart(encodeURI(tEventCategory),encodeURI(tEventObject),encodeURI(tEventAction));
        }
      });
      // clicking external links
      $('.et_externallink').click(function(){
        if(typeof ET_Event !== 'undefined'){
          var tEventCategory    = 'Spielangebot';
          var tEventObject      = $(this).attr('href') + ' aufrufen';
          var tEventAction      = 'Externer Link';
          ET_Event.eventStart(encodeURI(tEventCategory),encodeURI(tEventObject),encodeURI(tEventAction));
        }
      });
      // clicking links for PDF download
      $('.et_pdfdownload').click(function(){
        if(typeof ET_Event !== 'undefined'){
          var tEventCategory    = 'Spielangebot';
          var tEventObject      = $(this).data('pdf-type') + ' PDF aufrufen';
          var tEventAction      = 'Download';
          ET_Event.eventStart(encodeURI(tEventCategory),encodeURI(tEventObject),encodeURI(tEventAction));
        }
      });
      // loading a game from already bought instantgames
      $('.et_playinstant').click(function(){
        if(typeof ET_Event !== 'undefined'){
          var tEventCategory    = 'Spielangebot';
          var tEventObject      = 'Games & Rubbellose - ' + $(this).data('et-gametype');
          var tEventAction      = 'Laden';
          ET_Event.eventStart(encodeURI(tEventCategory),encodeURI(tEventObject),encodeURI(tEventAction));
        }
      });
      // sharing via email after buying tickets
      $('.et_share_mail').click(function(){
        if(typeof ET_Event !== 'undefined'){
          var tEventCategory    = 'Spielangebot';
          var tEventObject      = 'Spielauftrag teilen - E-Mail';
          var tEventAction      = 'Teilen';
          ET_Event.eventStart(encodeURI(tEventCategory),encodeURI(tEventObject),encodeURI(tEventAction));
        }
      });

      function getIntervalString(aValue, aIntervalSize) {
        if (typeof aValue == 'undefined' || aValue == '')
          return '-';

        var tIntervalStart = parseFloat(aValue) - (parseFloat(aValue) % aIntervalSize);

        return tIntervalStart + ' - ' + (tIntervalStart + aIntervalSize);
      }

    </script>
