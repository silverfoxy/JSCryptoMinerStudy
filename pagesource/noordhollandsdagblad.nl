    <script type="text/javascript">
    // just in case this code will be executed on a server side
    if (typeof window !== "undefined") {
        var query = window.location.search || "";
        var platform = window.sessionStorage.getItem("platform") || "";

        // retrieve platform from the query string
        if (query.indexOf("platform=") !== -1) {
            platform = query.substr(query.indexOf("platform=") + "platform=".length).toLowerCase();
        }

        // now in the platform we have a string "android&otherParam=otherParamValue..."
        // so we have to get rid of everything after "&"
        if (platform.indexOf('&') !== -1) {
            platform = platform.substr(0, platform.indexOf('&'));
        }

        // save the info about the platform to sessionStorage
        window.sessionStorage.setItem("platform", platform);

        // the first time app is loaded query param "app" exists, save it to sessionStorage
        var isApp = query.indexOf("app=true") !== -1 || window.sessionStorage.getItem("isCordovaApp");

        // result from sessionStorage is a string "null" or "false" or "undefined"
        // convert it to boolean value
        if (isApp === "null" || isApp === "false" || isApp === "undefined" || isApp === "") {
            isApp = false;
        }

        window.sessionStorage.setItem("isCordovaApp", isApp);
        window.runningCordova = isApp;
    }
</script>

    
