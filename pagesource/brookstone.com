function fixCurrentOutOfStockPrice(){
try {
$('div.product-standard-price').each(function() {
  if ( $( this ).text().trim() == "Currently Out Of Stock" )
  {
	$( this ).hide();
	$( this ).parent().find( "div.product-sales-price" ).first().css("float", "right");
  }
});
} catch( e ) {}

try {
$('div.product-sales-price').each(function() {
  if ( $( this ).text().trim() == "Currently Out Of Stock" )
  {
	  $( this ).find("span.price").text( $( this ).parent().parent().find("input").first().val().replace( "USD ", "$" ) );
  }
});
} catch( e ) {}
}

try {
fixCurrentOutOfStockPrice();
} catch( e ) {}

