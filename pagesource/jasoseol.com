      <uib-carousel interval="5000" template-url="app/templates/index/indexCarousel.html">
        <uib-slide ng-repeat="slide in slideGGs track by $index" active="slide.active" data-track-content data-content-name="{{ 'main_' + $index }}">
          <a ng-href="{{ slide.link_url }}" ng-style="{ 'background-image': 'url({{ slide.image_url }})' }" target="_blank" data-content-piece="{{ slide.tag }}" data-content-target="{{ slide.link_url }}" data-content-ignoreinteraction ng-click="ggClick(slide, $index)"></a>
        </uib-slide>
      </uib-carousel>
    </div>

    <div class="view-more-recruits">
      <img src="/assets/index/view-more-recruits-c4a7a9645cd2b88fce8628132c3078020e3b7f6e5fc042b8cbcd60ca3a72442c.png" alt="View more recruits c4a7a9645cd2b88fce8628132c3078020e3b7f6e5fc042b8cbcd60ca3a72442c" />
      <a href="/recruit">채용 공고 보기</a>
