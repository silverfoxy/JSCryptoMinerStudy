<script type="text/ng-template" id="requestRecruitMail.html">
  <div class="modal-header">
    <img class="x-button" src="http://jasoseol.com/assets/icon/x_btn-067cbbb80c6e60dd22a2f6302252529d5b10dd90d943cf943a80d124fb6f169c.png" ng-click="cancel()" />
