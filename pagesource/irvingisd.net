<script type="text/javascript">
(function($) {
	$.fn.linkDisclaimer = function(settings) {
		//SETTINGS
		var config = {
			'allowed': [
				'javascript:', 
				'mailto:', 
				'schoolwires.com', 
				'schoolwires.net',
				"irvingisd.net",
				"engradepro.com",
				"myschoolbuilding.com",
				"drive.google.com",
				"classroom.google.com",
				"outlook.com",
				"outlook.office365.com",
				"office.com",
				"irvingisd.tedk12.com",
				"secure.infosnap.com"
			],
			'logoUrl': '/cms/lib/TX01917973/Centricity/template/globalassets/images/popup-logo.png',
			'title': 'You are about to leave the Irving ISD website.',
			'disclaimer': 'Our website contains links to other websites and we have provided such links as a convenience for the public. Inclusion of any link does not imply our endorsement or approval of the linked site or its content.'
		};
		if (settings) {
			$.extend(true, config, settings);
		}
		// loop thru each matched element
		return this.each(function() {
			// loop thru each matched element (a tag from document ready code
			var element = this;
			var myHREF = "#";
			myHREF = String($(element).attr("href"));
			//CHECKS AGAINST ARRAY FOR A MATCH AND MARKS THOSE THAT ARE NOT ALLOWED AND ARE EXTERNAL
			var extLink = 1;
			for (var i = 0; i < config.allowed.length; i++) {
				if (myHREF.match(/:/g)) {
					if (myHREF.match(config.allowed[i])) {
						extLink = 0;
					}
				} else {
					extLink = 0;
				}
			}
			if (extLink == 1) {
				$(element).addClass("external");
			}
			createDisclaimerWindow(config.title, config.logoUrl, config.disclaimer);
		});
		$(".sw-mystart-button.manage a").removeClass("external");

		function createDisclaimerWindow(title, logoPath, disclaimerText) {
			$(".external").on("click", function(e) {
				e.preventDefault();
				$(".pop-up").remove();
				var topPosition = $(this).offset();
				var bodyHeight = $("body").height();
                if(bodyHeight < (topPosition.top + 200)){
                    topPosition = topPosition.top - 250;
                } else {
                    topPosition = topPosition.top - 30;
                }
				var htmlForPopup = '<div class="ui-widget app pop-up" style="border:1px solid #333;background-color: #FFF; width: 60%;min-width: 200px;max-width: 400px;padding: 20px; position: absolute; top: ' + topPosition + 'px;left: 0;right: 0;margin: auto;-moz-box-shadow: 0 0 40px #000;-webkit-box-shadow: 0 0 40px #000;box-shadow: 0 0 40px #000;z-index: 9000;"><div class="ui-widget-header"><h1 style="border-bottom: solid 1px; margin-bottom: 12px; padding-bottom: 12px;">' + title + '</h1></div><div class="ui-widget-detail"><div class="ui-article"><span><div><img class="popup-image" src="' + logoPath + '" height="70" style="float:left; margin: 0px 10px 10px"/><p style="float: right; width: calc(100% - 153px)">' + disclaimerText + '</p></div></span></div></div><div class="ui-widget-footer" style="padding-top: 15px;"><div class="clear"></div><a href="' + $(this).attr("href") + '" target="_blank" class="accept popup button" style="background-color: #FFF;color: #000;display: inline-block;padding: 5px;text-decoration: none;border: 1px solid #000;width: 50%;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;text-align: center;">Accept</a><a href="#" class="cancel popup button" style="background-color: #FFF;color: #000;display: inline-block;padding: 5px;text-decoration: none;border: 1px solid #000;width: 50%;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;text-align: center;">Cancel</a></div></div>';
				$("body").append(htmlForPopup);
				disclaimerClickAccept();
				disclaimerClickCancel();
			});

		}

		function disclaimerClickAccept() {
			$(".accept.popup.button").on("click", function() {
				$(".pop-up").remove();
			});

		}

		function disclaimerClickCancel() {
			$(".cancel.popup.button").on("click", function() {
				$(".pop-up").remove();
				return false;
			});

		}


	}; // ends linkDisclaimer function

})(jQuery);

