	display: none !important;
}
/* GroupBegin Mystart */
#sw-mystart-outer {
	width: auto;
    background: none;
    display: none;
    font-family: 'Titillium Web', sans-serif;
}
#sw-mystart-inner {
	width: auto;
	max-width: 100%;
}
#sw-mystart-left {
	display: none;
}
#sw-mystart-right {
	text-align: left;
    margin-right: 21px;
}
.sw-mystart-button, .sw-mystart-dropdown, #sw-mystart-account, .sw-mystart-button.home {
    padding: 0;
    margin: 17px 0 0 24px;
}
.sw-mystart-button, .sw-mystart-dropdown, #sw-mystart-account {
	font-size: 12px;
    font-weight: 400;
    letter-spacing: .6px;
}
.sw-mystart-button a, .sw-mystart-button:hover a, #sw-mystart-account, .sw-mystart-dropdown, .sw-mystart-button a, .sw-mystart-button a:active, .sw-mystart-button a:visited, .sw-mystart-dropdown .sw-dropdown-arrow {
	color: #FFFFFF;
}
.sw-mystart-button:before {
	content: '';
    display: block;
    position: absolute;
    bottom: -7px;
    left: 0;
    width: 0%;
    height: 2px;
    background: #FFFFFF;
    float: right;
    transition: all .3s ease-in-out;
    -webkit-transition: all .3s linear 0s;
	-moz-transition: all .3s linear 0s;
	-o-transition: all .3s linear 0s;
}
.sw-mystart-button:hover:before {
	width: 100%;
    float: left;
    left: 0;
    right: auto;
}
#sw-home-icon {
	display: none;
}
.sw-mystart-dropdown {
	background: transparent none;
    width: auto;
    padding-right: 13px;
}
.sw-mystart-button:hover, #sw-mystart-account:hover {
	background: transparent none;
}
.sw-mystart-dropdown .sw-dropdown-arrow {
	height: 14px;
	width: 17px;
	top: 2px;
	right: 1px;
	background: none;
	padding: 2px 0px 0px 6px;
	color: #FFFFFF;
	font-size: 11px;
}
.sw-dropdown-arrow, .sw-myaccount-chevron {
	-moz-transform: rotate( 0deg );
	-ms-transform: rotate( 0deg );
	-o-transform: rotate( 0deg );
	-webkit-transform: rotate( 0deg );
	transition: transform 550ms ease;
	-moz-transition: -moz-transform 550ms ease;
	-ms-transition: -ms-transform 550ms ease;
	-o-transition: -o-transform 550ms ease;
	-webkit-transition: -webkit-transform 550ms ease;
    border: 0 none;
}
.sw-dropdown-arrow.rotate-arrow, .sw-myaccount-chevron.rotate-arrow {
	-moz-transform: rotate( -90deg );            
	-ms-transform: rotate( -90deg );            
	-o-transform: rotate( -90deg );            
	-webkit-transform: rotate( -90deg );
	transition: transform 550ms ease;
	-moz-transition: -moz-transform 550ms ease;
	-ms-transition: -ms-transform 550ms ease;
