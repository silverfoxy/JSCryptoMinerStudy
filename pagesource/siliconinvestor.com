<script>
    function getquote1() {
        this.inputEl = document.getElementById('widget_121154_input');
        this.dropdownEl = document.getElementById('widget_121154_dropdown');

        this.myQuotes = new Array();
        this.search_header = '<table cellpadding="0" cellspacing="0" border="0" width="100%">';
        this.search_footer = '</table>';
        this.search_close = false;
        this.search_timer = null;
        this.search_name = null;


        this.headTag = document.getElementsByTagName("head").item(0);

        this.close = function () {
            this.search_close = true;
            setTimeout("document.widget_121154.closeReal()", 500);
        };

        this.closeReal = function () {
            if (this.search_close == true) {
                this.dropdownEl.style.display = 'none';
            }
        };

        this.keyPress = function (e) {
            var key = e.keyCode || e.charCode;
            if (key == 13) {
                document.widget_121154.getQuote(this.inputEl.value);
            }
        };

        this.update = function () {
            this.search_name = this.inputEl.value = this.inputEl.value.toUpperCase();
            if (this.search_timer == null) {
                this.search_timer = setTimeout("document.widget_121154.updateReal()", 200);
            }
        };

        this.updateReal = function () {
            clearTimeout(this.search_timer);
            this.search_timer = null;
            this.dropdownEl.style.display = 'block'; 
            var url = 'http://feeds.financialcontent.com/JSQuote?Account=siliconinvestor&Callback=document.widget_121154.dataCallback&GetName=' + this.inputEl.value;

            var scriptTag = document.createElement("script");
            scriptTag.src = 'http://feeds.financialcontent.com/JSQuote?Account=siliconinvestor&Callback=document.widget_121154.dataCallback&GetName=' + this.inputEl.value;
            this.headTag.appendChild(scriptTag);
        };

        this.dataCallback = function (quotes) {
            this.myQuotes = quotes;
            this.redrawSymbolLookup();
        };

        this.redrawSymbolLookup = function () {
            var HTML = this.search_header;
            var search_regex = new RegExp("(" + this.search_name + ")", "i");
            for (var key in this.myQuotes) {
                var quote = this.myQuotes[key];
                quote['Name'] = quote['Name'].replace(search_regex, "<b>$1</b>");
                quote['SymbolID'] = quote['ExchangeCode'] + ':' + quote['Valoren'];
                quote['DisplayTicker'] = quote['Ticker'].replace(search_regex, "<b>$1</b>");
                HTML += this.getRowHTML(quote);
            }
            HTML += this.search_footer;
            this.dropdownEl.innerHTML = HTML;
        };

        this.getQuote = function (symbol) {
            if (this.inputEl.isValid) {
                document.location.href = 'http://markets.siliconinvestor.com/siliconinvestor/quote' + '?Symbol=' + symbol;
            }
        };

        this.getRowHTML = function (quote) {
            return '<tr onclick=\"document.widget_121154.getQuote(\'' + quote['SymbolID'] + '\')\">' +
                   '<td class="ticker">' + quote['DisplayTicker'] + '</td>' +
                   '<td class="name">' + quote['Name'] + '</td>' +
                   '</tr>';
        };
    }

    document.widget_121154 = new getquote1();
</script>

