  <div class="options" data-bind="visible: showOptions">
   <h4>Newsletter:</h4>
   <div data-bind="foreach: newsletters" class="newsletterOptions">
    <label>
     <input name="newsletters" type="checkbox" data-bind="value: value, checked: checked">
     <span data-bind="text: name"></span>
    </label>
   </div>
   <button value="Signup" type="button" data-bind="click: signupClicked, visible: nNewslettersChecked() > 0 && notProcessing()" >
    Sign Up
   </button>
   <p data-bind="visible: ! notProcessing()" style="text-align:center; color:#001265">
