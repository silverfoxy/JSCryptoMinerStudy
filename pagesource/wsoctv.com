     <div class="cmNewsletterSignupMessage"></div>
 <form class="cmNewsletterSignup" data-bind="visible: formVisible" style="display:none;">
  <input type="hidden" name="groupId" value="12333160" />
  <input type="hidden" name="companyId" value="10155" />
 
    <input class="zipcode" name="zipcode" placeholder="Zip Code" type="text" data-bind="value: zipcode, css: zipcodeValidation()">
    <input class="email" name="email" placeholder="Email* (required)" type="text" data-bind="value: email, css: emailValidation()">
  
  <div class="newsletterErrorMsg" data-bind="visible: zipcodeValidation() == 'invalid' || emailValidation() == 'invalid'">
   <div><span data-bind="text: zipcodeErrorMsg()"></span></div>
   <div><span data-bind="text: emailErrorMsg()"></span></div>
