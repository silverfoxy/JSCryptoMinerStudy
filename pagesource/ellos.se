<dd class="lineitem-size-value">{{= item.Color }}</dd>
{{ } else { }}
<dd>{{= item.Color }}</dd>
{{ } }}
{{ } }}
{{ if(item.Size != null && item.Size != "" && item.Size != "No size" && item.Color != null && item.Color != "" && item.Color != "0") { }}
<dt class="product-attributes-divider hidden-xs">,</dt>
<dt class="product-attributes-divider hidden-sm hidden-md hidden-lg">|</dt>
{{ } }}
{{ if(item.Size != null && item.Size != "" && item.Size != "No size") { }}
