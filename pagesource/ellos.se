{{ } }}
</dl>
<div class="product-table-quantity-minicart">
<div class="stepper-container">
<div class="custom-stepper custom-stepper-round custom-stepper-xs product-table-quantity">
{{ if(item.IsSample != "true" && item.IsFreeGift != "true") { }}
<a href="javascript:void(0);" class="custom-stepper-action custom-stepper-action-decrease js-custom-stepper-decrease">
<svg class="custom-stepper-icon custom-stepper-icon-minus">
<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="&#35;minus"></use>
</svg>
</a>
{{ } }}
<div class="custom-stepper-value">
<input class="js-custom-stepper-original-value" type="hidden" value="{{= item.Quantity }}">
<form class="quantityForm">
{{ if(item.IsMeasuredFabric == "true") { }}
{{ if(item.IsSample == "true") { }}
<span>Varuprov</span>
<a href="javascript:void(0);" class="custom-stepper-action custom-stepper-action-decrease js-custom-stepper-remove-sample">
<svg class="custom-stepper-icon custom-stepper-icon-minus">
<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="&#35;close"></use>
</svg>
</a>
{{ } else { }}
<input type="text" class="form-control measured-fabric js-custom-stepper-value" value="{{= item.Quantity }}"
data-parsley-trigger="change"
data-parsley-trigger-after-failure="change"
data-parsley-required="true"
data-parsley-measure-fabric-quantity-validation="^(\\d+)(((,|\\.)\\d)?)$"
data-parsley-error-message="M&ouml;jlig l&auml;ngd att k&ouml;pa &auml;r 1,0 m - 99,0 m.">
{{ } }}
<span class="measured-fabric-unit-container js-measured-fabric-unit-container">
<span class="measured-fabric-unit">m</span>
</span>
{{ } else { }}
<input type="number" step="1" minlength="1" maxlength="2" class="form-control js-custom-stepper-value" value="{{= item.Quantity }}"
data-parsley-trigger="change"
data-parsley-trigger-after-failure="change"
data-parsley-required="true"
data-parsley-type="integer"
data-parsley-minlength="1"
data-parsley-maxlength="2"
data-parsley-min="0"
data-parsley-max="99"
data-parsley-error-message="M&ouml;jligt antal att k&ouml;pa &auml;r 1 - 99 st."
{{ if(item.IsFreeGift == "true") { }}
disabled="disabled"
{{ } }}
>
{{ } }}
</form>
</div>
{{ if(item.IsSample != "true" && item.IsFreeGift != "true") { }}
<a href="javascript:void(0);" class="custom-stepper-action custom-stepper-action-increase js-custom-stepper-increase {{= (item.Quantity >= 99) ? 'is-disabled' : '' }}">
<svg class="custom-stepper-icon custom-stepper-icon-plus">
<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="&#35;plus"></use>
</svg>
</a>
{{ } }}
</div>
</div>
<div class="product-table-removed-container">
<div class="product-table-removed-restore-text">Vill du ta bort artikeln fr&aring;n varukorgen?</div>
<div class="product-table-removed-restore-buttons">
<button class="btn btn-primary btn-md js-product-table-remove-confirmation-no btn-remove-confirmation">Nej</button>
<button class="btn btn-primary btn-md js-product-table-remove-confirmation-yes btn-remove-confirmation">Ja</button>
</div>
</div>
</div>
</div>
<div class="product-table-col product-table-col-price">
{{ if(item.HasHigherOriginalPrice){ }}
<div class="product-table-price product-table-price-original">
<span class="price-amount line-through">{{= item.OriginalPrice }}</span><span class="price-currency">{{= currentCurrencyMnemonic}}</span>
</div>
<div class="product-table-price product-table-price-discount">
<span class="price-amount">{{= item.Price }}</span><span class="price-currency">{{= currentCurrencyMnemonic}}</span>
</div>
{{ } else { }}
<div class="product-table-price">
<span class="price-amount">{{= item.Price }}</span><span class="price-currency">{{= currentCurrencyMnemonic}}</span>
</div>
{{ } }}
</div>
</div>
{{ }); }}
</div>
</script>
