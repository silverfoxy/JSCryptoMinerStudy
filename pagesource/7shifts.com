<!-- Google Code for Restaurant Scheduling Summer 2016 Conversion Page -->
<script type="text/javascript">
    /* <![CDATA[ */
    goog_snippet_vars = function() {
        var w = window;
        w.google_conversion_id = 1063206051;
        w.google_conversion_label = "J_hVCKmo9GoQo_n8-gM";
        w.google_remarketing_only = false;
    }
    // DO NOT CHANGE THE CODE BELOW.
    goog_report_conversion = function(url) {
        goog_snippet_vars();
        window.google_conversion_format = "3";
        var opt = new Object();
        opt.onload_callback = function() {
            if (typeof(url) != 'undefined') {
                window.location = url;
            }
        }
        var conv_handler = window['google_trackConversion'];
        if (typeof(conv_handler) == 'function') {
            conv_handler(opt);
        }
    }
    /* ]]> */

    /**
     * site wide signup form code
     */

    var signup_form;
    var text_download_link_form;
    function removeAndReturnFormAttrs(){
        var form_attrs = {};
        var form_id;
        $(".signup-form-start").each(function(){
            form_id = $(this).attr('id');
            form_attrs[form_id] = {};
            $(this).find('[form]').each(function(){
                if(typeof $(this).attr('id') != 'undefined'){
                    form_attrs[form_id][$(this).attr('id')] = $(this).attr('form');
                }
                $(this).removeAttr('form');
            });
        });
        return form_attrs;
    }

    function restoreFormAttrs(form_attrs){
        try{
            $(".signup-form-start").each(function(){
                form_id = $(this).attr('id');
                $.each(form_attrs[form_id],function(key,value){
                    $('#'+key).attr('form',value);
                });
            });
        } catch(e){
        }
    }

    function extraSuccessfulConversionCode(){
        _pq.push(['track', 'already_signed_up']);
        _pq.push(['track', 'signed_up', { orderId: encodeURIComponent(signup_form.options.form_fields.signup_email).replace(/%40|\./gi, '_') }]);
        if(campaign_utm_source && campaign_utm_source == 'facebook'){
            fbq('track', 'CompleteRegistration');
        }
        if(typeof(goog_report_conversion) == 'function'){
            goog_report_conversion();
        }
        ga('send', 'event', { eventCategory: 'registration', eventAction: 'account', eventLabel: signup_form.options.mixpanel_funnel_properties.initial_form_section, eventValue: 1});
        fbq('track', 'Lead');
    }

    $(function(){
        signup_form = new Shifts.Views.Form({
                        el: '#signup-form',
                        section: 'create_account',
            mixpanel_funnel_properties: {},
            success_msg: (is_mobile == true) ? '' : 'Success! Redirecting&hellip;',
            additional_data: {}
        });

        //these signup form events should probably be put right into Form 

        signup_form.on('submit_attempt', function(e){
            var form_section = e.$el.data('property-form-section');
            signup_form.options.mixpanel_funnel_properties.form_section = (form_section == '') ? 'was not defined' : form_section;
            if(e.$el.attr('data-property-initial-form-section')){
                var initial_form_section = e.$el.data('property-initial-form-section');
                signup_form.options.mixpanel_funnel_properties.initial_form_section = (initial_form_section == '') ? 'was not defined' : initial_form_section;
                signup_form.options.mixpanel_funnel_properties.ab_experiment_variant = Shifts.ab_experiment_variant;
            }
            if(signup_form.$el.attr('data-property-geoip-city')){
                signup_form.options.mixpanel_funnel_properties.geoip_city = signup_form.$el.attr('data-property-geoip-city');
            }
            
            signup_form.options.additional_data.signup_form = signup_form.options.mixpanel_funnel_properties.initial_form_section;
            
            var event_label;
            var hotjar_event_label;
            var default_label = 'Attempted to submit signup form section';
            var event_properties = signup_form.options.mixpanel_funnel_properties;
            
            switch(form_section){
                case 'personal-info':
                    event_label = "Attempted to submit personal info signup form section";
                    hotjar_event_label = "/attempt-personal-info";
                    break;
                case 'user-type':
                    event_label = "Attempted to submit user type signup form section";
                    break;
                case 'company-info':
                    event_label = "Attempted to submit company info signup form section";
                    hotjar_event_label = "/attempt-company-info";
                    break;
            }
            
            if(form_section != 'user-type'){
                if(Shifts.Utils.isProduction() == false){
                    console.log(event_label);
                    console.log(event_properties);
                    console.log(default_label);
                    console.log(event_properties);
                }
                mixpanel.track(default_label, event_properties);
                mixpanel.track(event_label, event_properties);
                hj('vpv',hotjar_event_label)
            }

            //on submit attempt check if we're on the last form section, if we are then remove inputs outside of #signup-form that have form="signup-form", except for when they are inside the current initial_form_section
             //save all the inputs as a property of signup_form, we will re-add them on form_submit_success and form_submit_fail
            if(e.$el.data('property-form-section') == 'company-info'){
                var form_attrs = removeAndReturnFormAttrs();
                signup_form.options.form_inputs_outside_form = form_attrs;
                form_id = $('[data-property-initial-form-section="'+signup_form.options.mixpanel_funnel_properties.initial_form_section+'"]').attr('id');
                $.each(form_attrs[form_id],function(key,value){
                    $('#'+key).attr('form',value);
                });

            }
        });

        signup_form.on('submit_validate_success', function(e){
            var event_label;
            var hotjar_event_label;
            var default_label = 'Successfully submitted signup form section';
            var event_properties = signup_form.options.mixpanel_funnel_properties;
            switch(signup_form.options.mixpanel_funnel_properties.form_section){
                case 'personal-info':
                    event_label = "Successfully submitted personal info signup form section";
                    hotjar_event_label = '/success-personal-info';
                    break;
                case 'user-type':
                    event_label = "Successfully submitted user type signup form section";
                    break;
                case 'company-info':
                    event_label = "Successfully submitted company info signup form section";
                    hotjar_event_label = '/success-company-info';
                    break;
            }
            if(signup_form.options.mixpanel_funnel_properties.form_section != 'user-type'){
                if(Shifts.Utils.isProduction() == false){
                    console.log(event_label);
                    console.log(event_properties);
                    console.log(default_label);
                    console.log(event_properties);
                }
                mixpanel.track(default_label, event_properties);
                mixpanel.track(event_label, event_properties);
                hj('vpv',hotjar_event_label);
            }
        });

        signup_form.on('submit_validate_fail', function(e){
            var field_errors_list = new Array();
            $.each(e.errors,function(key,value){
                if(key == 'signup_phone' && value == 'Invalid phone number'){
                    var phone_number = signup_form.$el.find("[name='"+key+"']").val();
                    var initial_form_section = signup_form.options.mixpanel_funnel_properties.initial_form_section;
                    var alert_data = {message:'Invalid phone number: '+phone_number+' (timezone: ' + Shifts.Utils.getClientTimezone()+')',initial_form_section:initial_form_section};
                    $.ajax({
                        url: '/shifts-marketing/submit-server-response-fail-alert/shawn',
                        data: alert_data,
                        type: 'post',
                        dataType: 'json',
                        cache: false
                    }).done(function(res){
                        
                    });
                }
                field_errors_list.push(key + ': ' + value);
            });
            var event_label;
            var default_label = 'Submitted invalid signup form section';
            var event_properties = $.extend({}, {field_errors: field_errors_list}, signup_form.options.mixpanel_funnel_properties);
            switch(signup_form.options.mixpanel_funnel_properties.form_section){
                case 'personal-info':
                    event_label = "Submitted invalid personal info signup form section";
                    break;
                case 'user-type':
                    event_label = "Submitted invalid user type signup form section";
                    break;
                case 'company-info':
                    event_label = "Submitted invalid company info signup form section";
                    break;
            }
            if(Shifts.Utils.isProduction() == false){
                console.log(event_label);
                console.log(event_properties);
                console.log(default_label);
                console.log(event_properties);
            }
            mixpanel.track(default_label, event_properties);
            mixpanel.track(event_label, event_properties);
        });

        signup_form.on('submit_server_response_fail', function(e){
            var event_label = 'Server responded with error(s) after signup form submit';
            var event_properties = $.extend({}, {server_response: e.message}, signup_form.options.mixpanel_funnel_properties);
            if(Shifts.Utils.isProduction() == false){
                console.log(event_label);
                console.log(event_properties);
            }
            mixpanel.track(event_label,event_properties);
        });

        signup_form.on('submit_server_response_success', function(e){
            var event_label;
            var hotjar_event_label;
            var event_properties;
            if(e.hasOwnProperty("data") && e.data.hasOwnProperty("mobile") && e.data.mobile == true){
                event_label = 'Successfully signed up';
                hotjar_event_label = '/success-signup';
                event_properties = signup_form.options.mixpanel_funnel_properties;
                if(Shifts.Utils.isProduction() == false){
                    console.log(event_label);
                    console.log(event_properties);
                }
                mixpanel.track(event_label,event_properties);
                hj('vpv',hotjar_event_label)
                extraSuccessfulConversionCode();
                $('.modal').modal('hide');
                $('#signup-complete-on-desktop').modal('show');
            } else if(e.hasOwnProperty("data") && e.data.hasOwnProperty('redirect_url')){
                event_label = 'Successfully signed up';
                hotjar_event_label = '/success-signup';
                event_properties = signup_form.options.mixpanel_funnel_properties;
                if(Shifts.Utils.isProduction() == false){
                    console.log(event_label);
                    console.log(event_properties);
                }
                mixpanel.track(event_label, event_properties);
                hj('vpv',hotjar_event_label)
                extraSuccessfulConversionCode();

                //give half a second for mixpanel to track event
                setTimeout(function(){
                    window.location.href = e.data.redirect_url;
                }, 2000);
                
            } else{
                signup_form.ajaxFail();
            }
        });

        signup_form.on('submit_server_response_fail submit_server_response_success',function(e){
            restoreFormAttrs(signup_form.options.form_inputs_outside_form);
            delete signup_form.options.form_inputs_outside_form;
        });


        //open modal automatically if they come from login page
                //we fashion our own submit actions for each individual form section using the signup_form validate methods
        $(".signup-form-start").submit(function(e){
            
            e.preventDefault();
            var $el_saved = signup_form.$el;
            signup_form.$el = $(this);
            signup_form.trigger('submit_attempt', {$el:signup_form.$el});
            //can't validate form unless form attributes are removed. add them back afterwards
            var form_attrs = removeAndReturnFormAttrs();
            $(".shifts-modal .user-type-col").removeClass("active");
            $(".shifts-modal .user-type-owner").addClass("active");
            $(".shifts-modal .user-type-signup-form").removeClass("active");
            $(".modal-dialog").removeClass('big');

                            $("#signup-form").addClass('active');
                $(".user-type-owner").addClass('active');
                $(".user-type").addClass('active');
                $("#initial-question").removeClass('active');
                $(".modal-dialog").addClass('big');
            
            
            signup_form.nextModalFormSection($('#signup-form-modal'));
            restoreFormAttrs(form_attrs);
            signup_form.$el = $el_saved;
        });

        //validate second form section 
        $("#user-type-form").submit(function(e){
            e.preventDefault();
            var event_label;
            var event_properties;
            var $el_saved = signup_form.$el;
            signup_form.$el = $(this);
            signup_form.trigger('submit_attempt', {$el:signup_form.$el});
            var val = $(this).find("button[type=submit][clicked=true]").val();
            if(val == 'manager/owner'){
                event_label = "User type 'manager/owner' chosen";
                event_properties = signup_form.options.mixpanel_funnel_properties;
                if(Shifts.Utils.isProduction() == false){
                    console.log(event_label);
                    console.log(event_properties);
                }
                mixpanel.track(event_label, event_properties);
                ga('send', 'event', { eventCategory: 'registration', eventAction: signup_form.options.mixpanel_funnel_properties.initial_form_section, eventLabel:event_label, eventValue: 1});
                signup_form.nextModalFormSection($('#signup-form-modal'));
            } else{
                event_label = "User type 'employee' chosen";
                event_properties = signup_form.options.mixpanel_funnel_properties;
                if(Shifts.Utils.isProduction() == false){
                    console.log(event_label);
                    console.log(event_properties);
                }
                mixpanel.track(event_label, event_properties);
                $('.modal').modal('hide');
                $("#employee-signup-modal").modal('show');
                // Shifts.Utils.showErrorModal("<div class='center'><p>Please contact your manager.</p><p>They'll need to setup an account for you and invite you to&nbsp;7shifts.</p></div>"); 
            }
            signup_form.$el = $el_saved;
            
        });
        //lets us know which button user clicked on submit event
        $("#user-type-form button[type=submit]").click(function() {
            $(this).parents('form').find('button[type="submit"]').each(function(){
                $(this).removeAttr("clicked");
            });
            $(this).attr("clicked", "true");
        });

        //prevent submit on the other form sections when hitting enter in first form section (because we use form attribute)
        $(".signup-form-start").find('input').on('keydown', function(e){
            var keyCode = e.keyCode || e.which;
            if(keyCode == 13){
                e.preventDefault();
                $(this).parents("form.signup-form-start").submit();
            }
        });

        $('#signup-form-modal').on('hide.bs.modal', function () {
            $("#signup-form-modal form .notice-area").html('').removeClass('success error').hide();
        });

        $(".shifts-modal .user-type-col").click(function(e){
            $(".shifts-modal .user-type-col").removeClass("active");
            $(this).addClass("active");
            $(".user-type-signup-form").removeClass("active");
            var mixpanel_properties = {};
            if($(this).hasClass("user-type-owner")){
                mixpanel_properties.user_type = 'owner/manager';
                $(".shifts-modal #signup-form").addClass("active");
            } else{
                mixpanel_properties.user_type = 'employee';
                $(".shifts-modal #signup-employee").addClass("active");
            }
            try{
                mixpanel_properties = $.extend({}, mixpanel_properties, signup_form.options.mixpanel_funnel_properties);
            } catch(e){
            }
            mixpanel.track('Signup user type toggled',mixpanel_properties);
            
        });

        $(".shifts-modal .signup-employee-manager").click(function(e){
            if($(this).hasClass("i-am-manager")){
                $("#signup-form").addClass('active');
                $(".user-type-owner").addClass('active');
                $(".user-type").addClass('active');
                $("#initial-question").removeClass('active');
                $(".modal-dialog").addClass('big');
            }else{
                $("#signup-employee").addClass('active');
                $(".user-type-employee").addClass('active');
                $("#initial-question").removeClass('active');
                $(".user-type-owner").removeClass('active');
                $(".user-type").addClass('active');
                $(".modal-dialog").addClass('big');
            }
        });
        
        $(".shifts-modal .signup-employee-option").click(function(e){
            if($(this).hasClass("signup-employee-option-yes")){
                $("#signup-employee-yes").addClass('active');
                $('#signup-employee-question').removeClass('active');
            }else{
                $("#signup-employee-no").addClass('active');
                $('#signup-employee-question').removeClass('active');
            }
        });

        //about page link tracking
        $("a[data-track-link-label]").click(function(e){
            e.preventDefault();
            var link_label = $(this).data("track-link-label");
            mixpanel.track('clicked link: '+ link_label);
            var $this = $(this);
            setTimeout(function(){
                window.location.href = $this.attr('href');
            },700);
        });

        text_download_link_form = new Shifts.Views.Form({
            el: '#text-download-link-form-modal',
            section: 'download-app-link',
            mixpanel_funnel_properties: {},
            success_msg: 'Text sent. Check your phone to download the app.',
            failure_msg: "We weren't able to text that number.<br>Make sure you enter the number like so:<br>'555-555-5555'"
        });

        //track events with mixpanel 
        text_download_link_form.on('submit_attempt', function(e){
            var form_section = e.$el.data('property-form-section');
            text_download_link_form.options.mixpanel_funnel_properties.form_section = (form_section == '') ? 'was not defined' : form_section;
            mixpanel.track('Attempted to submit text download link form', text_download_link_form.options.mixpanel_funnel_properties);
        });
        text_download_link_form.on('submit_validate_success', function(e){
            mixpanel.track('Successfully submitted text download link form', text_download_link_form.options.mixpanel_funnel_properties);
        });
        text_download_link_form.on('submit_validate_fail', function(e){
            mixpanel.track('Submitted invalid text download link form', text_download_link_form.options.mixpanel_funnel_properties);
        });
        text_download_link_form.on('submit_server_response_fail', function(e){
            mixpanel.track('Server responded with error(s) after text download link form submit', text_download_link_form.options.mixpanel_funnel_properties);
        });
        text_download_link_form.on('submit_server_response_success', function(e){
            mixpanel.track('Employee got app',text_download_link_form.options.mixpanel_funnel_properties);
        });

        //track download links
        Shifts.Utils.trackLinkEvent('.mobile-icon-appstore', 'download-app-link-click', function(){
            mixpanel.track('Employee got app', {form_section:'appstore-link'});
        });
        Shifts.Utils.trackLinkEvent('.mobile-icon-gplay', 'download-app-link-click', function(){
            mixpanel.track('Employee got app', {form_section:'gplay-link'});
        });

        //create a mixpanel event when the sticky nav signup cta is clicked. If things look weird that's because this was originally coded to support multiple buttons in nav as an optimize experiment. Will cleanup later if we don't choose to experiment
        $('.btn-create-free-trial').on('click', function(e){

            var click_event_name = 'mobile_nav_signup_button_clicked';    
            if( 
                ($(this).parents('#header-wrap').hasClass('original') && $(this).css('display') != 'inline-block') || 
                ($(this).parents('#header-wrap').hasClass('variant-1') && $(this).hasClass('mobile-nav-item')) 
            ){
                e.preventDefault();
                $(this).trigger(click_event_name);
            }
        });
        $('.btn-create-free-trial').on('mobile_nav_signup_button_clicked', function(e){
            var $link = $(e.target);
            var variant = $(e.target).parents('#header-wrap').attr('class');
            mixpanel.track('mobile nav signup button clicked', {variant:variant});
            ga('send', 'event', { eventCategory: 'mobile_nav_signup_button', eventAction: 'click'});
            Shifts.Utils.delayLink($link);
            //destroy event handler so we don't track twice
            $(this).off('mobile_nav_signup_button_clicked');
        });

        //banner questions
        $(".top-nav-question .close").click(function(e){
            $(".top-nav-question").hide();
            mixpanel.track("banner question close button clicked");
            Cookies.set(
                'hide-banner-question',
                true, 
                { expires: 30, path: '/' }
            );
        });

        $(".question-group-1 button").click(function(){
            $(".question-group").hide();
            if($(this).text().trim().toLowerCase() == 'yes'){
                $(".question-group-2a").show();
            } else {
                $(".question-group-2b").show();
            }
            mixpanel.track("banner question 1 answered: " + $(this).text().trim().toLowerCase());
        });

        $(".question-group-2a button, .question-group-2b button").click(function(){
            mixpanel.track("banner question 2 answered: " + $(this).text().trim().toLowerCase());

            if($(this).text().trim().toLowerCase() == 'yes'){
                setTimeout(function(){
                    window.location.href = "signup?utm_source=website-banner-question";
                }, 2000);
            } else{
                $("#header-wrap").removeClass('has-question');
                $(".question-group").hide();
                var cookie_value = {
                    'source': 'website-banner-question-no-no',
                    'date': current_date,
                    'campaign': campaign_utm_campaign,
                    'medium': campaign_utm_medium
                };
                cookie_value = JSON.stringify(cookie_value);
                Cookies.set(
                    'utm_source',
                    cookie_value, 
                    { expires: 30, path: '/' }
                );
            }
        });
        var check_utm_source_cookie = Cookies.get('utm_source');
        if(typeof check_utm_source_cookie !== 'undefined'){
            try {
                check_utm_source_cookie = JSON.parse(check_utm_source_cookie);
                if(check_utm_source_cookie.hasOwnProperty('source')){
                    if(check_utm_source_cookie.source == 'website-banner-question-no-no'){
                        $("#header-wrap").removeClass('has-question');
                        $(".top-nav-question").hide();
                    }
                }
            } catch(e){
            }
        }
        var check_hide_banner_question_cookie = Cookies.get('hide-banner-question');
        if(typeof check_hide_banner_question_cookie !== 'undefined'){
            if(check_hide_banner_question_cookie == 'true'){
                $("#header-wrap").removeClass('has-question');
                $(".top-nav-question").hide();
            } else{
            }
        }
    });

</script>

<script type="text/javascript" src="//www.googleadservices.com/pagead/conversion_async.js"></script>
<script type="text/javascript">
$(function(){
	//partners directory mixpanel event tracking
    $("#partnerships-homepage .partner-item").on('click', function(e){
        var $link;
        if($(e.target).prop("tagName").toLowerCase() == 'a'){
            $link = $(e.target);
        } else {
            $link = $(e.target).parents('a');
        }
    
        if($link.length > 0){
            e.preventDefault();
            $(this).trigger('ip-link-click');
        }
    });
    $("#partnerships-homepage .partner-item").on('ip-link-click', function(e){
        var $link = $(e.target).find("a");
        var partner_name = $(this).find('.partner-name').text();
        var event_properties = {"partner": partner_name};
        mixpanel.track('Clicked integrated partner home page link', event_properties);
        Shifts.Utils.delayLink($link);
        //destroy event handler so we don't track twice
        $(this).off('ip-link-click');
    });
});
mixpanel.track('page viewed', {
    'page name' : document.title,
    'url' : window.location.pathname
});
</script></body>
