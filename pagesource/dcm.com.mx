						   //Expresión regular que valida que la dirección de correo eléctronico sea valido
						   function validar_email(valor){
							   var filter = /[\w-\.]{3,}@([\w-]{2,}\.)*([\w-]{2,}\.)[\w-]{2,4}/;
							   if(filter.test(valor))
							   		return true;
								else
									return false;
								}
							
							
