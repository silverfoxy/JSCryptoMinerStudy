function getClaveOperacion(){
	var checkPersonal = $(".personaAutorizada").length
	var checkUsuarios = $(".usuariosInternet").length
	var clave = $("#claveO").val();
	if (parseInt(checkPersonal) > 1 || parseInt(checkUsuarios) > 1){
		if (clave == "" ){
		
			alert("Clave de operaci&oacute;n vacia.");
		}
		else{
			var checkIdPer1 = "";
			$( ".personaAutorizada" ).each(function() {
			  if ($(this).is( ":checked" )){
				checkIdPer1+= $(this).attr("id")+","
			  }
			});
			checkIdPer = checkIdPer1.substring(0,checkIdPer1.lastIndexOf(","))
			var checkIdUsua1 = "";
			$( ".usuariosInternet" ).each(function() {
			  if ($(this).is( ":checked" )){
				checkIdUsua1+= "'"+$(this).attr("id")+"',"
			  }
			});
										
			checkIdUsua = checkIdUsua1.substring(0,checkIdUsua1.lastIndexOf(","))
			
			$.ajax({
				url: "/usuarios/modeloUsuarios.asp?tipo=C&clave="+clave,
				type: "GET",
				success: function(data){
					$("#claveOperacionCarga").html("");
					$("#claveOperacionCarga").html("Validando Clave de Operaci&oacute;n..");
					setTimeout(function(){ 
						
						if(data == "Verdadero" || data == "true"){	
							$("#claveOperacionCarga").html("");	
							$("#claveO").val("");
							
						//	insertBitacora(checkIdUsua,checkIdPer);
							$("#userInter").val(checkIdUsua);
							$("#personalInter").val(checkIdPer);
							eliminarUsuarios(checkIdUsua);
							eliminarPersonal(checkIdPer);
						}
						else{
							$("#claveOperacionCarga").html("Clave Incorrecta");	
						}
					}, 2000);
				},
				error: function(){
					alert("error");
				}
			});
		}
	}
	else{
		Alert("Sin usuarios");
	}
	
}	

