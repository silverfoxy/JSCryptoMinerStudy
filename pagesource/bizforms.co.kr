//	메인 서식영역 & 매거진영역 시작
	$(function (){
		bizmain_magazine_change(1);

		bizmain_form_start = setInterval(function (){
			for (i=0; i<=3; i++){
				if ($("#main_form" + i).attr("class") == "on"){
					bizmain_form_view_num	= i + 1;

					break;
				}

				bizmain_form_view_num		= 0;
			}

			if (bizmain_form_view_num > 3){
				bizmain_form_view_num = 0;
			}

			bizmain_form_change(bizmain_form_view_num);
		}, 3000);
	});

//	서식영역 변경 시
	function bizmain_form_change(num){
		for (i=0; i<=3; i++){
			if (num == i){
				$("#main_form" + i).attr("class", "on");
				$("#bizmain_form_" + i).show();
			}else{
				$("#main_form" + i).attr("class", "");
				$("#bizmain_form_" + i).hide();
			}
		}
	}

	function bizmain_format(type){
		if (type == "stop"){
			clearInterval(bizmain_form_start);
		}else{
			bizmain_form_start = setInterval(function (){
				for (i=0; i<=3; i++){
					if ($("#main_form" + i).attr("class") == "on"){
						bizmain_form_view_num = i + 1;

						break;
					}

					bizmain_form_view_num	= 0;
				}

				if (bizmain_form_view_num > 3){
					bizmain_form_view_num = 0;
				}

				bizmain_form_change(bizmain_form_view_num);
			}, 3000);
		}
	}

//	매거진영역 page변경 시
	function bizmain_magazine_change(num){
		$.ajax({
			type:'post',
			url:'./main/bizmain_magazine.asp?id=' + num,

			success:function (data){
				$("#bizmain_magazine_list").html(data);
			}
		});
	}
