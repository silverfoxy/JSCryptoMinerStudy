	window.onload = function () {
		allquestion_count_interval();
	}

	function allquestion_count_interval(){
		var all_question_count	 = "23139";
		var today_question_count = "";
		var count_num = 0;
		var base_count			 = 10000;
		if(all_question_count < base_count){base_count = 6000;}
		var timing_count			 = Math.floor((all_question_count-base_count)/today_question_count);
		var total_timing_count		 = 0;
		count_num = base_count;
		var today_count = 0;
		var interval = setInterval(function(){
			count_num = count_num+8;
			if(total_timing_count < count_num-base_count){
				$("#today_question_count").text(commaNum(today_count));
				total_timing_count = total_timing_count + timing_count;
				today_count = today_count + 1;
			}
			if(all_question_count>count_num){
				number_count(count_num);
			}else{
				count_num = all_question_count;
				number_count(count_num);
				clearInterval(interval);
			}
		}, 1);
	}

	function number_count(num){$("#all_question_count").text(commaNum(num));}

	function commaNum(num) {  
        var len, point, str;    
        num = num + "";  
        point = num.length % 3  
        len = num.length;    
        str = num.substring(0, point);  
        while (point < len) {  
            if (str != "") str += ",";  
            str += num.substring(point, point + 3);  
            point += 3;  
        }  
        return str;  
    }	
