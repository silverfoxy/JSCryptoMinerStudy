<script type="text/javascript">
	$(function() {
		$(".any-class").jCarouselLite({
			auto:1000,
			speed:500,
			vertical: true,
			hoverPause: true,
			visible: 5,
			btnNext: ".next",
			btnPrev: ".prev"
		});
