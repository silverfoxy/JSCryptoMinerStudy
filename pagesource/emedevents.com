function newgetsearchConferencesData(formId, pageId, searchType) {
if (formId == 'newconfHeaderSearchForm')
                {
                     if ($("#" + formId + " #keywordSearch").val() == '')
                        {
                                alert("Please enter search keyword");
                                $("#" + formId + " #keywordSearch").focus();    

                        if( $("#" + formId + " #search-time").val() == 'customdate') {
                            $('#custom_date').show();  
                       } else {

                       }
                         return false;
                    }
                
                    if ($("#custom_startdate").val() != '' || $("#custom_enddate").val() != '')
			        {
			           $('.search-time-option').val('customdate');

			        }
                                else
                                {
                                    $('.search-time-option').val('0');
                                }
                    var idvalue = $('.is-active').attr('id');
                    if ($("#" + formId + " #"+idvalue+"_keywordSearch").val() == '')
                    {
                        alert("Please enter search keyword");
	        	$("#" + formId + " #"+idvalue+"_keywordSearch").focus(); 
                        return false;
                    }
                    else {
		           
	            $("#" + formId + " #pageId").val(pageId);
	            if ($("#" + formId + " #headerSearchType").val() == 5)
	            {
	                $("#" + formId).attr("action", "/speakers/index");
	            } else if ($("#" + formId + " #headerSearchType").val() == 6)
	            {
	                $("#" + formId).attr("action", "/organizers/index");
	            } else {
	                if($('#headerSearchTypee').val() == 'conference')
                        $("#" + formId).attr("action", "/Conferences/searchConference");
                    else
                        $("#" + formId).attr("action", "/search-result");
	            }
	            if (searchType == 'search')
	            {
	                $("#" + formId).attr("onsubmit", '');
	                $("#" + formId).submit();
	            }
	        }
                }
            }
