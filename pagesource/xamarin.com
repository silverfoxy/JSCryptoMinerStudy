        <script>
          // Global variable to determine cookie consent
          var hasMsccConsent = false;

          $(document).ready(function() {
        
            function CookieConsent(resp) {
                // Creat cookie banner container
                var cookieBanner = document.createElement("div");
                cookieBanner.id = "cookie-banner";
                $('body').prepend(cookieBanner);

                // Hide cookie banner container so that the response markup doens't flicker
                document.getElementById("cookie-banner").style.display = "none";

                // Add styles from response
                var styleSheet = $('<link type="text/css" id="stylesheetest" href="'+resp.Css+'" rel="stylesheet" />');
                $('head').append(styleSheet);

                // Append markup from response to container
                $('#cookie-banner').html(resp.Markup);

                // Add script from response to DOM
                var script = document.createElement("script");
                script.type = "text/javascript";
                script.id = "xmn-mscc";
                script.src = resp.Js;
                document.body.appendChild(script);

                // When mscc script is loaded, check for consent
                script.onload = function() {
                    var mscc = window.mscc;
                    hasMsccConsent = mscc.hasConsent() === true;
                    if (hasMsccConsent) {
                        startGtm()
                    } else if (!hasMsccConsent) {
                        document.getElementById("cookie-banner").style.display = "block";
                        mscc.on('consent', function() {
                            startGtm();
                            hasMsccConsent = true;
                        });
                    }
                }
            }

            var startGtm = function() {
                // Add Google Tag Manager
                var script = document.createElement('script');
                script.innerHTML = "(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start': new Date().getTime(),event:'gtm.js'}); var f=d.getElementsByTagName(s)[0], j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src='//www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);})(window,document,'script','dataLayer','GTM-5HJW6B');";
                document.getElementById('gtm-noscript').insertAdjacentElement('afterend', script);
            }

            var lang = navigator.userLanguage || navigator.language;
            
            // Use euregion for country until there's a better solution for determining country
            // doesn't have geo location
            $.ajax({
                url: "https://uhf.microsoft.com/"+lang+"/shell/api/mscc?sitename=xamarin&domain=xamarin.com&country=euregion",
                type: "GET"
            }).done(function(resp) {
                //able to get country region from the response
                CookieConsent(resp);
            }).fail(function(err) {
                console.log(err)
            })
          })
