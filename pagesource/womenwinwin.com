<script type="text/javascript">
    
function isValidEmailAddress(emailAddress) {
    var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
    return pattern.test(emailAddress);
}


$("#frmLogin").submit(function(){
    var txtLogin=$("#txtLogin").val();
    var txtPassword=$("#txtPassword").val();
    
    if(txtLogin===''){
        alert("Preencha o seu utilizador.");
        return false;
    }
    
    if(txtPassword===''){
        alert("Preencha a sua password.");
        return false;
    }
   
});


$(".frmBannerRegister").submit(function(){
    var txtName=$("#txtName").val();
    var txtLastName=$("#txtLastName").val();
    var txtEmail=$("#txtEmail").val();
    var txtAccessPassword=$("#txtAccessPassword").val();
    var txtRepetirEmail=$("#txtRepetirEmail").val();
    var txtDuplicateAccessPassword=$("#txtDuplicateAccessPassword").val();
     
    if(txtName===''){
        alert("Preencha o campo Nome");
        return false;
    }
    
    if(txtLastName===''){
        alert("Preencha o campo Apelido");
        return false;
    }
    
   
    
    if(!isValidEmailAddress(txtEmail)){
            alert("Preencha o campo Email correctamente");
            return false;
    }
    $("#txtRepetirEmail").val(txtEmail);
    $("#txtDuplicateAccessPassword").val(txtAccessPassword);
    
    if(txtAccessPassword===''){
        alert("Preencha o campo Senha");
        return false;
    }
    
    if($('#chkTermsAndConditions').is(":checked")===false){
        alert("Tem de aceitar os termos e condições");
        return false;
    }
        
    
});


$(".btn-search-top").click(function(){
    var pesquisaComunidade=$("#pesquisaComunidade").val();
    
    if(pesquisaComunidade===''){
        alert("Preencha o texto a pesquisar");
        return false;
    }
    
    $("#form-search-members").submit();
});

</script>

