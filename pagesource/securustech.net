//*****This function ensures the footer is, at a minimum, at the bottom of the page.*****
function footerAdjust() {
    var windowHeight,
        footerOffset,
        footerHeight;

    windowHeight = $(window).height();
    footerOffset = $('footer').offset().top;
    footerHeight = $('footer').height();

    if (footerOffset + footerHeight < windowHeight) {
        $('footer').offset({ top: footerOffset + (windowHeight - (footerOffset + footerHeight)) });
    }
}

$(document).ready(function() {
    footerAdjust();
});

$(window).resize(function() {
    footerAdjust();
});
