                }
            }
        };

        var _isValid = function() {
            return typeof _id == "string" && _id.indexOf('wiwo') !== -1 && _domElem != null;
        };

        // Public methods
        this.getId = function() {
            return _id;
        };
        this.getDomElem = function() {
            return _domElem;
        };

        this.updateOptions = function() {
            _wiwo.push(['postMessage', _id, 'wiwo.dido.setData', _defaults]);
        };
        this.setTeaserType = function(mode) {
            if (str == null || str === "") return;
            _defaults.input.rpyControlViewModel.teaserStart = mode !== "" && mode !== "NONE";
            _defaults.input.rpyControlViewModel.teaserMode = mode;
            this.updateOptions();
        };
        this.setOptionsToggle = function(bool) {
            if (bool == null) return;
            _defaults.input.rpyControlViewModel.enableModeSwitch = bool;
            this.updateOptions();
        };
        this.setSelectedProduct = function(productCode, loanType) {
            if (productCode != null) _defaults.input.repaymentModel.product = productCode;
            if (loanType != null) _defaults.input.repaymentModel.productGroup = loanType;
            this.updateOptions();
            console.log('Product set: ' + productCode + '-' + loanType);
        };

        this.replaceProductRates = function() {
        
            /* Initialize widget if widget doesn't need latest product rates
             * Currently they are -
             *   wiwo-bugiyotsu : Budget Planner
             *   wiwo-daduyazo : Deposit Planner
             *   wiwo-bijipaha : Income Tax
             *   wiwo-hezape : Stamp Duty
             */
            if (_id === "wiwo-bugiyotsu" || _id === "wiwo-daduyazo" || _id === "wiwo-bijipaha" || _id === "wiwo-hezape") {
                this.init();
                return;
            }

            // Product rates API callback function
            const productRatesCallback = function(wwInstance) {

                return function(data, status) {

                    var productRatesList = data;

                    // Block widget loading if invalid data
                    if (!data && !productRatesList) {
                        console.log("Invalid product data.", data, productRatesList);
                        return;
                    }

                    // Push new products list into widget
                    _wiwo.push(['setBucket', _id, { productRates: productRatesList }]);

                    // Initialize the widget immediately after this
                    wwInstance.init();
                }
            };

            // Load products rates for this widget
            $.get('/api/widget/' + _id, null, productRatesCallback(this));
