    /* WidgetWorks wrapper "class". Requires valid iframe id starting with "wiwo-" */
    function WidgetWorksEmbed(widgetWorksId, loanType, productCode, teaserType, showOptions, redirectUrl) {

        // Private variables
        var _id = widgetWorksId;
        var _domElem = document.getElementById(widgetWorksId);
        var _defaults = {
            "id": _id,
            "version": 1,
            "input": {
                "rpyControlViewModel": {
