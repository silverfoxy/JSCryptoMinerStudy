			function gtmTriggerEvent(category, action, label) {
				dataLayer.push({ 'event': 'GenericEvent', 'eventCategory': category, 'eventAction': action, 'eventLabel': label });
			}

			$("#SearchButton").click(function () {
				var path = "/Find-A-Doctor/Provider-Listing";

				var searchFilter = fixedEncodeURIComponent($("#SearchText").val());

				if (searchFilter === '') {
					window.location.href = path;
					return;
				}

				window.location.href = path + "#?SearchText=" + searchFilter;
			});

			$('a:not([href^="mailto:"]):not([href^="tel:"]):not([href^="javascript:"])').each(function () {
				var a = new RegExp('/' + window.location.host + '/');
				if (!a.test(this.href)) {
					$(this).attr("target", "_blank");
				}
			});

			var anchorTarget = window.location.hash;
			window.location.hash = '';

			$(window).load(function () {
				if (typeof anchorTarget !== "undefined") {
					if (anchorTarget) {
						$('html, body').animate({
