	<script type="text/javascript">
		function UpgradeASPNETValidation() {
			if (typeof (Page_ClientValidate) != "undefined") {
				AspValidatorUpdateDisplay = ValidatorUpdateDisplay;
				ValidatorUpdateDisplay = NicerValidatorUpdateDisplay;
			}
		}

		function NicerValidatorUpdateDisplay(val) {
			var ctrl = document.getElementById(val.controltovalidate);
			if (ctrl == null || ctrl.style == null) return;

			if (!val.isvalid) {
				ctrl.style.background = '#FFAAAA';
				ctrl.focus();
			}
			else
				ctrl.style.backgroundColor = '';
		}

		$(function () {

			if (localStorage.getItem("EmergencyClose")) {
				$(".emergencyNotification").removeClass("js-active");
			}

			areaFocusFix();
			bodyOffset();
		});

		$(window).on("debouncedresize", function () {
			areaFocusFix();
			bodyOffset();
		});

		function pageLoad() {
			$(document).ready(UpgradeASPNETValidation);
			if ($(window).width() <= 1024) {
				subNavTap();
			}
			if (typeof contentPageLoad == 'function')
				contentPageLoad();
		}

		(function () {
			var linksList = [];

			$('#footerNavListColumn1').find('li').each(function () {
				linksList.push($(this).html());
			});

			var halfCount = Math.round(linksList.length / 2);
			var firstList = linksList.splice(0, linksList.length % 2 !== 0 ? halfCount - 1 : halfCount),
				secondList = linksList;

			function createHTML(list) {
				generatedHTML = '';
				for (var i = 0; i < list.length; i++) {
					generatedHTML += '<li>' + list[i] + '</li>';
				};
				return generatedHTML;
			}

			$('#footerNavListColumn1').html(createHTML(firstList));
			$('#footerNavListColumn2').html(createHTML(secondList));
		})();

		function areaFocusFix() {
			var maxHeight = 0;

			$(".HeightFix").each(function () {
				if ($(this).children().height() > maxHeight) { maxHeight = $(this).children().height(); }
			});

			$(".HeightFix").height(maxHeight);
		}

		$('.SmoothScroll').click(function (e) {
			$('html, body').animate({
				scrollTop: $($.attr(this, 'href')).offset().top
			}, 500);
			e.preventDefault();
		});

		function fixedEncodeURIComponent(str) {
			return encodeURIComponent(str).replace(/[!'()*]/g, function (c) {
				return '%' + c.charCodeAt(0).toString(16);
			});
		}

		function searchSite(e) {
			if (e && e.keyCode !== 13) {
				return;
			}

			var path = "/Search";
			var searchFilter = fixedEncodeURIComponent($("#SearchText").val());

			if (searchFilter === '') {
				window.location.href = path;
				return;
			}

			window.location.href = path + "?searchText=" + searchFilter;
		}

		function searchProviders(e) {
			if (e && e.keyCode !== 13) {
				return;
			}

			window.location.href = "/Find-A-Doctor/Provider-Listing?SearchText=" + $("#ProviderSearchText").val();
		}

		function subNavTap() {
			$(".SubBarMenu a").click(function () {
				if (!$(this).parent(".SubBarMenu").hasClass("Expanded")) {
					event.preventDefault();
					$(this).parent(".SubBarMenu").addClass("Expanded");
				} else {
					$(this).parent().removeClass("Expanded");
					$(this).siblings().removeClass("Active");
					$(this).addClass("Active");
				}
			});
		}

		$(".js-toggle").click(function () {
			var elementClass = $(this).attr("data-toggle");
			$("." + elementClass).toggleClass("js-active");

			if (elementClass === "emergencyNotification") {
				bodyOffset();
				localStorage.setItem("EmergencyClose", true);
			}
		});

		// HEADER SCRIPTS

		//the global header is fixed position
		//this script changes the margin offset of the body, based on header height
		//it also changes the offset of the mainNavWrapper, based on header height
		function bodyOffset() {
			var headerHeight = $("header").innerHeight();
			$("body").css({ "margin-top": headerHeight });
			$(".mainNavWrapper").css({ "top": headerHeight });
		}

		//clicking/tapping "menuToggle" opens and closes menu
		$(".menuToggle").click(function () {
			$(this).toggleClass("js-open");
			$(".mainNavWrapper").toggleClass("js-open");
			$(".mainNav_dropdown").removeClass("js-open");
		});

		//once the menu is open, the first level of links have the class "mainNav_link-strong"
		//clicking/tapping these links opens the next level of navigation
		$(".mainNav_link-strong").click(function () {
			event.preventDefault();
			$(this).siblings().addClass("js-open");
		});

		//clicking/tapping "mainNav_back" goes back to the top next level of navigation
		$(".mainNav_back").click(function () {
			event.preventDefault();
			$(this).parent().removeClass("js-open");
		});

		//the Services & Treatments section of the nav has another "level" of navigation
		//clicking/tapping on "mainNav_dropdown_categoryLabel" opens the next level of navigation
		$(".mainNav_dropdown_categoryLabel").click(function () {
			$(this).parent().siblings().children(".mainNav_dropdown_categoryLabel").removeClass("js-open");
			$(this).toggleClass("js-open");
		});

		function ready(fn) {
			if (document.readyState !== 'loading') {
				fn();
			} else {
				document.addEventListener('DOMContentLoaded', fn);
			}
		}
	</script>
	


