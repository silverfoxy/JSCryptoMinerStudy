    jQuery.noConflict();
    
    jQuery(document).ready(function() {
        
        var buttonPanel         =   jQuery('#alogin');
        var quickPanel          =   jQuery('.quicklogin');
        var customerAccountLink =   jQuery('.customer-account a');
        var quickPanelInputs    =   jQuery('.quicklogin .input-text');
        var quickPanelMouseOver =   false; // fix pour autocomplete sur Safari 6 (MACOS)
        
        function initCustomerPanel() {
            
            buttonPanel.bind('mouseover', function() {
                quickPanel.show();
                customerAccountLink.addClass('active');
                quickPanelMouseOver = true;
            });
            
            buttonPanel.bind('mouseout', function() {
                    quickPanel.hide();
                    customerAccountLink.removeClass('active');
            });
            
            quickPanel.bind('mouseover', function() {
                    quickPanel.show();
                    customerAccountLink.addClass('active');
            });
            
            quickPanel.bind('mouseout', function() {
                    quickPanel.hide();
                    customerAccountLink.removeClass('active');
            });
            
        }
        
        initCustomerPanel();
        
        quickPanelInputs.bind('focus', function() {
            
            if (quickPanelMouseOver) {
                buttonPanel.unbind();
                quickPanel.unbind();
                quickPanel.show();
                customerAccountLink.addClass('active');
            }

        });
        
        quickPanelInputs.bind('blur', function(e) {
            
            //console.log('blur');
            var blur = true;
            
            quickPanel.bind('click', function() {
                
                initCustomerPanel();
                blur = false;
                
            });
            
            quickPanelInputs.bind('click', function() {
                
                blur = false;
   
            });
            
            quickPanelInputs.bind('focus', function() {
                
                blur = false;
   
            });
            
            setTimeout(function() {
                
                if (blur) {
                    
                    quickPanel.hide();
                    customerAccountLink.removeClass('active');
                    //quickPanelInputs.unbind('click');
                    quickPanel.unbind('click');
                    initCustomerPanel();
                }
                
            }, 300);
        });
    });
</script>

