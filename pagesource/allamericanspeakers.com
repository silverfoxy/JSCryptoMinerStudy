function chkvalid()
{
	form = document.frmContact
		
	if(form.first_name.value=="")
	{
		document.getElementById("divname").innerHTML = "Please Enter Your name.";
		form.first_name.focus();
		return false;
	}
	else
	{	document.getElementById("divname").innerHTML = "";	}
	
	if(form.email.value=="")
	{		
		document.getElementById("divemail").innerHTML = "Please Enter Your Email.";
		form.email.focus();
		return false;
	}
	if(!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(form.email.value))) 
	{		
		document.getElementById("divemail").innerHTML = "Please Enter Valid Email Address.";
		form.email.focus();
	    return false;
	}
	else
	{	document.getElementById("divemail").innerHTML = "";	
		getmail(document.getElementById('first_name').value, document.getElementById('email').value, document.getElementById('company').value, document.getElementById('phone').value, document.getElementById('Event_Budget').value, document.getElementById('Event_City').value, document.getElementById('Comments').value, document.getElementById('subject').value);
	}
}
function getmail(first_name,email,company,phone,Event_Budget,Event_City,Comments,subject) {
	
	$.ajax({
		type: "POST",
		url: "https://www.allamericanspeakers.com/float_box/contact_code.php",
		data:  "first_name=" + escape(first_name) + "&email=" + escape(email)  + "&company=" + escape(company)  + "&phone=" + escape(phone) + "&Event_Budget=" + escape(Event_Budget) + "&Event_City=" + escape(Event_City) + "&Comments=" + Comments + "&subject=" + escape(subject),
		success:function(text){
			//alert(text)
			window.location.href='https://www.allamericanspeakers.com/miscscripts/redirect.php?t=ftf';
		}
	});
}
</script>
