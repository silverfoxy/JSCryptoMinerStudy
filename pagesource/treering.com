		$('.navbar .dropdown').hoverIntent({
			over : navHoverOn,
			out : navHoverOff,
			timeout : navHoverDelay
		});

		$('.navbar .dropdown-toggle').on('click', function(e) {
			var dropdown = $(this).parent('.dropdown');

			if (dropdown.hasClass('click'))
				dropdown.removeClass('click').addClass('closing');
			else
				dropdown.addClass('click');
		});

	    $('.dropdown').on('show.bs.dropdown', function(e){
	    	$('.dropdown.open').not($(this)).each(function() {
	    		$(this).removeClass('click');
	    		$(this).children('.dropdown-toggle').dropdown('toggle');
	    	});

	    	$(this).find('.dropdown-menu').first().stop(true, true).slideDown(navTransition);
	    });

 		$('.dropdown').on('hide.bs.dropdown', function(e) {
 			if (! $(this).hasClass('click') )
 			{
	 			$(this).find('.dropdown-menu').first().stop(true, true).slideUp(navTransition, function() {
					$(this).siblings('.dropdown-toggle').blur();
					$('.dropdown').removeClass('noIntent');
				});
			}
			else
			{
				e.preventDefault();
			}
  		});

  		$('html').click(function() {
  			$('.dropdown.open').removeClass('click').children('.dropdown-toggle').dropdown('toggle');
  		});

  				    $.ajax({
		        url: document.location.protocol + '//munchkin.marketo.net/munchkin.js',
		        dataType: 'script',
		        cache: true,
		        success: function() {
		          Munchkin.init('810-YFS-978');
		          // Register the optimizely integration
				  //window.integrator.registerIntegration(window.integration);
		        }
		      });
			});	

	function navHoverOn() {
		$('.dropdown').not($(this)).addClass('noIntent');
		$(this).find('.dropdown-toggle').first().dropdown('toggle');
	}

	function navHoverOff() {
		if (! $(this).hasClass('noIntent') && ! $(this).hasClass('closing'))
			$(this).find('.dropdown-toggle').first().dropdown('toggle');
		else
			$(this).removeClass('closing');
	}

