<script>
(function(window, document) {
    if(is_delivery()) {
        document.write('<script type="text/javascript" src="http://js.gsspcln.jp/t/096/379/a1096379.js"></sc' + 'ript>');
    }
    function is_delivery() {
        var gn_option = window.__gn_interstitial_param;
        var rate, path, fq;
        if(typeof gn_option === "undefined") return true;
        return rate_limitation(gn_option["rate"]) && path_limitation(gn_option["path"]) && fq_limitation(gn_option["fq"]);
    }
    function rate_limitation(param) {
        if(typeof param === "undefined") return true;
        var r = Math.floor(Math.random() * 100) + 1;
        var delivery_prob = param["probability"] || 100;
        if(r < delivery_prob) return true;
        return false
    }
    function path_limitation(param) {
        if(typeof param === "undefined") return true;
        var path_list = param["path_list"].split(",");
        var l = path_list.length;
        var i;
        if(l === 0) return true;
        for(i = 0; i < l; i++) {
            if(location.pathname === path_list[i]) return false;
        }
        return true;
    }
    function fq_limitation(param) {
        if(typeof param === "undefined") return true;
        var fq_limit = param["cnt"];
        var fq_expr = param["expire"];
        var ckname = "gninstifq";
        var current_fq_cnt = (function() {
            var cs = document.cookie;
            var cl = cs.split(";");
            var cll = cl.length;
            var i;
            var fqck = "";
            for(i = 0; i < cll; i++) {
                if(cl[i].indexOf(ckname) !== -1) {
                    fqck = cl[i].split("=");
                    return Number(fqck[1]);
                }
            }
            return 0;
        })();
      if(current_fq_cnt >= fq_limit) {
          return false;
      }else {
          update_fqcnt(current_fq_cnt, fq_expr, ckname);
          return true;
      }
    }
    function update_fqcnt(current_fq, exp_min, ckname) {
        var expire = new Date();
        expire.setTime(expire.getTime() + exp_min*60*1000);
        document.cookie = ckname + "=" + (++current_fq) + "; expires=" + expire.toGMTString() + ";";
    }
})(window, document);
</script>

