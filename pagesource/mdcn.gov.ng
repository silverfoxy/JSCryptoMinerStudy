   $(document).ready(function () {
      var cmenu_timeout = setInterval(function () {
         if ($("#primary-menu li").children().length === 0) {
            return;
         }

         $("#primary-menu li.sub-menu").each(function () {
            if ($(this).hasClass("mega-menu-title")) {
               return;
            }

            var links = $(this).find("a");
            var found = false;

            $(links).each(function () {
               if (window.location.href.indexOf($(this).attr("href")) > 0) {
                  found = true;
               }
            });

            if (found) {
               $("#primary-menu li").removeClass("current");
               $(this).addClass("current");
            }
         });

         clearTimeout(cmenu_timeout);
      }, 300);
   });
