         return false;
      } else
      if (
         $('#qremail').val() == "") {
         $('#msg2').html('Enter Reporters Email !');
         return false;
      } else
      if (
         $('#rmessage').val() == "") {
         $('#msg2').html('Enter Details of Complain !');
         return false;
      } else
      if (
         $('#rsname').val() == "") {
         $('#msg2').html('Enter Suspects Name !');
         return false;
      } else
      if (
         $('#rgender').val() == "") {
         $('#msg2').html('Enter Suspects Gender!');
         return false;
      } else
      if (
         $('#rsaddress').val() == "") {
         $('#msg2').html('Enter Suspects Address!');
         return false;
      } else
      if (
         $('#rstate').val() == ""
         ) {
         $('#msg2').html('Select Suspects State of Practice!');
         return false;
      } else
      if (
         $('#rlga').val() == "") {
         $('#msg2').html('Select Suspects Local Government Area of Practice');
         return false;
      } else
      if (
         $('#rdat').val() == "") {
         $('#msg2').html('Enter Date of Incident!');
         return false;
      } else
      if (
         $('#rstyp').val() == ""
         ) {
         $('#msg2').html('Select Suspect practice Type!');
         return false;
      }

      var rname = $('#qrname').val();
      var remail = $('#qremail').val();
      var rphone = $('#qrphone').val();
      var rmessage = $('#rmessage').val();
      var rsname = $('#rsname').val();
      var rsaddress = $('#rsaddress').val();
      var rgender = $('#rgender').val();
      var rstate = $('#rstate').val();
      var rlga = $('#rlga').val();
      var rdat = $('#rdat').val();
      var rstyp = $('#rstyp').val();

      $.ajax({
         type: 'POST',
         url: '.?s=reportquack',
         dataType: 'html',
         data: {rname: rname,
            remail: remail,
            rphone: rphone,
            rmessage: rmessage,
            rsname: rsname,
            rsaddress: rsaddress,
            rgender: rgender,
            rstate: rstate,
            rlga: rlga,
            rdat: rdat,
            rstyp: rstyp
         },
         success: function (data) {
            alert("Quack Doctor Form Submitted.");
            $('#msg2').hide();
            $(".display-default").hide();
            $('#quackformMsg').show();
         }
      });
      return false;
   });
</script>



