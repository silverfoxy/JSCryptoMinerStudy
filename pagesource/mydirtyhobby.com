        /**
         *
         * @param timeLeft
         */
        function updateTimer(timeLeft, template) {

            if (typeof timeLeft == 'undefined') {
                return false;
            }

            template = template.replace(/%DAYS%/, addZero(timeLeft.totalDays));
            template = template.replace(/%HOURS%/, addZero(timeLeft.hours));
            template = template.replace(/%MINUTES%/, addZero(timeLeft.minutes));
            template = template.replace(/%SECONDS%/, addZero(timeLeft.seconds));

            _self.html(template);
        }
