<script type="text/javascript">

function hiliteSlide( sender, overslide )
{  
	//-- get inner wrapper div as dom element 
	//-- ( ids not useable in this context - jQuery duplicates slides for carousel )
   var slide = sender;
   
   //-- get array of span elements within dom element
   var spans = slide.getElementsByTagName('span'); 
   
   
   if ( overslide )		//-- mouse is over slide
   { //-- set background and cursor of current dom element
     slide.style.backgroundColor = '#E3E3E3';
     slide.style.cursor = 'pointer'; 

	 //-- add underline if either of first two lines [spans] are classed as headline
	 if ( spans[0].className == 'carouselitem_txtHeadline' )
		 { spans[0].style.textDecoration = 'underline'; }
	 if ( spans[1].className == 'carouselitem_txtHeadline' )
		 { spans[1].style.textDecoration = 'underline'; }

   }
   else			//-- mouse has moved off slide
   { //-- restore background and cursor of current dom element
     slide.style.backgroundColor = 'transparent';
     slide.style.cursor = 'default';
     
     //-- remove underline if either of first two lines [spans] are classed as headline
	 if ( spans[0].className == 'carouselitem_txtHeadline' )
		{ spans[0].style.textDecoration = ''; }
	 if ( spans[1].className == 'carouselitem_txtHeadline' )
		{ spans[1].style.textDecoration = ''; }

   }

}


function slideClick( url, isNews )
{
  if ( url == '#' )
  { return false; }
  else
  { ( isNews )? window.location = url : window.open( url );  }
  
  return false;
  
}



