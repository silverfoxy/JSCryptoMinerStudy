<script type="text/javascript" src="http://silveryasha.web.id/wp-content/themes/syfansub/js/navigation.js?ver=manual"></script>
<script type="text/javascript" src="http://silveryasha.web.id/wp-content/themes/syfansub/js/skip-link-focus-fix.js?ver=manual"></script>

<script>

	// Cek lebar browser

	function getScrollBarWidth(){

		if($(document).height() > $(window).height()){

			$('body').append('<div id="fakescrollbar" style="width:50px;height:50px;overflow:hidden;position:absolute;top:-200px;left:-200px;"></div>');

			fakeScrollBar = $('#fakescrollbar');

			fakeScrollBar.append('<div style="height:100px;">&nbsp;</div>');

			var w1 = fakeScrollBar.find('div').innerWidth();

			fakeScrollBar.css('overflow-y', 'scroll');

			var w2 = $('#fakescrollbar').find('div').html('html is required to init new width.').innerWidth();

			fakeScrollBar.remove();

			return (w1-w2);

		}

		return 0;

	}

	

	function browserWidth() {

		var globalWidth = getScrollBarWidth() + $(window).outerWidth();

		return globalWidth;

	}



	function infoFixed() {

		if($(this).scrollTop() > ($('.section-header').height() + $('#kepala').height()) ) { // --> tinggal logic IF di sini 

			$('.infoanimanga').addClass('fixedDiv');

			$('.infoanimanga').width($('.manga-info').width());

		}

		else {

			$('.infoanimanga').removeClass('fixedDiv');

			$('.infoanimanga').width("100%");

		}



		// - tinggi header

		// - tinggi content di atas manga-info

		// - tinggi manga-info

		// - tinggi layar/browser

		// - beserta padding/margin

	}



	$(function(){

		var nav = $('#site-navigation');



		function autoHeader() {

			if(browserWidth() < 640){

				$('.site-branding').height( ($('.site-branding').width()/1100) * 307);

			}

			else{

				$(".site-branding").css("height", "");

			}

		}		



		function autoNav(){



			if(browserWidth()>640) {				

				$(".silveryasha").css("margin-top", 0);



				if ($(this).scrollTop() > $('.site-branding').outerHeight()-30) {

					nav.addClass("f-nav");

					$("#site-navigation").css("width", $('.site-branding').width()+"px");

				} else {

					nav.removeClass("f-nav");

					$("#site-navigation").removeAttr("style");

				}

			}

			else{

				// nav.removeClass("f-nav");

				// $("#site-navigation").removeAttr("style");

				nav.addClass("f-nav");

				$("#site-navigation").css("width", $('.site-branding').width()+"px");

				$(".silveryasha").css("margin-top", $('#site-navigation').outerHeight()+"px");

			}	

		}



		autoNav();

		autoHeader();

		// infoFixed();



		$(window).scroll(function () {

			autoNav();

			infoFixed()

		});



		$(window).resize(function() {

		 	autoNav();

		 	autoHeader();

		 	infoFixed()

		});







	});

</script>



