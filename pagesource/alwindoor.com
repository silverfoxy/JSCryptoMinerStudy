			if( t ){
				if( t.contentDocument && t.contentDocument.body.offsetHeight ){
					t.height = t.contentDocument.body.offsetHeight;
				}else if( t.Document && t.Document.body.scrollHeight ){
					t.height = t.Document.body.scrollHeight;
				}
			}
		};
		;jQuery(function($){
			$('#J_ccwindexNav').contents().find('.win8Nav').click(function(){
				var t = this,
					$t = $(t);
				art.dialog({
					id: 'mypj',
					title: $t.attr('data-title'),
					content: '<iframe height="515" width="800" src="users/'+ $t.attr('data-url') +'/" onload="setAutoHeight(this)" marginwidth="0" marginheight="0" scrolling="no" align="center" border="0" frameborder="0"></iframe>',
					lock: 1,
					fixed: 1,
					padding: '0'
				});
			})
		});
		
		
		$(document).ready(function () {
    $('.CentreBox .Menu .List1 li').mouseenter(function () {
        var index = $(this).parent().children().index(this);
        $(this).parent().children().each(function () {
            if ($(this).hasClass('Select1')) {
                $(this).removeClass('Select1');
            }
        });
        $(this).addClass('Select1');

        $('.CentreBox .Menu').each(function () {
            if (!$(this).hasClass('Hide')) {
                $(this).addClass('Hide');
            }
        });
        $('.CentreBox .Menu').eq(index).removeClass('Hide');
    });

    $('.CentreBox .Menu ul li a').mouseenter(function () {
        var index = $('.CentreBox .Menu ul li a').index(this);
        $('.CentreBox .Menu ul li').each(function () {
            if ($(this).hasClass('Select1')) {
                $(this).removeClass('Select1');
            }
        });
        $(this).parent().addClass('Select1');
    });
});


