            // This should be removed after the EM
            // remove start
            var clickHandler, groups, parent, updateLinks;
            groups = container.find('.s1-table.s1-content-standings > div:not([class])');

            if (groups && groups.length > 1) {
                updateLinks = function(nextElement) {
                    if (nextElement.next().length) {
                        $('.s1-next', $div).css('display', 'inline-block');
                    } else {
                        $('.s1-next', $div).css('display', 'none');
                    }
                    if (nextElement.prev().length) {
                        return $('.s1-prev', $div).css('display', 'inline-block');
                    } else {
                        return $('.s1-prev', $div).css('display', 'none');
                    }
                };
                clickHandler = function(next) {
                    var clickedElement, nextElement;
                    window.ivwPI();
                    clickedElement = container.find('.s1-table.s1-content-standings > div[style]:visible');
                    nextElement = next ? clickedElement.next() : clickedElement.prev();
                    nextElement.css('display', 'inline-block');
                    clickedElement.css('display', 'none');
                    return updateLinks(nextElement);
                };
                groups.each(function(index, element) {
                    var header, newHeader;
                    element = angular.element(element);
                    header = element.find('.s1-table-headline');
                    newHeader = angular.element('<div class="s1-table-headline s1-table-headline-group"><span class="s1-prev"></span>' + header.html() + '<span class="s1-next"></span></div>');
                    return element.find('.s1-table-headline').replaceWith(newHeader);
                });
                $($div).delegate('.s1-next', 'click', function() {
                    return clickHandler(true);
                });
                $($div).delegate('.s1-prev', 'click', function() {
                    return clickHandler(false);
                });
                groups.css('display', 'none');
                parent = $('[s1-full-name="Deutschland"]', $div).closest('div[style]');
                parent = parent.length ? parent : groups.first();
                parent.css('display', 'inline-block');
                updateLinks(parent);
            }
            // remove finished
        };

