    <script>
      function setIdAndLoadAds(divClass, adIds, passbackIds){
        jQuery(divClass).each(function (index, value){
          if(adIds.length > 0){
            var adId = adIds.pop();
            if(adId) {
              jQuery(this).attr('id',adId);
              Criteo.events.push(function() {
                Criteo.Passback.RenderAd(adId,function(adunit){
                  gweini.adStack.push({
                      placement_id: adunit,
                      zone_id: passbackIds.pop()
                  });
                  if(typeof gweini.writeAds !== "undefined"){
                      gweini.writeAds();
                  }
                });
              });
            }
          }
        });
      }
      setIdAndLoadAds('.big-ad',bigAdIds, bigAdPassbacks);
      setIdAndLoadAds('.small-ad',smallAdIds, smallAdPassbacks);
      setIdAndLoadAds('.ad-1',smallAdIds, smallAdPassbacks);
      setIdAndLoadAds('.ad-2',smallAdIds, smallAdPassbacks);
    </script>
