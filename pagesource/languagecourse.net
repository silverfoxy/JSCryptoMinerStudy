// open contact box (uso del .stop para mejorar respuesta del click parando cualquier animacin en funcionamiento => http://stackoverflow.com/questions/14945633/slow-unresponsive-animation-with-jquery-animation#answer-14945906 )
function openContactBox() {
	// Don't open contact box if email is not allowed (C country)
	var email_enabled = document.getElementById('cb_email_enabled').value;
	if (email_enabled == '0')
		return;
	jQuery('#submit_button').removeAttr('disabled');
	contactBoxStatuses.locked = true;
	var windowsize = window.innerWidth;
	if (windowsize > 1000) {
		jQuery('#contactBoxWrapper').stop().animate({right: 0,});
	} else if (windowsize <= 1000)  {
		contactBoxStatuses.locked = false;
		jQuery(".overlayDimmer").css('display', 'block').stop().animate({
			opacity: 0.6,
		}, 100, function() {
			// callback triggered on overlay layer opacity animation end
			jQuery('#TypingBoxDeployedWrapper').css('display', 'inline-block').stop().animate({top: 0, opacity: 1,}, 200);
		});
		// disable scrolling when modal is open
		if ( /chrom(e|ium)/.test(navigator.userAgent.toLowerCase()) ) {
			// if its Chrome browser apply the following
			jQuery('body').css({'overflow' : 'hidden'});
			// re-calculate cBar elements widths due to disappearing of scrollbar
			//calculateWidths();
		} else {
			jQuery(document.body).bind('touchmove', function(event) {
				event.preventDefault();
				event.stopPropagation();
			});
		}
		if (jQuery('.modalBack').css('top') != '0px') {
			// when user open cBox on mobile add the '#cBoxModal' parameter to the URL
			history.pushState({ par : "cBoxModal" }, null, '#cBoxModal');
		}
	}
}

// click to open contact box
jQuery('#contactBoxTextareaDiv, #contactBoxChat, #cBoxVertTab').click(function() {
	if (!chat_loaded)
		showLiveHelperChat();
		openContactBox();
		// tambin cierra cualquier popover (si estuviera abierto) 
		jQuery('.popover').popover('hide');
		// tb se esconde la textarea...
		jQuery('#contactBoxTextareaDiv').css('display', 'none');
});
/*** END => OPEN CONTACT BOX ***/

/*** CLOSE CONTACT BOX ***/
// click to close contact box
jQuery('.closeContactBox').click(function() {
	var windowsize = window.innerWidth;
	if (windowsize > 1000) {
		smartCloseContactBox(true);
	} else if (windowsize <= 1000) {
		// 2 browser history steps behind if the calcModal also remained opened below cBox
		if (jQuery('#calcModal').length !== 0 && jQuery('#calcModal').hasClass('in')) {
			history.back();
		} else {
			closeContactBox();
			history.back();
		}
	}
});

// close contact box until initial position
function closeContactBox() {
	contactBoxStatuses.initialTabPosition = true;
	var windowsize = window.innerWidth;
	if (windowsize > 1000) {
		jQuery('#contactBoxWrapper').stop().animate({
			right: -252,
		}, function() {
			// callback triggered on animation end
			jQuery('#contactBoxTextareaDiv.show_row').css('display', 'inline-block');
		});
	} else if (windowsize <= 1000) {
		closeContactBoxMobile()
	}
};

function closeContactBoxMobile(on_done) {	
	jQuery('#TypingBoxDeployedWrapper').stop().animate({
		top: -531, opacity: 0,
	}, 200, function() {
		// callback triggered on form modal box animation end
		jQuery('#TypingBoxDeployedWrapper').css('display', 'none');
		jQuery(".overlayDimmer").stop().animate({
			opacity: 0,
		}, 100, function() {
			// callback triggered on overlay opacity animation end
			jQuery('.overlayDimmer').css('display', 'none');
		});
		// when all above is executed execute the function of the parameter (see above when says: "if user opened mobile cBox, close it and go one step back in history browser")
		if (typeof on_done === "function") {
			on_done();
		}
	});
	// re-enable scrolling when modal is closed
	if ( /chrom(e|ium)/.test(navigator.userAgent.toLowerCase()) ) {
		// if its Chrome browser apply the following
		jQuery('body').css({'overflow-y' : 'auto'});
		// re-calculate cBar elements widths due to re-appearing of scrollbar
		//calculateWidths();
	} else {
		jQuery(document.body).unbind('touchmove');
	}
}

// close contact box until big tab
function closeContactBoxTab() {
	contactBoxStatuses.initialTabPosition = false;
	jQuery('#contactBoxTextareaDiv.show_row').css('display', 'inline-block');
	jQuery('#contactBoxWrapper').stop().animate({
		right: -334,
	});
};

// detect user scroll amount and trigger close contact box tab function
jQuery(window).scroll(function () {
