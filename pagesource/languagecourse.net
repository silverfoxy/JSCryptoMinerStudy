// to dinamically increase size of reserve button when scrolling until course, accommodation and FB share block
function smartIncreaseButtonSize() {
	var increaseSize = jQuery('#schoolCoursesBlock').isOnScreen(0, -1) || jQuery('#schoolAccommodationBlock').isOnScreen(0, -1) || jQuery('#ShareBlock').isOnScreen(0, -1);
	if (increaseSize === true) {
		jQuery('#table_quickprice_book').addClass('increaseSize');
	} else if(jQuery('#table_quickprice_book').hasClass('increaseSize') === true) {
		jQuery('#table_quickprice_book').removeClass('increaseSize');
	}
};

/*
 * Close the contact box to the desired position
 *
 * @param boolean force_close true if you want to animate the contact box closing, even if it's locked. False by default.
 */
function smartCloseContactBox(force_close) {
	// comprobar si el header y el footer estn visibles mediante el plugin llamado en el html_head: isOnScreen => https://github.com/moagrius/isOnScreen
	var headerVisible = jQuery('#bottomHead').isOnScreen(0, 0.2);
	var footerVisible;
	if (jQuery('div.applicantInfo').length) {
		footerVisible = jQuery('#insuranceInfo').isOnScreen(0, -1) || jQuery('#payment_section').isOnScreen(0, -1);
	}
	else if (jQuery('#photosVideoBlock').length) {
		footerVisible = jQuery('#photosVideoBlock').isOnScreen(0, -1) || jQuery('#preFooter').isOnScreen(0, -1);
	}
	else if (jQuery('#key_events').length) {
		footerVisible = jQuery('#key_events').isOnScreen(0, -1) || jQuery('#bigmap_background').isOnScreen(0, -1) || jQuery('#preFooter').isOnScreen(0, -1);
	}
	else {
		footerVisible = jQuery('#preFooter').isOnScreen(0, -1);
	}
	// setting flag 'force_close'
	force_close = typeof force_close === "undefined"? false : force_close;
	if (force_close) {
		contactBoxStatuses.locked = false;
	}
	// cuando cBox est cerrada y se escrollea hacia abajo
	if (!contactBoxStatuses.locked) {
		if (!headerVisible && !footerVisible) {
			if (force_close || contactBoxStatuses.initialTabPosition) {
				closeContactBoxTab();
			}
	// cuando cBox est cerrada y se escrollea hacia arriba
		} else {
			if (force_close || !contactBoxStatuses.initialTabPosition) {
				closeContactBox();
			}
		}
	}
};
/*** END => CLOSE CONTACT BOX ***/
	
	/* extra click on any html's part to close contact box (excluding elements inside the 'contactBoxWrapper' itself -using .closest-) => http://stackoverflow.com/questions/2321836/jquery-selector-with-exlude-of-element-elements-children#answer-2321912
	jQuery('html').click(function(e) {
		if (userScroll() >= 380) {
			if (!jQuery(e.target).closest('#contactBoxWrapper').length) {
				closeContactBox();
			}
		}
	});*/

