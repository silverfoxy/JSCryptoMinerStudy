                    if (window.innerWidth > 1000)
                        document.getElementById("contactBoxWrapper").style.display = 'block';
                    else
                        initCboxOnResize();
                    //document.getElementById("contactBoxWrapper").style.boxSizing = 'content-box';
                }
            else
                document.getElementById("contactBoxNScriptsLoaded").value = nScripts;
        }


