jQuery(window).on('popstate', function() {
	// condition given by added class 'modalback' because both cBoxes (desktop/mobile) have the same ID (+ opacity of cBox to know if is visible or not)
	if (jQuery('#TypingBoxDeployedWrapper').hasClass('modalBack') && jQuery('.modalBack').css('opacity') == 1) {
