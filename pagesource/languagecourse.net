			// add class 'modalBack' to cBox
			if (!jQuery('#TypingBoxDeployedWrapper').hasClass('modalBack')) {
				jQuery('#TypingBoxDeployedWrapper').addClass('modalBack');
			}
		}
	}
	previousSize = currentSize;
}

function calculateWidths() {
	// width values of each element in the bar
	// +21 (the width of the 3 hor. line icon + 6px of margin right)
	if (document.getElementById("contactBoxNScriptsLoaded").value < 3)
		return; // Contact Box is not complete
	var contactBoxPhonesMobText = jQuery('.contactBoxPhonesMobText').outerWidth() + 21;
	var contactBoxPhonesMob = jQuery('#contactBoxPhoneMobNum').outerWidth();
	/*de momento no usaremos skype en mvil 
	function contactBoxSkype() {
		if (jQuery('#contactBoxSkype').css('display') == 'none') {
			return 0;
		} else {
			return jQuery('#contactBoxSkype').outerWidth();
		}
	}*/
	function contactBoxTextareaDiv() {
		if (jQuery('#contactBoxTextareaDiv').css('display') == 'none') {
			return 0;
		} else {
			return jQuery('#contactBoxTextareaDiv').outerWidth();
		}
	}
	function contactBoxChatWidth() {
		if (jQuery('#contactBoxChat').css('display') == 'none') {
			jQuery('#contactBoxChat').css('padding', '0');
			return 0;
		} else {
			jQuery('#contactBoxChat').css('padding', '0 9px');
			return jQuery('#contactBoxChat').outerWidth();
		}
	}
	
	function searchFormSmall() {
		return jQuery('#searchFormSmallWrap').outerWidth();
	}
	
	// width of the contact bar row
	var totalRowWidth = jQuery('#cBoxTabWrap').outerWidth();
	
	// we sum the separated elements width + 7 pixels of the 'contactBoxPhonesMob' left padding
	var totalCalcWidth = contactBoxPhonesMobText + contactBoxPhonesMob /*+ contactBoxSkype()*/ + contactBoxTextareaDiv() + contactBoxChatWidth() + searchFormSmall() + 7

	// final calculation to hide/show the text beside the phone number
	if ( totalCalcWidth > totalRowWidth ) {
		jQuery('.contactBoxPhonesMobText').css('display', 'none');
	} else {
		jQuery('.contactBoxPhonesMobText').css('display', 'inline-block');
	}
	
	// calculate 3 variables to apply to the 3 conditionals below in order to always have 'contactBoxPhonesMob' div fitting the available width of the contact bar row
	var noButtonsWidth = totalRowWidth - contactBoxChatWidth() - 52;
	var noContactBoxWidth = totalRowWidth - 96;
	var allButtonsWidth = totalRowWidth - contactBoxChatWidth() - 96;
	
	if (jQuery('#contactBoxSkype').css('display') == 'none' || jQuery('#contactBoxTextareaDiv').css('display') == 'none') {
		jQuery('#contactBoxPhonesMob').css('width', noButtonsWidth);
	} else if (jQuery('#contactBoxChat').css('display') == 'none') {
		jQuery('#contactBoxPhonesMob').css('width', noContactBoxWidth);
	} else {
		jQuery('#contactBoxPhonesMob').css('width', allButtonsWidth);
	}
}

