<script>
	// we establish init status to 'normal' (normal top header height size of 50px) if there's no scroll at the beginning, otherwise we will trigger shrinking function 'headResizeToShrinked();'
	jQuery(window).on('load', function() {
		var windowsize = window.innerWidth;
		if (windowsize > 1000) {
			if(jQuery(document).scrollTop() == 0) {
		    	jQuery('#main').data('size','normal');
			} else if (jQuery(document).scrollTop() > 0) {
				headResizeToShrinked();
			}
		}
	});

	// stop all animations below on resize
	jQuery(window).on('resize', stopHeaderShrink);

	function stopHeaderShrink() {
		var windowsize = window.innerWidth;
		if (windowsize > 1000) {
			jQuery('#main').find('*').stop(true, true);
			if (jQuery('#main').offset().top == 0) {
				jQuery('#main').data('size','normal');
				headResizeToNormal();
			} else {
				jQuery('#main').data('size','shrinked');
				headResizeToShrinked();
			}
		} else {
			jQuery('#main').data('size','normal');
			headResizeToNormal();
		}
	}

	function headResizeToShrinked() {
    	// change data status to 'shrinked'
        jQuery('#main').data('size','shrinked');
        // stop the currently-running animation and start animation again to 'shrinked' status
        jQuery('.vertLineRightHeader').stop().animate({
            bottom:'0px'
        }, 100);
        jQuery('.vertLineRightHeader span').stop().animate({
            height:'17px'
        }, 100);
        jQuery('.headSlogan p').stop().animate({
            fontSize:'0'
        }, 100);
        jQuery('#headerHelp a, #headerHelpMob a').stop().animate({
            fontSize:'12px', bottom: '1px'
        }, 100);
        jQuery('#main').stop().animate({
            height:'37px'
        }, 100);
        jQuery('#head_title a span').stop().animate({
            fontSize:'0'
        }, 100);	            
        jQuery('#head_title img').stop().animate({
            height:'23px'
        }, 100);
        jQuery('#searchForm_small').stop().animate({
            marginTop:'12px'
        }, 100);
       	jQuery('#main').addClass('topAffixShadow');
	}

	function headResizeToNormal() {
    	// change data status to 'normal'
        jQuery('#main').data('size','normal');
        // stop the currently-running animation and start animation again to 'normal' status
        jQuery('.vertLineRightHeader').stop().animate({
            bottom:'3px'
        }, 100);
        jQuery('.vertLineRightHeader span').stop().animate({
            height:'24px'
        }, 100);
        jQuery('.headSlogan p').stop().animate({
            fontSize:'12px'
        }, 100);
        jQuery('#headerHelp a, #headerHelpMob a').stop().animate({
            fontSize:'13px'
        }, 100);
        jQuery('#main').stop().animate({
            height:'50px'
        }, 100);
        jQuery('#head_title a span').stop().animate({
            fontSize:'11px'
        }, 100);	            
        jQuery('#head_title img').stop().animate({
            height:'24px'
        }, 100);
        jQuery('#searchForm_small').stop().animate({
            marginTop:'19px'
        }, 100);
       	jQuery('#main').removeClass('topAffixShadow');
    }

	// function to execute on scroll event (see above)
	function headerShrink() {
		// To animate top header on scrolling: https://stackoverflow.com/questions/16442016/jquery-sticky-header-that-shrinks-when-scrolling-down
		// on scroll
	    if(jQuery(document).scrollTop() > 0) {
	        if(jQuery('#main').data('size') == 'normal') {
				headResizeToShrinked();
	        }
	    } else {
	    	// init position (no scroll)
	        if(jQuery('#main').data('size') == 'shrinked') {
				headResizeToNormal();
	        }  
	    }
	}

	jQuery(window).scroll(function () {
		var windowsize = window.innerWidth;
		if (windowsize > 1000) {
			// execute the animated shrinking top header in desktop view (see script at the bottom of this file)
			headerShrink();
		}
	});
</script>
