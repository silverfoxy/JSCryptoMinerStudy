	// check if 'calcModal' is also opened below so if we are in the 'calcModal' state (checking object { par : "calcModal" )} (see the definition school overview) and if current history.state is null (because when 'history.back();' is executed the on.popstate events are also executed again and can give a "null"
	if (history.state != null && history.state.par == "calcModal") {
		history.back();
	}
});

function initCbox() {
	var windowsize = window.innerWidth;
	if (windowsize > 1000) {
		jQuery('#contactBoxSkype').css('display', 'inherit');
		//jQuery('.overlayDimmer, #contactBoxPhonesMob').css('display', 'none');
		jQuery('#TypingBoxDeployedWrapper').css('opacity', '1');
		jQuery('#TypingBoxDeployedWrapper').css('display', 'inline-block');
		jQuery('#content_list, body').css('padding-top', 'inherit');
		// re-enable scrolling when modal is closed
		if ( /chrom(e|ium)/.test(navigator.userAgent.toLowerCase()) ) {
			// if its Chrome browser apply the following
			jQuery('body').css({'overflow-y' : 'auto'});
		} else {
			jQuery(document.body).unbind('touchmove');
		}
		// re-enable zooming
		jQuery(document.body).unbind('gesturestart');
	} else if (windowsize <= 1000) {
		jQuery('#contactBoxSkype').css('display', 'none'); // de momento no usaremos skype en mvil
		jQuery('#contactBoxPhonesMob').css('display', 'inline-block');
		jQuery('#TypingBoxDeployedWrapper').css('display', 'none');
		jQuery('.overlayDimmer').css({'opacity' : '0', 'top' : '-531'});
		// execute function to calculate width of elements and act in consequence
		calculateWidths();
		
		// issue with Android native browsers: search button misplaced, so remove it completely if search icon distance to top is considerably high
		if (jQuery('.glyphicon-search').offset().top > 20) {
			jQuery('#searchFormSmallWrap').css('display', 'none');
		}
		/* add paddings to floating elements to separate them from other elements
		var paddingNeeded = jQuery('#cBoxTabWrap').outerHeight();
		jQuery('#content_list, body').css('padding-top', paddingNeeded);*/
		// disable zooming on mobile
		jQuery(document.body).bind('gesturestart', function(event) {
			event.preventDefault();
			event.stopPropagation();
		});
		// add class 'modalBack' to cBox
		jQuery('#TypingBoxDeployedWrapper').addClass('modalBack');
	}
}

function initCboxOnResize() {
	var currentSize = getCurrentSize();
	if(currentSize.width != previousSize.width) {
		if (currentSize.width > 1000) {
			// if user opened mobile cBox, close it and go one step back in history browser
			if (jQuery('.overlayDimmer').css('display') == 'block') {
				// see below 'closeContactBoxMobile(on_done)'. The function is passed as the parameter 'on_done'
				closeContactBoxMobile(function() {
					jQuery('#TypingBoxDeployedWrapper').css({'opacity' : '1', 'display' : 'inline-block'});
				});
				// history back after closing mobile cBox
				history.back();
			}
			
			jQuery('#contactBoxSkype').css('display', 'inherit');

			jQuery('#contactBoxWrapper').removeClass('top30');
			if ( jQuery('#lang_suggest').hasClass("height30") ) {
				jQuery('#contactBoxWrapper').addClass('contactTop165');
			}

			jQuery('.overlayDimmer, #contactBoxPhonesMob').css('display', 'none');
			jQuery('#TypingBoxDeployedWrapper').css('opacity', '1');
			jQuery('#TypingBoxDeployedWrapper').css('display', 'inline-block');
			// Only enable email div if email is available for the country
			jQuery('#contactBoxTextareaDiv').css('display', jQuery("#cb_email_enabled").val()=='1' ? 'inline-block' : 'none');
			jQuery('#contact_floating_form').css('display', 'table');
			jQuery('#content_list, body').css('padding-top', 'inherit');
			jQuery('.overlayDimmer').css({'opacity' : '0', 'top' : '-531'});	
			jQuery('#contactBoxChat').css('padding', '0');
			// re-enable scrolling when modal is closed
			if ( /chrom(e|ium)/.test(navigator.userAgent.toLowerCase()) ) {
				// if its Chrome browser apply the following
				jQuery('body').css({'overflow-y' : 'auto'});
			} else {
				jQuery(document.body).unbind('touchmove');
			}
			// re-enable zooming
			jQuery(document.body).unbind('gesturestart');
			// re-enable searchbar if that was hidden
			if (jQuery('#searchFormSmallWrap').css('display') == 'none') {
				jQuery('#searchFormSmallWrap').css('display', '');
			}
			// remove class 'modalBack' from cBox
			if (jQuery('#TypingBoxDeployedWrapper').hasClass('modalBack')) {
				jQuery('#TypingBoxDeployedWrapper').removeClass('modalBack');
			}
		} else if (currentSize.width <= 1000) {
			jQuery('#contactBoxSkype').css('display', 'none'); // de momento no usaremos skype en mvil
			//jQuery('#contact_floating_form, #TypingBoxDeployedWrapper').css('display', 'none');

