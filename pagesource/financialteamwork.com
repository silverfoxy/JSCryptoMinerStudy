	}
	.breadcrumb a:hover{

		color: #fb9e08;
	}


	/*  single-blog.php ------------------------ */
	.post-title {
		color: #161616;
		width: 100%;
		padding: 0 0 10px 0;
		position: relative;
		text-shadow: none;
	}
