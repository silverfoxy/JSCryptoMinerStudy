	$(function(){
		//Keep track of last scroll
		var lastScroll = 0;
		$(window).scroll(function(event) {
			var st = $(this).scrollTop();
			if (document.location.pathname != "/" && document.location.pathname != "/index" && $(".l2Nav").length) {
				$(".l1Nav").addClass('sticky');
				//Safari does a weird bounce at bottom and top of scroll, so:
				//Only hide l1Nav if we have scrolled down at least 60px
				//Only show l1Nav on scroll up if at least 60px are still hidden at the bottom of the page
				if($(window).scrollTop() > 60 && (($(document).height() - $(window).height() - st) > 60) || ($(document).height() == $(window).height())) {
					if (st > (lastScroll)){
						//move l1Nav, l2Nav and l2NavSpacer up by 60px
						if (!$(".l1Nav").hasClass('navTransformUp')) $(".l1Nav").addClass('navTransformUp',600);
						if (!$(".l2Nav").hasClass('navTransformUp')) $(".l2Nav").addClass('navTransformUp',600);
						if (!$(".l2NavSpacer").hasClass('navTransformUp')) $(".l2NavSpacer").addClass('navTransformUp',600);
					} else {
						//move l1Nav, l2Nav and l2NavSpacer down by 60px
						if ($(".l1Nav").hasClass('navTransformUp')) $(".l1Nav").removeClass('navTransformUp',600);
						if ($(".l2Nav").hasClass('navTransformUp')) $(".l2Nav").removeClass('navTransformUp',600);
						if ($(".l2NavSpacer").hasClass('navTransformUp')) $(".l2NavSpacer").removeClass('navTransformUp',600);
					}
				}
			}
			if ($(".l3Nav").length) {
				var l3NavStartOffset = $(".l3NavStart").offset().top - lastScroll;
				var l3NavContentEndOffset = $(".l3NavContentEnd").offset().top;
				var l3NavMenuEndOffset = $(".l3NavMenuEnd").offset().top;
				if (l3NavStartOffset < 100) {
					if (!$(".l3Nav").hasClass('fixPosition')) {
						$(".l3Nav").addClass('fixPosition');
					}
				} else {
					if ($(".l3Nav").hasClass('fixPosition')) {
						$(".l3Nav").removeClass('fixPosition');
					}
				}
				if (l3NavMenuEndOffset > l3NavContentEndOffset) {
					$(".l3Nav").addClass('hidePosition');
				} else {
					$(".l3Nav").removeClass('hidePosition');
				}
			}
			lastScroll = st;
		});
	});
	$(function() {
		$('a[href*="#"]:not([href="#"])').click(function() {
			if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
				var target = $(this.hash);
				target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
				if (target.length) {
					$('html, body').animate({ scrollTop: (target.offset().top - 60) }, 1000);
					return false;
				}
			}
		});
	});
</script>
<script type="text/javascript">
	var homeLogoElement = $(".homeLogo").offset().top - 26;
