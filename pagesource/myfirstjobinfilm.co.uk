<script type="text/javascript">
	var cTypes = ["WS", "WAS", "CRS", "INS", "CSS", "TS", "PCS", "MRS", "BS", "BLS", "WBS", "IBS", "FFS", "SRS", "CPS", "COPS"]; 
	function carousel(cType, cDir) {
		if (jQuery.inArray( cType, cTypes ) > -1) {
			var cPos = parseInt($('#' + cType + 'Pos').val(), 10);
			var cEnd = parseInt($('#' + cType + 'End').val(), 10);
			var cLazy = parseInt($('#' + cType + 'Lazy').val(), 10);
			if (cDir == 'right' && cPos < cEnd) {
				if (cLazy == cPos + 3) {
					$.get('/ajax/getAnotherBox.php?type=' + cType + '&pos=' + cLazy, function(data) { $('#' + cType + 'Box').replaceWith(data); });
					cLazy = cLazy + 1;
					$('#' + cType + 'Lazy').val(cLazy);
				}
				cWidth = 240 * cPos;
				cPos = cPos + 1;
				$('#' + cType + 'Pos').val(cPos);
				$('.' + cType + ' .stdCarouselInner').animate({ 'right' : cWidth + 'px' }); 
			}
			if (cDir == 'left' && cPos > 1) {
				cPos = cPos - 1;
				cWidth = (240 * cPos) -240;
				$('#' + cType + 'Pos').val(cPos);
				$('.' + cType + ' .stdCarouselInner').animate({ 'right' : cWidth + 'px' }); 
			}
			if (cPos > 1 && cEnd > 4) $('.' + cType + ' .carouselLeft').show();
			if (cPos == 1) $('.' + cType + ' .carouselLeft').hide();
			if ((cPos + 3) == cEnd) $('.' + cType + ' .carouselRight').hide(); else $('.' + cType + ' .carouselRight').show();
		}
	}
