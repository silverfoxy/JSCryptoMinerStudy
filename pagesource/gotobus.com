            latlngbounds.extend(myLatlng); 
            showTitle(map,beach[0],marker,myLatlng);         	           	                    
                        }
					map.fitBounds(latlngbounds);
					changeSmallZoom();					                       
                    }
              
				overlayObj.prototype = new google.maps.OverlayView();
				function overlayObj() 
				{ 
				 	this.div_ = null;
					this.setMap(map);
				}				
				overlayObj.prototype.onAdd = function() {
					var div = document.createElement('DIV');
					div.style.border = 'none';
					div.style.position='absolute';   
					this.div_ = div;   
					var panes = this.getPanes();
					panes.overlayLayer.appendChild(div);	
				};				
				overlayObj.prototype.onRemove = function() {
					this.div_.parentNode.removeChild(this.div_);
					this.div_ = null;
				};				
				overlayObj.prototype.draw = function() {};
				
				function showTitle(map,title,marker,myLatlng) {					
					var tip_div = document.getElementById("tip_div");
					var tip_show = document.getElementById("tip_show");																											
					google.maps.event.addListener(marker, "mouseover", function(ev){												
						var overlay = new overlayObj(map);												
						var overlayProjection = overlay.getProjection();
						if (typeof(overlayProjection) != "undefined")
						{
							var point = overlayProjection.fromLatLngToContainerPixel(ev.latLng);
							var span_lenght = title.length * 8;
							span_lenght = point.x + span_lenght;					
							if (span_lenght + 10 > 960)
							{
								tip_div.style.left= 2 * point.x - 10 - span_lenght + 'px';
								tip_div.style.top=point.y - 20 + 'px';
							}
							else
							{				
								tip_div.style.left=point.x + 10 + 'px';
								tip_div.style.top=point.y + 'px';
                        }
							tip_show.style.backgroundColor= "#ffffff";
							tip_show.style.border = "#CCCCCC 1px solid";													
							tip_show.innerHTML = title;
                    }
                        });
					google.maps.event.addListener(marker, "mouseout", function(){
						tip_show.innerHTML = "";
						tip_div.style.left=0 + 'px';
						tip_div.style.top=-280 + 'px';     	  
        		tip_show.style.backgroundColor= "";
						tip_show.style.border = "";	
                        });
                    }
                    
				function clearStatesCitysMarkers() {
				  for (var i = 0; i < usStatesAndNonUsCitysMarkers.length; i++) {
	    			usStatesAndNonUsCitysMarkers[i].setMap(null);
	  			}
                        }
				function clearAllCitysMarkers() {
				  for (var i = 0; i < allCitysMarkers.length; i++) {
	    			allCitysMarkers[i].setMap(null);
                        }
            	}
                                  
              function changeBigZoom() {                       								        								        								               
       				 			google.maps.event.addListener(map, 'zoom_changed',function() {
       				 						var zoom = map.getZoom();      				 						      				 							
           				 				if (zoom >= changeZoomLevel)
           				 				{             				 					         				 					        				 					          				 					          	
		 					showAllCitys();
           				 					map.setZoom(zoom);
           				 				}           				 					           				 				           				 				
           				 	});	
           		}	
         			function changeSmallZoom() {	        										        								        								               
										google.maps.event.addListener(map, 'zoom_changed',function() {
       				 						var zoom = map.getZoom();      				 						      				 							
           				 				if (zoom < changeZoomLevel)
           				 				{             				 					         				 					        				 					          				 					          	
		 					initMap();
           				 					map.setZoom(zoom);
           				 				}           				 					           				 				           				 				
           				 	});			         			 					
							}	
													
              google.maps.event.addDomListener(window, 'load', initMap);
			});
			</script>
					
	
