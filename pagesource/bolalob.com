            touchRatio: 0.2,
            loop: true,
            slideToClickedSlide: false,
            nextButton: $('#focus .control-right'),
            prevButton: $('#focus .control-left')

        });
        var hlSwiper = new Swiper('#headline .swiper-container', {
            pagination: '#headline .swiper-pagination',
            paginationType: 'progress',
            slidesPerView: 1,
            autoplay: 10000,
            effect: 'fade',
            nextButton: $('#headline .control-right'),
            prevButton: $('#headline .control-left'),
            centeredSlides: true,
            loop: true
        });
        var swiper = new Swiper('#gallery .swiper-container', {
            pagination: '#gallery .swiper-pagination',
            slidesPerView: 1,
            paginationClickable: true,
            spaceBetween: 5,
            autoplay: 10000,
            nextButton: $('#gallery .control-right'),
            prevButton: $('#gallery .control-left'),
            centeredSlides: true,
            loop: true
        });

        // variabel Swiper for Serba-serbi
        var serbaSerbi = new Swiper('#medsos .swiper-container', {
            spaceBetween: 10,
            slidesPerView: 3,
            touchRatio: 0.2,
            loop: true,
            slideToClickedSlide: false,
            nextButton: $('#medsos .control-right'),
            prevButton: $('#medsos .control-left')
        });
        $(document).on('click', '#medsos li .bl-icon', function(event) {
            /* Act on the event */
            var cloning = $('<ul>');
            var h = $('#medsos ul .item').width();
            for (x = 0; x < 6; x++) {
                $('<li>', {
                    'class': 'loading item'
                }).height(h).appendTo(cloning);
            }
            $('#medsos .wrapper ul').append(cloning.html());
        });
        $(window).on('load', function(event) {
            $('#gallery .swiper-container .control').height($('#gallery .swiper-container img').height());
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                $('.swiper-container .control').show();
            }
            /* Act on the event */
        });
        $(function() {
            $('footer').removeClass('collapse');
            if ($(window).width() > 768) {
                $('.list-body').removeClass('collapse');
                $('.standings ul li').removeClass('collapse');
                $('.standings th').removeClass('collapse');
                $('.standings td').removeClass('collapse');
            }
            if ($(window).width() > 320) {
                $('.standings ul').each(function(index, el) {
                    $(this).find('li').eq(6).removeClass('collapse');
                    $(this).find('li').eq(7).removeClass('collapse');
                });

                $('.standings thead').each(function(index, el) {
                    $(this).find('th').eq(6).removeClass('collapse');
                    $(this).find('th').eq(7).removeClass('collapse');
                });

                $('.standings tbody tr').each(function(index, el) {
                    $(this).find('td').eq(6).removeClass('collapse');
                    $(this).find('td').eq(7).removeClass('collapse');
                });
            }
            $('.list-body').each(function(index, el) {
                var x = truncText($(this).text(), 140, '...');
                $(this).text(x);
            });

            $('#medsos .item').each(function(index, el) {
                var trim = $(window).width() < 768 ? 80 : 120;
                var x = truncText($(this).find('p').text(), trim, '...');
                /*$(this).height($(this).width());*/
                $(this).find('p').text(x);
            });
            if ($(window).width() < 321) {
                $('.list-title').each(function(index, el) {
                    var x = truncText($(this).text(), 60, '');
                    $(this).text(x);
                });
            }
            $('.dot-slider span').click(function(event) {
                /* Act on the event */
                if (!$(this).hasClass('active')) {
                    var element_curr = $('.content-slider li.active');
                    var element_next = $('.content-slider li').eq($(this).index());
                    new contentSlider(element_curr, element_next);
                    clearInterval(intervalSlider);
                    intervalSlider = setInterval(function() {
                        new contentSlider($('.content-slider li.active'), null);
                    }, 10000);
                }
            });

            $('.tabbing-header h3').click(function(event) {
                /* Act on the event */
                if ($(this).hasClass('popular')) {
                    $(this).parent().addClass('active').prev().removeClass('active');
                    $('.tabbing-body .new').fadeOut();
                    $('.tabbing-body .popular').fadeIn();
                }
                if ($(this).hasClass('new')) {
                    $(this).parent().addClass('active').next().removeClass('active');
                    $('.tabbing-body .popular').fadeOut();
                    $('.tabbing-body .new').fadeIn();
                }
            });
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                $('.standings table tbody').width($('.standings').width());
                $('#focus .swiper-container .control').addClass('touch-control');
                $('#headline .swiper-container .control').addClass('touch-control');
                $('#gallery .swiper-container .control').addClass('touch-control');
            }
            $('.standings thead th').each(function(index, el) {
                var ind = index;
                $('.standings table').each(function(index, el) {
                    $(this).find('td').eq(ind).css('width', $(this).width())
                });
            });
            if (!/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                $('.standings tbody').slimScroll({
                    height: '315px',
                    width: $('.standings').width()
                });
            }

            $.getJSON(hoststatic + 'api/special-canal.json?v=1', function(json, textStatus) {
                /*optional stuff to do after success */
                $.each(json.object, function(index, val) {
                    /* iterate through array or object */
                    if (val.tag == 'aff-futsal-club-championship')
                        $('#spec-channel .aff-futsal-club-championship img').prop('src', hoststatic + 'img/' + val.tag + '-depan.jpg')

                    if (val.tag == 'timnas-futsal-indonesia')
                        $('#spec-channel .timnas img').prop('src', hoststatic + 'img/' + val.tag + '-depan.jpg')

                    if (val.tag == 'pro-futsal-league')
                        $('#spec-channel .pfl img').prop('src', hoststatic + 'img/' + val.tag + '-depan.jpg')


                    if (val.tag == 'afc-u20-futsal-championship')
                        $('#spec-channel .afc-u20-futsal-championship img').prop('src', hoststatic + 'img/' + val.tag + '-depan.jpg')

                    if (val.tag == 'sea-games')
                        $('#spec-channel .sea-games img').prop('src', hoststatic + 'img/' + val.tag + '-depan.jpg')

                    if (val.tag == 'liga-1')
                        $('#spec-channel .liga-1 img').prop('src', hoststatic + 'img/' + val.tag + '-depan.jpg')
                });
            });

            /*ADS*/
            var url = 'https://js.mtburn.com/advs-instream.js';
            $.when($.getScript(url), $.ready).done(function() {
                var smallAds = '<div data-advs-adspot-id="MDE0OjU5Nzg" style="display:none"></div>';
                var bigAds = '<div data-advs-adspot-id="ODY0OjU5Nzc" style="display:none"></div>';
                $('#featured .default-featured .card:eq(0)').after(bigAds);
                $('#popular-new .new .content:eq(1)').after(smallAds);
                //$('#popular-new .popular .content:eq(1)').after(smallAds);

                MTBADVS.InStream.Default.run({
                    "immediately": true
                })
            });

        });


        $(document).on('click', '.standings .block-header', function(event) {
            if ($(this).next().hasClass('active')) {
                $(this).next().removeClass('active');
                $(this).find('span').removeClass('rotate45');
            } else {
                $('.standings .active').removeClass('active');
                $('.standings .rotate45').removeClass('rotate45');
                $(this).find('span').addClass('rotate45');
                $(this).next().addClass('active');

            }
        });
        $(window).load(function() {
            $('#jfy .greeting .greet').text(greetReturn());
            if ($('#jfy').length > 0) {
                setInterval(function() {
                    $('#jfy .greeting .greet').text(greetReturn());
                }, 3600000);
            }
            $('#jfy').find('.visibly').removeClass('visibly');

            setTimeout(function() {
                $('.verification-success').addClass('animated slideOutUp');
            }, 3000);
        });

        function greetReturn() {
            var hours = new Date().getHours();
            var text;
            if (hours >= 0)
                text = 'Selamat Pagi';
            if (hours >= 11)
                text = 'Selamat Siang';
            if (hours >= 15)
                text = 'Selamat Sore';
            if (hours >= 18)
                text = 'Selamat Malam';
            return text;
        }
