            function GetPairValue(RItemCount) {
                var Num = RItemCount / 4;
                var sNum = Num + '';
                var arrNum = sNum.split('.')
                var nNum = Number(arrNum[0]);
                var DecimalIndex = sNum.indexOf('.');
                var NumPlus = DecimalIndex >= 0 ? 1 : 0;
                var FinalNum = nNum + NumPlus;
                return FinalNum;
            }

            function MakeSVIndicator(RItemCount) {

                var LI = '';
                for (i = 0; i < GetPairValue(RItemCount); i++) {
                    if (i == 0)
                        LI += '<li class="SVItemLi active"></li>';
                    else
                        LI += '<li class="SVItemLi"></li>';
                }

                $('#SVMIndicator').html('<ol style="float: left;" class="carousel-indicators">' + LI + '</ol>');
                GetSetSVEventIndicator();
            }

            function GetSetSVEventIndicator() {
                $('.SVItemLi').click(function() {
                    if (!$(this).hasClass('active')) {
                        var index = $(this).index();
                        $('#SVMIndicator').find('li').removeClass('active');
                        $(this).addClass('active');
                        var items = $('.student_videos').find('.item').find('.col-md-2');
                        items.hide();
                        for (i = 4 * index; i < 4 * index + 4; i++) {
                            try {
                                items.eq(i).fadeIn(RSpd);
                            } catch (e) {}
                        }
                    }
                });
            }
			
