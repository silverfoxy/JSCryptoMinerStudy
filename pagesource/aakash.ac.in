		}
	}) 
	
	
	/*Script to set the position for sprite image */
	if($(window).width() >= 768 && $(window).width() < 1280 ){ 
		var spriteImgLength = $(".sprites").length;		
		if(spriteImgLength > 0){
			for(var i=1;i <= spriteImgLength;i++){
				var StripeImgSrc = $("#sprite"+i).css("background-image");
				var thumCount = $("#sprite"+i).attr("data-thumcount");
				var className ='';
				if(thumCount ==3){
					className ='sprite-img-3';
				}else if(thumCount ==4){
					className ='sprite-img-4';
				}else if(thumCount ==5){
					className ='sprite-img-5';
				}else if(thumCount ==6){
					className ='sprite-img-6';
				}
				var resSRc = StripeImgSrc.replace('url("','').replace('")','');
				$("#sprite"+i).removeAttr('style');
				$("#sprite"+i).html("<img class='img-responsive col-centered "+className+" ' src='"+resSRc+"'/>");
			}
		}
	}
	
	$('.left-sprite').hide();
	//var uTubeSrc = ['action1','action2','action3','action4','action5','action6'];
	$('.left-sprite').click(function(){
		
		var targetImg = $(this).attr('data-targetImg'); 		
		leftClick = parseInt($("#"+targetImg).attr("data-leftCount"));
		rightClick = parseInt($("#"+targetImg).attr("data-rightCount"));
		var row_id = parseInt($(this).attr("data-row_id"));

		var thumb_width = parseInt($(this).attr('data-thumb_width')); 
		var thumb_count = parseInt($(this).attr('data-thumb_count')); 
		var thumb_gap = parseInt($(this).attr('data-thumb_gap')); 
		
		if(rightClick > 1){
			//rightClick -=1;
			$("#"+targetImg).attr("data-rightCount",rightClick-1);			
		}
		
		var ImgPosition = "-"+(((thumb_width+thumb_gap)*(rightClick))-(thumb_width+thumb_gap))+"px 0px";
		if(rightClick >= 1){
			$("#"+targetImg).css("background-position",ImgPosition);
			//leftClick +=1;
			 $("#"+targetImg).attr("data-leftCount",leftClick+1);
		}
		if((leftClick+1) == thumb_count){			
			$('#sprite_img'+row_id).find('.left-sprite').hide();
			$('#sprite_img'+row_id).find('.right-sprite').show();
			$("#"+targetImg).attr("data-rightCount",1);
		}
		
	})
	
	$('.right-sprite').click(function(){
		var targetImg = $(this).attr('data-targetImg'); 
		leftClick = parseInt($("#"+targetImg).attr("data-leftCount"));
		rightClick = parseInt($("#"+targetImg).attr("data-rightCount"));
		var row_id = parseInt($(this).attr("data-row_id"));
		
		$('#sprite_img'+row_id).find('.left-sprite').show();
		$('#sprite_img'+row_id).find('.right-sprite').show();
			
		var thumb_width = parseInt($(this).attr('data-thumb_width')); 
		var thumb_count = parseInt($(this).attr('data-thumb_count')); 
		var thumb_gap = parseInt($(this).attr('data-thumb_gap')); 
		
		var ImgPosition = "-"+(thumb_width+thumb_gap)*rightClick+"px 0px";	
		
		if(rightClick < thumb_count){
			$("#"+targetImg).css("background-position",ImgPosition);
			$("#"+targetImg).attr("data-rightCount",rightClick+1);
		}
		if((rightClick+1) == thumb_count){
			$('#sprite_img'+row_id).find('.left-sprite').show();
			$('#sprite_img'+row_id).find('.right-sprite').hide();
			var curRightCount = $("#"+targetImg).attr("data-rightCount");
			$("#"+targetImg).attr("data-rightCount",curRightCount-1);
			$("#"+targetImg).attr("data-leftCount",1);
		}
	})
	
	$('#quote-carousel div.item').on('click',function(e){		
		if($(this).attr('data-href') != ""){
			var groupLink = $(this).attr('data-href');
