		<script type="text/javascript" defer>	
	///////////////// action to click on right panel /////////////
	$('.right-card').click(function(){	
		$('.left-card').show();
		var currCard = $(this).attr('data-current'); 
		var nextCard = $(this).attr('data-next'); 
		var TotalSlide = $(this).attr('data-ngTotal'); 
		var CurSlid = $(this).attr('data-ngCur'); 		
		if(parseInt(CurSlid) < parseInt(TotalSlide)){			
		
			/* remove class active and add class hide in current div */	
			
			$('#'+currCard).removeClass("active-card");
			$('#'+currCard).addClass("hide-card");
			
			//$('#'+currCard).hide('slow');
			
			/* remove class active and add class hide in next div */
			
			$('#'+nextCard).removeClass("hide-card");
			$('#'+nextCard).addClass("active-card");
			
			//$('#'+nextCard).show('slow');
		}else{
			$('.right-card').hide();
			$('.left-card').show();
		}		
	})
	
	///////////////// event on left nav/////////////
	
	$('.left-card').click(function(){		
		$('.right-card').show();
		var currCard = $(this).attr('data-current'); 
		var nextCard = $(this).attr('data-next'); 
		var TotalSlide = $(this).attr('data-ngTotal'); 
		var CurSlid = $(this).attr('data-ngCur'); 	
		if(parseInt(CurSlid) > 1){		
			// remove class active and add class hide in current div/////////		
			$('#'+currCard).removeClass("active-card");
			$('#'+currCard).addClass("hide-card");		
			//$('#'+currCard).hide('slow');
			// remove class active and add class hide in next div/////////		
			$('#'+nextCard).removeClass("hide-card");
			$('#'+nextCard).addClass("active-card");
			$('#'+nextCard+1).addClass("active-card");
			//$('#'+nextCard).show('slow');			
		
		}else{
			$('.left-card').hide();
			$('.right-card').show();
		}
	})
