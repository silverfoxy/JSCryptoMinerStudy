            picturefill();

            $(".fadein").delay(300).animate({opacity:1}, 700);
        }

       	$("#refresh-hero").on("click", function(e) {
            index++;

            if (index >= heroes.length) {
                index = 0;
            }

			var hero = JSON.parse(heroes[index]);
			$('.output').html(hero.html);
            picturefill();

            saveHeroCookie(index);

            e.preventDefault(); 
        });
    });
