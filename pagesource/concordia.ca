            $.cookie(cookieId, index, {expires: 3, path: '/'});
        }

        if (heroes.length > 0) {

            if (ordered && navigator.cookieEnabled) {
                var currentIndex = $.cookie(cookieId);

                if (currentIndex != null) {
                    index = parseInt(currentIndex) + 1;
                }
                if (index >= heroes.length) {
                    index = 0;
                }

                saveHeroCookie(index);
            }
            else {
            	index = Math.floor(Math.random() * heroes.length);
            }

            var hero = JSON.parse(heroes[index]);
