<script type="text/javascript">
  $(function(){
    var main_size = $('body').width();
    var $banner_vertical = $('#banner-vertical');
    var $banner_flutuante = $('#banner-flutuante');
    if(main_size >= 1100){
      var margin_banner_vertical = (main_size/2)+490;
      var margin_banner_flutuante = (main_size/2)-200;
      $banner_vertical.animate({left: margin_banner_vertical}, 0).show();
    }else{
      $('#banner-vertical').hide();
      $('#main').css('width',1024);
      var margin_banner_flutuante = (main_size/2)-200;
    }

    if($("#banner-flutuante:contains('container')").length){
      $banner_flutuante.animate({left: margin_banner_flutuante}, 0).show();
    }

    $(".fechar").click(function(event){
      event.preventDefault();
      $banner_vertical.hide();
    });
    $(".fechar_flutuante").click(function(event){
      event.preventDefault();
      $banner_flutuante.hide();
    });

    setTimeout(function(){$banner_flutuante.hide()},10000);

    if( /Android/i.test(navigator.userAgent) ) {
      $("#calendar-image").attr("href", "http://play.google.com/store/apps/details?id=com.paperlit.android.tribunadonorte&hl");
      $("#flip_sidebar").attr("href", "http://play.google.com/store/apps/details?id=com.paperlit.android.tribunadonorte&hl");
    }
    if (/iPhone|iPad|iPod/i.test(navigator.userAgent)) {
      $("#calendar-image").attr("href", "https://itunes.apple.com/br/app/tribuna-do-norte/id738466198?mt=8&uo=4");
      $("#flip_sidebar").attr("href", "https://itunes.apple.com/br/app/tribuna-do-norte/id738466198?mt=8&uo=4");
    }

  });
</script>

