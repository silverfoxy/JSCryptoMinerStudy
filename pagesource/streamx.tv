<div id="post-tmp" style="display:none;" class="notranslate">
<div class="{{post-type-class}} add-anim"  id="post_div_{{post-id}}">
    <div class="total-desc">
        <div class="post-title">
            <div class="dp-comment">
                <a href="{{uname}}" style="background-image:url('../imgs/user/thumb/{{user-id}}-profile.jpg?v={{u-i-id}}')">
                </a>
            </div>
            <div class="post-header">
                <span class="title"> 
                    <a href="{{uname}}">{{dname}}</a>
                </span>
                {{more_option}}
                <small class="time">{{post-time}}</small>	
            </div>						
        </div>
        <div class="post-desc" id="ed_post_{{post-id}}">
            <span id="post_caption_{{post-id}}">{{post-caption}}</span>
            <div class="text-edit tpost" >
                <textarea class="post-edit-text" rows="4" cols="50" value="" id="edit_caption_txt_{{post-id}}"></textarea>
                <input type="hidden" value="{{post-original-caption}}" id="edit_caption_hdn_{{post-id}}"/>
                <div class="post-edit-btn">
                    <button class="btn-common" onClick="updatePost({{post-id}})">Submit</button>
                    <button class="btn-cancel" onClick="editPost({{post-id}})">Cancel</button>
                </div>
            </div>
        </div>
        {{post-type-part}}
        <!--<div class="post-video-1">
            <label>Video</label>
            <div id="myElement-1"></div>  
        </div>-->
    </div>

    <div class="like-n-all">
        <div class="cmt-l">
            <small onclick="like({{post-id}},1);">
                <button class="like {{like-class}}" id="like_btn_{{post-id}}"></button>
            </small>
            <span class="l-c" id="like_count_{{post-id}}">{{like-count}}</span>
        </div>
        <div class="cmt-c" style="{{display_cmt}}">
            <button class="comment-symbol" id="comments_btn_{{post-id}}" onclick="loadComments({{post-id}},0)"></button>
            <span class="l-c" id="total_comment_{{post-id}}">{{comment-count}}</span>
        </div>
    </div>
    <div class="comment-div" id="post_comments_div_{{post-id}}" style="display:none;">
        <div class="load-more-comment-btn" id="load_cmt_btn_{{post-id}}" onclick="loadComments({{post-id}},1)">View Previous</div>
        <div class="comment" id="comments_add_{{post-id}}" style="display:none;">
            <div class="cssloader" id="comment_loader_{{post-id}}"><div class="loader"></div></div>
        </div>
        <input type="hidden" id="last_commnet_id_{{post-id}}" value="0"/>
        <div class="comment">
            <div class="comment-div">
                <div class="dp-comment col-xs-1 col-sm-1 col-md-1">
                    <a href="{{log-uname}}" style="background-image:url('../imgs/user/thumb/{{log-user-id}}-profile.jpg?v={{u-i-id}}')">
                    </a>
                </div>
                <div class="cmt-1 col-xs-11 col-sm-11 col-md-11">
                    <input class="comment-txt" id="cmt_txt_box_{{post-id}}" placeholder="Add a comment" onkeypress="comment(event,{{post-id}},1)">
					<button class="cmt-send" onclick="comment('button',{{post-id}},1)"></button>
                        <!--<button class="btn-cancel">Cancel</button>-->
                </div>
            </div>
        </div>
    </div>
</div>
</div>
<div class="notranslate" id="video-type-html" style="display:none;" >
    <div class="thumb-grid isVideo" style="background-image:url('{{thumb}}');" onclick="openJwPopup('{{thumb}}','{{path}}','y')"></div>
    <div class="post-video-1">
        <label>Video</label>
        <div id="video_{{post-id}}">
			
		</div>  
    </div>
</div>
<div class="notranslate" id="videon-type-html" style="display:none;" >
