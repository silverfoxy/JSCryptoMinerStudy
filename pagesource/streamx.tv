	<!--<iframe width="560" height="315" src="https://www.youtube.com/embed/pqSMm7R3Jfs" frameborder="0" allowfullscreen></iframe>-->
    <div class="post-video-1">
        <label>Video</label>
        <div id="video_{{post-id}}" class="v-container">
                <div class="video-play-button"></div>
                <video poster="{{v_poster}}" controls>
                        <source src="{{vid_src}}">
                </video>
	</div>  
    </div>
</div>
<div class="notranslate" id="audio-type-html" style="display:none;">
