    <div class="post-audio-box">
        <label>Music</label>
        <div id="audio_{{post-id}}">Loading the player</div>  
    </div>
</div>
<div class="notranslate" id="image-type-html" style="display:none;">
	
   <button class="post-gallery-1" onclick="showImagePopup('{{user-id}}-{{post-id}}')">
	<img src="../imgs/user/{{user-id}}-{{post-id}}.jpg" alt="streamx" title=""/>
   </button>
</div>
<div class="notranslate" id="doc-type-html" style="display:none;">
