                <form method="get" action="{{doc-path}}">
                <button type="submit" class="download"></button>
                </form>
                <p class="mb">{{doc-size}}</p>
            </div>
        </div>	
    </div>
</div>
<div class="notranslate" id="comment-html" style="display:none;">
    <div id="comment_box_{{comment-id}}" class="comment-div">
        <div class="dp-comment col-xs-1 col-sm-1 col-md-1">
            <a href="{{uname}}" style="background-image:url('../imgs/user/thumb/{{user-id}}-profile.jpg?v={{u-i-id}}')" title="">
            </a>
        </div>
        <div class="cmt-1 col-xs-11 col-sm-11 col-md-11">
            <div class="comment-title">
                <span class="title"> 
                    <a href="{{uname}}" title="">{{dname}}</a>
                </span>
                <small class="time right">{{comment-time}}</small>
            </div>
            <div class="comment-div-1">
                <p class="comment-tag">{{user-comment}}</p>
                {{delete-comment-html}}
            </div>
            <div class="inner-l-c">
                <div class="like-div">
                    <small class="like-mar" onClick="like({{comment-id}},2)">
                        <button class="like {{like-class}}" id="clike_btn_{{comment-id}}"></button>
                    </small>
                    <span class="l-c l-c-inner" id="clike_count_{{comment-id}}">{{like-count}}</span>
                </div>
                <div class="comment-div-inner">
                    <button class="comment-2" id="reply_btn_{{comment-id}}" onClick="loadReplies({{comment-id}},0)"></button>
                    <span class="l-c l-c-inner" id="total_replies_{{comment-id}}">{{reply-count}}</span>
                </div>
            </div>
            <div class="inner-comment col-md-12" id="replay_div_{{comment-id}}" style="display:none;">
                <div class="load-more-comment-btn" id="load_reply_btn_{{comment-id}}" onclick="loadReplies({{comment-id}},1)" style="display:none">View Previous</div>
                <div class="comment" id="replies_add_{{comment-id}}" style="display:none;">
                    <div class="cssloader" id="reply_loader_{{comment-id}}" style="display:none;"><div class="loader"></div></div>
                </div>
                <input type="hidden" id="last_reply_id_{{comment-id}}" value="0"/>
                <div class="dp-comment col-xs-1 col-sm-1 col-md-1">
                    <a href="{{log-uname}}" style="background-image:url('../imgs/user/thumb/{{log-user-id}}-profile.jpg?v={{u-i-id}}')" title="">
                    </a>
                </div>
                <div class="cmt-1 cmt-inner col-xs-11 col-sm-11 col-md-11">
                    <input class="comment-txt" placeholder="Add a reply" id="reply_txt_box_{{comment-id}}" onkeypress="reply(event,{{post-id}},{{comment-id}},2,'t')"/>
                     <button class="cmt-send" onclick="reply(event,{{post-id}},{{comment-id}},2,'b')"></button>
                        <!--<button class="btn-cancel">Cancel</button>-->
                </div>
            </div>	
        </div>
    </div>
</div>
<div class="notranslate" id="reply-html" style="display:none;">
    <div id="comment_box_{{comment-id}}" class="user-replay">
        <div class="dp-comment col-xs-1">
            <a href="{{uname}}" style="background-image:url('../imgs/user/thumb/{{user-id}}-profile.jpg?v={{u-i-id}}')" title="">
            </a>
        </div>
        <div class="col-sm-11 comment-area">
