	function dynamicContent() {
        	//Box Shadow
        	if ($.browser.msie) {
			$("div.hp.column div.ui-widget.app, div.sp.column.one div.ui-widget.app.navigation, div.sp.column.two div#sw-content-layout-wrapper, div.spn.column").boxShadow( 0, 0, 10, "#B08B5A" );
			$("div.sp.column.two div.IEBoxShadow").css("z-index", "-1");	
		}
		//Rounded Corners
		if($("div.hp.column.one div.ui-widget.app").length == "1") {
			$("div.hp.column.one div.ui-widget.app div.ui-widget-header").corner("tl 10px");
			$("div.hp.column.one div.ui-widget.app div.ui-widget-footer").corner("bl 10px");
			$("div.hp.column.one div.IEBoxShadow").corner("tl bl 10px");
		} else {
			$("div.hp.column.one div.ui-widget.app:first div.ui-widget-header").corner("tl 10px");
			$("div.hp.column.one div.IEBoxShadow:first").corner("tl 10px");
			$("div.hp.column.one div.ui-widget.app:last div.ui-widget-footer").corner("bl 10px");
			$("div.hp.column.one div.IEBoxShadow:last").corner("bl 10px");
		}
		if($("div.hp.column.three div.ui-widget.app").length == "1") {
			$("div.hp.column.three div.ui-widget.app div.ui-widget-header").corner("tr 10px");
