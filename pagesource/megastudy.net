<script type="text/javascript">
    //lp = LecPre
    var LPTabNo = "2";
    var LPTabTgt = $("#divLecPreTab a");
    var LPTabCnt = LPTabTgt.length;
    function fncLPTabOver(t) { LPTabTgt.eq(t - 1).addClass("on"); }
    function fncLPTabOut(t) { LPTabTgt.each(function (index) { if ((LPTabNo - 1) != index) { LPTabTgt.eq(index).removeClass("on"); } }); }
    function fncLPTabClk(t) {
        LPTabNo = t; fncLPTabOver(t); fncLPTabOut(t);
        //페이지에 따른 이벤트 처리
        jQuery(".divLPDomArea").hide();
        jQuery(".preview .section_indecator").hide();
        jQuery("#divLPDomArea" + t).show();
        jQuery("#divLPIndecatorArea" + t).show();

        fncGoMainSlide("click", "LP", t, 0);
    }

    $(document).ready(function () {
        LPTabTgt.bind({
            click: function () { fncLPTabClk(LPTabTgt.index(this) + 1); },
            mouseover: function () { fncLPTabOver(LPTabTgt.index(this) + 1); },
            mouseout: function () { fncLPTabOut(LPTabTgt.index(this) + 1); }
        });
        if (LPTabNo == "") { LPTabNo = Math.round(Math.random() * (LPTabCnt - 1) + 1); }
        fncLPTabClk(LPTabNo);
    });
</script>

