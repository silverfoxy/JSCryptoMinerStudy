    function fncSTTabOver(t) { STTabTgt.eq(t - 1).addClass("on"); }
    function fncSTTabOut(t) { STTabTgt.each(function (i) { if ((STTabNo - 1) != i) { STTabTgt.eq(i).removeClass("on"); } }); }
    function fncSTTabClk(t) {
        STTabNo = t; fncSTTabOver(t); fncSTTabOut(t);
        //페이지에 따른 이벤트 처리
        jQuery("#divSTArea .commonList").hide();
        jQuery("#divSTArea .section_indecator").hide();
        jQuery("#divSTDomArea" + t).show();
        jQuery("#divSTIndecatorArea" + t).show();
        fncGoMainSlide("click", "ST", t, 0);
    }

    $(document).ready(function () {
        STTabTgt.bind({
            click: function () { fncSTTabClk(STTabTgt.index(this) + 1); },
            mouseover: function () { fncSTTabOver(STTabTgt.index(this) + 1); },
            mouseout: function () { fncSTTabOut(STTabTgt.index(this) + 1); }
        });
        if (STTabNo == "") { STTabNo = Math.round(Math.random() * (STTabCnt - 1) + 1); }
        fncSTTabClk(STTabNo);
    });
