			// toggle 아이콘 변수
			var targetLayerIcon = {
				a : jQuery("#megaGnb .gnb1 em"),
				b : jQuery("#megaGnb .gnb9"),
				c : jQuery("#hSub .megaCampain"),
				d : jQuery("#hSub .menu_all a"),
				e : jQuery("#hSub .brPass"),
				f : jQuery("#megaGnb"),
				g : jQuery("#teachers"),
				h : jQuery("#wrapMegauniv"),
				i : jQuery("#megaGnb .top_ban_mn"),
				j : jQuery("#wrapRussel")
			}

			//toggle 아이콘
			function toggle_ico(Num) {
				if(Num === 1){
					targetLayerIcon.a.toggleClass('on');
					targetLayerIcon.b.removeClass('all_view3');
					targetLayerIcon.c.removeClass('megaCampain__on');
					targetLayerIcon.d.removeClass('all_view3');

					if(targetLayerIcon.e.length === 1 && targetLayerIcon.g.css("display") == "none") {
						targetLayerIcon.e.removeClass('brPass__hide');
					}else{
						targetLayerIcon.e.addClass('brPass__hide');
					}
					targetLayerIcon.f.toggleClass('zIndexHigh');
					targetLayerIcon.i.removeClass('top_ban_mn_on');
				}else if(Num === 2){
					targetLayerIcon.b.toggleClass('all_view3');
					targetLayerIcon.a.removeClass('on');
					targetLayerIcon.c.removeClass('megaCampain__on');
					targetLayerIcon.d.removeClass('all_view3');
					if(targetLayerIcon.e.length === 1 && targetLayerIcon.h.css("display") == "none"){
						targetLayerIcon.e.removeClass('brPass__hide');
					}else{
						targetLayerIcon.e.addClass('brPass__hide');
					}
					targetLayerIcon.f.removeClass('zIndexHigh');
					targetLayerIcon.i.removeClass('top_ban_mn_on');
				}
				else if(Num === 3){
					targetLayerIcon.c.toggleClass('megaCampain__on');
					targetLayerIcon.a.removeClass('on');
					targetLayerIcon.b.removeClass('all_view3');
					targetLayerIcon.d.removeClass('all_view3');
					targetLayerIcon.f.removeClass('zIndexHigh');
					targetLayerIcon.i.removeClass('top_ban_mn_on');
				}
				else if(Num === 4){
					targetLayerIcon.d.toggleClass('all_view3');
					targetLayerIcon.a.removeClass('on');
					targetLayerIcon.b.removeClass('all_view3');
					targetLayerIcon.c.removeClass('megaCampain__on');
					targetLayerIcon.f.removeClass('zIndexHigh');
					targetLayerIcon.i.removeClass('top_ban_mn_on');
				}
				else if (Num === 5) {
					targetLayerIcon.i.toggleClass('top_ban_mn_on');
					targetLayerIcon.b.removeClass('all_view3');
					targetLayerIcon.a.removeClass('on');
					targetLayerIcon.c.removeClass('megaCampain__on');
					targetLayerIcon.d.removeClass('all_view3');
					if (targetLayerIcon.e.length === 1 && targetLayerIcon.j.css("display") == "none") {
						targetLayerIcon.e.removeClass('brPass__hide');
					} else {
						targetLayerIcon.e.addClass('brPass__hide');
					}
					targetLayerIcon.f.removeClass('zIndexHigh');
				}
			}

			//toggle
			function toggle_top(obj) {
				var fMenu = jQuery("#" + obj)
				var fMenuDisp = fMenu.css("display");
				jQuery('.lay_allclose').hide();
				//  jQuery("#megaGnb").find(".all_view").attr('class','all_view');

				//  jQuery("#hSub").find(".all_view").remove();

				if (fMenuDisp == 'none') {
					if (obj == "teachers") {

						try {
							$("#centerarea").css("z-index", 1);
						} catch (e) { }
						try {
							$("#layer_learning").css("z-index", 1);
						} catch (e) { }

						if (fMenu.html() == "") {
							fMenu.load("/Common/menu/teachers_ax.asp");
						}

					}
					// fMenu.show();
					showEffect(fMenu);
				} else {
					fMenu.hide();
				}

				function getNumberOnly(obj){
				    var val = obj;
				    val = new String(val);
				    var regex = /[^0-9]/g;
				    val = val.replace(regex, '');
				    obj= val;
					return obj;
				}

				function showEffect(obj){
					var Top = getNumberOnly(obj.css("top"));
					obj.css("top",Top-20);
					obj.show().animate({top:Top},300);
				}
			}
