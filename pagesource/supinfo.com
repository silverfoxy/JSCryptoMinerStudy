		var agendaRows = Math.ceil( $("#agenda .other-dates #homeAgenda ul li").length / 3);
		
		var agendaInterval = setInterval(function(){
			if( agendaAutoSlide ){
				if( parseInt($("#agenda .other-dates #homeAgenda").css("top")) <= (parseInt(agendaRows-1) * -70) ){ 
					$("#agenda .other-dates #homeAgenda").animate({
