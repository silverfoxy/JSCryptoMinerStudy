        		var v = false;
        		var c = document.cookie;
        		if (c.length > 0) {
        			var s = c.indexOf(" " + window.location.host + "-cookie-header-popup=");
        			if (s == -1) s = c.indexOf(window.location.host + "-cookie-header-popup=");
        			if (s == -1) {
        				c = null;
        			} else {
        				s = c.indexOf("=", s) + 1;
        				var e = c.indexOf(";", s);
        				if (e == -1) e = c.length;
        				c = unescape(c.substring(s,e));
        			}
        			if (c!=null && c!="") v = true;
        		}
        		return v;
        	}
        	if (!checkCookieHeader()){
        		var div = document.getElementById("cookie-header-popup");
        		document.getElementById("cookie-policy-header").className += " cookie-popup-header";
        		div.style.display = 'block';
        		
        		
        	}
        </script>
			
		
	</body>
</html>