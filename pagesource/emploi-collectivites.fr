						pElt.removeClass("opened");
						pElt.addClass("closed");
						$('#search_block').css("border-bottom","solid 1px #3577a9");						
						$('#tabs').hide();						
					}else{
						pElt.removeClass("closed");
						pElt.addClass("opened");
