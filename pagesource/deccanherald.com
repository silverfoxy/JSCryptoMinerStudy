//get X Position value for any given object
function getX(obj){
	var o = obj;
	var x = o.offsetLeft;
	o = o.offsetParent;
	while(o != null){
		x += o.offsetLeft;
		o = o.offsetParent;
	}
	return x;
}

//get Y Position value for any given object
function getY(obj){
	var o = obj;
	var y = o.offsetTop + o.offsetHeight;
	o = o.offsetParent;
	while(o != null){
		y += o.offsetTop;
		o = o.offsetParent;
	}
	return y;
}

