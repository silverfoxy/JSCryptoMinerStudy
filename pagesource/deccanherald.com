<script>
$(document).ready(function () 
{
   var refreshinterval = 30000;
   var presentCount=0;
   var loading=0;
   var id = setInterval(updateLiveBlog, refreshinterval);
   InitalizeLiveblog();
   updateLiveBlog();

   function InitalizeLiveblog() 
   {
      loading = 1;

      $.getJSON('../election/json/homepage_liveblog_feed.json', function(jsonData) 
      {       
         var obj  = $.parseJSON(JSON.stringify(jsonData)); 
         if (parseInt(obj.data.liveblog_status) == 1)
         {
         showLiveBlog();
         }
         else
         {
           hideLiveBlog();
         }  
         if(obj.data.headline_status == 1)
         {
            if(obj.data.headline != '')
            {
               $('#liveblogheadline').empty();
               $('#liveblogheadline').append('<h1 style="color: #055083">'+obj.data.headline+'</h1>'); 
            }     
         } 
         else
         {
          $('#liveblogheadline').empty(); 
         }
         var i=0;
         $.each(obj.feeds, function (key, val)
         {  
            var htmlData = [];
            htmlData.push('<div class="item">');
            htmlData.push('<div class="feedtime">')
            htmlData.push(val.time);
            htmlData.push(':</div>');
            htmlData.push('<div class="description">');
            htmlData.push(val.text);
            if (val.type == 'article')
            {
               htmlData.push('<a target="_blank" href="'+val.link+'">  Click Here to read more</a>');
            }
            else if (val.type == 'image')
            {
               htmlData.push('<a class=" topopup photoFeed " data-desc="'+val.text+'" href="#" data-link="'+val.link+'"  >  <img width="14px" style="margin-left:5px;" src="/images/camera-icon.gif"></a>');
            }
            else if (val.type == 'video')
            {
               htmlData.push('<a data-link="'+val.link+'" data-desc="'+val.text+'" class="topopup videoFeed " href="#">  <img style="margin-left:5px;" src="/images/video_icon.gif" ></a>');
            }
            else if (val.type == 'twitter')
            {
               htmlData.push('<a data-link="'+val.link+'" data-desc="'+val.text+'" class="topopup videoFeed " href="#">  <img style="margin-left:5px;" src="/images/twitter-bird.png" ></a>');
            }
            htmlData.push('</div>'); 
            htmlData.push('</div>')
            $('#liveblogfeed').append(htmlData.join('')).trigger('create');
         });     
            presentCount =  obj.data.latestid;  
            loading = 0;                  
      });
   }
   
   function updateLiveBlog()
   {
      if (loading == 1)
      return;
      $.getJSON('../election/json/homepage_liveblog_feed.json', function(jsonData) 
      {
         var obj  = $.parseJSON(JSON.stringify(jsonData));  
         if (parseInt(obj.data.liveblog_status) == 1)
         {
            showLiveBlog();
         }
         else
         {
            hideLiveBlog();
         }  
         if(obj.data.headline_status == 1)
         {
            if(obj.data.headline != '')
            {
               $('#liveblogheadline').empty();
               $('#liveblogheadline').append('<h1 style="color: #055083">'+obj.data.headline+'</h1>'); 
            }     
         } 
         else
         {
          $('#liveblogheadline').empty(); 
         }
         var htmlData = [];
         if (parseInt(obj.data.latestid) > parseInt(presentCount))
         {
            $.each(obj.feeds, function (key, val)
            {   
               if (parseInt(val.id) > parseInt(presentCount))
               {
                  htmlData.push('<div class="item">');
                  htmlData.push('<div class="feedtime">')
                  htmlData.push(val.time);
                  htmlData.push(':</div>'); 
                  htmlData.push('<div class="description">');
                  htmlData.push(val.text);
                  if (val.type == 'article')
                  {
                    htmlData.push('<a target="_blank" href="'+val.link+'">  Click Here to read more</a>');
                  }
                  else if (val.type == 'image')
                  {
                     htmlData.push('<a data-desc="'+val.text+'" data-link="'+val.link+'" class="photoFeed" href="#"> <img src="/images/camera-icon.gif"></a>');
                  }
                  else if (val.type == 'video')
                  {
                     htmlData.push('<a data-desc="'+val.text+'" data-link="'+val.link+'" class="videoFeed" href="#"><img src="/images/has_video.png" ></a>');
                  }
                  else if (val.type == 'twitter')
                  {
                     htmlData.push('<a data-desc="'+val.text+'" data-link="'+val.link+'" class="videoFeed" href="#">  <img style="margin-left:5px;" src="/images/twitter-bird.png" ></a>');
                  }
                  htmlData.push('</div>'); 
                  htmlData.push('</div>');                         
               }
            });
            $('#liveblogfeed').prepend(htmlData.join('')).trigger('create');                  
            presentCount =  obj.data.latestid;  
         }   
      });          
   }
   
   function hideLiveBlog()
   {
      $('#livefeedsection').slideUp();
   }
         
   function showLiveBlog()
   {            
      $('#livefeedsection').slideDown();
   }
});



</script>

