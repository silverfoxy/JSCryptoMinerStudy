		var date2 = new Date();
		var month2 = date2.getMonth()+1;
        var day2 = date2.getDate();
        var output = date2.getFullYear() + '-' +
            ((''+month2).length<2 ? '0' : '') + month2 + '-'+ 
            ((''+day2).length<2 ? '0' : '') + day2;
		var dateexplode = this.data.published.$t.split('T');
  if (dateexplode[0] == output) {
	today = 1;
  }
		this.catUl = $('<ul>', {class: 'feed-label-list'});
      	for (ci = 0; ci<catSize; ci++) {
          this.catUl.append($('<a>',{href:'http://'+domain+'/search/label/'+categories[ci].term}).append($('<li>', {class: 'feed-label-item'}).text(categories[ci].term)));
      	}
		var link = this.data.link[this.data.link.length-1].href;
  if (typeof(this.data.media$thumbnail) != 'undefined') {
		var image = this.data.media$thumbnail.url;
		var image = image.replace('/s72-c/','/s' + this.conf.dimension + this.conf.crop + '/');
  } else {
	var image = 'http://i.imgur.com/x7NPC7i.jpg';
  }
		var p = /<\S[^>]*>/g;
		var y = content;
        y = y.replace(p, "");
        if (this.conf.showpostsummary == true) {
            if (y.length > this.conf.summaryChar) {
                y = y.substring(0, this.conf.summaryChar);
                var e = y.lastIndexOf(" ");
                y = y.substring(0, e);
                y = y + "...";
            }
        }
		this.li = $('<li>', {class: 'feed-item'});
		this.thumbContainer = $('<div>', {class: 'feed-thumb-container'});
		this.feedContainer = $('<div>', {class: 'feed-container'});
        this.athumb = $('<a>', {class: 'feed-thumb-link',href: link});
		this.thumb = $('<img>', {class: 'feed-thumb',src: image});
        this.footer = $('<div>', {class: 'feed-footer'});
		this.feedDate = $('<div>', {class: 'feed-date'});
		this.feedDate.text(date);
		this.feedDate.prepend($('<span>', {class: 'glyphicon glyphicon-calendar'}));
		this.footer.append(this.feedDate);
		this.athumb.append(this.thumb);
		this.thumbContainer.append(this.athumb);
		this.feedContainer.append(this.thumbContainer);
  if (today == 1) {
	this.today = $('<div>', {class: 'today'});
	this.today.text('Hari ini');
	this.thumbContainer.append(this.today);
  }
		this.feedContent = $('<div>', {class: 'feed-content'});
		this.feedContainer.append(this.feedContent);
		if (this.conf.shareCounter == true) {
		this.shares = $('<div>', {class: 'feed-shares'});
		this.shares.text('Loading...');
		this.shareNumber = $('<span>', {class: 'feed-shares-num'});
		var self = this;
	//	$.getJSON('http://graph.facebook.com/?id='+link,function(shareData) {
	//		self.shares.text(' SHARE');
	//		if (typeof(shareData.shares) != 'undefined') {
	//			if (shareData.shares > 1) {
	//			self.shares.text(' SHARES');
	//			}
	//			self.shareNumber.text(shareData.shares);
	//		} else {
	//			self.shareNumber.text(0);
	//		}
	//	self.shares.prepend(self.shareNumber);
	//	self.shares.prepend($('<span>', {class: 'thumb-share icon-share'}));
	//	});

	//	this.footer.append(this.shares);
		}
		this.li.append(this.feedContainer);
		this.feedContent.append(this.catUl);
		this.ah2 = $('<a>', {class: 'feed-title-link',href: link});
		this.h2 = $('<h2>', {class: 'feed-title'});
		this.h2.text(this.title);
		this.ah2.append(this.h2);
if (this.conf.titleFirst == true) {
this.feedContainer.prepend(this.ah2);
} else {
		this.feedContent.append(this.ah2);
}
if (this.conf.showpostsummary == true) {
this.sum = $('<div>', {class: 'feed-summary'});
this.sum.text(y);
this.feedContent.append(this.sum);
}
		this.feedContent.append(this.footer);
}

var scroller = function scroller(document) {
this.doc = $(document);
this.lastScroll = this.doc.scrollTop();
this.lastDirection = null;
var self = this;
this.registered = {};
this.doc.scroll(function() {
	self.scrolling();
});
}
scroller.prototype = {};
scroller.prototype.scrolling = function() {
	if (this.doc.scrollTop() > this.lastScroll) {
if (this.lastDirection != 'down') {
this.lastDirection = 'down';
$('#tab').toggleClass('hide-tab');
}
} else {
if (this.lastDirection != 'up') {
this.lastDirection = 'up';
$('#tab').toggleClass('hide-tab');
}
}
this.lastScroll = this.doc.scrollTop();
/*for(var obj in this.registered) {
obj.scroll(this.lastScroll,this.lastDirection);
console.log(obj);
}*/

for (var key in this.registered) {
   var obj = this.registered[key];
   for (var prop in obj) {
      if(obj.hasOwnProperty(prop)){
       obj.scroll(this.lastScroll,this.lastDirection);
      }
   }
}


//console.log(this.lastDirection);
}

scroller.prototype.register = function(key,obj) {
	if (typeof(this.registered[key]) == 'undefined') {
	this.registered[key] = obj;
} else {
	console.log('Object with key '+key+' already registered');
}
}


var tabrider = function tabrider(title) {
	this.registered = [];
	this.elements = $(document).find('[tab]');
	this.title = $(title);
	this.originalTitle = this.title.text();
	this.initiate();
}

tabrider.prototype = {};
tabrider.prototype.initiate = function() {
var self = this;
this.elements.each(function(index) {
self.registered.push({id: $(this).attr('tab'), offset: 0});
$(this).click(function(){
$('html,body').animate({
          scrollTop: $('[tab-id='+$(this).attr('tab')+']').offset().top
        }, 1000);

});
});
}


tabrider.prototype.scroll = function(top,direction) {
	var self = this;
if ($(window).width() <= 767) {
	$.each(this.registered, function( index, value ) {
		self.registered[index].offset = $('[tab-id='+self.registered[index].id+']').offset().top;
	});
	this.registered = this.registered.sort(function(a, b) {   
		return (b['offset'] > a['offset']) ? 1 : ((b['offset'] < a['offset']) ? -1 : 0);
	});
	$.each(this.registered, function( index, value ) {
		if (top > (self.registered[index].offset-80)) {
			self.elements.each(function(index2) {
				if($(this).attr('tab') != self.registered[index].id) {
					$(this).removeClass('active');
				} else {
					$(this).addClass('active');
					self.title.text($(this).attr('cotitle'));
				}
			});
		return false;
		}
		self.title.text(self.originalTitle);
	});
} else {
self.title.text(self.originalTitle);
}
}

$(document).ready(function(){
var tabobj = new tabrider('#title');
myScroller.register('tab',tabobj);
});
var myScroller = new scroller(document);

