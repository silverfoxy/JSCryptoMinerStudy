var domain = window.location.host;

  var ContentLoader = function ContentLoader(widgetData) {
      this.widgetData=widgetData;
	  this.id = this.widgetData.id;
	  this.label = this.widgetData.label;
	if (typeof(this.widgetData.manualload) == 'undefined') {
	  this.manualload = false;
    } else {
    	this.manualload = this.widgetData.manualload;
	}
    if (typeof(this.widgetData.dimension) == 'undefined') {
	  this.dimension = 400;
    } else {
    	this.dimension = this.widgetData.dimension;
	}
	if (typeof(this.widgetData.summaryChar) == 'undefined') {
	  this.summaryChar = 200;
    } else {
    	this.summaryChar = this.widgetData.summaryChar;
	}
	if (typeof(this.widgetData.showpostsummary) == 'undefined') {
	  this.showpostsummary = false;
    } else {
    	this.showpostsummary = this.widgetData.showpostsummary;
	}
	if (typeof(this.widgetData.display) == 'undefined') {
	  this.display = 'thumb';
    } else {
    	this.display = this.widgetData.display;
	}
	if (typeof(this.widgetData.crop) == 'undefined') {
	  this.crop = '';
    } else if (this.widgetData.crop == true) {
    	this.crop = '-c';
    } else {
		this.crop = '';
    }
	if (typeof(this.widgetData.shareCounter) == 'undefined') {
	  this.shareCounter = false;
    } else {
    	this.shareCounter = this.widgetData.shareCounter;
	}
	if (typeof(this.widgetData.autoload) == 'undefined') {
	  this.autoload = false;
    } else {
    	this.autoload = this.widgetData.autoload;
	}
	if (typeof(this.widgetData.title) == 'undefined') {
	  this.title = false;
    } else {
    	this.title = this.widgetData.title;
	}
	if (typeof(this.widgetData.titleFirst) == 'undefined') {
	  this.titleFirst = false;
    } else {
    	this.titleFirst = this.widgetData.titleFirst;
	}
	if (typeof(this.widgetData.whitelist) == 'undefined') {
	  this.whitelist = false;
    } else {
    	this.whitelist = this.widgetData.whitelist;
	}
	this.conf = {};
	this.conf.dimension = this.dimension;
	this.conf.summaryChar = this.summaryChar;
	this.conf.showpostsummary = this.showpostsummary;
	this.conf.display = this.display;
	this.conf.crop = this.crop;
	this.conf.titleFirst = this.titleFirst;
	this.conf.shareCounter = this.shareCounter;
	if (this.label != '') {
	this.feed = 'http://'+domain+'/feeds/posts/default/-/'+this.label+'?alt=json-in-script&callback=?';
} else {
		this.feed = 'http://'+domain+'/feeds/posts/default/?alt=json-in-script&callback=?';
}
if (typeof(this.widgetData.startIndex) == 'undefined') {
	  this.startIndex = 1;
	  this.index = 1;
} else {
      this.index = this.widgetData.startIndex;
	  this.startIndex = this.widgetData.startIndex;
}
if (typeof(this.widgetData.max) == 'undefined') {
this.max = null;
} else {
this.max = this.widgetData.max;
}
this.request = false;
this.complete = false;
this.initiate();
  }

	
  ContentLoader.prototype = {};
  ContentLoader.prototype.initiate = function() {
	myScroller.register(this.id,this);
	document.write('<div id='+this.id+'></div>');
	this.container = $('body').find('#'+this.id);
	if (typeof(this.widgetData.class) != 'undefined') {
	this.container.addClass(this.widgetData.class);
	this.container.addClass('widget-container');
	}
	this.ul = $('<ul>', {class: 'feed-list '+this.display});
	this.container.append(this.ul);
	if (this.manualload == true) {
	this.loadButton();
}
	this.top = this.container.offset().top;
	this.loadfeed();
  }

ContentLoader.prototype.loadButton = function() {
	this.loadButton = $('<div>', {class: 'feed-load-button'});
	if (this.title != false) {
	this.loadButton.text('Lagi yang '+this.title);
	} else if (this.label != '')  {
	this.loadButton.text('Lagi yang '+this.label);
	} else {
	this.loadButton.text('Lagi yang Terkini');	
}
	this.container.append(this.loadButton);
	var self = this;
	this.loadButton.click(function() {
	self.tryLoad();
});
}

ContentLoader.prototype.loadfeed = function() { //by scroll or after load
if ((this.index == this.startIndex) || ($(window).width() > 767 )) {
	this.tryLoad();
} else {
}
}

ContentLoader.prototype.tryLoad = function() {
	if ((this.complete == false) && (this.request == false)) {
	var self = this;
if (this.manualload == true) {
	this.loadButton.text('Loading...');
}
	var feedurl = this.feed+'&start-index='+this.index;
	if (this.max != null) {
	feedurl = feedurl+'&max-results='+this.max
	}
    this.request = $.getJSON(feedurl,function(data) {
	self.request = false;
		self.process(data);
		
	});
}
}

ContentLoader.prototype.process = function(data) {
var count = 0;
console.log(data);
if(typeof(data.feed.entry) != 'undefined') {
    for (i = 0; i<data.feed.entry.length; i++) {
		var entry = data.feed.entry[i];
		if(typeof(entry) != 'undefined') {
		this.index++;
		count++;
          if (this.whitelist != false ) {
		var show = 0;
          for (a1 = 0; a1<entry.category.length; a1++) {
            if (entry.category[a1].term == this.whitelist) {
				show = 1;
            }
          }
          } else {
			var show = 1;
          }
          if (show == 1) {
		var item = new articleItem(entry,this.conf);
		this.ul.append(item.li);
		var actualH = item.h2.height();
		item.h2.css('white-space','nowrap');
		var itemH = item.h2.height();
		item.h2.css('white-space','normal');
if ((actualH)/itemH > 3) {
item.h2.css('max-height',(itemH*3));
item.h2.dotdotdot();
}
          }
}
    }
} else {
if (this.manualload == true) {
this.loadButton.remove();
}
this.complete = true;
}
if (this.manualload == true) {
if (this.title != false) {
	this.loadButton.text('Lagi yang '+this.title);
	} else if (this.label != '')  {
	this.loadButton.text('Lagi yang '+this.label);
	} else {
	this.loadButton.text('Lagi yang Terkini');	
}
}
this.customFunction();
this.scroll($(document).scrollTop(),'down');

}

ContentLoader.prototype.customFunction = function() {

}

ContentLoader.prototype.scroll = function(top,direction) {
if (this.autoload == true) {
if (direction == 'down') {
if((this.container.height()+this.container.offset().top)-(top+$( window ).height()) < 150) {
this.loadfeed();
}
}
}
}

var articleItem = function articleItem(data,conf) {
	this.data = data;
	this.conf = conf;
	this.bulan = new Array("Januari", "Februari", "Mac", "April", "Mei", "Jun", "Julai", "Ogos", "September", "Oktober", "November", "Disember");;
	this.initiate();
}

articleItem.prototype = {};

articleItem.prototype.initiate = function() {
		this.title = this.data.title.$t;
		var content = this.data.summary.$t;
		var categories = this.data.category;
		if (typeof(categories) != 'undefined') {
		var catSize = categories.length;
		} else {
		var catSize = 0;
		}
		var today = 0;
		var date = new Date(this.data.published.$t);
		var day = date.getDate();
        var month = date.getMonth();
        var year = date.getFullYear();
