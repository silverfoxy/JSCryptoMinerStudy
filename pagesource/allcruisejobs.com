<script type="text/javascript">
function equalHeight(group) {
	var tallest = 0;
	group.each(function() {
		var thisHeight = $(this).height();
		if(thisHeight > tallest) {
			tallest = thisHeight;
		}
	});
	if (tallest > 0) {
		group.height(tallest);
	}
}


$(document).ready(function() {
