<script>
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
     var data = JSON.parse(this.responseText);
     var stars = Math.round( (data["stargazers_count"] / 1000) * 10 ) / 10;
     var forks = Math.round( (data["forks_count"] / 1000) * 10 ) / 10;
     document.getElementsByClassName("star_count")[0].innerHTML = stars + "k";
     document.getElementsByClassName("star_count")[1].innerHTML = stars + "K";
     document.getElementsByClassName("fork_count")[0].innerHTML = forks + "K";
    }
  };
  xhttp.open("GET", "https://api.github.com/repos/RocketChat/Rocket.Chat", true);
  xhttp.send();
</script>

