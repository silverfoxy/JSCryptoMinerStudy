    function freeval(obj){
        var divn = $(obj).attr('name');
        var place = $(obj).attr('placeholder');  
        if ($(obj).val().length === 0 ){
            $(obj).css("border","1px solid #f00"); 
            $("#error"+divn).show().css("color","red").html(place+" can not be empty"); 
        }else{
            if(divn=="email"){
                if(!isEmail($(obj).val()) && $(obj).val().length > 0){
                    $("#error"+divn).show().css("color","red").html("Please input a valid email.");
                }
                if(isEmail($(obj).val()) || $(obj).val().length === 0){
                    $(obj).css("border","");   
                    $("#error"+divn).hide(); 
                }
            }else if($(obj).attr('name')=="number"){
                var mob = /^[1-9]{1}[0-9]{9}$/;
                if(mob.test($(obj).val()) == false){
                    $("#error"+divn).show().css("color","red").html("Please input a valid number.");
                }
                if(mob.test($(obj).val()) == true){
                    $(obj).css("border","");   
                    $("#error"+divn).hide();  
                }
            }else if(divn == "name"){
                if ($(obj).val().match(/^[a-zA-Z ]*$/) == null){
                    $("#error"+divn).show().css("color","red").html("Name should only have characters.");
                }
                if($(obj).val().match(/^[a-zA-Z ]*$/) != null){
                    $(obj).css("border","");   
                    $("#error"+divn).hide();                    
                }
            }else{
                $(obj).css("border","");   
                $("#error"+divn).hide();  
            }
        }  
    }
