$('#searchForm').on('submit', function () {
    var searchVal = $.trim($("#searchValue").val());
    if(searchVal == ""){
        $('input.search-bar').addClass('search-alert-msg');       
        $('.search-error-msg').show().text('Please enter Keyword');
        $('input.search-bar').removeAttr('placeholder','');
        setTimeout(function(){
             $('input.search-bar').attr('placeholder','Please enter Keyword');
         },2500);
        return false;
    } else if(searchVal.length < 3){
        $('.search-error-msg').show().text('Please enter atleast three character');
        return false;
    }  else {       
        return true;
    }
});

function showDropDown() {
    var URL_PREFIX = solrURL+"/select?indent=on&q=";
    var URL_SUFFIX = "&hl.fl=title&hl.q=&hl=on&hl.simple.pre=<strong>&hl.simple.post=</strong>&fl=title,id&sort=score desc&wt=json&rows=10";
    var contentFlag = 0;
    $("#searchValue").autocomplete({        
        source : function(request, response) {
            var contentHtml ='<div class="search-box board-box"><label>Suggested Search</label>';
            var searchVal = $("#searchValue").val();
            var searchStrFix = 'title:('+searchVal+')%20OR%20syllabus:('+searchVal+')%20OR%20service_name:('+searchVal+')%20OR%20leaf_rack_name:('+searchVal+')%20OR%20service_type:('+searchVal+')%20OR%20keywords:('+searchVal+')%20OR%20question:('+searchVal+')%20OR%20subject_name:('+searchVal+')%20OR%20board_name:('+searchVal+')';
            var URL = URL_PREFIX+'('+searchStrFix+')' + URL_SUFFIX+"&group=true&group.field=title&group.name=true&group.format=simple&fq=category:boardContainerContent";
            $.ajax({
                url : URL,
                dataType : 'jsonp',
                jsonp : 'json.wrf',
                success : function(data) {
                    var jsonData = data.grouped.title.doclist.docs;
                    var highlighting = data.highlighting;
                                    
                    response($.map(jsonData, function(value, key) {
                        var high_id = value.id;
                        if(value.hasOwnProperty("title") && value.title!="" && value.title!="NULL"){
                            var conRow = "";
                            $.each(highlighting, function (index, highData) {
                                if(highData.hasOwnProperty("title") && high_id == index){
                                    conRow = highData.title;
                                }
                            });
                            if(conRow != ""){
                                contentHtml += '<div style="cursor:pointer" onClick="searchFormSubmit(\''+value.title+'\');" class="item">'+conRow[0]+'</div>';
                                contentFlag = 1;
                            } else {
                                contentHtml += '<div style="cursor:pointer" onClick="searchFormSubmit(\''+value.title+'\');" class="item">'+value.title+'</div>';
                                contentFlag = 1;
                            }
                        }
                    }));
                    if(contentFlag == 1){
                        contentHtml += '</div>';
                    } else {
                        contentHtml ="";
                    }                                 
                    if(contentFlag > 0){
                        $('#search-item-list').html(contentHtml);
                        $('#search-item-list').slideDown();
                    } else {
//                        $('#search-item-list').slideUp();
                    }
                    contentFlag = 0;
                }
            });
        },
        minLength : 3
    });
}
$(document).mouseup(function(e) 
{
    var container = $("#searchForm");

    // if the target of the click isn't the container nor a descendant of the container
    if (!container.is(e.target) && container.has(e.target).length === 0) 
    {
        $('#search-item-list').slideUp();
        container.children('input').removeClass('search-bar-focus') ;
         $('.search-error-msg').hide();     
    }
});

function searchFormSubmit(searchValue){
    console.log(searchValue);
    $('#searchValue').val(searchValue);
    $('#searchForm').submit();
}

