function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;
        return true;
    }
    function fetchclasslist(value)
     { 
        var optionString = '<option value="">--Select Class--</option>'; 
        $("#classcustom").html('');
        $("#select2-chosen-2").html("Select Class");
        if(value){
            $('.cssload-loading').removeClass('hide'); // Add By Sanjay Verma for enabling css loader
            var url = "/lms/index/custom-containerlist/"+value;
                                url = "/weblpt/index/custom-containerlist/"+value;
                        $.ajax({
                type: 'POST',
                url:  url,
                dataType: 'json',
                success: function(data) {
                    var optionString = '<option value="">--Select Class--</option>';
                    $("#classcustom").html(optionString);

                    $.each(data.options, function(key, obj)
                    {
                        optionString += '<option value="'+obj.rack_id+'">'+obj.rack_name+'</option>';

                    })
                    $("#classcustom").html(optionString);
                    $('.cssload-loading').addClass('hide'); // Add By Sanjay Verma for disabling css loader
                }
              });
            }
