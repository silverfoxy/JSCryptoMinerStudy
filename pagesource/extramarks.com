<script type="text/javascript">
    $(function(){
        $('ul.study-board-listing li a').click(function(e){
                e.preventDefault(); 
                e.stopPropagation(); 
            
             $('ul.study-board-listing li a').removeClass('active');
            var boardId = $(this).attr('data-id');
            $(this).addClass('active');
            fetchclasslist(boardId,$(this).text().trim());
        });
        
        $(this).on('click','ul.class-list li a',function() {
            var selectedBoardId = $('ul.study-board-listing li a.active').attr('data-id');
            var selectedClassId = $(this).attr('data-id');
            if(selectedBoardId !='' && selectedClassId !='') {
              $.ajax({
                    type: 'POST',
                    url: '/website/index/set-class',
                    data: {classid: selectedClassId, boardid:selectedBoardId},
                    success: function(data) {
                        location.href =  '/website/index/dashboard/' + selectedClassId;
                    }
                });
            }
        });
    });
    function fetchclasslist(value,boardName)
     {

        var optionString = '<div class="text-center mt50"><img alt="loading" src="/images/loading.gif"/></div>'; 
        $("#study-class-listing").html(optionString);
        if(value){
            $.ajax({
                type: 'POST',
                url:  "/weblpt/index/custom-containerlist/"+value,
                dataType: 'json',
                success: function(data) {
                    if(boardName=='IIT JEE'){ 
                        var optionLoopF=0;
                        var optionString = '<div class="col-sm-12 iit-class-list">';
                    } else {
                        var optionString = '<div class="col-sm-9">';
                    }
                    
                    optionString += '<ul class="class-list">';
                    var totalOptions = data.options.length;
                    var halfOptions=0;
                    
                    if(boardName!='IIT JEE') {
                        if(totalOptions%2==0){
                            halfOptions=totalOptions/2;
                        } else {
                            halfOptions=(totalOptions+1)/2;
                        }
                        var optionLoopF=1;
                    }
                    
                    $.each(data.options, function(key, obj)
                    {
                        if(optionLoopF<=halfOptions){
                            optionString += '<li><a data-id="'+obj.rack_id+'" href="javascript:void(0)">'+obj.rack_name+'</a></li>';
                        }
                        optionLoopF++;
                    });      
                    optionString += '</ul>';	  
                    optionString += '</div>';
                    if(boardName=='IIT JEE') { 
                        optionString += '<div class="col-sm-12 iit-class-list">';
                    } else {
                        optionString += '<div class="col-sm-3">';
                    }
                    
                        optionString += '<ul class="class-list">';
                        if(boardName=='IIT JEE') {
                            var optionLoopS=0;
                        } else {
                            var optionLoopS=1;
                        }
                        $.each(data.options, function(key, obj)
                        {
                            if(optionLoopS>halfOptions){
                                optionString += '<li><a data-id="'+obj.rack_id+'" href="javascript:void(0)">'+obj.rack_name+'</a></li>';
                            }
                            optionLoopS++;
                        });
                         optionString += '</ul>';	  
                    optionString += '</div>';
                    $("#study-class-listing").html(optionString);


                }

              });
            }
     }
</script>

