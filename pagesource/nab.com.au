<div><div class="product-rates-fees section">







	

	
		<div class="bubblesColoured" style="display: none">
			</div>
		
		
            <div class="rates-and-fees">
				<ul class="">
					
					
						
						
							
								<li>
									<div class="inner">
										<p class="number"></p>
										<p class="description"></p>
									</div>
								</li>
							
								<li>
									<div class="inner">
										<p class="number"></p>
										<p class="description"></p>
									</div>
								</li>
							
						
					
				</ul>
			</div>
        

		<script>
        $(document).ready(function() {
            var bubblesColoured = $('.bubblesColoured').html();
            //elements which holds the html list
            var dataElement = $('.product-rates-fees .rates-and-fees');
            
            //Loop through each rate or fee and assign colour/symbol
            $(dataElement).each(function( index )  {
                var tokens = $(this).find('li');
                var numberOfTokens = tokens.length;            
                $(tokens).each(function( tokenIndex )  {
                    if($.trim(bubblesColoured)==="true" && numberOfTokens===2){
                          $(this).addClass('fill');
                    }else if (numberOfTokens==2 && tokenIndex==0){
                          $(this).addClass('fill');
                      }else if(numberOfTokens!=2 && tokenIndex<2){
                          $(this).addClass('fill');
                      }
                      var numberElement=''; 
                      var isFeeValue = false;
                      var isPercentageFeeValue = false;
                      determineValueType($(this), numberElement, isFeeValue, isPercentageFeeValue);
                });
            });
        });
        
        determineValueType = function(currentElement, numberElement, isFeeValue, isPercentageFeeValue) {
            numberElement = currentElement.find('.number');
            var numberValue = numberElement.text();
            var description = currentElement.find('.description').text();
			var overrideSign = currentElement.find('.overrideSign').text();
            //Check for the word fee in the description. If it exists, we are assuming we'll place a $ symbol in front of the number, otherwise it will be %p.a. The only exception being reload fee for the traveller card
            
            //Override for new Sings provided now.
            if(overrideSign !==undefined && overrideSign.trim().length>0){
            	if(overrideSign ==='none'){
            		numberElement.html(numberValue);
            	}else if(overrideSign ==='$'){
            		numberElement.html('$'+ numberValue);
            	}else if(overrideSign ==='c'){
            		numberElement.html(numberValue+ 'c');
            	}else if(overrideSign ==='%'){
            		numberElement.html(numberValue + '<span>%</span>');
            	}else{
            		numberElement.html(numberValue+'<span>%<br/>p.a.</span>');
            	} 
            }else{
            
	            if ((description.search('fee') > -1) || (description.search('charge') > -1)) {
	                if ((description.search('reload fee') == -1)) {
	                    isFeeValue = true;
	                }
	                else {
	                    isPercentageFeeValue = true;
	                }
	            }
	            
	            var percentageSymbolHTML;
	            
	            if (!isFeeValue && isPercentageFeeValue) {
	                percentageSymbolHTML = '<span>%</span>';
	            }
	            else if (!isFeeValue && !isPercentageFeeValue) {
	                percentageSymbolHTML = '<span>%<br/>p.a.</span>';
	            }
	            else {
	                percentageSymbolHTML = '';
	            } 
	            if(numberElement.text().indexOf('%')===-1 && numberElement.text().indexOf('$')===-1){
	                numberElement.html(((isFeeValue)?'$':'') + numberValue + percentageSymbolHTML);
	            }
            
            }
        }
        </script>

	





