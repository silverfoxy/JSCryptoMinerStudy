<script>
    $(function(){
        fromvalidate("#form1","#sub",feedbackResponse,Request);
        function feedbackResponse(responseText, statusText) {
            if (statusText = 'success'){
                alert('提交成功');
                window.location.reload();
            };
        };
    });
    $(function() {
        var isSelect = $('.index_opinion_popbox_select input:checked').length;
        $('.index_opinion_popbox_button').click(function () {
            if (!isSelect==1) {
                $('.index_opinion_popbox_div1 .tips').css('display','block');
            }else {
                $('.index_opinion_popbox_div1 .tips').css('display','none');
            };
        });
    });
    $('.index_opinion_popbox_select label').click(function () {
        $('.index_opinion_popbox_div1 .tips').html('');
    })
    function statusValidate() {
        setTimeout(function () {
            var ifFir = !$('.index_opinion_popbox_div2 textarea').val()=="",ifSec = $('.index_opinion_popbox_div3 .tips').css('display')=="none",ifThr = $('.index_opinion_popbox_div3 .errorinfo label').css('display')=="none"||!$('.index_opinion_popbox_div3 .errorinfo label').hasClass('invalid'),isSelect = $('.index_opinion_popbox_select input:checked').length;
            if (ifFir&&ifSec&&ifThr&&isSelect==1){
                $('.index_opinion_popbox_button').addClass('index_opinion_popbox_button_on');
                }else {
                    $('.index_opinion_popbox_button').removeClass('index_opinion_popbox_button_on');
                }
            }, 100)
        };
    $('#content').keyup(function() {
        var length=$(this).val().length;
        if (length<=140) {
            if (length==140) {
                $('.index_opinion_popbox_sum').addClass('index_opinion_popbox_sum_on');
                $('.index_opinion_popbox_sum span').text(length);
                $('.index_opinion_popbox_div2 .errorinfo').html('• 请勿超过140个字符');
                $('.index_opinion_popbox_div2 .errorinfo').prev().css('display','none');
            }else {
                $('.index_opinion_popbox_sum').removeClass('index_opinion_popbox_sum_on');
                $('.index_opinion_popbox_sum span').text(length);
                $('.index_opinion_popbox_div2 .errorinfo').html('&nbsp;');
                $('.index_opinion_popbox_div2 .errorinfo').prev().css('display','block');
            }
        }
    });
    //更换单选框复选框的样式
    $(function() {
        if($('.mbshop_radio').length>0){
            $('body').delegate('.mbshop_radio','click',function(){
                var select = $(this).children('input').prop('checked');
                var name= $(this).children('input').prop('name');
                if (select) {
                    $('.mbshop_radio').children("input[name="+name+"]").next('i').html('\ue609').removeClass('iconfontselect');
                    $(this).children('i').html('\ue603').addClass('iconfontselect');
                }
            }); 
        }
        $('.mbshop_checkbox').click(function () {
            var select = $(this).children('input').prop('checked');
            if (select) {
                $(this).children('i').html('\ue600').addClass('iconfontselect');
            }else {
                $(this).children('i').html('\ue602').removeClass('iconfontselect');
            }
        });
    });
