		adShowCommon("quick_search","searchWord");
		adShowCommon("head_top","text");
	}
	var jsonAdMark={};
	for(var i in adMark){
	 jsonAdMark[i]=adMark[i];
	}

	/*advert over*/
	$(function(){
		$('#input_textbox').focusin(function() {
			if ($(this).val()==$("#saveWord").val()) {
				$(this).val('');
			};
		}).focusout(function() {
			if ($(this).val()=='') {
				$(this).val($("#saveWord").val());
			};
		});
		/*将请输入关键字变为空*/
		$(".mbshop_searchContent .mbshop_searchLeft .iconfont").click(function() {
			$("form[name='quicksearch']").submit();
		});
		$("form[name='quicksearch']").submit(function(){
			var keyword=$('.mbshop_searchLeft #input_textbox');
			if(keyword.val().toString()==$("#saveWord").val()){
				if($("#saveWord").val()!="请输入关键词或商品编号"){
					$("[name=quicksearch]").attr('action',consts('index')+CMSURL);
					var a = "请输入关键词或商品编号";
				}else{
					$("[name=quicksearch]").attr('action',searchUrl);
					keyword.val('');
				}
			}else{
				$("[name=quicksearch]").attr('action',searchUrl);
			}
		});
	});
</script>
