    window.clypCampaigns.getFromAddressBar = function () {
        var queryString = window.location.search.toLowerCase();
        for (var i = 0; i < window.clypCampaigns.length; i++) {
            var campaign = window.clypCampaigns[i];
            if (campaign.queryString === queryString) {
                return campaign;
            }
        }
        return null;
    };

    function setCampaign() {
        var campaign = window.clypCampaigns.getFromAddressBar();
        if (!campaign) return;
        var data = campaign.campaignData;
        ga('set', 'campaignName', data.name);
        ga('set', 'campaignSource', data.source);
        ga('set', 'campaignMedium', data.medium);
    };

