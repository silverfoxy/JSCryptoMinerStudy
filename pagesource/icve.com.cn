    <script type="text/html" id="teaStudioList_html">
        {{if list.length > 0}}
        {{each list as item i}}
        <div class="course-view clefix">
            {{if item.cover.length > 0 }}
            <img src='{{item.cover}}' style="width: 110px; height: 65px" class="courseimg" />
            {{else}}
            <img src="/project/img/smile.jpg" style="width: 110px; height: 65px" class="courseimg" />
            {{/if}}
            <div class="course-name">
                <h3 title="{{item.title}}">{{subString(item.title,0,15)}}</h3>
                <a href="/project/welcome/welcome.html?PJId={{item.propertyId}}" class="enter-btn" target="_blank">点击进入</a>
            </div>
        </div>
        {{/each}}
        {{else}}
        暂无课程
        {{/if}}
    </script>
    <script type="text/html" id="collection_html">
        {{if list.length > 0}}
        {{each list as item i}}
        <div class="course-view clefix">
            {{if item.PreviewUrl_Cover.length > 0 }}
            <img src='{{item.PreviewUrl_Cover}}' style="width: 110px; height: 65px" class="courseimg" />
            {{else}}
            <img src="/studycenter/img/course.jpg" style="width: 110px; height: 65px" class="courseimg" />
            {{/if}}
            <div class="course-name">
                <h3 title="{{item.title}}">{{subString(item.title,1,15)}}</h3>
                <a href="{{item.linkUrl}}" class="enter-btn" target="_blank">点击进入</a>
            </div>
        </div>
        {{/each}}
        {{else}}
        暂无收藏
        {{/if}}
    </script>
