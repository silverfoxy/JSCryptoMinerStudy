<script type="text/javascript">
	$(document).ready(function() {
		$.fn.cycle.transitions.mohd_slide = function($cont, $slides, opts){
			opts.before.push(function(curr,next,opts) {
				$.fn.cycle.commonReset(curr,next,opts);
				opts.cssBefore.opacity = 0;
			});
			opts.fxFn = function(curr, next, opts){
				$(curr).addClass("waiting").removeClass("active");
				$(next).addClass("waiting").addClass("active");
				if ($(curr).find(".text").hasClass("open")) {
					$(curr).find(".text").animate({bottom: -216},400, function() {
						$(next).show().css({opacity:0}).animate({opacity:1},opts.speed);
						$new_left = $(next).find(".text").position().left;
						$("#home_slide_line").animate({left: $new_left},opts.speed,function() {
							$(curr).css({opacity:0});
							$(curr).removeClass("waiting");
							$(next).removeClass("waiting");
							if ($("#home_slide").hasClass("hover")) {
								$(next).find(".text").addClass("open").animate({bottom:0},400);
							}
						});
					});
				} else {
					$(next).show().css({opacity:0}).animate({opacity:1},opts.speed);
					$new_left = $(next).find(".text").position().left;
					$("#home_slide_line").animate({left: $new_left},opts.speed,function() {
						$(curr).css({opacity:0});
						$(curr).removeClass("waiting");
						$(next).removeClass("waiting");						
					});
				}
				opts.busy=0;
			}
		};
		$("#home_slide").cycle({
			fx:'mohd_slide',
			timeout:6000,
			next:$("#home_slide_next span"),
			prev:$("#home_slide_prev span"),
			pause:1,
			slideExpr:$(".slide"),
			speed:600
		});
		$("#home_slide").mouseenter(function() {
			$(this).addClass("hover");											 
			$slide_active = $(this).find(".slide.active");
			if (!$($slide_active).hasClass("waiting")) {
				$($slide_active).find(".text").addClass("open").animate({bottom:0},400);
			}
		}).mouseleave(function () {
			$(this).removeClass("hover");			
			$slide_active = $(this).find(".slide.active");
			if (!$($slide_active).hasClass("waiting")) {			
				$($slide_active).find(".text").removeClass("open").animate({bottom:-216},400);
			}
		});
		$("#home_slide_next,#home_slide_prev").hover(function() {
			$(this).addClass("hover");
		},function() {
			$(this).removeClass("hover");
		});
	});
</script>
