<script type="text/javascript">
	var LTIE9;
	function drawCurve() {
		if($("#background-image").length){
			initCurveColor();
			initCurve();
	      	}
	      	if($(".hp.column.two .ui-widget-header").length) {
	      		initCanvasHeader();
	      	}
	}
	function initCurve() {
		var cv = document.getElementById("background-curve");
		//IF IE
		if (typeof(G_vmlCanvasManager) != "undefined"){G_vmlCanvasManager.initElement(cv);}
		var ctx = cv.getContext('2d');
		
		var imageObj = new Image();
		imageObj.onload = function() {
			var pattern = ctx.createPattern(imageObj, 'repeat');
			ctx.save();
			ctx.beginPath();
			ctx.moveTo(0.0, 0.0);
			ctx.lineTo(0.0, 59.0);
			ctx.lineTo(1478.0, 59.0);
			ctx.lineTo(1478.0, 0.0);
			ctx.lineTo(1362.8, 0.0);
			ctx.bezierCurveTo(1362.8, 0.0, 776.4, 112.9, 128.2, 0.0);
			ctx.lineTo(0.0, 0.0);
			ctx.closePath();
			ctx.fillStyle = pattern;
			ctx.fill();
			ctx.restore();
		};
		imageObj.src = '/cms/lib/NC01000976/Centricity/Template/11/stripe.png';
	}
	function initCurveColor() {
		var curveColor = document.getElementById("background-curve-color");
		//IF IE
		if (typeof(G_vmlCanvasManager) != "undefined"){G_vmlCanvasManager.initElement(curveColor);}
		var context = curveColor.getContext('2d');
		draw(context);
	}
	function draw(context) {
		// layer1/Path
		context.save();
		context.beginPath();
		context.moveTo(0.0, 0.0);
		context.lineTo(0.0, 59.0);
		context.lineTo(1478.0, 59.0);
		context.lineTo(1478.0, 0.0);
		context.lineTo(1362.8, 0.0);
		context.bezierCurveTo(1362.8, 0.0, 776.4, 112.9, 128.2, 0.0);
		context.lineTo(0.0, 0.0);
		context.closePath();
		context.fillStyle = '#F7F7F7';
		context.fill();
		context.restore();
	}
	function initCanvasHeader() {
		var cheader = document.getElementById("canvas-header");
		//IF IE
		if (typeof(G_vmlCanvasManager) != "undefined"){G_vmlCanvasManager.initElement(cheader);}
		var h = cheader.getContext('2d');
		canvasHeader(h);
	}
	function canvasHeader(h) {
		// layer1/Path
		h.save();
		h.beginPath();
		h.moveTo(0.0, 0.0);
		h.lineTo(206.0, 0.0);
		h.lineTo(237.0, 7.0);
		h.lineTo(268.0, 0.0);
		h.lineTo(475.0, 0.0);
		h.lineTo(475.0, 33.0);
		h.lineTo(0.0, 33.0);
		h.lineTo(0.0, 0.0);
		h.closePath();
		h.fillStyle = '#8CB202';
		h.fill();
		h.restore();
    	}
</script>
