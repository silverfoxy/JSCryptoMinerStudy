        <script type="text/javascript">
          // When the window has finished loading create our google map below
          google.maps.event.addDomListener(window, 'load', init);
          function init() {
                                    var map_icon = 'https://bluewater.co.uk/sites/bluewater/files/images/footer/map-marker/pin_65.png';
            var icon_width = 65;
            var icon_height = 65;
            
            var map_x = 44;
            var map_y = 69;
            var address = '<strong>Bluewater Shopping Centre</strong><br /> Bluewater Shopping Centre, Greenhithe, Kent DA9 9ST<br /> e: <a href=\"mailto:BluewaterReception@incentive-fm.com</a>';

            // Define the Lattitude and Longitude for the marker location
            var lat = 51.439223;
            var lng = 0.271463;



            // Basic options for a simple Google Map
            // For more options see: https://developers.google.com/maps/documentation/javascript/reference#MapOptions
            var mapOptions = {
              // How zoomed in you want the map to start at (always required)
              zoom: 12,
              scrollwheel: false,
              disableDefaultUI: true,
              gestureHandling: 'cooperative',

              // The latitude and longitude to center the map (always required)
              center: new google.maps.LatLng(lat, lng),

                          };

            // Get the HTML DOM element that will contain your map
            // We are using a div with id="map" seen below in the <body>
            var mapElement = document.getElementById('footerMap');

            // Create the Google Map using out element and options defined above
            var footerMap = new google.maps.Map(mapElement, mapOptions);

            // Define the image to use for the map marker
            var theLatLng = new google.maps.LatLng(lat, lng);

            // Define the map marker characteristics
                        var footerMapMarker = new google.maps.Marker({
              position: theLatLng,
              map: footerMap,
              icon: new google.maps.MarkerImage(map_icon,
              null,null,null,
              //Set the size of the image. This assumes a @2x image that is scaled down
              new google.maps.Size((icon_width / 2),(icon_height / 2)),
              //Set the origin of the image
              new google.maps.Point(0,0),
              // //Set the pointer of the image
              new google.maps.Point(map_x,map_y)
              ),
              title: null,
            });
            
            // Following Lines are needed if you use the Info Window to display content when map marker is clicked
            var infowindow = new google.maps.InfoWindow({
              content: address
            });

            // Following line turns the marker, into a clickable button and when clicked, opens the info window
                        google.maps.event.addListener(footerMapMarker, 'click', function() {
              infowindow.open(footerMap, footerMapMarker);
            });
                        google.maps.event.addListener(footerMap, 'tilesloaded', function() {
                jQuery('#footerMap').find('img').attr('nopin','nopin');
            });
            google.maps.event.addListener(footerMap, 'bounds_changed', function() {
                jQuery('#footerMap').find('img').attr('nopin','nopin');
            });
          }
        </script>
