    function loadListComments(coupon_id,e,limit){
        $.ajax({
            type: 'get',
            url: "https://www.couponsplusdeals.com/comment/getComment",
            data: {coupon_id: coupon_id,limit:limit}
        }).done(function (data) {
            if (data.status == 'success') {
                var list_comments = '';
                for (var i = 0; i < data.data.length; i++) {
                    var $fullname, $via = '', $address = '', $type;
                    if (data.data[i].type) {
                        $type = '<i class="fa fa-thumbs-o-up"></i>';
                    }else $type = '<i class="fa fa-comment"></i>'
                    if (!data.data[i].fullname) {
                        $fullname = 'Anonymous';
                    }else $fullname = data.data[i].fullname;
                    if (data.data[i].via) {
                        $via = ' via ' + data.data[i].via;
                    }
                    if (data.data[i].cityCode) {
                        $address = ' in ' + data.data[i].cityCode + ',' + data.data[i].countrycode;
                    }
                    list_comments += '<div class="comment-item">'
                        + '<div class="comment-type">'+ $type +'</div>'
                        + '<div class="comment-detail">'
                        + '<div class="comment-content">' + data.data[i].content + '</div>'
                        + '<div class="comment-info"><i><span title="' + data.data[i].createdAt + '" class="timeago"></span> by ' + $fullname + $via + $address + '</i></div>'
                        + '</div></div>';
                }
                if (!limit) limit =20;
                if (data.data.length >= limit)
                    list_comments += '<div class="comment-show-more">'
                        + '<div class="table-cell"></div>'
                        + '<div class="table-cell">'
                        + '<a class="load-more-comment" cm-count="'+ data.data.length +'"><i class="fa"></i>Load more</a>'
                        + '</div></div>';
                $(e.currentTarget).find('.recent-comment-count').text(data.data.length);
                $(e.currentTarget).find('.coupon-list-comments').empty().append(list_comments);
                setTimeout(function(){
                    truncateSomething();
                    convertTimeAgo();
                }, 100);
            } else if (data.status == 'error') {
            }
        });
    }
