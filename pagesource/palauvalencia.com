	jQuery.ajax({
		type: "POST",
		url: "/wp-admin/admin-ajax.php", 
		data: {"action":"caleventos","fecha":fecha},
		success: function(msg){
			jQuery("#calendario-eventos").html(msg);
		},
		error: function(msg){
			console.log(msg.statusText);
		}
	});
}
function pintar_un_evento_calendario(fecha) {
	jQuery.ajax({
		type: "POST",
		url: "/wp-admin/admin-ajax.php", 
		data: {"action":"eventofecha","fecha":fecha},
		success: function(msg){
			jQuery("#evento-lista").html(msg);
			var position = jQuery("#fecha_"+fecha).position();
							jQuery("#evento-lista").css( "left",position.left);
				jQuery("#evento-lista").css( "top",position.top);
						
			jQuery("#evento-lista").show();
			jQuery("#evento-lista").css('visibility','visible');
		},
		error: function(msg){
			console.log(msg.statusText);
		}
	});
}
