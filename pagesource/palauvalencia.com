        if (numeroFoto_pm >= 6) {
            numeroFoto_pm = 1;
        }
        if (numeroFoto_pm <= 0) {
            numeroFoto_pm = 5;
        }
        foto_var = numeroFoto_pm;
        jQuery("#foto" + foto_var).fadeIn("slow");
        jQuery("#texto" + foto_var).fadeIn("slow");

    }
    jQuery(".nav-prev").click(function() {
