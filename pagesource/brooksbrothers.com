	font-size: 1.1vw;
}
.bm-slot .featContent {
	position: relative;
}
.bm-slot.promoCenter {
	text-align: center;
}
.bm-slot.fullWidth {
	clear: both;
}
.bm-slot.offSetBox.promoLeft {
	text-align: right;
}
.bm-slot.offSetBox.promoRight,
.bm-slot.offSetNoBox.promoRight,
.bm-slot.offSetNoBox.promoLeft {
	text-align: left;
}

.bm-slot.splitSides {
	text-align: center;
    line-height: 5em;
    display: table;
    margin: 0 auto;
}
@media only screen  and (max-width: 991px){
	/* MIDDLE HP GO TO FULL WIDTH AT 990, FONT INCREASE TO MATCH */
	.bm-slot {
		font-size: 1.4vw;
	}
}
@media only screen  and (max-width: 767px){
	.bm-slot {
		font-size: 2.6vw;
	}
	/* mobOutline - border + gutters */
	.bm-slot.mobOutline {
	    border: 2px solid #e0dfdb;
	    width: 95%;
	    margin: 0 auto 1em;
	}
	.bm-slot.offSetNoBox.promoRight,
	.bm-slot.offSetNoBox.promoLeft {
		text-align: center;
	}
}


/* Split Side Inner Structure */
.bm-slot.splitSides .splitLeft,
.bm-slot.splitSides .splitRight {
	display: inline-block;
    width: 49%;
    vertical-align: top;
}
.bm-slot.splitSides .splitLeft {
	text-align: center;
	display: table-cell;
	padding-right: 4em;
}
.bm-slot.splitSides .splitRight {
	text-align: left;
	display: table-cell;
	vertical-align: middle;
}
.bm-slot.splitSides .featImgCont {
	width: 34em;
	margin: 0 auto;
}
.bm-slot.splitSides .featImg {
	width: 100%;
}
@media only screen  and (max-width: 767px) {
	.bm-slot.splitSides .splitLeft,
	.bm-slot.splitSides .splitRight {
		display: block;
		width: 100%;
		padding-right: 0;
	}
	.bm-slot.splitSides .splitRight {
		text-align: left;
	}
}


/* content/promo-overlay */
.bm-slot .promoOverlay {
	position: absolute;
	width: 100%;
	padding: 3em 0 0;
}
/* Bottom aligment */
.bm-slot.promoBottom .promoOverlay {
	bottom: 5vw;
}
/* Top alignment */
.bm-slot.colItem.promoTop .promoOverlay {
	top: 1vw;
	padding: 0 0 0;
}
/* Category LP - mobPromoNoDrop - promoOverlay Vert Centered */
.bm-slot.offSetNoBox.promoVertCenter .promoOverlay,
.bm-slot.colItem.promoVertCenter .promoOverlay,
#main.decorator-categorylandingpage .bm-slot.mobPromoNoDrop.colItem.promoVertCenter .promoOverlay,
.bm-slot.fixedBox.promoVertCenter .promoOverlay {
    top: 50%;
    margin-top: -20%;
    padding: 0 0 0;
}


@media only screen  and (max-width: 483px){
/* promo-bottom alighment @483-mobile */
	.bm-slot.promoBottom .promoOverlay {
		bottom: 9vw;
	}
}
/* promoOverlay horizontal divider */
.bm-slot .promoOverlay .horizDivider {
	background: #666;
    height: 1px;
    width: 50%;
    margin: 1.5em auto;
}
.bm-slot .promoOverlay .horizDivider.navDivider.dashed {
	display: block;
	background: none;
	height: 1px;
	width: 100%;
	margin: 1.5em auto;
	border-top: 0.1em dashed #666;
}
.bm-slot .promoOverlay .featTitle .horizDivider.navDivider {
	margin: .3em auto 0;
}


/* Column Item (smaller element at tablet/desktop), bottom variation */
.bm-slot.colItem.promoBottom .promoOverlay {
	bottom: 1vw;
}
/* Off Set Box - Left Alignment  */
.bm-slot.offSetBox.promoLeft .promoOverlay {
	position: absolute;
	left: 0;
	top: 2em;
	bottom: 2em;
	width: 50%;
	z-index: 2;
	text-align: center;
	background: #c01b30;
}
/* Off Set Box - Right Alignment  */
.bm-slot.offSetBox.promoRight .promoOverlay {
    position: absolute;
    right: 0;
    top: 2em;
    bottom: 2em;
    width: 50%;
    z-index: 2;
    text-align: center;
}
/* Off Set Box @-MOBILE - Drop - overlay/inside - with image margin */
@media only screen  and (max-width: 767px) {
	.bm-slot.offSetBox.promoLeft.mobDropInside.insideOffset .promoOverlay, 
	.bm-slot.offSetBox.promoRight.mobDropInside.insideOffset .promoOverlay {
		width: 88%;
		position: relative;
		top: auto;
		bottom: auto;
		margin-top: -40%;
	}
}
/* Promo Center */
.bm-slot.offSetBox.promoCenter .promoOverlay {
	width: 50%;
	left: 50%;
	margin-left: -25%;
	background: #c01b30;
}
.bm-slot.offSetBox.promoCenter.promoFullHeight .promoOverlay {
	top: 0;
	bottom: 0;
}
@media only screen  and (max-width: 767px) {
	.bm-slot.offSetBox.promoCenter .promoOverlay {
		position: relative;
		left: auto;
		bottom: auto;
		width: 100%;
		margin: 0 auto 0;
	}
}



/* Off Set Box No Promo Box */
.bm-slot.offSetNoBox .promoOverlay {
	width: 50%;
}
/* right alignment */
.bm-slot.offSetNoBox.promoRight .promoOverlay {
	right: 0;
}
/* right alignment */
.bm-slot.offSetNoBox.promoLeft .promoOverlay {
	left: 0;
}
/* offSetNoBox - mobPromoNoDrop @mobile767 */
.bm-slot.offSetNoBox.mobPromoNoDrop .promoOverlay {
	width: 100%;
	margin-top: 0;
	right: auto;
	left: auto;
}
/* Category Landing Page - Col Item - Right Alighment */
#main.decorator-categorylandingpage .bm-slot.colItem.promoRight .promoOverlay {
	right: 1.5em;
	width: 50%;
}


/* In-set Box promoOverlay */
.bm-slot.insetBox.promoBottom .promoOverlay {
    bottom: 2em;
    left: 2em;
    width: calc(100% - 4em);
    /* height: 23em; */
    margin: 0 auto;
    padding: 4em 0;
    text-align: center;
}
.bm-slot.insetBox.promoTop .promoOverlay {
    top: 2em;
    left: 2em;
    width: calc(100% - 4em);
    /* height: 23em; */
    margin: 0 auto;
    padding: 4em 0;
    text-align: center;	
}
 @media only screen  and (max-width: 767px) {
	.bm-slot.insetBox.promoBottom .promoOverlay,
	.bm-slot.insetBox.promoTop .promoOverlay {
		padding: 2em 0 2em;
	}
} 
/* Split Sides promoOverlay variation */
.bm-slot.splitSides .promoOverlay {
	position: inherit;
	vertical-align: text-top;
}

@media only screen  and (max-width: 767px) {
	/* mobOutline promoOverlay - TRANSITION TO FULL STACK */
	.bm-slot.mobOutline .promoOverlay,
	.bm-slot.colItem.promoBottom .promoOverlay,
	#main.decorator-categorylandingpage .bm-slot.colItem.promoRight .promoOverlay {
		position: relative;
		overflow: hidden;
		width: 100%;
		top: auto;
		left: auto;
		bottom: auto;
		right: auto;
		margin: 0 auto 0;
		padding: 1em 0 2em;
		background: #fff;
		color: #001f37;
	}

	/* OFF SET BOX promoOverlay - TRANSITIONS TO FULL STACK */
	.bm-slot.offSetBox.promoLeft .promoOverlay,
	.bm-slot.offSetBox.promoRight .promoOverlay {
	    position: relative;
	    overflow: hidden;
	    width: 100%;
	    top: auto;
	    left: auto;
	    bottom: auto;
	    margin: 0 auto 0;
	    padding: 1em 0 2em;
	}
	/* InsetBox promoOverlay - TRANSITION TO FULL STACK */
	.bm-slot.insetBox.promoBottom .promoOverlay {
	    position: absolute;
	    left: auto;
	    right: auto;
	    bottom: 0;
	    width: 100%;
	}
	/* Promooverlay - No Drop Below on Mobile - stay on top of image */
	.bm-slot.mobOutline.mobPromoNoDrop .promoOverlay {
		position: absolute;
		background: none;
	}
	.bm-slot.offSetNoBox .promoOverlay {
		position: static;
		width: 100%;
		top: auto;
		left: auto;
		right: auto;
		bottom: auto;
		background: #fff;
		margin: 0 auto 0;
		padding: 1em 0 2em;
	}
	.bm-slot.offSetNoBox.mobPromoNoDrop .promoOverlay,
	#main.decorator-categorylandingpage .bm-slot.offSetNoBox.mobPromoNoDrop .promoOverlay {
		position: absolute;
		background: none;
	}

}

/* FeatTitle/h2 */
.bm-slot .featTitle {
	font-size: 5.2em;
	letter-spacing: .02em;
	line-height: 1.2em;
}
/* Category Landing Page size variation */
#main.decorator-categorylandingpage .bm-slot .featTitle {
	font-size: 3.4em;
}
@media only screen  and (max-width: 767px) {
	#main.decorator-categorylandingpage .bm-slot .featTitle {
		font-size: 3.8em;
	}
}
@media only screen  and (max-width: 483px) {
	#main.decorator-categorylandingpage .bm-slot .featTitle {
		font-size: 4em;
	}
}

/* two line variation */
.bm-slot .featTitle.twoLine {
	line-height: 1em;
}
.bm-slot .featTitle.twoLine span {
	display: block;
}
/* mobOutline - Dual featTitle handling - show/hide desk & @mobile483 */
.bm-slot.mobOutline .featTitle.deskHdrOverlay {
		display: block;
}
.bm-slot.mobOutline .featTitle.mobHdrOverlay {
		display: none;
}
/* FeatTitle Under-line - dashed */
.bm-slot .featTitle span.underDash {
	display: inline-block;
    border-bottom: 1px dashed #8e7743;
    padding-bottom: .3em;
}
/* Offset Box FeatTitle */
.bm-slot.offSetBox.redFleece .featTitle {
	font-size: 2.74em;
	margin-bottom: .2em;
}
/* splitSides variation */
.bm-slot.splitSides .promoOverlay .featTitle {
	display: inline-block;
	font-size: 4em;
}
@media only screen  and (max-width: 991px) {
	.bm-slot.splitSides .promoOverlay .featTitle {
		font-size: 3em;
	}	
}
@media only screen  and (max-width: 767px) {
	/* Split Sides FeatTitle/h2 variation */
	/* mobile switch to left alignment, inset/gutter via the width */
	.bm-slot.splitSides .promoOverlay .featTitle {
		display: block;
		width: 100%;
		margin-top: 0;
		margin-bottom: 0;
		margin-left: auto;
		margin-right: auto;
	}
	/* sets container around images so mobHdrOverlay can be positioned img-bottom */
	.bm-slot.mobOutline .mobOutlineImgCont {
		position: relative;
	}
	/* mobOutline - Dual featTitle handling - show/hide desk & @mobile483 */
	.bm-slot.mobOutline .featTitle.mobHdrOverlay {
		display: block;
	    position: absolute;
	    bottom: 3vw;
	    margin: 0 auto;
	    width: 100%;
	}
	.bm-slot.mobOutline .featTitle.deskHdrOverlay {
		display: none;
	}
}
@media only screen  and (max-width: 483px) {

	.bm-slot .featTitle span.underDash {
		border-bottom: none;
		padding-bottom: 0;
	}
}

/* Feature Pre Title / h4 element */
.bm-slot .featPreTitle {
    margin: 0 0 .2em;
    padding: 0;
    font-size: 1.4em;
    line-height: 1.2em;
}
@media only screen  and (max-width: 767px) {
	.bm-slot .featPreTitle {
	    font-size: 1.8em;
	    line-height: 1.4em;
	}
	/* mobile switch to left alignment, inset/gutter via the width */
	.bm-slot.splitSides .featPreTitle {
		width: 100%;
		margin-left: auto;
		margin-right: auto;
	}
}


/* Slot Text/FeatText */
.bm-slot .featText {
	display: inline-block;
	font-size: 1.6em;
	line-height: 1.6em;
	letter-spacing: .04em;
}
/* Category Landing Page size variation */
.bm-slot .featText {
	font-size: 1.4em;
	line-height: 1.4em;
}
@media only screen  and (max-width: 767px) {
	.bm-slot .featText {
	    font-size: 1.8em;
	    line-height: 1.4em;
	}
}

.bm-slot.wBoxNav .featText {
	margin: 0 0 2.2em;
}
.bm-slot.offSetBox.wBoxNav .featText {
	margin: 0 0 1em;
}
.bm-slot.wArrowNav .featText {
	width: auto; /* override of core */
	margin: 0 0 0;/* padding + border, not margin */
	padding-bottom: 1.1em;
	border-bottom: 1px dashed #fff;
}
#main.decorator-categorylandingpage .bm-slot.wArrowNav .featText {
	/* category landing page featText padding variation - more space above/below */
	padding-top: .5em;
	padding-bottom: 1.5em;
}
#main.decorator-categorylandingpage .bm-slot.mobPromoNoDrop.wArrowNav .featText {
	padding-top: 0;
	padding-bottom: 1.1em;
}
/* featText with Nav Divider variations (margin/padding on the divider, not P tag) */
.bm-slot.wNavDivide .featText {
	padding-bottom: 0;
	border-bottom: none;
}
.bm-slot.wArrowNav.wNavDivide .featText {
	border-bottom: none;
	padding-bottom: 0;
}
.bm-slot.wArrowNav.wNoNavDivide .featText {
	border-bottom: none;
    padding-bottom: 1.5em;
}
.bm-slot.wArrowNav.wNoNavDivide .featText,
#main.decorator-categorylandingpage .bm-slot.wArrowNav.wNoNavDivide .featText {
	border-bottom: none;
	padding-bottom: 0;
	margin-bottom: 0;
}

.bm-slot.splitSides.wArrowNav .featText {
	font-size: 1.2em;
	border-bottom-color: transparent;
	padding-bottom: 0;
}
@media only screen  and (max-width: 767px) {
	/* FeatText - arrow nav variation - @mobile, no dash border, remove padding */
	.bm-slot.wArrowNav .featText {
		border-bottom: none;
		padding-bottom: 0;
		margin-bottom: 1.2em;
	}
	/* category landing page - mobPromoNoDrop variation - less margin/padding below featText - no promodrop at mobile */
	#main.decorator-categorylandingpage .bm-slot.mobPromoNoDrop.wArrowNav .featText {
		margin-bottom: 0;
	}
	/* reaffirms dash border on featText @767-mobile */
	.bm-slot.splitSides.wArrowNav .featText {
		padding-bottom: 0;
		font-size: 1.4em;
		border-bottom: none;
	}
	.bm-slot.splitSides.wArrowNav .featText .mobileDrop {
		display: none;/* to hide/show toggle featText via markup class */
	}

}
@media only screen  and (max-width: 483px) {	
	/* Feature Text with Arrow Nav at @483-mobile - remove border & padding */
	.bm-slot.offSetBox.wArrowNav .featText {
		border-bottom: none;
		padding-bottom: 0;
	}
}


/* Nav/CTAs */
/* featNav / CTA List */
.bm-slot .featNav,
.gridBanner .gridNav {
	margin: 0 0 0;
	padding: 0;
}
/* Feat SubNav */
.bm-slot .featSubNav {
	margin: 0 0 0;
	padding: 0;
}
/* offSetBox - BoxNav - BlockNav - contrained width */
.bm-slot .featSubNav {
	margin: 0 0 0;
	padding: 0;
}
.bm-slot.offSetBox .featNav.boxNav.blockNav {
	width: 80%;
    margin: 0 auto;
}

@media only screen  and (max-width: 767px) {
	/* Split Side %mobile - featNav variation */
	.bm-slot.splitSides .featNav {
		display: block;
	}
	/* mobOutline - Nav gets dash-border-top @767-mobile */
	.bm-slot.wArrowNav.mobOutline .featNav {
		width: 85%;
		margin: 0 auto;
		border-top: 1px dashed #8e7743;
	}
	.bm-slot.offSetBox.wArrowNav.mobOutline .featNav.solidBorder {
		border-top: 1px solid #fff;
	}
	.bm-slot.wArrowNav.mobOutline.wNoNavDivide .featNav,
	.bm-slot.offSetBox.wArrowNav.mobOutline.wNoNavDivide .featNav.solidBorder {
		border-top: none;
	}

}
@media only screen  and (max-width: 483px) {
	/* BoxNav + InlineNav @483-mobile goes 85% wide, centered */
	.bm-slot .featNav.boxNav.inlineNav {
    	width: 85%;
    	margin: 0 auto;
	}
}

/* Sub Nav (view details abs, etc.) */
.bm-slot.wViewDeetsNav .featSubNav.posAbs {
	position: absolute;
}
.bm-slot.wViewDeetsNav .featSubNav.posAbs.botDeets {
	bottom: .5em;
}
.bm-slot.wViewDeetsNav .featSubNav.posAbs.rightDeets {
	right: .5em;
}

/* NavItem - LI */
/* block display nav LIs */
.bm-slot .featNav.blockNav .navItem {
	display: block;
	margin: 0 0 1.6em;
}
/* category landing page - mobNoDrop column item nav-item margin-bottom variation - less space */
#main.decorator-categorylandingpage .bm-slot.mobPromoNoDrop .featNav.blockNav .navItem {
	margin: 0 0 1.3em;
}
/* category landing - navItem-Last - no bottom margin */
#main.decorator-categorylandingpage .bm-slot.mobPromoNoDrop .featNav.blockNav .navItem.last {
	margin: 0 0 0;
}

/* Block nav Last Item closure - relies on ".last" LI markup classes */
.bm-slot .featNav.blockNav .navItem.last {
	margin-bottom: 0;
}
/* Inline Nav display LIs */
.bm-slot .featNav.inlineNav .navItem,
.bm-slot .featSubNav.viewDeetsCont .navItem {
	display: inline-block;
	margin: 0 .5em;
}
.bm-slot .featSubNav.posAbs.viewDeetsCont .navItem {
	margin: 0 0 0;
}

/* Inline nav Row Start/End closures - add to markup on LIs when intending row-start/end */
.bm-slot .featNav.inlineNav .navItem.rowStart,
.bm-slot .featSubNav.viewDeetsCont .navItem.rowStart,
.gridBanner .gridNav.inlineNav .gridNavItem.rowStart {
	margin-left: 0;
}
.bm-slot .featNav.inlineNav .navItem.rowEnd,
.bm-slot .featSubNav.viewDeetsCont .navItem.rowEnd,
.gridBanner .gridNav.inlineNav .gridNavItem.rowEnd {
	margin-right: 0;
}

@media only screen  and (max-width: 483px) {
	/* legacy mobile nav navItem */
	.bm-slot .mobileSlotNav .navItem {
		margin: 0 0 1em;
	}
	/* Box Nav variation - Inline Nav switches to Block @483-mobile */
	.bm-slot .featNav.inlineNav .navItem,
	.bm-slot.mobOutline .featNav.blockNav .navItem {
		display: block;
		margin: 0 auto 1em;
	}
	.bm-slot .featNav.boxNav.inlineNav .navItem.last,
	.bm-slot.mobOutline .featNav.blockNav .navItem.last {
		margin-bottom: 0;
	}
}

/* FeatLink/CTA A tags */
