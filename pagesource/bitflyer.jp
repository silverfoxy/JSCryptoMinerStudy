                LIST_BANNER = "list_banner",
                FOOTER = "footer",
                $as = $('body').find('a[href]');

            function getNewHref( $a ) {
                var href = $a.attr('href'),
                    url = href.split('#')[0],
                    hash = ( href.indexOf('#') > 0 ) ? "#" + href.split('#')[1]:"",
                    newHref = ( url.indexOf('?') > -1 ) ? url + "&" + TOP_LINK : url + "?" + TOP_LINK;
                if( $a.parents('#carousel-landing-banner').length > 0 ){
                    newHref += "&" + CAROUSEL_BANNER;
                } else if ($a.parents('#sideBanner').length > 0) {
                    newHref += "&" + SIDE_BANNER;
                } else if ($a.parents('#listBanner').length > 0) {
                    newHref += "&" + LIST_BANNER;
                } else if ($a.parents('#footerarea').length > 0) {
                    newHref += "&" + FOOTER;
                }
                return newHref + hash;
            }
            $as.each(function (_, a) {
                var $a = $(a),
                    href = $a.attr('href');

                if ( href !== "" && href.indexOf("#") !== 0 && href.toLowerCase().indexOf('javascript:') < 0) {
                    $a.attr('href', getNewHref($a));
                }
            })
        });
