<script type='text/javascript'>

    var lastTabContentSelected;
    var lastTabSelected;

    var categoriaEvento = "";

    //variabile per la dim delle select
    varWidthSelect = true;

    jQuery(document).ready(function () {
        lastTabSelected = jQuery('.text-box-toc li ')[0];
        //lastTabContentSelected = 'Tab-0';
        lastTabContentSelected = jQuery(".quick-quota-centrale").children().attr('id');

        quickQuotaUtility.keyDownComuniCasa();

        quickQuotaUtility.capChangeCasa();

        //sovrascrivo l'onclick delle label per prevenire l'azione di default che ci sarebbe senza questo
      //  jQuery("label[onclick='settaRadioByLabel(this);']").attr("onclick", "settaRadioByLabel(this);return false;");

        
        //per resettare il flag privacy se l'utente lo clicca quando è in errore
        jQuery(".cust_checkbox.checkbox").click(function () {
            if (jQuery(this).hasClass("cust_checkbox_on") && jQuery(this).hasClass("error")) {
                jQuery(this).removeClass("error");
            }
        });
        
    });


    function keyDownComuni()
    {
        jQuery("#cap[rel='capCasa']").keydown(function (e) {

            var cap = jQuery("#cap[rel='capCasa']").val();
            var codiceCompagnia = listParameters["Codice Compagnia"];
            var params = "Cap=#CAP#&Device=WEB&CodiceApplicazione=UL001P&Compagnia=" + codiceCompagnia;

            if (cap != undefined) {

                var code = parseInt(e.keyCode || e.which);
                if (code > 57) code = code - 48; //block num

                if (cap.length == 4 && (code >= 48 && code <= 57)) {
                    cap = cap + String.fromCharCode(code); //aggiungo quinto carattere appena premuto

                    params = params.replace("#CAP#", cap);

                    quickQuotaUtility.recuperaComuniCasa(cap, params);
                }
                else {
                    jQuery("#comuneCasa").empty();
                    jQuery("#comuneCasa").append("<option value='0'></option>");
                    jQuery("#comuneCasa").selectmenu("destroy").selectmenu({ style: 'dropdown' });
                }
