<script type="text/javascript">
    require([
        'jquery',
        'utility',
        'underscore',
        'hoverIntent',
        'drillDown',
        'jquery/ui'
    ], function($, utility, _) {
        'use strict';

        $('.mobile-menu-container').removeClass('loading');

        $.widget('mattblatt.mbmenu', {
            options: {},

            _create: function () {
                var self = this;

                self.$pageHeader = $('.page-header');
                self.$mainContent = $('.column.main');
                self.$mobileMenuTrigger = $('.mobile-menu-container');
                self.$nav = $('.drilldown.navigation');
                self.container = self.element.closest('.nav-sections');

                self._prepareMenu();
                self._desktopHoverMenu();
                self._initStickyMenu();
                self._mobileDrillDownMenu();
                self._bindEvents();
                self._onResizeHeaderEvent();
                self._navHeight();

                //IOS Safari related issue
                if(utility.isMobileSafari) {
                    $('html').addClass('mobile-safari');
                    $('html').addClass('addressbar-expanded');
                }

                if(!utility.isDesktop()) {
                    $('.page-header').addClass('sticky-element');
                    self.$nav.addClass('sticky-element');
                }
            },

            _bindEvents: function () {
                var self = this;

                $(window).on('subscriptionPopupOpen', function () {
                    $('.sidebar-main, .page-footer').addClass('subscription-open');
                });
                $(window).on('subscriptionPopupClose', function () {
                    $('.sidebar-main, .page-footer').removeClass('subscription-open');
                });
                $(window).on('breakpoint-change', function () {
                    self._mobileDrillDownMenu();
                    self._desktopHoverMenu();
                });
                $(window).resize(function() {
                    self._navHeight();
                });

                $(window).data('iHeight', window.innerHeight).on('resize', _.throttle(self._onResizeHeaderEvent, 100, {leading: true, trailing: true}));


                self.$mobileMenuTrigger.on('click', function () {
                    var $this = jQuery(this);

                    if($this.hasClass('active')) {
                        jQuery('html').removeClass('mobile-menu-open');
                        jQuery('body').scrollTop($this.data('curPosY'));
                        jQuery(window).trigger('closed.mobileMenu.MB');
                        setTimeout(function () {
                            //reset to initial state
                            self.$nav.drilldown('reset');
                        }, 400);
                    }else{
                        $this.data('curPosY', jQuery('body').scrollTop());
                        jQuery('html').addClass('mobile-menu-open mobile-menu-opening');
                        setTimeout(function () {
                            jQuery('html').removeClass('mobile-menu-opening');
                        }, 200);
                        jQuery(window).trigger('opened.mobileMenu.MB');

                    }

                    if(!utility.isDesktop()) {
                        $this.toggleClass('active');
                        self.$nav.toggleClass('active');
                    }
                });

            },

            _initStickyMenu: function () {
                var self = this;
                var lastScrollTop = 0;
                $(window).on('scroll', _.throttle(function(){
                    var st = $(this).scrollTop();
                    if (st > lastScrollTop && st > 122){
                        $('.page-header').removeClass('sticky-element');
                        self.$nav.removeClass('sticky-element');
                    } else {
                        $('.page-header').addClass('sticky-element');
                        self.$nav.addClass('sticky-element');
                    }
                    lastScrollTop = st;
                }, 100, {leading: true, trailing: true})).trigger('scroll');
            },

            _sticky_onScroll: function () {

            },

            _onResizeHeaderEvent: function () {
                if(!utility.isMobileSafari) {
                    return false;
                }
                var _ih = window.innerHeight;
                var curHeight = $(window).data('iHeight');
                if(_ih < curHeight) {
                    $('html').addClass('addressbar-expanded');
                }else if(_ih){
                    $('html').removeClass('addressbar-expanded');
                }

                $(this).data('iHeight', _ih);
            },

            _desktopHoverMenu: function () {
                var self = this;

                if(utility.isTouch() && !utility.isDesktop()) {
                    //click event
                    self.element.find('.level0.parent > a').off('click.mainNav.submenu').on('click.mainNav.submenu', function (e) {
                        e.preventDefault();
                        if($(this).parent().hasClass('hover')) {
                            self._hideSubmenu.call($(this).parent());
                        }else{
                            self._showSubmenu.call($(this).parent());
                        }
                    });
                }else{
                    self.element.hoverIntent({
                        interval: 50,
                        timeout: 50,
                        over: self._showSubmenu,
                        out: self._hideSubmenu,
                        selector: 'li.level0.parent'
                    });
                }
            },

            _showSubmenu: function () {
                if(utility.isDesktop()) {
                    $(this).toggleClass('hover');
                }
            },

            _hideSubmenu: function () {
                if(utility.isDesktop()) {
                    $(this).toggleClass('hover');
                }
            },

            _mobileDrillDownMenu: function () {
                var self = this;

                if(utility.isDesktop()) {
                    //remove drilldown menu
                    if(self.$nav.data('mobileMenu') == 1) {
                        self.$nav.data('mobileMenu', 0).drilldown('destroy');
                    }

                    self.$nav.removeClass('active');
                    self.$mobileMenuTrigger.removeClass('active');
                }else{
                    //apply drilldown menu
                    self.$nav.data('mobileMenu', 1).drilldown({
                        speed: 300,
                        cssClass: {
                            container: 'drilldown-container',
                            root: 'drilldown-root',
                            sub: 'submenu',
                            back: 'drilldown-back'
                        }
                    });
                }
            },

            _prepareMenu: function () {
                var self = this;

                /* Rename 'View All' to more relevant text.*/
                self.element.find('.parent').each(function () {
                    var $this = $(this);
                    var editedText = "All " + $this.children('a').text().trim();
                    var submenuShowAllLinkElem = $this.children('.submenu').find('.submenu-show-all-link a');
                    var submenuShowAllLinkElemLink = submenuShowAllLinkElem.attr('href');

                    //mobile go back button
                    $this.children('.submenu').find('.submenu-name').before('<div class="drilldown-back"><a>Back</a></div>');

                    //remove submenu section if it doesn't have any sub links
                    if($.trim($this.children('.submenu').find('.submenu-custom-links').text()) === '') {
                        $this.children('.submenu').remove();
                        $this.removeClass('parent');
                    }
                });
            },

            _navHeight: function() {
                if(!utility.isDesktop()) {
                    var mheight = $(window).height();
                    $(".navigation").height(mheight);
                    $(".drilldown-root").height(mheight - 169);
                    $(".submenu-custom-links").height(mheight - 273);
                }else{
                    $(".navigation").height('');
                    $(".drilldown-root").height('');
                    $(".submenu-custom-links").height('');
                }
            }
        });

        $('.drilldown-root').mbmenu();
    });
</script>
