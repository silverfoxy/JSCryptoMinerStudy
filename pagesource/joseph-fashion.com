<script language="javascript">

  function desktopToMobileReorder() {
      $('.home-slots-slider').addClass('mobile-view');
      $(".desktop-only").parent('.grid').hide();
      $('.show-both-sections').each(function(){
        var grids = $(this).find(".grid");
        var newSlide = grids[grids.length - 1];
        var showOnlyOneSlide = false;
        for(var i=0;i<grids.length;i++){
          var grid = grids[i];
          if($(grid).hasClass("desktop-only")) {
            showOnlyOneSlide = true;
          }
          if($(grid).find('.drop-me').length > 0) {
            newSlide = $(grid);
          }
        }
        var content = $(this).find('.absolute-center-text');
        var extraClass = "";
        if(content > 0){
          extraClass="fp-auto-height";
        }

        if(!$(newSlide).hasClass("desktop-only") && !showOnlyOneSlide){
          console.log($(newSlide).hasClass("desktop-only")+' && '+showOnlyOneSlide);
          $(newSlide).insertAfter($(this)).wrap( "<div class='section "+extraClass+" hibrid-content-slot'></div>");
        }
        
        if(content.length > 0){
          $(content[0]).insertAfter($(this)).wrap( "<div class='section fp-auto-height hibrid-content-text-slot'></div>");
        }
        
      });
    }
     function desktopToMobileFooterReorder() {
      $('footer').appendTo($('.home-slots-slider')).wrap('<div class="section fp-auto-height mobile-footer"></div>');
    }
    function adjustColoring() {
      if($(window).width() <= 768){
        if($('body').hasClass('right-content-dark') && $('body').hasClass('left-content-dark')) {
           console.log("here comesss");
            $('body').removeClass('right-content-dark');
        }
      }
    }
    function initCarousel() {
      
      $('.home-slots-slider').fullpage({
        responsiveSlides: true,
      navigation: true,
      verticalCentered: false,
      sectionSelector: '.section',
      afterLoad: function(anchorLink, index){

          // hide down arrow on last item of carousel
          if (index >= $('.section').length-1) {
            $('.home-slider-next').addClass('hide');
          } else {
            $('.home-slider-next').removeClass('hide');
          }
          setTimeout(function(){ window.scrollTo(0, 1);},2000);
          currentHomeSlide = index;
        }
      });
    }
 function defer() {
    if (window.jQuery) {
      
        if($(window).width() == 768){
          var k = setInterval(function(){

            if ( $( 'html' ).hasClass( 'fp-enabled' ) ) {
                $.fn.fullpage.destroy('all');
                desktopToMobileReorder();
                desktopToMobileFooterReorder();
                setTimeout( function(){ 
                  initCarousel();
                  clearInterval(k);
                  
                 if (window.$) {
                  $(".create-slick-slider").each(function(elm){
                    $(this).slick({
                      slidesToShow: 1,
                      slidesToScroll: 1,
                      autoplay: true,
                      autoplaySpeed: 3000,
                      arrows: false,
                      vertical: false,
                      dots: false,
                      fade: true,
                      speed: 3000,
                          nextArrow: null,
                          prevArrow:null
                    });
                  });
                  }
                },1000);
            }
            
          },4000);
        }
    } else {
        setTimeout(function() { defer(); console.log("test") }, 50);
    }
}
defer();
