  <script type="text/javascript">

var pwdByUser = false;
var loginParsleyForm;

$(document).ready(function(){
        loginParsleyForm = $("#login_form").parsley();

		$("#password").keypress(function(evnt) {
          		if(evnt.keyCode != 13){
                      pwdByUser = true;
                }
        });

        $("#password").bind('paste', function() {
               pwdByUser = true;
        });
});

var pwdAutofill_Diag = $("#pwdautofill_dialog").dialog({
	  dialogClass: "no-close",
	  autoOpen: false,
	  modal: true,
	  width: 500,
	  buttons: [{
        	id:"btn-continue",
        	text: "Continue",
        	click: function() {
            	/*Forcefull for submit*/
			    pwdByUser = true;
		    	$("#login_form").submit();
        	}
    	}, 
    	{
    		id:"btn-cancel",
        	text: "Cancel",
        	click: function() {
            	pwdAutofill_Diag.dialog("close");
		    	$("#password").select().focus();
        	}
      }],
      show: 'clip',
      open: function( event, ui ) { $(this).parent().focus();  }
});

function loginFormSubmit() {
	  var noErrors = loginParsleyForm.validate();
	  /*If no errors proceed with other validation submit*/
	  if (noErrors) {
		   	if (pwdByUser) {
		    	/* Continue with form submit */
		    	return true;
		   } else {
	    		pwdAutofill_Diag.dialog("open");
	    		return false;
	   	   }
	  } else {
	   		/*Allow parsley to do the validation*/
	   		return true;
 	  }
}

</script>

