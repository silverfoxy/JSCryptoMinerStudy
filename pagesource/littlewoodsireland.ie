border-radius: 0;
padding: 12px 18px;
}
.header-searchResults {
border-color: #5B5B5B;
border-width: 2px;
left: 0;
top: 40px;
width: 368px;
}
.header-searchResults > li > a {
padding: 9px 32px;
}
/* Header - Basket */
.header-basket {
background: url('https://www.littlewoodsireland.ie/assets/static/theme/images/header/header-sprite.png') 1px -60px no-repeat;
top: 59px;
}
.header-basketIcon {
background-color: #5B5B5B;
left: 19px;
top: 4px;
}
.header-basketLabel {
padding: 10px 0 10px 48px;
}
.header-searchTip {
background-color: #F2F2F2;
border: 1px solid #CCC;
-webkit-border-radius: 6px;
-moz-border-radius: 6px;
-o-border-radius: 6px;
border-radius: 6px;
padding: 5px 7px 5px 7px;
right: 0;
top: -32px;
}
#tcDialog {
clear: both;
}
