<script>
  $(document).ready(function() {
    if (location.hash === '#toyota-popup') {
      displayCarModel(25);
    } else if (location.hash === '#daihatsu-popup') {
      displayCarModel(5);
    } else if (location.hash === '#honda-popup') {
      displayCarModel(9);
    } else if (location.hash === '#suzuki-popup') {
      displayCarModel(23);
    } else if (location.hash === '#mitsubishi-popup') {
      displayCarModel(18);
    } else if (location.hash === '#nissan-popup') {
      displayCarModel(19);
    }
  });
</script>

<script>
  function displayCarModel(merk_id) {
    // $("#carmodel").modal({ show: true });
    $.ajax({
      url: "/front/ajax_data_model//"+merk_id,
      cache: false,
      success: function(msg){
        $("#data-model").html(msg);
        
        if (merk_id == 25) {
          $("#merk-title").text('Model Mobil Toyota');
        } else if (merk_id == 5) {
          $("#merk-title").text('Model Mobil Daihatsu');
        } else if (merk_id == 9) {
          $("#merk-title").text('Model Mobil Honda');
        } else if (merk_id == 23) {
          $("#merk-title").text('Model Mobil Suzuki');
        } else if (merk_id == 18) {
          $("#merk-title").text('Model Mobil Mitsubishi');
        } else if (merk_id == 19) {
          $("#merk-title").text('Model Mobil Nissan');
        }

        $("#carmodel").modal({ show: true });
      },
      "statusCode": {
        403: function() {
        alert('403 Error Server Side occured');
        },
        500: function() {
        alert('500 Error Server Side occured');
        }
      }
    });
  }
</script>

