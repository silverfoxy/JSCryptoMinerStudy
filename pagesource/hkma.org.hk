      $li.eq(_index).css('z-index', 2).siblings().css('opacity', 0).css('display', 'none');
      // èªç¡¬ã·çä¹±
      function autoClickNext() {
        _index = (_index + 1) % _len;
        show();
        timer = setTimeout(autoClickNext, _fadeSpeed + _speed);
      };
      // æ·¡å¥æ·¡åºåç
      function show() {
        $li.eq(_index).animate({opacity: 1}, _fadeSpeed).siblings(':visible').animate({opacity: 0}, _fadeSpeed);
        $li.eq(_index).css('z-index', 2).siblings().css('z-index', 1);
        $li.eq(_index).css('display', 'block');
        $li.eq(_index).siblings().css('display', 'none');
      };
      $this.mouseout( function () {
        timer = setTimeout(autoClickNext, _fadeSpeed + _speed);
      });
      $this.mouseover( function () {
        clearTimeout(timer);
      });
      timer = setTimeout(autoClickNext, _fadeSpeed + _speed);
