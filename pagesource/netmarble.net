									if (i < 8 ){
										jQuery('#aPopular'+i).show();
										jQuery('#sPopular'+i).hide();
									}
									else{
										jQuery('#aPopular'+i).hide();
										jQuery('#sPopular'+i).hide();
									}
								}
								else if(pageNo > lastPopPage-3){
									if (i > lastPopPage-7 ){
										jQuery('#aPopular'+i).show();
										jQuery('#sPopular'+i).hide();
									}
									else{
										jQuery('#aPopular'+i).hide();
										jQuery('#sPopular'+i).hide();
									}
								}
								else{
									jQuery('#aPopular'+i).hide();
									jQuery('#sPopular'+i).hide();
								}							
							}
							else{
								jQuery('#aPopular'+i).show();
								jQuery('#sPopular'+i).hide();
							}
						}
					}	
				}
				else{
					for( var i = 1 ; i < lastPopPage+1 ; i++ ){
						if ( pageNo == i ){
							jQuery('#aPopular'+pageNo).hide();
							jQuery('#sPopular'+pageNo).show();
						}
						else{
							jQuery('#aPopular'+i).show();
							jQuery('#sPopular'+i).hide();
						}
					}
				}
			}
			
			function displayPageNew(pageNo){
					
				for( var i = 1 ; i < lastNewPage+1 ; i++ ){
					if ( pageNo == i ){
						jQuery('#aNew'+pageNo).hide();
						jQuery('#sNew'+pageNo).show();
					}
					else{
						jQuery('#aNew'+i).show();
						jQuery('#sNew'+i).hide();
					}
				}
			}

