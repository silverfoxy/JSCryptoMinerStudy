function clickAppCr(gamecode, isRun, isHome){
	if(gamecode == null) return;
	
	gamecode = $.trim(gamecode);
	
	if(gamecode == "") return;
	
	try
	{
		if (__clickrd_enabled__)
		{
		    var clink, ifrm;
		    if(isRun){
		    	if(isHome) {
		    		clink = "http://nrd.netmarble.net/home_apprun_" + gamecode + ".cr";
		    	} else {
		    		clink = "http://nrd.netmarble.net/online_apprun_" + gamecode + ".cr";
		    	}
		    }else{
		    	if(isHome) {
		    		clink = "http://nrd.netmarble.net/home_appdown_" + gamecode + ".cr";
		    	} else {
		    		clink = "http://nrd.netmarble.net/online_appdown_" + gamecode + ".cr";
		    	}
		    }
		    
            ifrm = document.getElementById("DIRECT_GO_CLICK_RD");
            ifrm.src = clink;
            return true;
		}
	}
	catch(x)
	{
	}
	
	return true;
}

