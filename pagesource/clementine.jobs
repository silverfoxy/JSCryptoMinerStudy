	    var winTop =jQuery(window).scrollTop();
	    if(winTop >= 30){
	      jQuery('body').addClass('sticky');
	      jQuery('header').addClass('sticky');
	    }else{
	      jQuery('body').removeClass('sticky');
	      jQuery('header').removeClass('sticky');
	    }
	  });
	});
</script>	
