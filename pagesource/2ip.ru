	<script>
		(function (d) {
			'use strict';

			var ipElem = d.getElementById('d_clip_button');
			ipElem.style.cursor = 'pointer';
			var ipClipboard = new Clipboard(ipElem, {
				text: function () {
					return '54.166.146.211'
				}
			});
			ipClipboard.on('success', function(e) {
				d.getElementById('CopyBlock').style.display = 'block';
				setTimeout('hideEffectBlock(' + 'CopyBlock' + ');', 1000);
			});
			ipClipboard.on('error', function(e) {
				ipElem.style.cursor = 'text';
				console.error('Action:', e.action);
				console.error('Trigger:', e.trigger);
			});

			var infoElem = d.getElementById('btn-copy-info');
			infoElem.style.display = 'block';
			var infoClipboard = new Clipboard(infoElem, {
				text: function () {
					var i, copyInfoText = '', copyInfoDetails = d.querySelectorAll('.copy-info-details');
					for (i=0; i<copyInfoDetails.length; i++) {
						copyInfoText += copyInfoDetails[i].innerText + "\n";
					}
					return 'IP адрес: 54.166.146.211' + "\n" + copyInfoText + '© 2ip.ru' + "\n"
				}
			});
			infoClipboard.on('success', function(e) {
				d.getElementById('CopyInfoBlock').style.display = 'block';
				setTimeout('hideEffectBlock(' + 'CopyInfoBlock' + ');', 1000);
			});
			infoClipboard.on('error', function(e) {
				d.getElementById('ip-info-entry__arrow').style.display = 'none';
				console.error('Action:', e.action);
				console.error('Trigger:', e.trigger);
			});
		})(document);
	</script>
