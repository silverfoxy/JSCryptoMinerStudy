	<script type="text/template" data-sv-liItems>{{~setVar("template", "comon_custom_li_items")}}
{{~exec(function() {
	// svdm: capitalize the name for analytics purposes.
	data.seoAction = data.name.charAt(0).toUpperCase() + data.name.slice(1);
})}}

{{+field_link_or_label}}
