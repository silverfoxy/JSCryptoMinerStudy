	<script type="text/template" data-qv-template-listing>
		
		
			{{#data}}
				{{:~equal(global.type, "listing")}}
					{{+dmTrackingString}}  data-gtm-vars='{"tClient": {"eventCategory": "Listings", "eventAction": "Link to Detail", "eventLabel": "{{%%title}}", "recId": "{{recid}}", "et": "{{%%title}}", "vt": "Quick View", "ua": "Click"}}'  {{/}}
				{{?:~equal(global.type, "event")}}
					{{+dmTrackingString}}  data-gtm-vars='{"tClient": {"eventCategory": "Events", "eventAction": "Link to Detail", "eventLabel": "{{%%title}}", "recId": "{{recid}}", "et": "{{%%title}}", "vt": "Quick View", "ua": "Click"}}'  {{/}}
				{{?:~equal(global.type, "offer")}}
					{{+dmTrackingString}}  data-gtm-vars='{"tClient": {"eventCategory": "Coupons", "eventAction": "Link to Detail", "eventLabel": "{{%%title}}", "recId": "{{recid}}", "et": "{{%%title}}", "vt": "Quick View", "ua": "Click"}}'  {{/}}
				{{/}}
				{{~setVar("cityStateZip", helpers.plugins.stringLib.formatCityStateZip({ city : data.city, state : data.state, zip : data.zip }))}}
				{{~setVar("addressLine", [data.address1, data.address2, data.address3, helpers.var.cityStateZip].filter(function(val) { return val !== undefined }).join(", "))}}
				
				{{~setVar('image_settings', { height : 251, width : 380, crop : 'fill', quality : 50 })}}
				{{~setVar( data.type != undefined ? helpers.var.dateLabel = 'Valid' : helpers.var.dateLabel = 'Date' )}}
				{{~setVar("template", "common_custom_quickview_" + global.type)}}
				
				{{~setVar("fields", [
					{ name : "date", label : helpers.var.dateLabel, value : data.nextDate || data.startDate},
					{ name : "host", label : "Presented By", value : data.hostname || data.hostTitle, extra : { url : data.hostUrl }, valueTemplate : "link_or_label"},
					{ name : "location", label : "Location", value : data.location || data.listingTitle, extra : { url : data.listingUrl }, valueTemplate : "link_or_label"},
					{ name : "address", label : "", value : helpers.var.addressLine, valueTemplate : "qvaddress", liClass : "address"},
					{ name : "phone", label : "Phone", value : data.phone, extra : { ext : data.phoneext }, valueTemplate : "phone", liClass : "phone" },
					{ name : "tollfree", label : "Toll-free", value : data.tollfree, extra : { ext : data.phoneext }, valueTemplate : "phone", liClass : "phone" },
					{ name : "fax", label : "Fax", value : data.fax, extra : { ext : data.phoneext }, valueTemplate : "phone", liClass : "phone" },
					{ name : "email", label : "", value : "Send Email", extra : { url : data.email }, valueTemplate : "link_or_label" },
					{ name : "weburl", label : "", value : "Visit Website", extra : { url : data.weburl }, valueTemplate : "link_or_label" }
				])}}
				<div class="qv-content" {{>dmTrackingString}}>
					<div class="content-head clearfix">
						<div class="qv-header">
							Quick view
						</div>
						<div class="close-btn" data-qv-close data-gtm-click='{ "onclick" : true }' data-gtm-vars='{ "tCore" : { "action" : "{{~var.template}}_close", "label" : "Close Button" } }'>
							<img src="/includes/public/assets/images/close-icon.png" alt="">
						</div>
					</div>
					<div class="content listing">
						<div class="img">
							<a href="{{url}}" data-gtm-click='{ "follow" : true }' data-gtm-vars='{"tClient": {"ot": "Image"}, "tCore" : { "action" : "{{~var.template}}_detail_image" } }'>
								{{:image}}
									<img src="{{image.getThumbnailUrl(helpers.var.image_settings)}}" onerror="this.src='/includes/public/assets/shared/placeholder-large.jpg';this.onerror=null;" class="thumb">
								{{/}}
								{{!image}}
									<img src="/includes/public/assets/shared/placeholder-large.jpg" class="thumb">
								{{/}}
							</a>
							{{:title}}
								<div class="title">
									<a href="{{url}}" data-gtm-click='{ "follow" : true }' data-gtm-vars='{"tClient": {"ot": "Text"}, "tCore" : { "action" : "{{~var.template}}_detail_title" } }'>
											{{title}}
									</a>
								</div>
							{{/title}}
						</div>
						{{:tripadvisor}}
							<div class="ta-rating">
								{{#tripadvisor}}
									<a href="{{taurl}}" target="_blank" data-gtm-click='{ "follow" : true }' data-gtm-vars='{"tClient": {"eventAction": "Link to TripAdvisor"}, "tCore" : { "action" : "{{~var.template}}_ta_link" } }'>
										<img src="{{ratingimage}}">
										<p>{{rankstring}}</p>
									</a>
								{{/tripadvisor}}
							</div>
						{{/tripadvisor}}
						<ul class="no-bullet">
						   {{#~var.fields}}
								{{~fill(helpers.partial("custom_li_items"), data, {}, global)}}
							{{/}}
						</ul>
						{{:social}}
							<div class="social-icons">
								{{~setVar("fields", [
									{
										name : "social",
										label : "",
										value : (function() {
											if (data.social === undefined) { return undefined; }
											
											var socialMap = {
												"serviceid_4" : { class : "fa fa-facebook", service : "Facebook", url : "" },
												"serviceid_1" : { class : "fa fa-twitter", service : "Twitter", url : "" },
												"serviceid_2" : { class : "fa fa-youtube-play", service : "Youtube", url : "" },
												"serviceid_13" : { class : "fa fa-instagram", service : "Instagram", url : "" },
												"serviceid_13" : { class : "fa fa-instagram", service : "Instagram", url : "" }
											};
											
											data.social.forEach(function(val) {
												var item = socialMap[`serviceid_${val.smserviceid}`];
												if (item === undefined) { return; }
												item.url = val.value;
											});
											
											return Object.values(socialMap).filter(val => val.url.length > 0).map(
												val => `<a class="icon" data-sv-social data-sv-service="${val.service}" href="${val.url}" target="_blank" data-gtm-click='{ "follow" : true }' data-gtm-vars='{"tClient": {"eventAction": "Link to ${val.service}", "ot": "Social", "lt": "${val.service}", "href": "${val.url}"}, "tCore" : { "action" : "{{~var.template}}_social_link", label : "${val.service}" } }'><i class="${val.class}" aria-hidden="true"></i></a>`
											).join(" ");
										})()
									}
								])}}
								<ul class="no-bullet">
									{{#~var.fields}}
										{{~fill(helpers.partial("custom_li_items"), data, {}, global)}}
									{{/}}
								</ul>
							</div>
						{{/}}
						{{:description}}
							<div class="desc {{!loc}}no-map{{/}}">
								{{~plugins.stringLib.substringOnWord(helpers.plugins.stringLib.stripHtml(data.description), 475, { ellipsis : true })}}
							</div>
						{{/description}}
					</div>
				</div>
			{{/data}}
		
	</script>
</div>

