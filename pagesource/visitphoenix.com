		var videoPlayer;
		


		var videoWidth = data.width !== (undefined || "") ? data.width : "100%";
		var videoHeight = (function(){
			if(data.height !== (undefined || "")){ 
				videoImg.css("height", data.height); 
				contentWrapper.css({"height": data.height, "width": data.width});
				contentWrapper.addClass("loaded"); 
				return data.height; 
			}
			else if(videoWidth === "100%"){  
				videoImg.css({"height":  "auto", "width": videoWidth}); 
				contentWrapper.css({"height": videoContainer.width()/2, "width": videoWidth});
				contentWrapper.addClass("loaded");
				return videoContainer.width()/2; 
			}
			else{ 
				videoImg.css("height", "auto"); 
				contentWrapper.css({"height": "auto", "width": "100%"});
				contentWrapper.addClass("loaded"); 
				return 'auto'; 
			}
		})();

		playBtn.on("click", function(){
			if(videoPlayer){
				videoPlayer.player.playVideo()
			}
			else{
				buildVideo(videoContainer);
			}
            var self  = this;
            loading.addClass('showLoad');
            setTimeout(function(){
                hideImg.call(self);
            }, 3000);

			$(this).addClass('clicked')
		})

		function buildVideo(slideVideo){
		    var videoId = slideVideo.attr("data-sv-video-id");
		    var videoType =  slideVideo.attr("data-sv-video-type");
		    new vp().init({
		            options: {
		                parent: slideVideo,
		                type: videoType,
		                video_id : videoId,
		                iframe_id: 'video-iframe-' + videoId,
		                frameborder: 0,
		                scrolling: 'no',
		                width: "100%",
		                height: "100%",
		                volume: 100,
		                settings: {
		                    autoplay: 1,
		                    loop: 0,
		                    fullscreen: 1,
		                    controls : 1,
		                    modestbranding : 1,
		                    vq: '720',
		                    rel: 0,
		                    showinfo : 0,
		                    wmode: "transparent",
		                },
		                events: {
		                    ready: onReady,
		                    play: onPlay,
		                    finish: onFinish,
		                    pause: onPause,
		                },
		            }
		        }, onPlayerInit)
		}
		function onPlayerInit(_videoPlayer){
		    videoPlayer = _videoPlayer;
		}

		function onFinish(){
			playBtn.removeClass('clicked');
			videoImg.parent().removeClass('low-index');
			setTimeout(function(){
				videoImg.removeClass('fade');
			}, 10);
		}

		function onReady(){
			this.player.playVideo();
		}
		function onPause(){
		}
		function onPlay(){}


		function hideImg(){
			loading.removeClass('showLoad');
			videoImg.addClass('fade');
			setTimeout(function(){
				videoImg.parent().addClass('low-index');
			}, 500)
		}

		var setImage = function(){
			var width =  $(window).width();
			if( width < 1025  && width > 640){
				videoImg.removeClass('auto-height');
				videoImg.addClass('auto-height');
				contentWrapper.removeClass('auto-height');
			}
			else if( width < 641){
				videoImg.addClass('auto-height');
				contentWrapper.addClass('auto-height');
			}
			else{
				videoImg.removeClass('auto-height');
				contentWrapper.removeClass('auto-height');
			}
		}
		setImage();

		$(window).on('resize', _debounce(function(){
			setImage();
		}, 50));

		function _debounce(func, wait, immediate) {
			var timeout;
			return function() {
				var context = this, args = arguments;
				var later = function() {
					timeout = null;
					if (!immediate) func.apply(context, args);
				};
				var callNow = immediate && !timeout;
				clearTimeout(timeout);
				timeout = setTimeout(later, wait);
				if (callNow) func.apply(context, args);
			};
		};

	});
