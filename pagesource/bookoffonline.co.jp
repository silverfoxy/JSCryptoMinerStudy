	var viewTitleCharacters = 30; //タイトルの文字数制限
	var viewAutorCharacters = 15; //著者の文字数制限

$(".reco_panel").onReady(function(){

	var $recItems = $("#reco_items");
	var $recItem = $recItems.find(".reco_item_style");
	var recItemLen = $recItem.length;

	var $recTitleName = $recItem.find("#reco_title_name");
	var $recDescName = $recItem.find("#reco_desc_name");

	var $jsRecMore = $("#js-recMore");

	$recItem.hide();

	$("[id=reco_title_name],[id=reco_desc_name]").each(function(i){ //英数字を半角に変換
		var txt = $(this).text();
		txt = txt.replace(/[W-＞]/g, function(s) {
			return String.fromCharCode(s.charCodeAt(0) - 0xFEE0);
		});
		txt = txt.replace(/[＠-ｚ]/g, function(s) {
			return String.fromCharCode(s.charCodeAt(0) - 0xFEE0);
		});
		txt = txt.replace(/　/g," ").replace(/‐/g,"-").replace(/！！！/g,"!!!").replace(/！！/g,"!!").replace(/！？/g,"!?");
		$(this).text(txt);
	});

	setTimeout(function(){
		$recTitleName.each(function(i){
			var txt = $(this).text();
			txt = countLength(txt, viewTitleCharacters);
			$(this).text(txt);
		});
		$recDescName.each(function(){
			var txt = $(this).text();
			txt = countLength(txt, viewAutorCharacters);
			$(this).text(txt);
		});

		function countLength(txt, characters){
			var b = 0;
			for(var i = 0; i < txt.length; i++){
				b += txt.charCodeAt(i) <= 255 ? 0.5 : 1;
				if(b > characters){
					txt = txt.substr(0, i) + "…";
					break;
				}
			}
			return txt;
		}
	},0);

	$("#RecommendHead").html('<img src="/files/inc_img/recoindex_bar.gif" alt="あなたに今おススメの商品">');

//	for(var i = 0; i < viewFirstNumber; i++){
