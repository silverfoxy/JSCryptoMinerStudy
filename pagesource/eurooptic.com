        if (destroyCookie) {
            deleteCookie('preferred_site');
        }

        

            // first, check whether we have a cookie that tells us what to do
            var preferred_site = getCookie('preferred_site');
            if (typeof preferred_site === 'undefined') {
                preferred_site = '';
            }

            // if there is no cookie, then do the lookup
            if (preferred_site == "") {
				if (isNewSession && !destroyCookie) {
                    geoip2.country(function (location) {
                        // on geolocation success
                        if (location.country.iso_code == "ZA" && navigator.userAgent.toUpperCase().indexOf('GOOGLEBOT') == -1) {
                            $('#AfricaAlertLink').click();
                        }
                    }, function (error) {
                        if (typeof console !== 'undefined') {
                            console.log('Geolocation failed! ' + error);
                        }
                    });
                }
            } else {
                // if there is a cookie, check if the cookie says eoa; if so, redirect
                // currently, this should not be reached because the .NET code should redirect
                if (preferred_site == 'eoa') {
                    window.location = "http://euroopticafrica.co.za";
                }
            }

            $('body').on('click', '.africa-buttons a.button', function () {

                // use cookie to remember session if checked
                if ($(this).siblings('.chkRememberPreference').children('input').prop('checked')) {
                    var preferred_site = ($(this).hasClass('success') ? 'eoa' : 'eo');
                    setCookie('preferred_site', preferred_site, 90);
                } else {
                    deleteCookie('preferred_site');
                }

                // hide box or redirect based on button clicked
                if ($(this).hasClass('alert')) {
                    $('.pp_close').click();
                } else if ($(this).hasClass('success')) {
                    window.location = "http://euroopticafrica.co.za";
                }

                return false;
            });

    return "";
});

// cookie functions
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires;
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
}

function deleteCookie( name ) {
    document.cookie = name + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
}


