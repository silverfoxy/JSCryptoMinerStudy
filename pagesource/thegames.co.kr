$(document).ready(function(){
		var icons = {"01":"xi-sun","02":"xi-partly-cloudy","03":"xi-cloudy","04":"xi-cloudiness","05":"xi-umbrella","06":"xi-rain","07":"xi-snow","08":"xi-snowman"}
		,orgurl = "http://www.kma.go.kr/XML/weather/sfc_web_map.xml"
		,xmlurl = "http://query.yahooapis.com/v1/public/yql?q="+encodeURIComponent("select * from xml where url='" + orgurl + "'")+"&diagnostics=true";
	jQuery.ajax({
		url: xmlurl,
		dateType: 'xml',
		success: function(data){
			var target = jQuery("#weather-wiget-x1"),
				content = '',
				display = '',
				cnt = 0,
				copyright = '<a href="http://www.kma.go.kr/" target="_blank" class="kma-link">기상청제공</a>',
				nowDate = jQuery("weather", data).attr("year") + "." + jQuery("weather", data).attr("month") + "." + jQuery("weather", data).attr("day");
			content +=	'<div id="weather_box">'
