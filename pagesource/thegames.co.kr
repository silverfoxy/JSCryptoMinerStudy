				if(cnt) display = 'none';
				content +=	'<div class="width-full weather-cont">'
						+	jQuery(this).text() + ' ' + jQuery(this).attr("ta") + '℃'
						+	'<i class="' + iconpos +' xi-2x Default_textColor"></i>'
						+	'</div>';
				cnt++;
			});
			content +=	'</div>'
					+ ''+ copyright +''
					+	'</div>';
