<script type="text/javascript">
	$(document).ready(function(){
		var currentItem = 0;
		var nbItems = 0;

		if (nbItems > 1) {
			$('.backgrounds .background').eq(currentItem%nbItems).show();
			$('body').css("background", "none");
			setInterval(function() {
				currentItem++;

				var modulo = currentItem%nbItems;
				$('.backgrounds').each(function() {
					$('.backgrounds .background').eq((currentItem-1)%nbItems).hide();
					$('.backgrounds .background').eq(modulo).fadeIn(600);
				})

			}, 5000);
		}
		if (nbItems == 1){
			$('body').css("background", "none");
			$('.backgrounds .background').eq(0).show();
		}
	});
</script>


