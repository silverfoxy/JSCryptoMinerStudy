// Fixing VC Tabs Issue
// ==================================

jQuery(document).ready(function($) {

  var adminbarHeight       = $('#wpadminbar').outerHeight();
  var navbarFixedTopHeight = $('.x-navbar-fixed-top-active .x-navbar').outerHeight();

  function animateOffset( element, ms, easing ) {
    $('html, body').animate({
      scrollTop: $(element).offset().top - adminbarHeight - navbarFixedTopHeight + 1
    }, ms, easing);
  }

  $('a[href*="#"]').off('touchstart click');
  $('a[href*="#"]').on('touchstart click', function(e) {
    $href               = $(this).attr('href');
    notComments         = $href.indexOf('#comments') === -1;
    notAccordion        = $href.indexOf('#collapse-') === -1;
    notTabbedContent    = $href.indexOf('#tab-') === -1;
    notTabbedContent    = $href.indexOf('#tab-') === -1;
    notInTabContainer   = $(this).closest('.vc_tta-container').length === 0;
    
    if ( $href !== '#' && notComments && notAccordion && notTabbedContent && notInTabContainer ) {
      var theId = $href.split('#').pop();
      var $el   = $('#' + theId);
      if ( $el.length > 0 ) {
        e.preventDefault();
        animateOffset($el, 550, 'easeInOutExpo');
      } 
    }
  });
});

// Making tabs linkable from URL querystring
// To make use of this an external link should be formatted like this:
// <a href="#" class="enable_jump_to" data-jump-target="#tab-1">Jump and Open Tab/Accordion #1</a>

jQuery(function($){
	// When document loads, find tab on page using hash value from URL
	$(window).on("load", function(){ 
    jump_to_tab($);
  });
  
  $(window).on('hashchange', function() {
    jump_to_tab($);
  });
});

function jump_to_tab($) {
  var isBlogPage = $('body').hasClass('blog-page');
  var hasHash = window.location.hash;
    
  if(isBlogPage && hasHash) {
    var hash = window.location.hash.substring(1);
    var tab_nav = $('.x-nav-tabs #' + hash + '>a').first();
    // jump_to_tab( $, tab_nav);
    if(tab_nav.length >=1) {
      
      // if there's a tab with ID matching the URL's hash, simulate click on it  
      $(tab_nav).click();
      // animate page to the selected tab
      $('html,body').animate({scrollTop: $(tab_nav).offset().top - ( $('.x-navbar').height() + 50 )},500 ,'swing');
    }
  }
  
  
}    </script>

  
  
