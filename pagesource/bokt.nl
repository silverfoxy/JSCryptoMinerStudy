        function toggleVideo(toggle) {
            var videoHidden = (document.cookie.replace(/(?:(?:^|.*;\s*)hideVideo\s*\=\s*([^;]*).*$)|^.*$/, "$1") === "true");

            // Only set cookie if we actually want to toggle. Otherwise just update display.
            if (toggle) {
                videoHidden = !videoHidden;
                if (videoHidden) {
                    document.cookie = "hideVideo=true; expires=" + getNextWeekDay(new Date(), 1).toGMTString() + "; domain=bokt.nl";
                }
                else {
                    document.cookie = "hideVideo=; expires=expires=Thu, 01 Jan 1970 00:00:00 GMT; domain=bokt.nl";
                }
            }

            // This would mess up extra classes, suffices for now.
            document.getElementById('videoplayer').className = (videoHidden ? 'block videoHidden' : 'block');
        }

        toggleVideo(false); // Update display, don't toggle
        document.getElementById("videoHideButton").addEventListener("click", toggleVideo);

        // 0 = Sunday, 1 = Monday, ...
        // http://stackoverflow.com/a/27821003/1078310
        function getNextWeekDay(now,d){
            if (d > now.getDay()) var x = d - now.getDay();
            else var x = ((7 + d) - now.getDay())
            now.setDate(now.getDate() + x);
            return now;
        }

    </script>
