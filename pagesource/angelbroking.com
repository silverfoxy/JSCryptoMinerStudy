function validate_menu_form_email(formid, ref_url_in_cms){
var error = 0;
var name_regex = /^[a-zA-Z ]*$/;
var mobile_regex = /^[0-9]{1,10}$/;
var email_regex = /^[-a-z0-9~!$%^&*_=+}{\"?]+(\.[-a-z0-9~!$%^&*_=+}{\"?]+)*@([a-z0-9_][-a-z0-9_]*(\.[-a-z0-9_]+)*\.(aero|arpa|biz|com|coop|edu|gov|info|int|mil|museum|name|net|org|pro|travel|mobi|[a-z][a-z])|([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}))(:[0-9]{1,5})?$/i;
var name_id = "#"+formid+" #menu_input_name";
var name_field = $(name_id).val();
var mobile_id = "#"+formid+" #menu_input_mobile";
var mobile_field = $(mobile_id).val();
var email_id = "#"+formid+" #menu_input_email";
var email_field = $(email_id).val();
var city_id = "#"+formid+" #menu_city";
var city_field = $(city_id).val();
if(name_field=="" || name_field=="Name"){
var tootip_id = $(name_id).next();
msg="Please enter name";
showtoolTip(msg,tootip_id);
error++;
}else if(name_field.length < 2){
var tootip_id = $(name_id).next();
msg="Please enter more than 2 chracter";
showtoolTip(msg,tootip_id);
error++;
}else if(name_field.length > 30){
var tootip_id = $(name_id).next();
msg="Please enter less than 30 chracter";
showtoolTip(msg,tootip_id);
error++;
}else if(!name_regex.test(name_field)){
var tootip_id = $(name_id).next();
msg="Please enter valid name";
showtoolTip(msg,tootip_id);
error++;
}
if(mobile_field=="" || mobile_field=="Mobile"){
var tootip_id = $(mobile_id).next();
msg="Please enter mobile no.";
showtoolTip(msg,tootip_id);
error++;
}else if(mobile_field.length != 10){
var tootip_id = $(mobile_id).next();
msg="Please enter 10 digit mobile no.";
showtoolTip(msg,tootip_id);
error++;
}else if(!mobile_regex.test(mobile_field)){
var tootip_id = $(mobile_id).next();
msg="Please enter valid mobile no.";
showtoolTip(msg,tootip_id);
error++;
}
if(email_field=="" || email_field=="Email"){
var tootip_id = $(email_id).next();
msg="Please enter Email Id";
