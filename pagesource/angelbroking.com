error++;
}else if(!email_regex.test(email_field)){
var tootip_id = $(email_id).next();
msg="Please enter valid Email Id";
showtoolTip(msg,tootip_id);
error++;
}
if(city_field=="" || city_field=="Select city"){
var tootip_id = $(city_id).next().next();
msg="Please select city";
showtoolTip(msg,tootip_id);
error++;
}else if(!checkCityExists(city_field)){
var tootip_id = $(city_id).next().next();
msg="Please select valid";
showtoolTip(msg,tootip_id);
error++;
}
if(error == 0){
// AJAX Code To Submit Form.
path = "/ajax/submit_menu_form_lead";
var dataString = "name="+ name_field + "&mobile_no="+ mobile_field + "&email_id="+ email_field+"&city="+ city_field +"&web_placement_id=30&ref_url="+ref_url_in_cms;
$("#"+formid+" #apply_button1").hide();
$("#"+formid+" #gif_loader").show();
$.ajax({
type: "POST",
url: base_url+path,
data: dataString,
cache: false,
success: function(result){
console.log(result);
var retrive_data = $.parseJSON(result);
if (retrive_data.duplicate == 0) {
// window.location.href="http://www.angelbroking.com/node/9058?lmsId="+btoa(retrive_data.lms_id)+"&city="+btoa(city_field)+"&name="+btoa(name_field)+"&leadContact="+btoa(mobile_field)+"&leadEmail="+btoa(email_field);
// window.location.href="https://5minwebsite.angelbroking.com/Diykyc/Web?id=tyn"+"&lmsId="+btoa(retrive_data.lms_id)+"&city="+btoa(city_field)+"&name="+btoa(name_field)+"&mobile="+btoa(mobile_field);
window.location.href="http://www.angelbroking.com/node/134839?mobile="+btoa(mobile_field);
}
else
{
// window.location.href="http://www.angelbroking.com/node/9058?duplicate=1";
window.location.href="http://www.angelbroking.com/node/134839?mobile="+btoa(mobile_field);
}
}
});
}
}
