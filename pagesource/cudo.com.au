        is_checking = true;
        var subscrData = { email: $('#email_dd_input').val(), postal_code:$('#post_dd_input').val(), source_landingpage: 'toolbar'};
        if(isEmail(subscrData.email)){
            $.get('/checkpostcode/'+subscrData.postal_code, function(data){
                if(data == 'false'){
                    $('#email_dd_err').html("We're sorry, but the postcode you have entered does not seem to exist in our list of postcodes. Would you please check your information and try again - if you're still having issues, please don't hesitate to contact our support team for assistance.");
                    showPopUp(1);
                } else {
                    $.ajax({
                        type: 'POST', url: '/subscription/validate', data: subscrData,
                        success: function(data){
                            var obj = $.parseJSON(data);
                            if('success' == obj.status  || 'already_has_account' == obj.status){
                                $('#email_dd_box').hide();
                                $.ajax({
                                    type: 'POST', url: '/subscription/ajax_subscribe', data: subscrData,
                                    success: function(data){
                                        $("body").append('<iframe id="trackings" src="/trackings/index" width="1" height="1" marginwidth="0" marginheight="0" frameborder="0" scrolling="no"></iframe>');
                                        showPopUp(0);
                                    },
                                    statusCode: { 404: function(){ location.reload(); }, 500: function(){ location.reload(); } }
                                });
                            }else{
                                $('#email_dd_err').html(obj.message);
                                showPopUp(1);
                            }
                        },
                        statusCode: { 404: function(){ location.reload(); }, 500: function(){ location.reload(); } }
                    });
                }
            });
        }else if('Enter your email here...' == subscrData.email){
            $('#email_dd_err').html("We're sorry, but it appears that no email was entered. Would you please fill in an email address and try again? Thanks!");
            showPopUp(1);
        } else {
            $('#email_dd_err').html("We're sorry, but it appears this email address is not valid. Can you please check that everything is correct and try again? Thanks!");
            showPopUp(1);
        }
    }
    function isEmail(email){
        var filter = /^([^@\s]+)@((?:[-a-z0-9]+\.)+[a-z]{2,})$/i;
        return filter.test(email);
    }
</script>
