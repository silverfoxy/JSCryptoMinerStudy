function escapeHTML(n) {
		return n.replace(/&/g,'&amp;').
replace(/>/g,'&gt;').
replace(/</g,'&lt;').
replace(/"/g,'&quot;');
};
function unescapeHTML(n) {
		return n.replace(/&gt;/g,'>').
replace(/&lt;/g,'<').
replace(/&quot;/g,'"').
replace(/&amp;/g,'&');
};


