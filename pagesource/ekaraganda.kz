								nxt = ind * 1 + 1;
								if (nxt == sz) nxt = 0;
							}
							ths.addClass(w);
							th.find('li.act').removeClass('act');
							th.find('li').eq(nxt).addClass('act');
							tt.find('.li').eq(ind).fadeOut(400);
							tt.find('.li').eq(nxt).fadeIn(600, function () {
								ths.removeClass(w);
							});
						}
