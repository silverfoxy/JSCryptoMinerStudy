<script type="text/javascript">

	function setAjaxData(data){
		if(data.status == 'ERROR'){
			alert(data.message);
		}else{
			if(jQuery('#emob-header-container .block-cart')){
				jQuery('#emob-header-container .block-cart').html(data.sidebar);
			}

			if(jQuery('.header-minicart .count')){
				jQuery('.header-minicart .count').html(data.qty_in_cart);
			}

		}
	}
	function setLocationAjax(url,id){



		url += 'isAjax/1';
		var urlPopup = url.replace("checkout/cart/add","ajaxcart/cart_ajax/popup");
		url = url.replace("checkout/cart","ajaxcart/cart_ajax");

		jQuery('#ajax_loader').show();
		if($('crosssell-cancel')){
			$('crosssell-cancel').hide();
		}

		if($('crosssell-popup')){
			$('crosssell-popup').replace('');
		}

		var crosssellpopup = new CrosssellPopup('');
		crosssellpopup.display();


		try {
			jQuery.ajax({
				url: urlPopup,
				dataType: 'json',
				type : 'post',
				success: function(result){

					jQuery('#ajax_loader').hide();
					//alert(data.status + ": " + data.message);

					if(jQuery('#crosssell-content')){
						jQuery('#crosssell-content').replaceWith(result.popup);
					}

					$('crosssell-cancel').show();

					crosssellpopup.display();

					$('crosssell-cancel').observe('click', (function(event) {
						crosssellpopup.deactivate();
					}).bind(this));
					$('close-popup').observe('click', (function(event) {
						crosssellpopup.deactivate();
					}).bind(this));

					jQuery.ajax( {
						url : url,
						dataType : 'json',
						type : 'post',
						success : function(data) {
							setAjaxData(data);

							$('crosssell-cancel').show();

							jQuery('.checkout-button').removeAttr('style').removeClass('disabled');
							jQuery('.skip-cart.no-count .count').show();
							jQuery('.btn-disbaled-loader').hide();

						}
					});

				}
			});

		} catch (e) {

		}
	}
</script>
                    
