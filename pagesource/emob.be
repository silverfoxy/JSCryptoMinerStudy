<script type="text/javascript">
    jQuery( document ).ready(function() {
        lazyLoadedImages = document.getElementsByClassName("lazy-load");

        for (var i = 0; i < lazyLoadedImages.length; i++) {
            lazyloadImage(lazyLoadedImages[i]);
        }
    });

    function lazyloadImage(imageContainer) {

        var imageVersion = getImageVersion();

        if (!imageContainer || !imageContainer.children) {
            return;
        }
        var img = imageContainer.children[0];

        if (img) {
            var imgSRC = img.getAttribute("data-src-" + imageVersion);
            var altTxt = img.getAttribute("data-alt");
            if (imgSRC) {
                try {
                    var imageElement = new Image();
                    imageElement.src = imgSRC;
                    imageElement.setAttribute("alt", altTxt ? altTxt : "");
                    imageContainer.appendChild(imageElement);
                } catch (e) {
                    console.log("img error" + e);
                }
                imageContainer.removeChild(imageContainer.children[0]);
            }
        }
    }

    function getImageVersion() {

        //var pixelRatio = screenPixelRatio();
        //var width = win.innerWidth * pixelRatio;
        var pixelRatio = 0;
        var width = jQuery(document).width();
        //var width = win.innerWidth;

        // sizes: small = 320, medium = 640, high = 720
        if (width <= 400) {
            return "small"; // default version
        } else if (width <= 424 && width > 400) {
            return "smallmedium"; // default version
        } else if (width <= 475 && width > 424) {
            return "medium"; // default version
        } else if (width <= 570 && width > 475) {
            return "high"; // default version
        } else if (width <= 600 && width > 570) {
            return "x-high"; // default version
        } else if (width <= 690 && width > 600) {
            return "smallmedium"; // default version
        } else if (width <= 774 && width > 690) {
            return "medium"; // default version
        } else if (width <= 820 && width > 774) {
            return "small"; // default version
        } else if (width <= 894 && width > 820) {
            return "smallmedium"; // default version
        } else if (width <= 1085 && width > 894) {
            return "small"; // default version
        } else if (width <= 1291 && width > 1085) {
            return "smallmedium"; // default version
        } else if (width <= 1500 && width > 1291) {
            return "medium"; // default version
        } else if (width <= 1900 && width > 1500) {
            return "high"; // default version
        } else {
            return "x-high"; // default version
        }

    }
</script>

