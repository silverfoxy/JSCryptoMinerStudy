<script src="https://www.sdnlab.com/my_sdnlab/wp-content/themes/presscore-pro/js/jquery.flexslider-min.js?20130801"></script>
<script src="https://www.sdnlab.com/my_sdnlab/wp-content/themes/presscore-pro/js/jquery.ellipsis.js"></script>

<script type="text/javascript">
var post_page=0;
var loadmore=true;

var latest_cid = "48,41,184,158,256";
var news_cid = "48";
var view_cid = "184";
var tech_cid = "41";
var zhaopin_cid = "158";
var products_cid = "256";

var currentCid = latest_cid;//记录当前选择展示的栏目ID,默认都显示

var current_select_cat = 'latest_article_list';
//<!--异步加载文章-->

$(function(){
    
   
    $('.single_show p').ellipsis({
        "row" : 5
    });
    $('.single_show h4 a').ellipsis({
        "row" : 2
    });
    
    $("#btn-more").click(function(){
         if(loadmore){
            post_page++;
            ajaxRequest({
                cid:currentCid,
                flag:false
            });
        } 
        
    });

    //文章列表顶部分类点击
    $('.title-content-c').click(function(){

        var click_cat_id = $(this).attr('id');

        //相同的标签点击不处理
        if(click_cat_id === current_select_cat) return ;

        switch(click_cat_id){
            case 'latest_article_list' : {
                current_select_cat = 'latest_article_list';
                currentCid = latest_cid;
                post_page = 0;
            } break;
            case 'news_article_list' : {
                current_select_cat = 'news_article_list';
                currentCid = news_cid;
                post_page = 0;
            } break;
            case 'views_article_list' : {
                current_select_cat = 'views_article_list';
                currentCid = view_cid;
                post_page = 0;
            } break;
            case 'tech_article_list' : {
                current_select_cat = 'tech_article_list';
                currentCid = tech_cid;
                post_page = 0;
            } break;
            case 'recruitment_article_list' : {
                current_select_cat = 'recruitment_article_list';
                currentCid = zhaopin_cid;
                post_page = 0;
            } break;
            case 'product_article_list' : {
                current_select_cat = 'product_article_list';
                currentCid = products_cid;
                post_page = 0;
            } break;
        }

        ajaxRequest({
            cid:currentCid,
            flag:true
        });

    });
    
    //mobile通知轮播
    var newsVisible;
    
    if($(".mobile_notice").is(":visible")){
        newsVisible = true;
    } else {
        newsVisible = false;
    }
    
    if(newsVisible == true){ 
        var newTimer;
        
        $(".mobile_notice").hover(function(){
            clearInterval(newTimer);
        }, function(){
            newTimer = setInterval("newScroll()", 4000);
        }).trigger("mouseleave");
    }
});

// 通知滚动
function newScroll() {  
    var $listWrap = $(".mobile_notice .notice_list");
    var listH = $listWrap.find(".list").eq(0).outerHeight();
    
    $listWrap.animate({
        "top":"-=" + listH + "px"
    }, 1000, function(){
        $listWrap.find(".list").eq(0).appendTo($listWrap);
        $listWrap.css("top","0");
    });    
    
}

//ajax
function ajaxRequest(options){
	$.ajax({
		url: "?action=getArt", //域名换成你要用的
		data : {"post_page": post_page,"cid":options.cid,"flag":options.flag},
		type: 'get',
		beforeSend: function() {
			$("#btn-more").val("加载中...");
		},
		error: function(a) {
			post_page--;
			$("#btn-more").val("加载更多");
		},   //错误时输出
		success: function(a) {
			$("#btn-more").val("加载更多");
			if($.trim(a))
			{
				if(options.flag == true)
				{
					$("#artcontent").empty();
					post_page = 0;
				}
				$("#artcontent").append(a).fadeIn("slow");

                //文章浏览数刷新
                flushArtContentViews();
			}
			else
			{
				post_page--;
				loadmore=false;
				$("#btn-more").val("没有更多文章了");
				
			}
		}
	});
}

</script>

