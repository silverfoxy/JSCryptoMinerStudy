			success: function(a) {
				if(a.status==1){
					if(a.favorited){
						$('.wps').show();
						$('.wps-click').hide();
						$('.wps-share').addClass('wp_favorite');
						$('.wps-share').removeClass('wp_favorited');
						$('.wps-share').attr('title','收藏');
