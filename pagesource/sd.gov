    //This is the function that moves the bottom div up and down on click of the third item
    function slideExample3(elementId, headerElement, displayElement, hideElement1, hideElement2, hideElement3, hideElement4, appearMenu, photocredit) {
        var element = document.getElementById(elementId);
        if ((element.down == null || element.up == false) && (displayElement.down == null || hideElement1.down == true || hideElement2.down == true || hideElement3.down == true || hideElement4.down == true) && (displayElement != 'content0')) {
            //slide the entire container up into view
            animate(elementId, 0, -322, 100, 500, 550, null);
            element.down = false;
            element.up = true;

            //slide the content container selected into view
            animate(displayElement, 0, -139, 100, 322, 550, null);
            displayElement.up = true;
            displayElement.down = false;

            animate(hideElement4, 0, -139, 100, 322, 450, null);
            hideElement4.up = false;
            hideElement4.down = true;

            animate(appearMenu, x, 60, y, 40, 500, null);
            appearMenu.up = true;
            appearMenu.down = false;

        }
        else if ((displayElement == 'content0') && (element.down == false)) {
            animate(elementId, 0, 0, 100, 286, 550, null);
            element.up = false;
            element.down = true;

            animate(displayElement, 0, 183, 100, 0, 550, null);
            displayElement.up = false;
            displayElement.down = true;

            animate(hideElement1, 0, 183, 100, 0, 550, null);
            hideElement1.up = false;
            hideElement1.down = true;

            animate(hideElement2, 0, 183, 100, 0, 550, null);
            hideElement2.up = false;
            hideElement2.down = true;

            animate(hideElement3, 0, 183, 100, 0, 550, null);
            hideElement3.up = false;
            hideElement3.down = true;

            animate(hideElement4, 0, 183, 100, 0, 550, null);
            hideElement4.up = false;
            hideElement4.down = true;

            animate(appearMenu, x, 0, y, 0, 500, null);
            appearMenu.up = true;
            appearMenu.down = false;

        }
        //don't open the tab or do anything if clicking Close this Content when already closed.
        else if ((displayElement == 'content0') && (element.down == true)) {
            animate(elementId, 0, 0, 100, 286, 550, null);
            element.up = false;
            element.down = true;

            animate(hideElement1, 0, 183, 100, 0, 550, null);
            displayElement.up = false;
            displayElement.down = true;

            animate(hideElement2, 0, 183, 100, 0, 550, null);
            hideElement2.up = false;
            hideElement2.down = true;

            animate(hideElement3, 0, 183, 100, 0, 550, null);
            hideElement3.up = false;
            hideElement3.down = true;

            animate(hideElement4, 0, 183, 100, 0, 550, null);
            hideElement4.up = false;
            hideElement4.down = true;


        }
        else if ((element.down == false) && (displayElement.down == null)) {
            //slide the content container selected into view
            animate(displayElement, 0, -140, 100, 322, 550, null);
            displayElement.up = true;
            displayElement.down = false;

            animate(hideElement1, 0, 183, 100, 0, 550, null);
            hideElement1.up = false;
            hideElement1.down = true;

            animate(hideElement2, 0, 183, 100, 0, 550, null);
            hideElement2.up = false;
            hideElement2.down = true;

            animate(hideElement3, 0, 183, 100, 0, 550, null);
            hideElement3.up = false;
            hideElement3.down = true;

            animate(hideElement4, 0, -140, 100, 322, 550, null);
            hideElement4.up = false;
            hideElement4.down = true;

        }

        else if (elementId == 'secondaryborder') {
            //slide the content container selected into view
            animate(elementId, 20, 0, 80, 800, 550, null);

        }

        else {

            animate(elementId, 0, 0, 100, 183, 550, null);
            element.up = false;
            element.down = true;

            animate(displayElement, 0, 0, 100, 0, 550, null);
            displayElement.up = false;
            displayElement.down = true;

            animate(appearMenu, x, 0, y, 0, 500, null);
            appearMenu.up = true;
            appearMenu.down = false;

        }
    }
</script> 

