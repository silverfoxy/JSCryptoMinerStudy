<script type="text/javascript">
    function getCurrentNavIndex(){
        var homeUrl = "http://" + window.location.host + "/";
        var homeUrl1 = homeUrl.toLowerCase() + "default.html";
        var homeUrl2 = homeUrl.toLowerCase() + "default.aspx";
        var currentUrl = window.location.href.toLowerCase();
        if(currentUrl == homeUrl || currentUrl == homeUrl1 || currentUrl == homeUrl2) return 0;
        if(currentUrl.indexOf("/scene/") > -1) return 1;
        if(currentUrl.indexOf("/hotel/") > -1) return 2;
        if(currentUrl.indexOf("/route/") > -1) return 3;
        if(currentUrl.indexOf("/view/") > -1) return 4;
        return 0;
    }
    var $a = $(".nav-container a");
    var $li = $(".nav-container li");
