        var locationPageUrl = "/locations";

        function LoadLocationPage() {
            var locationTxt = jQuery('#citystatezip').val();
            if (locationTxt != 'Address, City, or Zip Code') {
                location.href = locationPageUrl + "?search=" + locationTxt;
            }

        }

        function FindMore() {

            location.href = locationPageUrl + "?search=" + $('#ctl11_Header1_LocationFinder1_txtHiddenCookie').val();

        }
        function CheckEnterLocation(event, id) {
            if (window.event) keycode = window.event.keyCode;
            else if (event) keycode = event.which;

            if (keycode == 13) {
                LoadLocationPage();
                return false;
            } else {
                return true;
            }
        }

        $('body').click(function () {
            if (jQuery('.locationFinder .current .alternatelocations').hasClass('visible')) {
                jQuery('.locationFinder .current .alternatelocations').slideUp('fast');
                jQuery('.locationFinder .current .alternatelocations').removeClass('visible');
            }
        });
        
    </script>


