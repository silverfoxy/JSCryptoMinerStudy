<script type="text/javascript">
function ContactCorporate() {
    $('#divForm').show();
    $('#divThanks').hide();

    jQuery('ul.footermenu li a').click(function () {

        if ($(this).html() == 'CONTACT CORPORATE') {
            if (window.outerWidth < 768) {
                if (jQuery('#contactform').hasClass('visible')) {
                    jQuery('#contactform').slideUp('fast', function () { jQuery('#contact .content').html(contentcontainer); });
                    jQuery('#contactform').removeClass('visible');
                } else {
                    contentcontainer = jQuery('#contact .content').html();
                    jQuery('#contactform').slideDown('fast');
                    jQuery('html, body').animate({ scrollTop: jQuery('#contactform').offset().top }, 500);
                    jQuery('#contactform').addClass('visible');
                }
            } else {
                jQuery("#lightboxFooter, #overlayFooter").fadeToggle();
            }
            loadSubmit();
            return false;
        }
    });
}

function closethiscustomFooter() {
    if (jQuery('body').hasClass('mobile')) {
        jQuery(".hiddenform").css('display', 'none');
        jQuery(".hiddenform").removeClass('visible');
        return false;
    } else {
        jQuery("#lightboxFooter, #overlayFooter").fadeToggle("fast", function () {
            jQuery("#lightboxFooter").removeClass('videolightbox');
            jQuery('#lightboxFooter').removeClass('wide');
        });
        return false;
    }
}

$(document).ready(function () {
    $("#lightboxFooter").draggable();
});
</script>


