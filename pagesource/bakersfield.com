                                /* Make note of the IDs of the shows that will continue to be live in the order
                                    that they will continue
                                */
                                continue_order.push(show.id);
                                /* Create show update object to record the names and values of the properties that changed */
                                update_obj = {id: show.id, updates: []};
                                for (k = 0; k < SHOW_PROPS.length; k++) {
                                    if (show[SHOW_PROPS[k]] !== prev_show[SHOW_PROPS[k]]) {
                                        update_obj.updates.push([SHOW_PROPS[k], show[SHOW_PROPS[k]]]);
                                    }
                                }// k
                                /* Add show update object if there are updates */
                                if (update_obj.updates.length) {
                                    update.push(update_obj);
                                }
                                /* Remove shows from <prev_shows> that are in <live_shows> to use <prev_shows> as a
                                    list of shows to be removed.
                                */
                                prev_shows.splice(j, 1);
                                /* Stop comparing to previous shows and skip to the next live show */
                                break;
                            }
                        }// j
                        /* If loop <j> of <prev_shows> exhausts itself, then no match was found; the show is new */
                        if (j === prev_shows.static_len) {
                            add.push(show);
                        }
                    }// i

                    /* Populate actions list */
                    if (prev_shows.length) {
                        actions.push('remove');
                        /* The list of remaining shows in <prev_shows> is the "remove" list */
                        remove = get_id_list(prev_shows);
                    }
                    if (update.length) {
                        actions.push('update');
                    }
                    for (i = 0; i < continue_order.length; i++) {
                        for (j = 0; j < prev_order.length; j++) {
                            if (continue_order[i] === prev_order[j]) {
                                if (j > last_matched_ndx) {
                                    last_matched_ndx = j;
                                    break;
                                }
                                else {
                                    reorder = true;
                                    actions.push('reorder');
                                    break;
                                }
                            }
                        }// j
                        if (reorder) {
                            break;
                        }
                    }// i
                    if (add.length) {
                        actions.push('add');
                    }
                    /* Send all information required to add, remove, update and/or reorder shows */
                    if (actions.length) {
                        player.$target.trigger('showManagementUpdate', {
                            player_id: player.id,
                            actions: actions,
                            remove: remove,
                            update: update,
                            order: get_id_list(live_shows),
                            add: add
                        });
                        player.prev_shows = live_shows;
                    }
                }
