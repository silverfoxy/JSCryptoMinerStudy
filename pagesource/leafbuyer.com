			{
				$(this).val(localStorage.getItem('searchform.' + $(this).attr('id')));
			}
		});

		if ($('#type').val() != '')
			$('#search-' + $('#type').val() + '-btn').show();

		if ($('#filter').val().match(/(strain|brand)/))
