		$('#search-btn').click( function() {
			console.log($('#search').val() + " " + $('#source').val());

			if ($('#location').is(':visible'))	// what/where search
			{
				if ($('#source').val() == '' || $('#where').val() == '')
				{
					$('#location').css('border','solid red 2px');
					return false;
				}
			}
			else
				$('#location').css('border','');

			$('#search-form :input').each(function() {
				if (typeof($(this).attr('id')) !== 'undefined')
					localStorage.setItem('searchform.' + $(this).attr('id'),
						$(this).val());
			});

			if ($('#search').val() != '')
			{
				if ($('#source').val() != '')
				{
					var	val = $('#search').val(),
						tgt = $('#source').val(),
						type = $('#type').val(),
						what = $('#what').val(),
						where = $('#where').val(),
						filter = $('#filter').val(),
						root = "/listings";

					ga('send','event','results','search',what.replace(/.*:/,'')+':'+type+':'+tgt+':'+where);

					if (type != '')
					{
						if (type == "locations")
							root = "/listings";
						else if (type == "deals")
							root = "/deals";
						else if (type == "menus")
							root = "/menus";
					}

					if (what != '' && filter != '')
//						filter = '&ptype=1&' + filter + '=' + what;
						filter = '&' + filter + '=' + what;
	
					if (tgt == 'gps')
					{
						$('#where').val(deviceLat + ':' + deviceLng)
						$('#search-form').submit();
					}
					else if (tgt == 'city')
					{
						document.location.href = root + "/dispensaries/" + where.replace(/(.*), *([A-Z]{2}).*/,'$2/$1').replace(/ +/,'+') + "?resetgps=true" + filter;
						return false;
					}
					else if (tgt == 'area')
					{
						document.location.href = root + "/dispensaries/" + where.replace(/(.*), *([A-Z]{2}).*/,'$2/$1').replace(/ +/,'+') + "?resetgps=true" + filter;
						return false;
					}
					else if (tgt == 'state')
					{
						document.location.href = root + "/dispensaries/" + where + "?resetgps=true" + filter;
						return false;
					}
					else if (tgt.match(/(vendor|strain)/))
					{
//						$('#who').val(what);
						$('#who').val(val);
//						$('#what').val('');

						if (searchItem != null)
							$('#whoexact').val('true');

						$('#search-form').submit();
					}
					else
					{
						$('#who').val($('#search').val());
						$('#search-form').submit();
					}
				}
				else
				{
					ga('send','event','results','search','name:'+$('#search').val());

					$('#search-form').submit();
				}
			}
		});

