<script language="javascript">
var st = 1;
var interval = 2; 
var obj=new Array();
var x=new Array();
var y=new Array();
var dx=new Array();
var dy=new Array();

function moveImage(o,dest_x,dest_y) {
	if(!o)
		return;
	obj[o.id]=o;
	x[o.id]=parseInt(obj[o.id].style.left);
	y[o.id]=parseInt(obj[o.id].style.top);

	dx[o.id]=parseInt(dest_x);
	dy[o.id]=parseInt(dest_y);


	move(o.id);
}

function move(i) {
	if(x[i]<dx[i]) x[i] = x[i] + interval; 
	if(y[i]<dy[i]) y[i] = y[i] + interval;
	
	if(x[i]>dx[i]) x[i] = x[i] - interval; 
	if(y[i]>dy[i]) y[i] = y[i] - interval;

	obj[i].style.top  = y[i]+'px';
	obj[i].style.left = x[i]+'px';

	if (((x[i]+interval) < dx[i]) || ((y[i]+interval) < dy[i])) {
		window.setTimeout('move("'+i+'")',20);
	}
	if (((x[i]-interval) > dx[i]) || ((y[i]-interval) > dy[i])) {
		window.setTimeout('move("'+i+'")',20);
	}
}

function reposfootbar() {
        sp=document.documentElement.clientWidth;
        o=document.getElementById("bar");
        o.width=sp-50;

	o=document.getElementById("footbaron");
	//o.style.top=(window.innerHeight-105)+'px';
	o.style.top=(document.documentElement.clientHeight-105)+'px';
	//o.style.left=Math.round((window.innerWidth-1270)/2)+'px';
	o.style.visibility="visible";

/*
	sp=document.documentElement.clientWidth-400;
	o=document.getElementById("sp0");
	o.width=sp;
	o=document.getElementById("sp1");
	o.width=sp;
*/

	o=document.getElementById("footbaroff");
	//o.style.top=(window.innerHeight-105+150)+'px';
	//o.style.left=Math.round((window.innerWidth-1470)/2)+'px';
        o.style.top=(document.documentElement.clientHeight-105+150)+'px';
        o.style.left=Math.round((document.documentElement.clientWidth-1470)/2)+'px';

	o.style.visibility="visible";

	st=1;
}

function updown() {
	switch(st) {
		case 0:
		o=document.getElementById('footbaron');
		moveImage(o,o.style.left,(parseInt(o.style.top)-150));
		o=document.getElementById('footbaroff');
		moveImage(o,o.style.left,(parseInt(o.style.top)+150));
		st=1;
		break;
		case 1:
		o=document.getElementById('footbaron');
		moveImage(o,o.style.left,(parseInt(o.style.top)+150));
		o=document.getElementById('footbaroff');
		moveImage(o,o.style.left,(parseInt(o.style.top)-150));
		st=0;
		break;
	}
}
</script>
