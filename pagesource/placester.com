<script>
    jQuery(function($){
        $('.js-inception-signup').click(function(event){
            event.preventDefault();
            
            var utm_parameters = $(this).data('utm');

            wp.launchOnboarding({
                'env': 'production',
                'signup_uuid': '',
                'campaign': true,
                'utm_parameters': utm_parameters
            });
        });

        var login_overlay = function(){
            if(wp && wp.subscribe_overlay_timeout && wp.subscribe_overlay){
                clearTimeout(wp.subscribe_overlay_timeout); 
            }

            setTimeout(function(){
                $('#dialog-holder').scrollTop(0);
                //$('#body').scrollTop(0);
            },100,$);
            function update_csrf_token() {
                return $.ajax({
                    url: '/wp-admin/admin-ajax.php',
                    data: {action: 'set_new_csrf_token'}
                });
            }

            function login_via_placester_api(params) {
                var d = $.Deferred();
                var url = 'https://acp.placester.com/user/login';

                var data = {user: {}};
                data.user.email = params.email;
                data.user.password = params.password;
                data.user.remember = params.remember;

                $.when(update_csrf_token()).done(function(csrf_token) {
                    $.ajax({
                        url: url,
                        type: "POST",
                        data: data,
                        headers: { "X-CSRF-Token": csrf_token },
                        success: function(res) {
                            d.resolve({ group: 'legacy', success: true });
                        },
                        error: function(res) {
                            d.reject({ group: 'legacy', success: false });
                        }
                    });
                }).fail(function() {
                    d.reject({ group: 'legacy', success: false });
                });

                return d.promise();
            }

            wp.require(['jquery-ui-dialog', 'handyform', 'custom-checkbox'], function(){

                var ext = ('production' === 'staging') ? "org" : "com";
                var ssl = ('production' === 'staging') ? "" : "s";
                var placesterBetaUrl = "http" + ssl + "://www.myrealestateplatform." + ext+ "/admin/#admin";
                var placesterOldUrl = "https://acp.placester.com/people";
                var $dialog = $('#login-inception').dialog({
                    width: $(window).width() >= 768 ? 350 : $(window).width(),
                    modal: true,
                    resizable: true,
                    dialogClass: 'dialog-fixed-top inception-dialog',
                    create:function(){
                        $('#remember-me').customCheckbox();
                        $('#login-inception .checkbox-wrapper').click(function(){
                            $(this).toggleClass('checked');
                            if($(this).hasClass('checked')){
                                $('#remember-me').prop('checked', true);
                            } else {
                                $('#remember-me').prop('checked', false);
                            }
                        });
                        $('#login-overlay-form').click(function(){
                            $('#login-inception .message').slideUp();
                        })

                        document.touchmoveEvent = function(e) {
                            if(window.innerHeight > window.innerWidth) {
								e.preventDefault();
							}
                        }

                        $(".forgot").click(function(){
                            var ext = ('production' === 'staging') ? "org" : "com";
                            var ssl = ('production' === 'staging') ? "" : "s";
                            var html =  '<div class="double">Sorry you&apos;re having trouble.  Choose:' +
                                        '<ul class="placester_web_menu">' +
                                        '<li id="placesterBetaUrl">Reset your Placester password</li>' +
                                        '<li id="placesterOldUrl">Login to Placester Sites Legacy</li>' +
                                        '</ul>' +
                                        '</div>';
                            $dialog.html(html);
                            $('#placesterBetaUrl').click(function(){
                                var redirect = window.addCampaignUrl("http" + ssl + "://www.myrealestateplatform." + ext + "/admin/#forgot");

                                if(window.loc_install_id){
                                    redirect = window.addParam(redirect, 'uii_id', window.loc_install_id);
                                }
                                window.location.href = redirect;
                            });
                            $('#placesterOldUrl').click(function(){
                                window.location.href = "https://acp.placester.com/user/login";
                            });
                        });

                        $('#login-overlay-form').handyForm({
                            validate_on_blur: false,
                            callback_success: function(){
                                var organization_uuid = 'c3906e4c-9cbf-45e2-8758-239b1af866fd';
                                var nar_oranization_id = 'ea830222-de47-4cb3-acb2-abf6f406b72c';
                                var userInception = {
                                    email : $("#overlay-login-email").val(),
                                    password : $("#overlay-login-pw").val()
                                }
                                if(userInception.password.length < 6){
                                    $('#login-inception .message').text('Password must be 6 characters.').slideDown();
                                    return;
                                }
                                $("#login-overlay-form ").fadeTo(500, 0.2);
                                $("#login-inception .spinner").show();
                                $("#login-overlay-form :input").prop("disabled", true);
                                $("#login-overlay-form :input, #login-overlay-form .checkbox-bg").toggleClass("ui-state-disabled");
                                userInception.organization_uuid = organization_uuid;
                                                                
                                function authenticatePlacesterSession(userData){
                                    return window.createPlacesterSession(userData.email, userData.password);
                                }

                                function authenticatePlacesterWebOld(){
                                    var userPlacester = {
                                        email : $("#overlay-login-email").val(),
                                        password : $("#overlay-login-pw").val(),
                                        remember : $('#remember-me').prop('checked') || false
                                    }

                                    return login_via_placester_api(userPlacester);
                                }

                                $.when(authenticatePlacesterSession(userInception)).always(function(response) {
                                    var incSession = response.sessionId ? true : false;

                                    (function(incSession) {
                                        authenticatePlacesterWebOld().always(function(response) {
                                            var legacy = response.success;
                                            
                                            $("#login-inception .spinner").hide();
                                            $("#login-overlay-form :input, #login-overlay-form .checkbox-bg").toggleClass("ui-state-disabled");
                                            $("#login-overlay-form :input").prop("disabled", false);
                                            $("#login-overlay-form ").fadeTo(500, 1);

                                            // var remember = $('#remember-me').prop('checked') || false;
                                            if ((!incSession && !legacy)) {
                                                $('#login-inception .message').text('Bad Email or Password.').slideDown();
                                                return;
                                            } else if (incSession && legacy) {
                                                var html = '<div class="double">Which of your Placester accounts would you like to log into.<ul class="placester_web_menu">';
                                                html += '<li id="placesterBetaUrl">Placester Sites<span class="beta">beta</span></li>';
                                                html += '<li id="placesterOldUrl">Placester Site Legacy</li></ul></div>';
                                                html += '</ul></div>';
                                                $dialog.html(html);
                                                $('#placesterBetaUrl').click(function(){
                                                    _placester.uii.send({
                                                        event : 'Marketing Login'
                                                    });
                                                    
                                                    var redirect = window.addCampaignUrl(placesterBetaUrl);

                                                    if(window.loc_install_id){
                                                        redirect = window.addParam(redirect, 'uii_id', window.loc_install_id);
                                                    }
                                                    window.location.href = redirect;
                                                });

                                                $('#placesterOldUrl').click(function(){
                                                    window.location.href = placesterOldUrl;
                                                });
                                            } else if (incSession) {
                                                $dialog.html('<div class="single">You are successfully logged into <br> <strong>Placester Sites</strong><span class="beta">beta</span></div>');
                                                _placester.uii.send({
                                                    event : 'Marketing Login'
                                                });

                                                var redirect = window.addCampaignUrl(placesterBetaUrl);

                                                if(window.loc_install_id){
                                                    redirect = window.addParam(redirect, 'uii_id', window.loc_install_id);
                                                }
                                                setTimeout(function(){
                                                    window.location.href = redirect;
                                                }, 500);

                                            } else if (legacy) {
                                                $dialog.html('<div class="single">You are successfully logged into <br> <strong>Placester Sites</strong> Legacy</div>');

                                                setTimeout(function(){
                                                    window.location.href = window.addCampaignUrl(placesterOldUrl);
                                                }, 500);
                                            }
                                        });

                                    })(incSession);
                                });
                            }
                        });
						setTimeout(function(){
							$(".ui-dialog-titlebar-close.ui-corner-all").show();
						},200);
                    },
                    open: function() {
						var loginInception = document.getElementById('login-inception');
						loginInception.addEventListener('touchmove', function(e) {
							if(window.innerHeight > window.innerWidth) {
								e.preventDefault();
							}
						}, false);
						document.addEventListener('touchmove', document.touchmoveEvent , false);

                        $(window).on("orientationchange", function(event) {
                            if(window.innerHeight < 768) {
                                if(window.orientation!=0) {
                                    $(".dialog-fixed-top.inception-dialog #logoimg").addClass("reduce-margin");
                                    document.removeEventListener("touchmove", document.touchmoveEvent);
                                } else {
                                    $(".dialog-fixed-top.inception-dialog #logoimg").removeClass("reduce-margin");
                                }
                            }
                        });

						$(loginInception).css("-webkit-overflow-scrolling","touch");
					},
                    close: function(){
                        $('#login-inception .message').slideUp();
                        if(wp && wp.subscribe_overlay){
                            wp.subscribe_overlay_timeout = setTimeout(wp.subscribe_overlay_timeout, 60000);
                        }
                        $('#dialog-holder').css('overflowY', 'scroll');
                        document.removeEventListener("touchmove", document.touchmoveEvent);
                    }

                });

            })
        };

        window.loginBind = function(){
            $('.login-link').click(function(){
                login_overlay();
            });
        }
        window.loginBind();
        
    });
</script>

</footer>


