<script>
    jQuery(function($){

        if(!Modernizr.input.placeholder){
            wp.require(['placeholder']);
        }

        $('.success-overlay a').click(function(){
            window.location = $(this).attr("href");
        })

        $(document).on("dialogopen", ".ui-dialog", function (event, ui) {
            var dh = $('.ui-dialog:visible').outerHeight();
            var wh = $(window).height();

            var win_scrolltop = $(window).scrollTop();

            $('body')
                .css({
                    'scrollTop': win_scrolltop,
                    'padding-right': getScrollBarWidth(),
                    'height': wh,
                    'overflow': 'hidden'
                });
            if($(window).width() >= 768){
                $('.ui-widget-overlay').css('height', dh);
                $('.ui-widget-overlay').appendTo('#dialog-holder');
                $('.ui-dialog:visible').appendTo('#dialog-holder');
                $('#dialog-holder').css('height', wh).css('top', win_scrolltop);
                if((dh+100)>wh){
                    $('.ui-widget-overlay').css('height', dh+140).css('width', '100%');
                }else{
                    $('.ui-widget-overlay').css('height', wh);
                }

                $(this).css('top', 60);
            }else{
                $('.ui-widget-overlay').css('height', $(window).height());
                $('.ui-widget-overlay').appendTo('#dialog-holder');
                $('.ui-dialog:visible').appendTo('#dialog-holder');
                $('#dialog-holder').css('height', $(window).height()).css('top', 0).css('overflow', 'hidden');
                $('.ui-widget-overlay').css('height', $(window).height());
                $('.inception-dialog').css('height', $(window).height());
                $(this).css('top', 0);
            }
            if(!Modernizr.input.placeholder){
                wp.require(['placeholder'], function(){
                    $(".ui-dialog:visible input[placeholder]").placeholder();
                })
            }

        });
		$(window).bind('resize', function() {
			if(parseInt($('#dialog-holder').css('height')) >= 10){
				if($('#dialog-holder .inception-dialog').css('display') == "block"){
					if($(window).width() <= 767){
						 $('.inception-dialog').css('height', $(window).height());
						 $('.inception-dialog').css('width', $(window).width());
						 $('#login-inception').css('height', $(window).height());
						 $('.ui-widget-overlay').css('height', $(window).height());
						 $('#dialog-holder').css('height',  $(window).height());
					}else{
						$('#login-inception').css('width', 350);
						$('#login-inception').css('height', 'auto');
						$('.inception-dialog').css('height', 'auto');
						$('.inception-dialog').css('width', 350);
						$('.inception-dialog').css('left', '25%');
						$('.inception-dialog').css('top', '15%');
					}
				}else if($('#dialog-holder .demo-holder').css('display') == "block"){
					 $('#demo').css('height', $(window).height());
					 $('.ui-widget-overlay').css('height', $(window).height());
					 $('#dialog-holder').css('height',  $(window).height());
				}
			}
		});
        $(document).on("dialogclose", ".ui-dialog", function (event, ui) {
            $('body').css({
                'padding-right': 0,
                'height': 'auto',
                'overflow': 'scroll'
            });
            $('#dialog-holder').css('height', '0px');
			$(".ui-dialog-titlebar-close.ui-corner-all").show();
        });

    });
</script>
