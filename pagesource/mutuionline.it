    function CambiaCodTipoIndice() {
        with (document.modulo) {

            if (CodiceDurata.selectedIndex>0)
                durataPrecedente = CodiceDurata.options[CodiceDurata.selectedIndex].value;
            else
                durataPrecedente = 0;

            if (((CodiceTipoTasso.options[CodiceTipoTasso.selectedIndex].value == 2) || (CodiceTipoTasso.options[CodiceTipoTasso.selectedIndex].value == 7)) && ((CodiceFinalita.options[CodiceFinalita.selectedIndex].value == 1) || (CodiceFinalita.options[CodiceFinalita.selectedIndex].value == 2) || (CodiceFinalita.options[CodiceFinalita.selectedIndex].value == 16))) {
                document.getElementById("divCodTipoIndice").style.visibility = "visible";
            }
            else {
                CodTipoIndice.selectedIndex = 0;
                document.getElementById("divCodTipoIndice").style.visibility = "hidden";
            }

            // sistemo tendina durate per surroga
            if (CodiceFinalita.options[CodiceFinalita.selectedIndex].value == 16) {
                CodiceDurata.length = 1 + 28;
                for (k = 5; k <= 30; k++) {
                    CodiceDurata.options[k - 4].value = k;
                    CodiceDurata.options[k - 4].text = k;
                }
                CodiceDurata.options[27].value = '35';
                CodiceDurata.options[27].text = '35';
                CodiceDurata.options[28].value = '40';
                CodiceDurata.options[28].text = '40';
            }
            else { //non surroga
                CodiceDurata.length = 1 + 10;
                CodiceDurata.options[1].value = '5';
                CodiceDurata.options[1].text = '5';
                CodiceDurata.options[2].value = '7';
                CodiceDurata.options[2].text = '7';
                CodiceDurata.options[3].value = '10';
                CodiceDurata.options[3].text = '10';
                CodiceDurata.options[4].value = '12';
                CodiceDurata.options[4].text = '12';
                CodiceDurata.options[5].value = '15';
                CodiceDurata.options[5].text = '15';
                CodiceDurata.options[6].value = '20';
                CodiceDurata.options[6].text = '20';
                CodiceDurata.options[7].value = '25';
                CodiceDurata.options[7].text = '25';
                CodiceDurata.options[8].value = '30';
                CodiceDurata.options[8].text = '30';
                CodiceDurata.options[9].value = '35';
                CodiceDurata.options[9].text = '35';
                CodiceDurata.options[10].value = '40';
                CodiceDurata.options[10].text = '40';
            }

            for (i = 0; i < CodiceDurata.length; i++) {
                if (CodiceDurata.options[i].value == durataPrecedente) {
                    CodiceDurata.selectedIndex = i;
                    break;
                }
                else
                    CodiceDurata.selectedIndex = 0;
            }

            // Set valore durata in caso di back
            if (indexDurataHidden.value != "" && durataPrecedente == 0) {
                CodiceDurata.selectedIndex = indexDurataHidden.value;
            }
        }
    }

    function submitenter(myfield, e) {
        var keycode;
        if (window.event) keycode = window.event.keyCode;
        else if (e) keycode = e.which;
        else return true;

        if (keycode == 13) {
            submitMostraMutui();
            return false;
        }
        else
            return true;
    }

    function ControllaImporto() {
        var valore = sostituiscistringa(document.modulo.ImportoMutuo.value, ".", "");
        if (valore < 1000) {
