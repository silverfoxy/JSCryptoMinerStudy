        $("#divLogoBanca10").show();
    }
    else
    {
        $("#divLoghi").css("height","359px");    
        numLoghiVisualizzati = 7;
        CaricaLoghiBanche();
        $("#divLogoBanca7").show();
        $("#divLogoBanca8").hide();
        $("#divLogoBanca9").hide();
        $("#divLogoBanca10").hide();
