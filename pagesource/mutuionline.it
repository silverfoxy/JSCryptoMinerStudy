    $("#bannerDX2").hide();
    $("#treBanner1").show();
    $("#treBanner2").show();
    $("#treBanner3").show();
    $("#divLoghi").css("height","300px");
    numLoghiVisualizzati = 6;
    CaricaLoghiBanche();
    $("#divLogoBanca7").hide();
    $("#divLogoBanca8").hide();
