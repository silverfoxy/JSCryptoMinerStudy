    $("#divLogoBanca10").hide();
    $("#divLogoBanca11").hide();
}

function AbilitaRichiestaPreventivo()
{
    $("#trEmailPreventivo").show();
    $("#trPrivacy").show();
    $("#bordinoPreventivoEmail").css("visibility","visible");
    $("#bannerDX1").show();
    $("#treBanner1").hide();
    $("#treBanner2").hide();
    $("#treBanner3").hide();
