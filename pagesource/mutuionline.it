}


function AttivazioneSezioneFlagConsesnso() {

    if ($('#divFlagConsensoAggiunti').css('display') == 'none')
    {
        // abilito, mostro
        $('#divFlagConsensoAggiunti').show();
        $("#bannerDX2").show();
        $("#divLoghi").css("height","499px");
        numLoghiVisualizzati = 10;
        CaricaLoghiBanche();
