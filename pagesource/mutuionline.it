        $('#divFlagConsensoAggiunti').hide();
        $("#bannerDX2").hide();
        $("#divLoghi").css("height","359px");
        numLoghiVisualizzati = 7;
        CaricaLoghiBanche();
        $("#divLogoBanca7").show();
        $("#divLogoBanca8").hide();
