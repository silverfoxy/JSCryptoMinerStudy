        document.getElementById('FlagConsensoComunicazioniPromozionali').checked = document.getElementById('FlagConsensoSelettore').checked;
    }

}

function CheckCampi(theForm) {
	with (theForm) {

		var strMsgValore=new String();
		var strMsgImporto=new String();
		var strMsgImportoMassimo=new String();
		var strMsgReddito=new String();
		var fltvaluta;

		strMsgValore="Il valore dell'immobile non può essere convalidato. Inserire la cifra in Euro senza centesimi.";
		strMsgImporto="L'ammontare del mutuo non può essere convalidato. Inserire la cifra in Euro senza centesimi. L'importo non dovrà superare il valore dell'immobile.";
		strMsgImportoMassimo="L'importo specificato non può essere convalidato. Inserire la cifra in Euro senza centesimi.";
		strMsgReddito="Il reddito mensile netto dei richiedenti non può essere convalidato. Inserire la cifra in Euro senza centesimi.";
	
		//se si tratta di euro
		fltValuta=1936.27/1000000;

		if ($("#CodiceFinalita").val() == "") {
			alert("Il campo \"Finalità del mutuo\" è obbligatorio ");
			CodiceFinalita.focus();
			return (false);
		}
	
		if (CodiceTipoTasso.selectedIndex == 0) {
			alert("Il campo \"Tipo di tasso\" è obbligatorio ");
			CodiceTipoTasso.focus();
			return (false);
		}
	
	
		// 3/12/10 si chiede di modificare l' importo massimo a 99.999.999
		// precedentemente i controlli erano fatti in lire
		if (!(controllo_importo_intero(Valore, "Valore immobile"))) return false;
		if (!(controlla_campo_intervallo(Valore,1000,9999999,true,"Valore Immobile")))
			return (false);
	
		if (!(controllo_importo_intero(ImportoMutuo, "Importo del mutuo"))) return false;
		if (!(controlla_campo_intervallo(ImportoMutuo,1000,sostituiscistringa(Valore.value,".","") * "1",true,"Importo mutuo")))
			return (false);
	
		if (ControllaImporto() == false) {
			return(false);
		}
	
		if (CodiceDurata.selectedIndex == 0) {
			alert("Il campo \"Durata del mutuo\" è obbligatorio ");
			CodiceDurata.focus();
			return (false);
		}

		if (!check_integer(Eta,true,true,"Il campo \"Età del richiedente\" è obbligatorio ")) 
			return false;
		if ((parseInt(Eta.value) < 18)||(parseInt(Eta.value) > 100)) {
			alert("L'età del richiedente deve essere compresa tra 18 e 100 anni");
			Eta.focus();
			return (false);
		}

	
		if (CodCategoriaReddituale.selectedIndex == 0) {
			alert("Il campo \"Posizione lavorativa\" è obbligatorio ");
			CodCategoriaReddituale.focus();
			return (false);
		}
	
		if (!(controllo_importo_intero(Reddito, "Reddito dei richiedenti"))) return false;
		if (!check_curr1(Reddito,2,0,parseFloat(sostituiscistringa(sostituiscistringa(Reddito.value,".",""),",",""))*fltValuta,false,false,strMsgReddito))
			return (false);
	
		if (CodiceResidenza.selectedIndex == 0 || CodiceResidenza.selectedIndex == 5 ) {
			alert("Il campo \"domicilio del richiedente\" è obbligatorio ");
			CodiceResidenza.focus();
			return (false);
		}
	
  
		if (CodiceLocalita.selectedIndex == 0 || CodiceLocalita.selectedIndex == 5) {
			alert("Il campo \"Provincia dell'immobile\" è obbligatorio ");
			CodiceLocalita.focus();
			return (false);
		}

    if ($("#CodiceFinalita").val() + "" == "1" || $("#CodiceFinalita").val() + "" == "2" || $("#CodiceFinalita").val() + "" == "7" || $("#CodiceFinalita").val() + "" == "8" || $("#CodiceFinalita").val() + "" == "9" || $("#CodiceFinalita").val() + "" == "12" || $("#CodiceFinalita").val() + "" == "13" || $("#CodiceFinalita").val() + "" == "21") {
            if ($("#CodStatoAvanzAcquisto").val() + "" == "0") {
	            alert("Effettuare una scelta nel campo \"Stato ricerca immobile\".");
	            $("#CodStatoAvanzAcquisto").focus();
	           	return (false);
            }
      }

		//testo che il valore dell'immobile non sia inferiore a 3000 di euro.
		if (parseFloat(sostituiscistringa(Valore.value, ".","")<3000) ){
			alert("Attenzione! Il valore dell'immobile deve essere superiore a 3000 Euro.")
			Valore.focus();
			return (false);
		}

		// Memorizzo il valore index della durata in caso di back
		indexDurataHidden.value = CodiceDurata.selectedIndex;

        // sezione per la ricezione del preventivo
        if($("#chkRiceviPreventivo").is(':checked'))
        {
            if (!controlla_email(EmailPreventivo)) {
                EmailPreventivo.focus();
                return (false);
            }

            if (FlagConsensoPrivacy.checked == false) {
                alert("Hai negato il consenso a trattare i dati personali. Se vuoi utilizzare il nostro servizio di richiesta online è necessario autorizzarci a trattare i dati inseriti. Se invece non vuoi autorizzare MutuiOnline.it, è sufficiente abbandonare questa area del sito. Non rimarrà alcuna traccia dei dati di cui non è stato autorizzato il trattamento.");
                if ($("#FlagConsensoSelettore").prop("checked") + "" == "true") {
                    $("#FlagConsensoPrivacy").focus();
                }
                else {
                    $("#FlagConsensoSelettore").focus();
                }

                return (false);
            }

        }

        if($("#chkRiceviPreventivo").is(':checked'))
        {
            ga('send', 'event', 'Invio preventivo', 'click homepage'); //tracker evento google analytics
        }

	}
	return (true);
