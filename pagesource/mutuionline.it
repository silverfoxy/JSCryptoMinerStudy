<script type="text/javascript">
function AbilitaODisabilitaRichiestaPreventivo()
{
    if ($("#chkRiceviPreventivo").is(':checked'))
    {
        // abilito, mostro
       AbilitaRichiestaPreventivo();
    }
    else
