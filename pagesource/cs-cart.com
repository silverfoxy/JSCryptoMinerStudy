<script type="text/javascript">
    $(document).ready(function () {
        $('.video-dialog-bg').appendTo('body');
        $('.video-opener').click(function (e) {
            e.preventDefault();
            var elm = $(this),
                container = $(elm.attr('href'));
            container.addClass('opened').fadeIn(500);
            $('body,html').addClass('video-player-opened');
        });

        $('.video-dialog-bg .icon-close').click(function () {
            var container = $(this).parent('.video-dialog-bg'),
                player = container.find('.video-player');

            player.remove().appendTo(container);

            $('.video-dialog-bg.opened').removeClass('opened').fadeOut(500);
            $('body,html').removeClass('video-player-opened');
        });
    });
</script>



                    

