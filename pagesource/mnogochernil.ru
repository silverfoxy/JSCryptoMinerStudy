<script type="text/javascript" src="catalog/view/javascript/jquery.maskedinput.min.js"></script>
<script>
    (function($){
        var $body = $('body');
        $body.on('change', 'input:radio[name=radios]:checked', function () {
            if ($("input[name=radios]:checked").val() == 2) {
                $('#orderId').show();
				$('input[name=order_id]').trigger('focus');
            }else{
                $('#orderId').hide();
            }
        });
		$('#phoneAll').mask("+7(999) 999-99-99");
		$body.on('click', '.collection', function(){
            var $el = $(this),
                product_id = $el.attr('id').split('-'),
                produt_name = $('h1').text();//$el.closest('div.row').find('.name').children('a').html();
				product_id = parseInt(product_id[1]);
            if(product_id && produt_name){
                $.ajax({
                    url: 'index.php?route=product/category/collection',
                    type: 'post',
                    dataType: 'json',
                    data: ({product_id:product_id}),
                    beforeSend: function() {
                        $el.addClass('fltr-disabled');
                    },
                    complete: function() {
                        $el.removeClass('fltr-disabled');
                    },
                    success: function(json) {
                        var modal = $('#collectionModal'),
                            html = '',
                            button = '';
                        switch (json.status) {
                            case 0:
                                html = 'Только зарегистрированные пользователи могут создавать коллекции';
                                modal.find('.modal-footer').html('<br /><a class="adtcrt modal-button" href="' + json.url + '">' + 'Войти / зарегистрироваться' + '</a><br />');
                                break;
                            case 1:
								modal.find('.modal-footer').html('<button type="button" class="adtcrt" data-dismiss="modal">Добавить и закрыть</button>');
                                html = '<strong>' + produt_name + '</strong>';
                                html += '<p>Коллекции:</p>';
                                html += '<ul>';
                                var checked;
                                for(i in json.collections){
                                    checked = json.collections[i]['is'] ? 'checked="checked"' : '';
                                    html += '<li><input type="checkbox" value="' + json.collections[i]['id'] + '"' + checked + ' class="add-to-collection"><a target="blank" href="' + json.collections[i]['url'] + '">' + json.collections[i]['name'] + '</a></li>';
                                }
                                html += '</ul>';
                                break;
                            case 2:
                                html = 'Вы еще не создали коллекцию';
                                modal.find('.modal-footer').append('<a class="adtcrt modal-button" href="' + json.url + '">' + 'Создать' + '</a>');
                                break;
                        }
                        modal.find('.modal-body').children('.container').html(html);
                        modal.modal(true);
                        $body.on('click', '.add-to-collection', function(){
                            var collection_id = $(this).val(),
                                check = $(this).is(':checked') ? 1 : 0;
                            $.ajax({
                                url: 'index.php?route=account/collection/addProduct',
                                type: 'post',
                                dataType: 'json',
                                data: ({collection_id:collection_id,product_id:product_id,check:check}),
                            });
                        });
                    }
                });
            }
        });
    })(jQuery);
	function perezv() {
		var form = $('#perezvonite'),
			phone = form.find('input[name=name]'),
			val = phone.val().replace(/[^\d;]/g, '');
		if(val.length < 10){
			phone.closest('div.form-group').addClass('has-error');
			phone.siblings(".error").text('Заполните поле телефон');
		}else{
			form.submit();
		}
	};
</script>

