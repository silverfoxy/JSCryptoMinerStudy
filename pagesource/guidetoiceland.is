		var overlay_timeout;
		function toggle_loading_overlay(action) {
			clearTimeout(overlay_timeout);
			if (window.page_leave_overlay_text.length) {
				$('#page-leave-overlay .page-leave-overlay-text')
					.addClass('page-leave-overlay-text_visible')
					.text(window.page_leave_overlay_text);
			}
			if (typeof action === 'undefined') {
				$('#page-leave-overlay').toggle('fade');
			} else {
				if (action == 'hide') {
					$('#page-leave-overlay').hide('fade');
					window.page_leave_overlay_text = '';
					$('#page-leave-overlay .page-leave-overlay-text')
						.removeClass('page-leave-overlay-text_visible')
				} else if (action == 'show' && !window.page_leave_silent) {
					$('#page-leave-overlay').show('fade');
				}
			}
			window.page_leave_silent = false;
			overlay_timeout = setTimeout(function() {
				toggle_loading_overlay('hide');
			}, 30000);
		}
		if ( /iPhone|iPad/i.test(navigator.userAgent) ) {
			window.addEventListener("pagehide", function (e) {
				toggle_loading_overlay('show');
			});

			window.addEventListener('pageshow', function(e) {
				if (e.persisted) {
					toggle_loading_overlay('hide');
				}
			});
		} else {
			$(window).on('beforeunload', function(){
				toggle_loading_overlay('show');
			});
		}

