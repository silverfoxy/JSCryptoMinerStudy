<script>

	
    (function($){

        /**
         * selector {string} jqeury selector that will be use this funcitonality
         */
        var ChangeLanguage = function(selector) {
            this.site = 'ab-inbev';
            this.langSite = '[en]';
            this.selector =  selector;
            this.initialize();
        };

        ChangeLanguage.prototype = {

            initialize: function() {
                this._getLanguage();
                this._filterLanguage();
            },

          /**
           * From backend returns and string with the data array-like.
           * Need convert this in an js array for the correct functionality
           * example: from "["en", "fr"]" to ["en", "fr"]
           */
            _setlanguageArray: function() {
                this.langSite = this.langSite.replace(/\s+/g, '');
                this.langSite = this.langSite.slice(1, -1);
                this.langSite = this.langSite.split(',');
            },

              /**
               * Use this method for manage the amount of languages.
               * TODO: Add conditional for more languages
               */
            _filterLanguage: function() {
                this._setlanguageArray();

                var enableLanguageSelection = false;

                
                    
                

                if(this.site === 'budlight' && enableLanguageSelection || this.site === 'michelobultra' && enableLanguageSelection){
                    this.langSite = this.langSite.slice(1);
                    // custom check for the current our-beers redirects
                    var ourBeersIndex = $.inArray('our-beers', this.langSite);
                    if(ourBeersIndex >= 0){
                        this.langSite.splice(ourBeersIndex, 1);
                    }
                }

                if(this.langSite.length === 2) {
                    this._chooseLanguage();
                }
            },

              /**
               * From Backend get the url of the site and divide it.
               * get the actual language and assignt to a object this.currentLanguage.
               */
            _getLanguage: function() {
                var pageUrl = '/content/ab-inbev/en/home' + '.html';

                this.language = pageUrl.split('/');
                this.currentLanguage = this.language[3];

            },

              /**
               * Logic for change the language only if two options exist.
               * TODO: add other method for more languages, we need talk about.
               * Depends on the way that te data will be populate.
               */
            _chooseLanguage: function() {
                if (this.currentLanguage === this.langSite[0]) {
                    this.selector.attr('data-language', this.langSite[1]);
                    this.language[3] = this.langSite[1];
                } else {
                    this.selector.attr('data-language', this.langSite[0]);
                    this.language[3] = this.langSite[0];
                }

            },

              /**
               * Logic for get the cookie and set a old date, in this way the cookie is deleted.
               *
               * name {string} name of the cookie
               * path {string} location of the cookie in the most of cases use '/' like parameter
               * domain {string} optional, if you want specify a url domain.
               */
            _deleteCookie: function(name, path, domain) {
                document.cookie = name + '=' +
                  ((path) ? ';path='+path:'')+
                  ((domain)?';domain='+domain:'') +
                  ';expires=Thu, 01 Jan 1970 00:00:01 GMT';
            },

              /**
               * Public method
               * Use for change the url, clear the cookie and redirect to the new language
               */
            languageRedirect: function() {
                var enableLanguageSelection = false;
                
                    
                

                if(this.site !== 'budlight' && !enableLanguageSelection || this.site !== 'michelobultra' && !enableLanguageSelection) {
                    this._deleteCookie('ldage' + this.site, '/');
                }

                var serviceUrl = this.language.join('/');
                
					
					
					
						window.location = '/content/ab-inbev/en' + '/home.html';		
					
				
                
            }


        };

        /**
         * At the moment only use that for coronasite
         */
        var temporalLanguage = 'ab-inbev';
        if(temporalLanguage === 'coronaextra') {
            var changeLanguage = new ChangeLanguage($('.footer-list li:last-child'));

            $('.footer-list li:last-child').on('click', function(e) {
                e.preventDefault();
                changeLanguage.languageRedirect();
            });
        }

        /**
         * Support language selection for budlight canada
         */
        var enableLanguageSelection = false;
        
            
        

        if(temporalLanguage === 'budlight' && enableLanguageSelection || temporalLanguage === 'michelobultra' && enableLanguageSelection) {
            var $selector = $('<li><a href="#"></a></li>');
            var changeLanguage = new ChangeLanguage($selector);
            var langText = (changeLanguage.language[3].indexOf('en') !== -1 ) ? 'English' : 'Fran&ccedil;ais';
                $selector.children().html(langText);
                $('footer ul.footer-list').prepend($selector);
                $('.footer-container').addClass('lang-selector-on');

            $selector.on('click', function(e) {
                e.preventDefault();
                changeLanguage.languageRedirect();
            });
        }

    })(jQuery);
</script>
