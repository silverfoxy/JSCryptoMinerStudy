                                        adjustPaddinBody();
                                    }
                                }

                                if($inputs && $('.sticky-main-header') && scrollPos != 0) {
                                    $('.sticky-main-header').width('100%');
                                } else {
                                    $mainHeader.width('100%');
                                }
                            }
                        }

                        /**
                         * function to adjust sticky menu bar on resize event.
