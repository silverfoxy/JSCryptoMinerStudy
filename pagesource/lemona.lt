				len = dml.elements.length; 
				var e=0;
				for( i=0 ; i<len ; i++) {
					
					if (dml.elements[i].name==chkName){
						
						//alert(dml.elements[i].value.toString()+'_'+e.toString());
						cookie_val = getCookieGB(dml.elements[i].value.toString()+'_'+e.toString());
