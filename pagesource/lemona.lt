		// Popup menu
		$(".main_menu > ul > li").each(function ()
		{
			var li = $(this);
			
			if ($(".popup_menu", li).length > 0)
			{
				var pageWidth = $("#header").width() - 3;
				
				// First - calculate the width of this popup FOR THE IDIOT THAT IS INTERNET EXPLORER
				if ($.browser.msie)
				{
					var width = (110 * $(".popup_menu .item", li).length)
							  + (1 * $(".popup_menu .splitter", li).length)
							  + 2;
					if (width > pageWidth)
						width = pageWidth;
					$(".popup_menu", li).css('width', width);
				}
				
				
				// First - find out the margin of this the popup, that it should have to be at the left
				$(".popup_menu", li).show();
				var marginZero = ($(".main_menu").offset().left + 2) - $(".popup_menu", li).offset().left;
				$(".popup_menu", li).hide();
				
				// Now, position the popup menu at the left, and give it a max-width, equal to .main_menu width
				// Also, if popup_menu is wider than .main_menu width, then positioning is done
				$(".popup_menu", li).css('margin-left', marginZero);
				var width = $(".popup_menu", li).width();
				
				if (width > pageWidth)
				{
					$(".popup_menu", li).css('width', pageWidth);
				}
				else
				{
					// Now, position the popup menu, to be centered below the parent li
					$(".popup_menu", li).css('margin-left', - ((width - $(li).width()) / 2));
					
					var offset = Number($(".popup_menu", li).css('margin-left').replace(/px$/, '')) - marginZero;
					
					// Make sure we are not protruding at the left
					if (offset < 0)
						$(".popup_menu", li).css('margin-left', marginZero);
					
					// Make sure we are not protruding at the right
					if (offset + width > pageWidth)
						$(".popup_menu", li).css('margin-left',
													Number($(".popup_menu", li).css('margin-left').replace(/px$/, ''))
												  - ((offset + width) - pageWidth));
				}
				
				
				
				
				// Now, make sure we are 
				
				// // Center the popup menu below this menu element
				// 
				
				
				// $(".popup_menu", li).show();
				// var offsetLeft = $(".popup_menu", li).offset().left;
				// $(".popup_menu", li).hide();
				
				// if (offsetLeft < $("#main_content").offset().left + 2)
					// $(".popup_menu", li).css('margin-left', Number($(".popup_menu", li).css('margin-left').replace(/px$/, ''))
														  // + ($("#main_content").offset().left - offsetLeft + 2));
				
				$(li).hover(function ()
				{
					$(".popup_menu", this).show();
				},
				function ()
				{
					$(".popup_menu", this).hide();
				});
			}
		});
		
