						if (cookie_val!=null && cookie_val==1) {
							dml.elements[i].checked=true;
						}
						e++;
					}
					
				}
				
				
			
			}
			
			function setFilterValues(chkName,filter){
				//alert('test');
				var form='goods_form';
				var str=escape(filter.replace(/(%20)/, '_').replace(/\s/, '_').replace(/\+/, '_').replace(/\t/, '_').replace(/\r/, '_').replace(/\n/, '_').replace(/ /,'_')); 
				dml=document.forms[form];
				len = dml.elements.length;  
				var e=0;
				for( i=0 ; i<len ; i++) {
					if (dml.elements[i].name==chkName){
						
						if (str.indexOf(escape(dml.elements[i].id.replace(/(%20)/, '_').replace(/\s/, '_').replace(/\+/, '_').replace(/\t/, '_').replace(/\r/, '_').replace(/\n/, '_').replace(/ /,'_')))>-1 ) {
							
							dml.elements[i].checked=true;
							//$(dml.elements[i]).click();
							$('input[id=all'+dml.elements[i].id.substring(1,2)+']').attr('checked',false);
							
							document.getElementById('all'+dml.elements[i].id.substring(0,2)).checked = false ;
						}
						e++;
					}	
				}
				
			}
			function SetChecked(val,chkName) {
