			//$(".list_cnt", dropd_cnt).css('width', $(".dropd_lr", dropd_cnt).width());
			var dropd_lr_over = false;
			var list_cnt_over = false;
			function hoverOn()
			{
				if (dropd_lr_over || list_cnt_over)
				{
					$(".list", dropd_cnt).slideDown("fast");
				}
			}
			function hoverOff()
			{
				setTimeout(function()
				{
					if (! dropd_lr_over && ! list_cnt_over)
					{
						$(".list", dropd_cnt).slideUp("fast");
					}
				}, 200);
			}
			$(".dropd_lr", dropd_cnt).hover(function()
			{
				dropd_lr_over = true;
				hoverOn();
			},
			function()
			{
				dropd_lr_over = false;
				hoverOff();
			});
			$(".list_cnt", dropd_cnt).hover(function()
			{
				list_cnt_over = true;
				hoverOn();
			},
			function()
			{
				list_cnt_over = false;
				hoverOff();
			});
		});
		
		
