		// Container and wrapper dimension calculation (different for IE6 and other browsers, because IE6 does
		// not support min-width and max-width)
		var percentBasedMaxWidthContainer = $('#main_wrapper'); 
    var minWidthContent = $('#main_wrapper_in');
    
    if ($.browser.msie && $.browser.version < 7)
    {
        // First - emulate max-width for IE6 (it does not support it)
        // Thankfully - it returns max-width percentage value (whereas other browsers return pixels value),
        // so that saves us
        var wrapperCssMaxWidthPercent = 80;
        var wrapperCssMaxWidth = wrapperCssMaxWidthPercent + '%';
        
        $(percentBasedMaxWidthContainer).css('width', $(percentBasedMaxWidthContainer).parent().width() * (wrapperCssMaxWidthPercent / 100));
        
        var containerMinWidth = Number($(minWidthContent).css('min-width').replace(/px$/, ''));
        
        function recalcWrapperWidthIE6()
        {
            // If container min-width is more, than what we get with our percentage-based wrapper - 
            // increase wrapper size to container min-width
            if (containerMinWidth > $(percentBasedMaxWidthContainer).width())
            {
                // But only increase, if there is any extra space in the parent of the container
                if ($(percentBasedMaxWidthContainer).width() < $(percentBasedMaxWidthContainer).parent().width())
                {
                    $(percentBasedMaxWidthContainer).css('width', containerMinWidth);
                }
            }
            
            // Container min-width is less then or equal wrapper width
            else 
            {
                // If wrapper width is not equal to what we should have with its percentage
                var wrapperPercentWidth = $(percentBasedMaxWidthContainer).parent().width() * (wrapperCssMaxWidthPercent / 100);
                if ($(percentBasedMaxWidthContainer).width() != wrapperPercentWidth)
                {
                    var wrapperWidth = wrapperPercentWidth > containerMinWidth ? wrapperPercentWidth : containerMinWidth;
                    $(percentBasedMaxWidthContainer).css('width', wrapperWidth);
                }
            }
        }
        
        $(window).resize(recalcWrapperWidthIE6);
        recalcWrapperWidthIE6();
    }
    else // if ($.browser.msie && $.browser.version < 7)
    {
        var wrapperCssMaxWidthPercent = 
            (100 * (parseFloat($(percentBasedMaxWidthContainer).outerWidth()) / parseFloat($(percentBasedMaxWidthContainer).parent().width())) );
        var wrapperCssMaxWidth = Math.round(wrapperCssMaxWidthPercent) + '%';
		//alert(wrapperCssMaxWidth);
        
        function recalcWrapperWidth()
        {
            // If container min-width is more, than what we get with our percentage-based wrapper - 
            // increase wrapper size to container min-width
            if (Number($(minWidthContent).css('min-width').replace(/px$/, '')) > $(percentBasedMaxWidthContainer).width())
            {
                // But only increase, if there is any extra space in the parent of the container
                if ($(percentBasedMaxWidthContainer).width() < $(percentBasedMaxWidthContainer).parent().width())
                {
                    $(percentBasedMaxWidthContainer).css('max-width', 'none');
                    $(percentBasedMaxWidthContainer).css('width', Number($(minWidthContent).css('min-width').replace(/px$/, '')));
                }
            }
            
            // Container min-width is less then or equal wrapper width
            else 
            {
                // If wrapper width is less than what we should have with its percentage
                if ($(percentBasedMaxWidthContainer).width() < $(percentBasedMaxWidthContainer).parent().width() * (wrapperCssMaxWidthPercent / 100))
                {
                    $(percentBasedMaxWidthContainer).css('max-width', wrapperCssMaxWidth);
                    $(percentBasedMaxWidthContainer).css('width', null);
                }
            }
        }
        
        $('#main_wrapper').resize(recalcWrapperWidth); //jei selectorius = window, supercrashable ie nugriuva ant resize
        recalcWrapperWidth();
    }
		

