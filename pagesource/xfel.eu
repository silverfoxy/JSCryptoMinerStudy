function showTitles(pageIndex, callback) {
	toggleSearch(true);
	var pageSize = parseInt($("span#limit").text());
	if (typeof pageIndex != "undefined" && pageIndex != null) {
		showTitlesParams['pageIndex'] = pageIndex;
		showTitlesParams['qs'] = pageIndex*pageSize;
