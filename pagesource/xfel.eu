<span id="combine_eventvars" style="display:none;" data-xcaebothheadings="0" data-xcaelayout="2"></span>
<script>
  /*** jQuery to combine content objects indico and announcement (BV 2017-06) ***/
  $(function() {
    // ZMS-object variables
    var xcae_layout = $('#combine_eventvars').data('xcaelayout');
    var xcae_bothheadings = $('#combine_eventvars').data('xcaebothheadings');
    // change div .teaser--event in .teaser--announcement-event
    $('.teaser-container.teaser--event').addClass('teaser--announcement-event xfel-combined').removeClass('teaser--event');
    // add mdl-grid div mit zwei columns
    $('.teaser-container.teaser--announcement-event.xfel-combined').prepend("<div class='mdl-grid mdl-grid--no-spacing'><div class='mdl-cell mdl-cell--24-col-tablet mdl-cell--12-col' id='xfel-announcements-intro'></div><div class='mdl-cell mdl-cell--12-col mdl-cell--hide-phone mdl-cell--hide-tablet' id='xfel-events-intro'></div></div>");
    // move .teaser-intro "Events"
    $('.teaser-container.teaser--announcement-event.xfel-combined').find('.teaser-intro').first().appendTo("#xfel-events-intro");
    if (xcae_bothheadings == 1) {
      // move .teaser-intro "Announcements"
      $('.teaser-container.teaser--announcement-event:not(.xfel-combined)').find('.teaser-intro').first().appendTo("#xfel-announcements-intro");
    } else {
      // remove Announcements-Headline
      $('#xfel-announcements-intro').remove();
      $('.teaser-container.teaser--announcement-event:not(.xfel-combined)').find('.teaser-intro').first().remove();
      $('#xfel-events-intro').removeClass('mdl-cell--12-col').addClass('mdl-cell--24-col');
    }
    // add mdl-grid div mit zwei columns
    $('.teaser-container.teaser--announcement-event.xfel-combined').find('.mdl-grid.mdl-grid--no-spacing:nth-child(2)').prepend("<div class='mdl-cell mdl-cell--24-col-tablet mdl-cell--12-col' id='xfel-announcements-wrapper'></div><div class='mdl-cell mdl-cell--24-col mdl-cell--hide-desktop' id='xfel-events-wrapper'></div>");
    // move announcements to #xfel-announcements-wrapper
    $('.teaser-container.teaser--announcement-event:not(.xfel-combined) > .announcement.announcement-js').appendTo("#xfel-announcements-wrapper");
    if (xcae_layout == 1) {
      // remove Event 3 and 4
      $('.teaser-container.teaser--announcement-event.xfel-combined .mdl-cell.mdl-cell--12-col-tablet.mdl-cell--6-col').slice(2).remove();
    }
    if (xcae_layout == 2) {
      // border-left fuer das dritte event
      $('.teaser-container.teaser--announcement-event.xfel-combined > .mdl-grid.mdl-grid--no-spacing:nth-child(2) > div.mdl-cell.mdl-cell--12-col-tablet.mdl-cell--6-col:nth-child(5) > a.teaser.teaser--first').css('border-left-width','1px');
    }
  });
</script>


