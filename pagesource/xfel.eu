	<script>
	/* Highlight active Nav-Parent */
	var thispatharr = $('.bodyweb').data('path').split("/");
	$('.xfel-header-navigation__link').each(function() {
		var itemid = $(this).attr("id");
		if ($.inArray(itemid,thispatharr) > -1) {
			$(this).addClass("active");
		}
	});
	/* Subscribe Newsletter */
	jQuery("#xfel_nl").on("submit", function(nl_subscr) {
		nl_subscr.preventDefault();
		var xfel_nl_email = jQuery("#xfel_nl_email").val();
		var xfel_nl_nobo = jQuery("#xfel_nl_nobo").val();
		if (xfel_nl_nobo == '') {
			jQuery.fancybox.open({
				padding : 0,
				margin:[30,30,30,30],
				autoSize:!0,
				autoResize:!0,
				closeBtn:!1,
				href:'/subscribe_to_newsletter.html?e_mail_address='+xfel_nl_email+'',
				type: 'iframe',
				tpl:{wrap:'<div class="fancybox-wrap" tabIndex="-1">	<div class="fancybox-skin">		<div class="fancybox-outer">			<div class="fancybox-header">				<h2>Subscribe to our newsletter</h2>				<div class="closeBtn">					<svg class="more" width="20px" height="20px" viewBox="0 0 20 20">						<use xlink:href="#icon-accordeon-more"></use>					</svg>				</div>			</div>		<div class="fancybox-inner">								</div>	</div></div></div>'},
				afterShow:function(){
					jQuery(".fancybox-header .closeBtn").one("click",function() {
						jQuery.fancybox.close()
					})
				}
			});
		}
	});
	/* DropDown Menu */
	$(".xfel-header-navigation__item").each(function() { 
