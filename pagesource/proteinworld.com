    jQuery(document).on('submit', '#discount-coupon-form', function(e) {
        e.preventDefault();
        e.stopImmediatePropagation();
        var _form = jQuery('#discount-coupon-form'),
            _coupon = jQuery('#coupon_code'),
            _cart = jQuery('#mini-basket');

        if (_coupon.hasClass('required') && !_coupon.val()) {
            alert(_coupon.data('empty'));
        } else {
            _cart.addClass('loading');
            jQuery.ajax({
                url:_form.attr('action'),
                type:'POST',
                dataType: 'json',
                data:_form.serialize(),
                success:function(promoData) {
                    if(promoData)
                    {
                        if (promoData.success) {
                            // Run the ajax cart update
                            jQuery.getJSON("/ajaxcart/cache/reloadCart",function(data) {
                                // Refresh the cart
                                if (ajaxAddToCart) {
                                    ajaxAddToCart.handleResponse(data, function (response) {});
                                }
                                // Add the message
                                if (promoData.message) {
                                    jQuery('#cart-sidebar').before('<div class="message">' + promoData.message + '</div>');
                                }
                                _cart.removeClass('loading');
                            });
                        } else if (promoData.message) {
                            _form.find('.message').html(promoData.message);
                            _cart.removeClass('loading');
                        }
                    }
                }
            });
        }
        return false;
    });

