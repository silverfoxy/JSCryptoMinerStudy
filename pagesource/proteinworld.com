<script type="text/javascript">
    var modalsPvCookieName = 'pw_mpv';
    var modalsStCookieName = 'pw_mst';
    var modalsStQueryParam = 'modalid';
    var modalsForceViewQueryParam = 'viewmodalid';
    var qParams = document.URL.toQueryParams();
    var ajaxData = {};


    // Handle force modal view ajax data
    if(qParams && qParams[modalsForceViewQueryParam]) {
        ajaxData[modalsForceViewQueryParam] = qParams[modalsForceViewQueryParam];
    }

    var canManageCookies = Mage && Mage.Cookies;
    if(canManageCookies) {
        // Handle split testing cookies
        if(qParams && qParams[modalsStQueryParam]) {
            Mage.Cookies.set(modalsStCookieName, qParams[modalsStQueryParam]);
        }

        // Handle page view counter cookies
        var mPageViewsValue = parseInt(Mage.Cookies.get(modalsPvCookieName));
        if(isNaN(mPageViewsValue)) {
            mPageViewsValue = 0;
            Mage.Cookies.set(modalsPvCookieName, mPageViewsValue);
        }
    }


    // Ping an ajax request to config endpoint
    new Ajax.Request('https://www.proteinworld.com/modals/index/ajax/', {
        parameters: ajaxData,
        onSuccess: function(transport) {

            // Verify we have some response text
            if (transport && transport.responseText) {

                // Parse as an object
                try {
                    response = eval('(' + transport.responseText + ')');
                }
                catch (e) {
                    response = {};
                }

                if(response.show_popup) {

                    if(response.popup_html){

                        $('popup-container').update(response.popup_html);

                        jQuery(".hidden_link").fancybox({

                            'openEffect' : 'fade',
                            'closeEffect' : 'fade',
                            'width' : 750,
                            'autoDimensions' : false,
                            'padding' : [0, 0, 0, 0],
                            beforeShow : function(){
                                jQuery(".fancybox-skin").addClass("promo-box");
                                if(jQuery(window).width() < 980){
                                    jQuery('body').find('.fancybox-wrap').css('visibility', 'hidden');
                                };
                            },
                            afterShow: function(){
                                if(jQuery(window).width() < 980){

                                    setTimeout(function(){
                                        jQuery('body').find('.fancybox-wrap').removeAttr('style').addClass('pw-newsletter-mobile');
                                        jQuery('body').find('.fancybox-inner').css('width', '100%');
                                    }, 10);
                                }
                            },
                            onUpdate: function(){
                                if(jQuery(window).width() < 980){

                                    setTimeout(function(){
                                        jQuery('body').find('.fancybox-wrap').removeAttr('style').addClass('pw-newsletter-mobile');
                                        jQuery('body').find('.fancybox-inner').css('width', '100%');
                                    }, 10);
                                }
                            }
                        });

                        // One second delay
                        setTimeout(function(){
                            jQuery(".hidden_link").click();
                        }, 1200);

                    }
                }
            }

            // Increment page view counter after the request has been processed
            if(canManageCookies) {
                Mage.Cookies.set(modalsPvCookieName, mPageViewsValue + 1);
            }
        }
    });

</script>

