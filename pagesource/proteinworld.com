    <script type="text/javascript">

        jQuery(function() {

            /**
             * Language / Country select action
             */
            var countrySelect = jQuery('#storeChangerLightbox');
            var languageSelect = jQuery('#languageChangerLightbox');

            jQuery('#storeChangerLightbox select').on('change', function(e) {

                e.preventDefault();

                languageSelect.addClass('loading').removeClass('active');
                var country_id = countrySelect.find(':selected').data('star');

                /* Load countries based on store ID */
                jQuery.getJSON('https://www.proteinworld.com/storemanager/ajax/getlanguagesajax/', {country_id: country_id}, function(data) {

                    if(data.response == 'success') {

                        var languages = data.languages;
                        var select = languageSelect.find('select');
                        select.empty();

                        jQuery.each(languages,function(key, value) {
                            select.append(jQuery('<option value="' + value['redirect'] + '">' + value['name'] + '</option>'));
                        });


                        /**
                         * If there is only 1 option, redirect to that site. If there is more than 1 show the language selector
                         */
                        if(data.count == 1){
                            jQuery.each(languages,function(key, value) {
                                window.location.href = value['redirect']
                            });
                        } else {
                            languageSelect.removeClass('loading').addClass('active');
                        }


                    } else {
                        alert('There has been a problem loading available countries, please refresh and try again.');
                    }

                });

            });


            /**
             * Redirect to store view
             */
            jQuery('#languageChangerLightbox select').on('change', function(e) {
                e.preventDefault();

                languageSelect.removeClass('loading active');
                countrySelect.addClass('loading');
                jQuery('#store_switcher_lightbox').addClass('loading');

                window.location.href = jQuery(this).val()
            });
        });

    </script>




