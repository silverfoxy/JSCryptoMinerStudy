        <script type="text/javascript">

            jQuery(function() {

                /**
                 * Language / Country select action
                 */
                var countrySelect = jQuery('#storeChanger');
                var languageSelect = jQuery('#languageChanger');
                var overlay = jQuery('.store-select-overlay');
                var region = jQuery('#storeChanger .region');

                jQuery('#storeChanger a').on('click', function(e) {

                    e.preventDefault();

                    countrySelect.find('a').removeClass('active');
                    countrySelect.addClass('loading');

                    jQuery(this).addClass('active');

                    languageSelect.removeClass('active').addClass('loading');

                    var country_id = jQuery(this).data('star');
                    var flag = jQuery(this).data('flag');

                    /* Load countries based on store ID */
                    jQuery.getJSON('https://www.proteinworld.com/storemanager/ajax/getlanguagesajax/', {country_id: country_id}, function(data) {

                        if(data.response == 'success') {

                            var languages = data.languages;
                            var select = languageSelect.find('ul');
                            select.empty();

                            languageSelect.find('.flag').css('background-image', 'url(' + flag + ')');

                            jQuery.each(languages,function(key, value) {
                                select.append(jQuery('<li><a href="' + value['redirect'] + '"><span>' + value['name'] + '</span></a><li>'));
                            });


                            /**
                             * If there is only 1 option, redirect to that site. If there is more than 1 show the language selector
                             */
                            if(data.count == 1){
                                jQuery.each(languages,function(key, value) {
                                    window.location.href = value['redirect']
                                });
                            } else {
                                languageSelect.addClass('active').removeClass('loading');
                                overlay.addClass('active');
                                countrySelect.removeClass('loading').addClass('language-active');
                            }


                        } else {
                            alert('There has been a problem loading available countries, please refresh and try again.');
                        }

                    });

                    jQuery('.language-close').on('click', function(e) {

                        e.preventDefault();

                        languageSelect.removeClass('active');
                        overlay.removeClass('active');
                        countrySelect.removeClass('language-active');

                    });


                });

                region.on('click', function(e) {
                    if(jQuery(this).hasClass('active')) {
                        jQuery(this).removeClass('active');
                        jQuery('.store-select-overlay').removeClass('active');
                        jQuery(this).children('ul').removeClass('active');
                    } else {
                        region.removeClass('active');
                        jQuery('#storeChanger .region ul').removeClass('active');

                        // Calc position of country list
                        var inner = jQuery(this).children('ul'),
                            regionWidth = jQuery(this).outerWidth()/ 2,
                            childWidth = inner.outerWidth()/2;
                        inner.css('left', ((-childWidth) + regionWidth));

                        jQuery(this).addClass('active');
                        jQuery('.store-select-overlay').addClass('active');
                        jQuery(this).children('ul').addClass('active');
                    }
                    return false;
                });

                // On click of the overlay
                jQuery(document).on('click','.overlay-active',function(e){
                    e.preventDefault();
                    // remove map
                    if (jQuery('.show-stores').hasClass('active')) {
                        jQuery('.show-stores').removeClass('active');
                        jQuery('body').removeClass('store-select-open');
                        jQuery('.sidebar-link.overlay').removeClass('overlay-active');
                        jQuery('header').css('z-index', '25');

                        region.removeClass('active').children('ul').removeClass('active');
                        overlay.removeClass('active');
                        languageSelect.removeClass('active');
                        countrySelect.removeClass('language-active');
                    }
                });

            });

        </script>

