<script type="text/javascript">
function abbToggleX(show) {
    
    var x = $('#ghaxyURtYd .close-button');    
    
    if (x.length > 0) {
        if      (show == 'false') { x.addClass('inactive').removeClass('active'); }
        else if (show == 'true')  { x.addClass('active').removeClass('inactive'); }
    }
}

function abbClosers() {
    
    var b = $('#ghaxyURtYd');                       
    var w = $('#wrapper-content');                          
    var r = $('#ghaxyURtYd .button');               
    var x = $('#ghaxyURtYd .close-button.active');  
    var e = [];
    if (b.length > 0 && w.length > 0) {
        if (r.length > 0) { e.push(r); }
        if (x.length > 0) { e.push(x); }
        $.each( e, function( i, v ) {
            $(this).off();
            $(e[i]).click(function() {
                b.css('display','');
                w.css('opacity','').css('filter','').css('pointer-events', '');
                if ($(this).hasClass('close-button') && typeof startAdBlockDetectionWithDelay == 'function') {
                    ga('send', 'event', { 
                        eventCategory: 'AdBlocker',
                        eventAction: 'click-close',
                        eventLabel: 'adblocker:layer:' + (spGetConfig().count - 1)
                    });
                    startAdBlockDetectionWithDelay(true);
                }
                if ($(this).hasClass('button')) {
                    ga('send', 'event', { 
                        eventCategory: 'AdBlocker',
                        eventAction: 'click-reload',
                        eventLabel: 'adblocker:layer:' + (spGetConfig().count - 1)
                    });
                    location.reload(); 
                }
            });
        });
    }
}

function abbUA() {
    
    var b = $('#ghaxyURtYd');                                   
    var c = 'ua-default';                                               
    var uaText1 = '';                                                   
    var uaText2 = '';                                                   
    var subSubHeadline = $('#ghaxyURtYd #sub-sub-headline');    
    var subSubHeadlineLink = '';                                        
    var statement = '';                                                 
    var statementLink = '';                                             

    if (spUA.isFirefox()) {
        c = 'ua-ff';
        uaText1 = '<b>Im Firefox</b><br/><br/>Klicken Sie neben der Adresszeile oben rechts auf das Adblocker-Symbol.';
        uaText2 = 'Wählen Sie die Option &bdquo;Deaktivieren auf: spiegel.de&ldquo;.<br/>Vielen Dank!';
    }
    else if (spUA.isInternetExplorer()) {
        c = 'ua-ie';
        uaText1 = '<b>Im Internet Explorer</b><br/><br/>Klicken Sie rechts oben neben der Adresszeile auf das Einstellungs&shy;rädchen.';
        uaText2 = 'Gehen Sie auf &bdquo;Add-Ons verwalten&ldquo;.<br/><br/>Deaktivieren Sie dort Ihren Adblocker. Vielen Dank!';
    }
    else if (spUA.isEdge()) {
        c = 'ua-edge';
        uaText1 = '<b>Im Edge-Browser</b><br/><br/>Klicken Sie neben der Adresszeile oben rechts auf die drei Einstellungs&shy;punkte.';
        uaText2 = 'Klicken Sie auf das Icon Ihres Adblockers.<br/><br/>Wählen Sie bitte &bdquo;Adblock auf Seiten dieser Domain deaktivieren&ldquo;. Vielen Dank!';
    }
    else if (spUA.isSafari() || spUA.isChrome()) {
        c = 'ua-webkit';
        uaText1 = '<b>Im Chrome- und Safari-Browser</b><br/><br/>Klicken Sie neben der Adresszeile oben rechts auf das Adblocker-Symbol.';
        uaText2 = 'Entfernen Sie das Häkchen bei &bdquo;Auf dieser Seite aktiviert&ldquo;.<br/>Vielen Dank!';
    }

    
    var customHeadline           = abbLayerCustomHeadline;
    var customSubHeadline        = abbLayerCustomSubHeadline;
    var customUAText             = abbLayerCustomUAText;
    var customBrowser            = abbLayerCustomBrowser;
    var customHint               = abbLayerCustomHint;
    var customSubSubHeadline     = abbLayerCustomSubSubHeadline;
    var customSubSubHeadlineLink = abbLayerCustomSubSubHeadlineLink;
    var customStatement          = abbLayerCustomStatement;
    var customStatementLink      = abbLayerCustomStatementLink;

    var ua = c.replace('ua-','');
    var customUAText1 = ua + '-1';
    var customUAText2 = ua + '-2';

    for (prop in customUAText) {
        if (prop == customUAText1 && customUAText[prop] != '') { uaText1 = customUAText[prop]; }
        if (prop == customUAText2 && customUAText[prop] != '') { uaText2 = customUAText[prop]; }
        
    }

    if (b.length > 0) {
        abbClosers();
        if (c != 'ua-default')              { b.removeClass('ua-default').addClass('ua-detected '+c); }
