	<script>
		
		var header = document.getElementsByClassName("Header--mobile")[0];
		var container = document.getElementsByClassName("Header-mobileLinkContainer")[0];
		var menuIcon = document.getElementsByClassName("Header-icon--menu")[0];
		var closeIcon = document.getElementsByClassName("Header-icon--close")[0];
		var clickBodyToClose = false;
		var toggleContainer = function() {
			container.classList
				.toggle("Header-mobileLinkContainer--closed");
			menuIcon.classList.toggle("is-hidden");
			closeIcon.classList.toggle("is-hidden");
		};

		document.body.onclick = function() {
			if (clickBodyToClose) {
				toggleContainer();
				clickBodyToClose = false;
			}
		};

		header.onclick = function(e) {
				var removeBodyOnClick = function() {
					document.body.onclick = function() {
						return false;
					}
				};

				toggleContainer();

				container.onclick = function(e) {
					e.stopPropagation();
				}
				e.stopPropagation();

				if (!container.classList
					.contains("Header-mobileLinkContainer--closed")) {
					clickBodyToClose = true;
				} else {
					clickBodyToClose = false;
				}
    };
	</script>

