    <script>
      var regionColumn = document.getElementsByClassName("Dealers-regionColumn")[0];
      var regionsArr = document.getElementsByClassName("Dealers-region");
      var countryColumn = document.getElementsByClassName("Dealers-countryColumn")[0];
      var countriesArr = document.getElementsByClassName("Dealers-country");
      var countryBack = document.getElementsByClassName("Dealers-back--country")[0];
      var dealerColumn = document.getElementsByClassName("Dealers-dealerColumn")[0];
      var dealersArr = document.getElementsByClassName("Dealers-dealer");
      var dealerBack = document.getElementsByClassName("Dealers-back--dealer")[0];
      var selecteddealersArr = document.getElementsByClassName("Dealers-selectedDealerContainer");

      var makeColumnActive = function(type) {
        if (type === "region") {
          regionColumn.classList.add("Dealers-column--active");
          countryColumn.classList.remove("Dealers-column--active");
          dealerColumn.classList.remove("Dealers-column--active");
        } else if (type === "country") {
          regionColumn.classList.remove("Dealers-column--active");
          countryColumn.classList.add("Dealers-column--active");
          dealerColumn.classList.remove("Dealers-column--active");
        } else {
          regionColumn.classList.remove("Dealers-column--active");
          countryColumn.classList.remove("Dealers-column--active");
          dealerColumn.classList.add("Dealers-column--active");
        }
      }

      var clickRegion = function() {
        var region = this.getAttribute("data-region"),
          hasCountries = false;

        [].forEach.call(regionsArr, function(region) {
          region.classList.remove("Dealers-region--active");
        });

        this.classList.add("Dealers-region--active");

        [].forEach.call(countriesArr, function(country) {
          country.classList.remove("Dealers-country--active");

          if (country.getAttribute("data-region") === region) {
            hasCountries = true;
            country.classList.add("Dealers-country--visible");
          } else {
            country.classList.remove("Dealers-country--visible");
          }
        });

        countryColumn.classList.remove("Dealers-countryColumn--showBorder");

        if (hasCountries) {
          regionColumn.classList.add("Dealers-regionColumn--showBorder");
        } else {
          regionColumn.classList.remove("Dealers-regionColumn--showBorder");
        }

        [].forEach.call(dealersArr, function(dealer) {
          dealer.classList.remove("Dealers-dealer--visible");
          dealer.classList.remove("Dealers-dealer--active");
        });

        [].forEach.call(selecteddealersArr, function(dealer) {
          dealer.classList.remove("Dealers-selectedDealerContainer--active");
        });

        makeColumnActive("country");
      }

      var clickCountry = function() {
        var country = this.getAttribute("data-country"),
          hasdealers = false;

        [].forEach.call(countriesArr, function(country) {
          country.classList.remove("Dealers-country--active");
        });

        this.classList.add("Dealers-country--active");

        [].forEach.call(dealersArr, function(dealer) {
          dealer.classList.remove("Dealers-dealer--active");

          if (dealer.getAttribute("data-country") === country) {
            hasdealers = true;
            dealer.classList.add("Dealers-dealer--visible");
          } else {
            dealer.classList.remove("Dealers-dealer--visible");
          }
        });

        if (hasdealers) {
          countryColumn.classList.add("Dealers-countryColumn--showBorder");
        } else {
          countryColumn.classList.remove("Dealers-countryColumn--showBorder");
        }

        [].forEach.call(selecteddealersArr, function(dealer) {
          dealer.classList.remove("Dealers-selectedDealerContainer--active");
        });

        makeColumnActive("dealer");
      }

      var clickDealer = function() {
        var _dealer = this.getAttribute("data-dealer");

        [].forEach.call(dealersArr, function(dealer) {
          dealer.classList.remove("Dealers-dealer--active");
        });

        this.classList.add("Dealers-dealer--active");

        [].forEach.call(selecteddealersArr, function(dealer) {
          if (dealer.getAttribute("data-dealer") === _dealer) {
            dealer.classList.add("Dealers-selectedDealerContainer--active");
          } else {
            dealer.classList.remove("Dealers-selectedDealerContainer--active");
          }
        });
      }

      var clickCountryBack = function() {
        makeColumnActive("region");
      }

      var clickDealerBack = function() {
        [].forEach.call(countriesArr, function(country) {
          country.classList.remove("Dealers-country--active");
        });

        [].forEach.call(selecteddealersArr, function(dealer) {
          dealer.classList.remove("Dealers-selectedDealerContainer--active");
        });

        makeColumnActive("country");
      }

      makeColumnActive("region");

      [].forEach.call(regionsArr, function(region) {
        region.onclick = clickRegion;
      });

      [].forEach.call(countriesArr, function(country) {
        country.onclick = clickCountry;
      });

      [].forEach.call(dealersArr, function(dealer) {
        dealer.onclick = clickDealer;
      });

      countryBack.onclick = clickCountryBack;
      dealerBack.onclick = clickDealerBack;

    </script>
  </div>
