        $('.bxslider').bxSlider({
            auto: true,
            minSlides: 5,
            maxSlides: 6,
            moveSlides: 1,
            slideWidth: 120,
            slideMargin: 50,
            autoHover: true,
            pause: 2000,
            onSliderLoad: function () {
                $('.bxslider li a').hover(function (e) {
                    // in
                    var item = $(this).attr('rel');
                    $('#' + item).show();
                }, function (e) {
                    // out
                    var item = $(this).attr('rel');
                    $('#' + item).hide();
                });
            }
        });

    })
</script>
