<script id='settings-page-template' type='text/backbone_template'>
<span class='page-title hidden'>
Settings
</span>
<div id='px'>
<div class='background header'>
<div class='clearfix' id='subheader'></div>
</div>
<div class='container'>
<div id='settings-menu-container'></div>
<div class='col d6x5 clearafter'>
<form action='/me' class='user-settings-form' method='put' role='form'>
<div class='controlgroup d12x5 g1 margin'>
<h3>
Information
</h3>
<div class='controls'>
<div class='arrow'></div>
<div class='check mail'></div>
<div class='checkinfo'>
<label class='smalllabel' for='email'>
Email address
</label>
<div class='showbox'>
<input class='full' disabled name='email' placeholder='Email address' type='text' value='{{_.escapeOnce(me.email)}}'>
</div>
</div>
<div class='clear'></div>
<div class='check person'></div>
<div class='checkinfo'>
<label class='smalllabel' for='firstname'>
First Name
</label>
<input class='full' name='firstname' placeholder='Your name' type='text' value='{{_.escapeOnce(me.firstname)}}'>
</div>
<div class='clear'></div>
<div class='check person'></div>
<div class='checkinfo'>
<label class='smalllabel' for='lastname'>
Last Name
</label>
<input class='full' name='lastname' placeholder='Your surname' type='text' value='{{_.escapeOnce(me.lastname)}}'>
</div>
<div class='clear'></div>
<div class='check company'></div>
<div class='checkinfo'>
<label class='smalllabel' for='company'>
Company Name
</label>
<input class='full' name='company' placeholder='Company name' type='text' value='{{_.escapeOnce(me.settings.company)}}'>
</div>
<div class='clear'></div>
<div class='check address'></div>
<div class='checkinfo'>
<label class='smalllabel' for='country'>
Country
</label>
<select class='full country-select' name='country'>
<option value=''></option>
[[ _.each(gon.countries, function(country) { ]]
[[ if (me.address && me.address.country && me.address.country.toLowerCase() == country.toLowerCase()) { ]]
<option selected='selected' value='{{_.escapeOnce(country)}}'>
{{_.escapeOnce(country)}}
</option>
[[ } else { ]]
<option value='{{_.escapeOnce(country)}}'>
{{_.escapeOnce(country)}}
</option>
[[ } ]]
[[ }); ]]
</select>
</div>
<div class='clear'></div>
<div class='check address'></div>
<div class='checkinfo'>
<label class='smalllabel' for='state'>
State/Province
</label>
<input class='full province-input' name='state' placeholder='State or province' type='text' value='{{_.escapeOnce(me.address.state)}}'>
<select class='full province-select' name='state'></select>
</div>
<div class='clear'></div>
<div class='check address'></div>
<div class='checkinfo'>
<label class='smalllabel' for='address1'>
Street Address
</label>
<input class='full' name='address1' placeholder='Street, number and apt.' type='text' value='{{_.escapeOnce(me.address.address1)}}'>
</div>
<div class='clear'></div>
<div class='check address'></div>
<div class='checkinfo'>
<label class='smalllabel' for='city'>
City
</label>
<input class='full' name='city' placeholder='City' type='text' value='{{_.escapeOnce(me.address.city)}}'>
</div>
<div class='clear'></div>
<div class='check address'></div>
<div class='checkinfo'>
<label class='smalllabel' for='zip'>
Zip/Postal Code
</label>
<input class='full' name='zip' placeholder='### ###' type='text' value='{{_.escapeOnce(me.address.zip)}}'>
</div>
<div class='clear'></div>
<div class='check address'></div>
<div class='checkinfo'>
<label class='smalllabel' for='phone'>
Contact Phone Number
</label>
<input class='full' name='phone' placeholder='(+00) 000 000 0000' type='text' value='{{_.escapeOnce(me.address.phone)}}'>
</div>
<div class='clear'></div>
</div>
</div>
<div class='controlgroup d12x5 g1'>
[[ if (subscription) { ]]
<h3>
Bundle Details
</h3>
<div class='controls'>
<div class='arrow'></div>
<div class='subscription-summary'>
<div class='checkinfo'>
[[ if (gon.flat_custom_license_price) { ]]
<span>
Licenses remaining:
</span>
<span class='pull-right'>
{{Math.floor(wallet.get('credit') / gon.flat_custom_license_price)}}
</span>
[[ } else { ]]
<span>
Funds remaining:
</span>
<span class='pull-right'>
{{Prime.Utils.Common.formatMoney(wallet.get("credit") / 100.0, gon.currency, { letterCurrency: true })}}
</span>
[[ } ]]
<div class='clear'></div>
</div>
<div class='checkinfo'>
<span>
For more information or help:
</span>
<span class='pull-right'>
<a href='/contact'>
Contact us
</a>
</span>
<div class='clear'></div>
</div>
</div>
<div class='clear'></div>
</div>
[[ } ]]
<h3>
Credit Cards
</h3>
<div class='controls'>
<div class='arrow'></div>
<div class='credit-card-form-container'></div>
<div class='clear'></div>
</div>
</div>
<div class='clear'></div>
<div class='linetop'></div>
<div class='alignright'>
<input class='submit button green' type='submit' value='Save changes'>
</div>
<div class='clear'></div>
</form>
</div>
</div>
</div>
</script>

<script id='purchases-page-template' type='text/backbone_template'>
