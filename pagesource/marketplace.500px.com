<script id='payment-method-form-template' type='text/backbone_template'>
[[ if (!cardManagement && !me.get('has_custom_license_prices')) { ]]
<a class='add-credit-card btn btn-mini pull-right display' href='#'>
Add
</a>
<a class='cancel-add-credit-card pull-right btn btn-mini add'>
Cancel
</a>
<div class='clearfix'></div>
[[ } ]]
<div class='display'>
<ul class='payment-methods'></ul>
</div>
[[ if (cardManagement) { ]]
<button class='add-credit-card pill action btn btn-primary display [[ if (emptyCards) { ]] btn-block [[ } else { ]] btn-xs [[ } ]]'>
Add Card
</button>
[[ } ]]
<form action='' class='add-credit-card-form edit add' method='POST' role='form'>
<input data-stripe='name' type='hidden' value="{{me.escape('firstname')}} {{me.escape('lastname')}}">
<input data-stripe='address_line1' type='hidden' value="{{me.get('address') ? _.escapeOnce(me.get('address').address1) : ''}}">
<input data-stripe='address_city' type='hidden' value="{{me.get('address') ? _.escapeOnce(me.get('address').city) : ''}}">
<input data-stripe='address_state' type='hidden' value="{{me.get('address') ? _.escapeOnce(me.get('address').state) : ''}}">
<input data-stripe='address_zip' type='hidden' value="{{me.get('address') ? _.escapeOnce(me.get('address').zip) : ''}}">
<input data-stripe='address_country' type='hidden' value="{{me.get('address') ? _.escapeOnce(me.get('address').country) : ''}}">
<div class='card-number-group form-group has-feedback'>
<label>
Card Number
</label>
<input class='card-number-input form-control' data-stripe='number' maxlength='16' size='20' tabindex='1' type='text'>
<img class='credit-card-icon'>
<span class='glyphicon glyphicon-remove form-control-feedback error-feedback label-padding'></span>
</div>
<div class='card-cvc-group form-group has-feedback pull-right'>
<label class='cvc-label'>
CVC
<div class='help-widget' data-placement='top' data-title='The CVC is a 3 or 4 digit security code that can be found on your credit card.' data-toggle='tooltip'>
<img class='icon info-icon' src='https://mktcdn.500px.org/assets/icons/info_icon_blue-01bc749c2227b87de7260bb1a026320919b3803206f60fea2e4b6537469e61b6.svg'>
</div>
</label>
<input class='card-cvc-input form-control' data-stripe='cvc' maxlength='4' size='4' tabindex='4' type='text'>
<span class='glyphicon glyphicon-remove form-control-feedback error-feedback label-padding'></span>
<span class='glyphicon glyphicon-ok form-control-feedback success-feedback label-padding'></span>
</div>
<div class='card-expiry-group form-group has-feedback'>
<label>
Expiry Date
</label>
<div class='wrapper month-wrapper'>
<input class='card-month-input form-control' data-stripe='exp-month' maxlength='2' placeholder='MM' size='2' tabindex='2' type='text'>
<span class='glyphicon glyphicon-remove form-control-feedback error-feedback'></span>
<span class='glyphicon glyphicon-ok form-control-feedback success-feedback'></span>
</div>
<div class='wrapper year-wrapper'>
<input class='card-year-input form-control' data-stripe='exp-year' maxlength='4' placeholder='YYYY' size='4' tabindex='3' type='text'>
<span class='glyphicon glyphicon-remove form-control-feedback error-feedback'></span>
<span class='glyphicon glyphicon-ok form-control-feedback success-feedback'></span>
</div>
</div>
[[ if (cardManagement) { ]]
<div class='form-group'>
<label>
<input class='default-card-checkbox' type='checkbox'>
Make this my default card
</input>
</label>
</div>
[[ } else { ]]
<div class='form-group'>
<label>
<input checked='checked' class='save-card-checkbox' type='checkbox'>
Remember this card
</input>
</label>
</div>
[[ } ]]
[[ if (cardManagement) { ]]
<div class='edit'>
<button class='submit-edited-credit-card btn btn-primary' type='submit'>
Update
</button>
<button class='delete-credit-card btn btn-danger'>
Delete
</button>
<button class='cancel-edit-credit-card btn btn-link'>
Cancel
</button>
</div>
<div class='add'>
<button class='submit-new-credit-card btn btn-primary' type='submit'>
Add
</button>
<button class='cancel-add-credit-card btn btn-link'>
Cancel
</button>
</div>
[[ } ]]
<div class='clearfix'></div>
</form>
<div class='payment-errors'></div>
</script>

