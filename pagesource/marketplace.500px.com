<script id='cart-items-template' type='text/template'>
[[ if(cartItems.length) { ]]
<ul class='cart-items__list'>
[[ PxTopnav._.each(cartItems, function(cartItem){ ]]
<li class='cart-items__cart-item'>
<div class='cart-items__image-section'>
<a href='{{getPhotoPagePath(cartItem.photo)}}'>
<img src="{{getPhotoImageUrl(cartItem.photo.images, '115')}}">
</a>
</div>
<div class='cart-items__details-section'>
<a class='cart-items__photo-id' href='{{getPhotoPagePath(cartItem.photo)}}'>
<span class="cart-items__hash">#</span>{{cartItem.photo_id}}
</a>
<div class='cart-items__license-type'>
{{getDisplayableLicenses(cartItem.licenses)}}
</div>
<div class='cart-items__photo-size'>
{{PxTopnav.Services.PhotoLicenseHelper.getDisplayableResolutionAndPrintSize(cartItem.photo, cartItem.licenses)}}
</div>
</div>
<div class='cart-items__price-section'>
{{PxTopnav.Services.PhotoLicenseHelper.formatMoney(getTotalLicensesPrice(cartItem.licenses) / 100.0, {hideTrailingZeros: true} )}}
</div>
</li>
[[ }) ]]
[[ if (hasMoreItems) { ]]
<li class='cart-items__cart-item cart-items__more'>
The rest of your items are in <a href="//marketplace.500px.com/cart">the cart</a>.
</li>
[[ } ]]
<li class='cart-items__cart-item last'></li>
</ul>
<div class='cart-items__cta'>
<div class='cart-items__cta-buttons'>
<a class='button submit cart-items__checkout-button' href='https://marketplace.500px.com/cart'>
Checkout
</a>
<a class='cart-items__dismiss-flyout-button' href='#'>
Continue shopping
</a>
</div>
[[ if (!hasMoreItems) { ]]
<div class='cart-items__total-price-section'>
Subtotal
<span class='cart-items__total-price'>
{{PxTopnav.Services.PhotoLicenseHelper.formatMoney(getTotalPrice(cartItems) / 100.0, { letterCurrency: true, hideTrailingZeros: true })}}
</span>
</div>
[[ } ]]
</div>
[[ } else { ]]
<div class='cart-items__empty-cart'>
<h2>
Your cart is empty
</h2>
<p>
User search to find the perfect photo to license
</p>
<a href='https://500px.com/search'>
Search for photos, location, or people
</a>
</div>
[[ } ]]
