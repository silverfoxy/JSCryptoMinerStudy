		if (skill == "LIS"){
			switch (score) {
				case 39: case 40:
					band = 9; level = "Expert"; break;
				case 37: case 38:
					band = 8.5; level = "Very Good"; break;
				case 35: case 36:
					band = 8; level = "Very Good"; break;
				case 32: case 33: case 34:
					band = 7.5; level = "Good"; break;
				case 30: case 31:
					band = 7.0; level = "Good"; break;
				case 26: case 27: case 28: case 29:
					band = 6.5; level = "Competent"; break;
				case 23: case 24: case 25:
					band = 6.0;level = "Competent";  break;
				case 18: case 19: case 20: case 21: case 22:
					band = 5.5; level = "Modest"; break;
				case 16: case 17:
					band = 5.0;level = "Modest";  break;
				case 13: case 14: case 15:
					band = 4.5; level = "Limited"; break;
				case 11: case 12:
					band = 4; level = "Limited"; break;
				case 8: case 9: case 10:
					band = 3.5; level = "Extremely Limited"; break;
				case 6: case 7:
					band = 3; level = "Extremely Limited"; break;
				case 4: case 5:
					band = 2.5; level = "Ultimately Limited"; break;
				default:
					band = 0; level = "Ultimately Limited"; break;
			}
		}

		if (skill == "R_A") {
			switch (score) {
				case 39: case 40:
					band = 9; level = "Expert"; break;
				case 37: case 38:
					band = 8.5;level = "Very Good";  break;
				case 35: case 36:
					band = 8; level = "Very Good"; break;
				case 33: case 34:
					band = 7.5; level = "Good"; break;
				case 30: case 31: case 32: 
					band = 7.0; level = "Good"; break;
				case 27: case 28: case 29:
					band = 6.5; level = "Competent"; break;
				case 23: case 24: case 25: case 26: 
					band = 6.0; level = "Competent"; break;
				case 19: case 20: case 21: case 22:
					band = 5.5; level = "Modest"; break;
				case 15: case 16: case 17: case 18: 
					band = 5.0; level = "Modest"; break;
				case 13: case 14: 
					band = 4.5; level = "Limited"; break;
				case 10: case 11: case 12:
					band = 4; level = "Limited"; break;
				case 8: case 9:
					band = 3.5; level = "Extremely Limited"; break;
				case 6: case 7:
					band = 3; level = "Extremely Limited"; break;
				case 4: case 5:
					band = 2.5; level = "Ultimately Limited"; break;
				default:
					band = 0;  level = "Ultimately Limited"; break;
			}
		}

		if (skill == "R_G") {
			switch (score) {
				case 40:
					band = 9; level = "Expert"; break;
				case 39:
					band = 8.5; level = "Very Good"; break;
				case 37: case 38:
					band = 8; level = "Very Good"; break;
				case 36:
					band = 7.5; level = "Good"; break;
				case 35: case 34:
					band = 7; level = "Good"; break;
				case 33: case 32: 
					band = 6.5; level = "Competent"; break;
				case 31: case 30:
					band = 6; level = "Competent"; break;
				case 27: case 28: case 29:
					band = 5.5; level = "Modest"; break;
				case 23: case 24: case 25: case 26:
					band = 5; level = "Modest"; break;
				case 19: case 20: case 21: case 22: 
					band = 4.5; level = "Limited"; break;
				case 15: case 16: case 17: case 18:
					band = 4; level = "Limited"; break;
				case 13: case 14: case 12:
					band = 3.5; level = "Extremely Limited"; break;
				case 10: case 9: case 11:
					band = 3; level = "Extremely Limited"; break;
				case 6: case 7: case 8:
					band = 2.5; level = "Ultimately Limited"; break;
				default:
					band = 0; level = "Ultimately Limited"; break;
			}
		}	
	}
	//console.log(band);
	//console.log(level);
	document.getElementById('IELTS_Result').innerHTML="<center>Your level is <a style=\"color:red\"> " + level + "</a></center><center>Your band is <a style=\"color:red\">" + band + "</a></center>";
}
