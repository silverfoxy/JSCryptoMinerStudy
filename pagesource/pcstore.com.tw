<script type="text/javascript">
	$(function(){	   
	//計算ptEventTab和子元素a的寬度
	if( $('.ptEventTab').length > 0 ){
		var total = $('.ptEventTab').length;
		for(i=0;i<total;i++){
			var ptEventTab = $('.ptEventTab:eq('+i+') > a').length;
			var ptEventTabW1 = $('.ptEventTab:eq('+i+')').width()/ptEventTab;
			var ptEventTabW2 = Math.floor( ptEventTabW1 );
			$('.ptEventTab:eq('+i+') > a').css({'width':ptEventTabW2-1+'px'});
	
			//開啟預設對應tab的pane
			for(j=0;j<ptEventTab;j++){
				$('.ptEventTab:eq('+i+') > a:eq('+j+')').attr('rel','pane'+(i+1)+j);
				$('.ptEventTab:eq('+i+') > a:eq('+j+')').attr('id','tab'+(i+1)+j);
				$('.ptEventPane:eq('+i+') > div:eq('+j+')').attr('id','pane'+(i+1)+j);
				var ptEventTabName = $('.ptEventTab:eq('+i+') > a:eq('+j+')').attr('class');
				
				if(ptEventTabName=='current'){
					$('.ptEventPane:eq('+i+') > div:eq('+j+')').addClass('current');
				}
			}
		}
		//切換tab
		$('.ptEventTab > a').hover(function(){
			var tabNum = $(this).attr('rel');
			$(this).parent('.ptEventTab').children('a').removeClass('current');
			$(this).parent('.ptEventTab').next('.ptEventPane').children('div').removeClass('current');
			$(this).addClass('current');
			$('.ptEventPane > div#'+tabNum).addClass('current');
		});
	}
	});
</script>
