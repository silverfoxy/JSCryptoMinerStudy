    for(i=0; i<index_ad_bimg_limit; i++) {
      np=i+nowsp;
      conts+=index_ad_bimg_cot_array[np];
    }
    
    if (typeof(jQuery) == 'function') {
      tmsv=conts+'<div class="tbarback"><a onmousedown="index_ad_bimg_updown_fun(-1,2)" style="cursor:pointer;"></a></div>';
      tmsv+='<div class="tbarnext"><a onmousedown="index_ad_bimg_updown_fun(1,2)" style="cursor:pointer;"></a></div>';
      $('#id_index_ad_bimg_cot').html(tmsv);    
    }
    else {
      $("id_index_ad_bimg_cot").innerHTML=conts;
      $("id_index_ad_bimg_cot").innerHTML+='<div class="tbarback"><a onmousedown="index_ad_bimg_updown_fun(-1,2)" style="cursor:pointer;"></a></div>';
      $("id_index_ad_bimg_cot").innerHTML+='<div class="tbarnext"><a onmousedown="index_ad_bimg_updown_fun(1,2)" style="cursor:pointer;"></a></div>';
    }
           
    if (userkf != 2) { Args_setTimeout(index_ad_bigimg_autorun, 10000, index_ad_bimg_loct); }
  }
  
  function index_ad_bimg_updown_fun(udf,userkf) {
    var nwp,i,bn,va,vz;

    nwp=index_ad_bimg_loct;
    if (udf > 0) {
      index_ad_bimg_iv=1;
      if (nwp + udf >= index_ad_bimg_max) { nwp=index_ad_bimg_max-1; return; }
      else { nwp+=udf; }
    }
    else {      
      index_ad_bimg_iv=-1;
      if (nwp + udf < 0) { nwp=0; return; }
      else { nwp+=udf; }
    }

    for (i=0; i<index_ad_bimg_limit; i++) {
      bn=i+index_ad_bimg_showloct;
      if (i == 0) { va=bn; }
      else if (i == index_ad_bimg_limit-1) { vz=bn; }
    }
    if (nwp == vz+1 || nwp == va-1) { index_ad_bimg_showloct+=udf; }
    index_ad_bigimg_show(index_ad_bimg_showloct,userkf);
    
    index_ad_bigimg_onmouseout(index_ad_bimg_loct,index_ad_bimg_onclass,index_ad_bimg_offclass);
    index_ad_bigimg_onmouseover(nwp,index_ad_bimg_array[nwp],index_ad_bimg_onclass,index_ad_bimg_offclass);    
  }
  
  function index_ad_bigimg_autorun(iabloc) {
    //if (index_ad_bimg_ast) { return; }
    
    if (index_ad_bimg_iv == 1 && iabloc == index_ad_bimg_max-2) { index_ad_bimg_iv=-1; }
    else if (index_ad_bimg_iv == -1 && iabloc < 0) { index_ad_bimg_iv=1; }
    index_ad_bimg_updown_fun(index_ad_bimg_iv,3);
  }
 
  index_ad_bigimg_show(index_ad_bimg_showloct,3);  
  index_ad_bigimg_onmouseover(index_ad_bimg_loct,index_ad_bimg_array[index_ad_bimg_loct],index_ad_bimg_onclass,index_ad_bimg_offclass);    
</script>
