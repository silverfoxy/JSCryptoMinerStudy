function temperatureConverter(from,valNum) {
  valNum = parseFloat(valNum);
  var frmFahrenheit = document.getElementById("frmFahrenheit");
  var frmCelsius = document.getElementById("frmCelsius");
  var frmKelvin = document.getElementById("frmKelvin");
  if (from=="frmFahrenheit") {
    frmCelsius.value=((valNum-32)/1.8).toFixed(2);
    frmKelvin.value=(((valNum-32)/1.8)+273.15).toFixed(2);
  }
  if (from=="frmCelsius") {
    frmFahrenheit.value=((valNum*1.8)+32).toFixed(2);
    frmKelvin.value=((valNum)+273.15).toFixed(2);
  }
  if (from=="frmKelvin") {
    frmFahrenheit.value=(((valNum-273.15)*1.8)+32).toFixed(2);
    frmCelsius.value=((valNum)-273.15).toFixed(2);
  }
}

function weightConverter(from,valNum) {
  valNum = parseFloat(valNum); if(valNum < 0 ){return false;}
  var frmPounds = document.getElementById("frmPounds");
  var frmKilograms = document.getElementById("frmKilograms");
  var frmOunces = document.getElementById("frmOunces");
  var frmGrams = document.getElementById("frmGrams");
  var frmStones = document.getElementById("frmStones");
  if (from=="frmPounds") {
    frmKilograms.value=(valNum/2.2046).toFixed(2);
    frmOunces.value=(valNum*16).toFixed(2);
    frmGrams.value=(valNum/0.0022046).toFixed();
