    frmYards.value=(valNum*0.027778).toFixed(3);    
    frmKilometers.value=(valNum/39370).toFixed(6);
    frmMiles.value=(valNum*0.000015783).toFixed(6);
  }
  if (from=="frmcm") {
    frmFeet.value=(valNum*0.032808).toFixed(3);
    frmMeters.value=(valNum/100).toFixed(3);
    frmInches.value=(valNum*0.39370).toFixed(2);
    frmYards.value=(valNum*0.010936).toFixed(3);    
    frmKilometers.value=(valNum/100000).toFixed(6);
    frmMiles.value=(valNum*0.0000062137).toFixed(6);
  }
  if (from=="frmYards") {
    frmFeet.value=(valNum*3).toFixed();
    frmMeters.value=(valNum/1.0936).toFixed(2);
