}

.btn.rounded {
  border-radius: 24px;
  display: inline-block;
  min-width: 16rem;
  padding: 0 1.6rem;
  width: auto;
}

.btn:hover {
    box-shadow: 0 5px 10px 0 rgba(0,0,0,0.1);
