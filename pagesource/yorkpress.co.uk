        else if (data.slotType === 'outofpage')
        {
            dfp = googletag.defineOutOfPageSlot(googleId, data.domName)
                .addService(googletag.pubads())
                .setTargeting("position", data.positionName)
                .setCollapseEmptyDiv(data.collapseContainer);
