<style>
			
	

	
/* RIBBON START */  
.ribbon {
    position: absolute;
    left: -5px;
    top: -5px;
    z-index: 1;
    overflow: hidden;
    width: 170px;
    height: 170px;
    text-align: right;
}

.ribbon span {
	font-size: 16px;
    color: #FFF;
    font-family: MrEavesModOT, Arial, sans-serif;
    text-transform: uppercase;
    font-weight: 600;
    text-align: center;
    line-height: 24px;
    transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    width: 220px;
    display: block;
    background: #d7282f;
/*    background: linear-gradient(#f01a19 0%, #d7282f 100%);*/
    box-shadow: 0 3px 10px -5px rgba(0, 0, 0, 1);
    position: absolute;
    top: 50px;
    left: -43px;
    padding-top: 5px;
    padding-bottom: 5px;
}

.ribbon span::before {
	content: "";
	position: absolute;
	left: 0px;
	top: 100%;
	z-index: -1;
	border-left: 3px solid #840100;
	border-top: 3px solid #840100;
	border-right: 3px solid transparent;
	border-bottom: 3px solid transparent;
}

.ribbon span::after {
	content: "";
	position: absolute;
	right: 0px;
	top: 100%;
	z-index: -1;
	border-left: 3px solid transparent;
	border-bottom: 3px solid transparent;
	border-right: 3px solid #840100;
	border-top: 3px solid #840100;
}
		
		
/* Ribbon ORANGE Start */
.ribbon.orange span {
	background: #f98703;
	background: linear-gradient(#f98703 0%, #e57d05 100%);
}

.ribbon.orange span::before {
	border-left: 3px solid #ba6603;
	border-top: 3px solid #ba6603;
}

.ribbon.orange span::after {
	border-right: 3px solid #ba6603;
	border-top: 3px solid #ba6603;
}
		
		
		
		
/* Ribbon PURPLE Start */
.ribbon.purple span {
	background: #C42790;
	background: linear-gradient(#C42790 0%, #a80e75 100%);
}

.ribbon.purple span::before {
	border-left: 3px solid #83075a;
	border-top: 3px solid #83075a;
}

.ribbon.purple span::after {
	border-bottom: 3px solid transparent;
	border-top: 3px solid #83075a;
}	



/* Ribbon YELLOW Start */
.ribbon.yellow span {
	background: #fde800;
	color: black;
}

.ribbon.yellow span::before {
	    border-left: 3px solid #4d4600;
    border-top: 3px solid #4d4600;
}

.ribbon.yellow span::after {
	    border-top: 3px solid #4d4600;
    border-right: 3px solid #4d4600;
}	
		
		
		
/* Ribbon BLACK Start */
.ribbon.black span {
	background: #1D262F;
	color: white;
}

.ribbon.black span::before {
	    border-left: 3px solid #000;
    border-top: 3px solid #000;
}

.ribbon.black span::after {
	    border-top: 3px solid #000;
    border-right: 3px solid #000;
}	
		
		
		
		
/* Ribbon BLUE Start */
.ribbon.blue span {
	background: #163b58;
	color: white;
}

.ribbon.blue span::before {
	    border-left: 3px solid #000;
    border-top: 3px solid #000;
}

.ribbon.blue span::after {
	    border-top: 3px solid #000;
    border-right: 3px solid #000;
}	
		
		
		
		

/* Ribbon MEGA Christmas Start */
.ribbon.christmas-mega span {
	background: #cf202f;
	color: white;
}

.ribbon.christmas-mega span::before {
	    border-left: 3px solid #77040e;
    border-top: 3px solid #77040e;
}

.ribbon.christmas-mega span::after {
	    border-top: 3px solid #77040e;
    border-right: 3px solid #77040e;
}	
		
		
		
		
/* Ribbon TECH */
.ribbon.tech span {
	background: #70909e;
	color: white;
}

.ribbon.tech span::before {
	    border-left: 3px solid #446472;
    border-top: 3px solid #446472;
}

.ribbon.tech span::after {
	    border-top: 3px solid #446472;
    border-right: 3px solid #446472;
}			
				

		
	
		
