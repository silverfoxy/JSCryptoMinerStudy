                            },5000);
                        } else if (section == 'static') {
                            subField.val(1);
                            ga("send", {hitType: "event", eventCategory: "Signup Form", eventAction: "NewOrleansOnline Monthly E-newsletter", eventLabel: "static_Conversions",  nonInteraction: true});
