        // validation and submission of the newsletter via the slideout/static
        function mecNewsletterValidation(section) {
            var el;
            switch (section) {
                case 'slideout': el = '.newsletter_slideout_content'; break;
                case 'static': el = '.static_header_form'; break;
            }
            var emailText =  $(el+' input[name="ea"]');
