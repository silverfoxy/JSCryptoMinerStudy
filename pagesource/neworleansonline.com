            var subField =  $(el+' .newsSub');
            var sForm =  $(el+' form');

            var regExp = new RegExp(/^\S+@\S+\.\S+$/);
            if ((emailText.val() == '') || emailText.val() == 'John.Smith@gmail.com' || (!regExp.test(emailText.val()))) {
                $(el+' form').find('.emError').show();
                return false;
            } else {
                $(el+' form').find('.emError').hide();
                if (subField.val() == 1) {
                    subField.val(2);
                    $.post('/tools/signup_done.html', sForm.serialize(), function(){

                        // close popup cookie and set converted newsletter cookie
                        var cookie_expire = getCookieExpireDate(730); // 2 years out
                        document.cookie = "closePop=1; expires="+cookie_expire+"; path=/";
                        document.cookie = "convertedNewsletter=1; expires="+cookie_expire+"; path=/";

                        // show thanks msg, fire GA event, and close asset if needed
                        if (section == 'slideout') {
                            $(".newsletter_slideout").addClass("closed");
                            setTimeout(function(){
                                $("#newsSignSlideout").hide();
                                $(".slideout_copy_head").html("THANK YOU FOR SIGNING UP!");
                                $(".slideout_copy_sub").css("width","95%");
