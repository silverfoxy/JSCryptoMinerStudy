    <script type="text/javascript">
        'use strict';

        var initialEl = $('.sticky-footer-wrapper, .main-body, footer');
		var disableStickyNav = false;
        if(!config.removeAgeGate){
            initialEl.hide();
        } else {
            initialEl.show();
            $('.sticky-on-modal').css('bottom', '0');
        }

        if (config.enableParallaxBackground) {
          require(['landingpageStellarConfig','jqueryStellar'], function(StellarConfig) {
            // check if stellar/parallax plugin is not initialized
            if ($(window).data('plugin_stellar') === undefined) {
              $.stellar(StellarConfig);
            }
          });
        }

        CQ.I18n.init({
            "locale": "en",
            "urlPrefix": "/content/michelobultra/",
            "urlSuffix": "/_jcr_content.dict.json"
        });

        require(['modules/ageGate','windowController'], function(AgeGateModule, WindowController){
            //Add Age Gate title
            AgeGateModule.opts = {};
            AgeGateModule.init("michelobultra",
                {
                    ageGateTitle: '',
                    siteLanguage: 'en',
                    countryCode: localStorage.getItem('countryCode') || 'US',
                    urlRedirect: 'http://www.thecoolspot.gov',
                    currentPageUrl: '/content/michelobultra/en/home'
                }
            );

            // getting the cookie saved on the user

            if(!config.removeAgeGate){
                AgeGateModule.checkCookieAgeGate(function(needCookie){
                    // if the cookie is already there
                    config.ageGateValid = !needCookie;
                    if(config.ageGateValid){
                        initialEl.show();
                        // we doesn't need the age-gate so is removed here
                        $('#age-checker-wrapper').remove();
                        // and the additional dialog that age-gate's create
                        $('.sense-ui').remove();
                        $('footer').removeClass('sticky-on-modal');

                        AgeGateModule.triggerAgeGatePassedEvent();
                    // no presence of any cookie
                    }else{
                        AgeGateModule.showProgress(true);
                        // delete dialog's cross-button
                        $('.close').hide();
                        // open dialog
                        $('#dialog-age-checker').trigger('click');
                        $('.modal-backdrop').addClass('fullopacity');
                        $('.sense-ui').remove();
                        // getting the AgeGateController aka logic
                        require(['ageGateController']);
                    }
                });
            } else {
                // settings when remove age gate is selected.
                config.ageGateValid = true;
                // trigger that the age gate has passed so scrollers initialize when age gate removed.
                AgeGateModule.triggerAgeGatePassedEvent();
            }

            $(window).load(function() {
                if(config.removeAgeGate){
                    $('.sticky-on-modal').css('bottom', 'auto');
                }
                $('.sticky-on-modal').css('display', 'block');
            });

            $(document).ready(function(){
                var w       = $(window),
                    d       = $(this),
                    $html   = $('html'),
                    $body   = $('body');
                var brightTagData;

                $(".age-checker-yes").on("click",function(event){
                  AgeGateModule.createCookieSimple();
                  event.preventDefault();
                });
                $(".age-checker-no").on("click",function(){
                  AgeGateModule.showUnderAgeModal();
                });

                var mainHeader = (function ($) {
                    var $mainHeader         = $('.main-header'),
                        pageWidth           = window.outerWidth,
                        mainHeaderHeight    = $mainHeader.height(),
                        $mhmainnavContainer = $('.mh-main-nav-container'),
                        $inputs             = $('.gigya-register-form input:focus'),
                        $newNav = $('.new-main-header');

                    w.resize(function() {
                        pageWidth = w.width();
                        mainHeaderHeight = $mainHeader.height();
                        if ($newNav.length) {
                            if (pageWidth >= 991) {
                                if ($newNav.children('div').hasClass('nav-out')) {
                                    $newNav.children('div').removeClass('nav-out');
                                    $('.mh-main-nav-button').removeClass('activeNav');
                                    $body.removeClass('lockBody');
                                }
                                if ($('.mh-mn-menu-item').hasClass('mh-mn-menu-item-selected')) {
                                    $('.mh-mn-menu-item').removeClass('mh-mn-menu-item-selected');
                                }
                            }
                        }
                    });
                    var togglerMethod = function(button, toggler) {
                        button.click(function(e) {
                            e.preventDefault();
                            var $this = $(this),
                                buttonSelectedClass = $this.data('selected-class'),
                                togglerSelectedClass = toggler.data('selected-class');

                            var selectedItems = $.makeArray( $('.mh-search-container, .mh-mn-icons-container').find('[class$="-selected"]').not($this).not(toggler) );
                            $.each(selectedItems, function(i, v) {
                                $this.removeClass($this.data('selected-class'));
                            });

                            if($newNav.length){
                                $this.toggleClass(buttonSelectedClass);
                                if(toggler.hasClass(togglerSelectedClass)){
                                    setTimeout(function(){
                                        toggler.toggleClass(togglerSelectedClass);
                                    },400);
                                }else{
                                    toggler.toggleClass(togglerSelectedClass);
                                }
                            }else{
                                $this.toggleClass(buttonSelectedClass);
                                toggler.toggleClass(togglerSelectedClass);
                            }
                        });
                    };


                    var mainNavTogglerMethod = function(button) {
                        button.on('click keypress', function(e) {
                            if (e.which === 13 || e.type === 'click') {
                                e.preventDefault();
                                var $this = $(this),
                                    menuParentItem = $this.parent('.mh-mn-menu-item'),
                                    menuParentItemSelectedClass = menuParentItem.data('selected-class');
                                
                                if ( pageWidth <= 991 || e.which === 13) {
                                    menuParentItem.toggleClass(menuParentItemSelectedClass); 
                                    $this.children('.mh-mn-menu-arrow').toggleClass('icon-arrow-up').toggleClass('icon-arrow-down');   
                                } else if ( pageWidth >= 992 && (Modernizr.touch || e.which === 13)) {
                                    $('.mh-mn-menu-item-selected').not($(this).parent('.mh-mn-menu-item')).removeClass('mh-mn-menu-item-selected');
                                    $this.children('.mh-mn-menu-arrow').toggleClass('icon-arrow-up').toggleClass('icon-arrow-down');
                                    menuParentItem.toggleClass(menuParentItemSelectedClass);
                                }
                            }
                        });

                        w.on('orientationchange', function() {
                            if ( pageWidth >= 992 && Modernizr.touch ) {
                                $('.mh-mn-menu-item-selected').find('.mh-mn-menu-arrow').toggleClass('icon-arrow-up').toggleClass('icon-arrow-down');
                                $('.mh-mn-menu-item').removeClass('mh-mn-menu-item-selected');
                            }
                        });
                    };

                    var mobileMenuBodyDisabler = function() {
                        var headerContentHeight,
                            headerNavHeight,
                            headerTotalheight,
                            windowHeight = window.innerHeight;

                        function updateHeaderVariables() {
                            headerContentHeight = $('.mh-content').outerHeight(),
                            headerNavHeight = $mhmainnavContainer.height(),
                            headerTotalheight = headerContentHeight + headerNavHeight;
                            windowHeight = window.innerHeight;
                        }

                        function headerStrech() {
                            $mhmainnavContainer.css('height', windowHeight - headerContentHeight);
                        }

                        function headerReset() {
                            $mhmainnavContainer.css('height', '');
                        }

                        function disableBody(height) {
                            $html.css({
                                height: height,
                                overflowY: 'scroll'
                            });
                            $body.css({
                                height: height,
                                overflowY: 'hidden'
                            });
                        }

                        function enableBody() {
                            $html.css({
                                height: '',
                                overflowY: ''
                            });
                            $body.css({
                                height: '',
                                overflowY: ''
                            });
                        }

                         if (!$newNav.length) {
                       // document -- .mh-main-nav-button events for some strange reason need to be the other way around :S
                            d.on('click', '.mh-main-nav-button:not(.mh-main-nav-button-selected)', function() {
                                if( pageWidth <= 991 ) {
                                    enableBody();
                                    headerReset();
                                }
                            })
                            .on('click', '.mh-main-nav-button-selected, .mh-mn-menu-link', function() {
                                if( pageWidth <= 991 ) {
                                    headerReset();
                                    updateHeaderVariables();

                                    if ( windowHeight < headerTotalheight ) {
                                        disableBody(headerTotalheight);
                                        headerReset();
                                    } else {
                                        disableBody(windowHeight);
                                        headerStrech();
                                    }
                                }
                            });
                        } else {

                            $('.mh-main-nav-button').on('click', function(){
                                 if ($(this).hasClass('activeNav')) {
                                  $(this).removeClass('activeNav');
                                  $(this).parent('div').removeClass('nav-out');
                                  setTimeout(function(){
                                    $body.removeClass('lockBody');
                                  },400);
                                 } else {
                                  $(this).addClass('activeNav');
                                  $(this).parent('div').addClass('nav-out');
                                  $body.addClass('lockBody');
                                 }
                              });
                        }
                        w.resize(function() {

                            windowHeight = window.innerHeight;

                            if ( pageWidth >= 992 ) {
                                enableBody();
                                headerReset();
                                $('.mh-main-nav-button-selected').removeClass('mh-main-nav-button-selected');
                                $('.mh-main-nav-container-selected').removeClass('mh-main-nav-container-selected');
                            }
                        });
                    };

                    var stickyMenu = function(disableAdjust) {
                        var disableStickyAdjust = disableAdjust || false;

                        /**
                         * window scroll event callback for sticky menu bar
                         * @return {void}
                         */
                        function onScrollEvent () {
                            if( pageWidth >= 992 ) {
                                var scrollPos = w.scrollTop(),
                                    mainHeaderHeight = $mainHeader.height();

                                if ( scrollPos > mainHeaderHeight ) {
                                    $mainHeader.addClass('sticky-main-header');
                                    if (!disableStickyAdjust) {
                                        adjustPaddinBody(mainHeaderHeight);
                                    }
                                } else {
                                    $mainHeader.removeClass('sticky-main-header');
                                    if (!disableStickyAdjust) {
                                        adjustPaddinBody();
                                    }
                                }

                                if($inputs && $('.sticky-main-header') && scrollPos != 0) {
                                    $('.sticky-main-header').width('100%');
                                } else {
                                    $mainHeader.width('100%');
                                }
                            }
                        }

                        /**
                         * function to adjust sticky menu bar on resize event.
                         * @param  {string} padding paddinTop value
                         * @return {void}
                         */
                        function adjustPaddinBody (padding) {
                            var paddingT = padding || 0;
                            $body.css({paddingTop:paddingT});
                        }

                        /**
                         * resize event callback to adujust sticky menu bar
                         * @return {void}
                         */
                        function onResizeEvent (){
                            if( pageWidth <= 991 && $mainHeader.hasClass('sticky-main-header') ) {
                                $mainHeader.width('100%');
                                $mainHeader.removeClass('sticky-main-header');
                                $body.css({ paddingTop: 0 });

                                var selectedItems = $.makeArray( $('.mh-search-container, .mh-mn-icons-container').find('[class$="-selected"]') );
                                $.each(selectedItems, function(i, v) {
                                    $(this).removeClass($(this).data('selected-class'));
                                });
                                selectedItems = [];
                            }
                        }
                         // new nav scroll
                            function newNavScroll() {
                              var scrollPos = w.scrollTop(),
                                  mainHeaderHeight = $newNav.height();

                             if (scrollPos > mainHeaderHeight) {
                                  $newNav.addClass('sticky-new-main-header');
                              } else {
                                  $newNav.removeClass('sticky-new-main-header');
                              }

                            }

                            //set window scroll and resize behavior
                            if ($newNav.length) {
                              w.scroll(newNavScroll);
                            } else {
                              w.scroll(onScrollEvent).resize(onResizeEvent);
                            }
                    };

                    $('span.mh-mn-menu-link').each(function(index, item) {
                        var $item = $(item);
                        mainNavTogglerMethod($item, $item.find('.mh-mn-submenu'));
                    });

                    // var delay = ($newNav.length) ? 400 : false;
                    togglerMethod( $('.mh-main-nav-button') , $mhmainnavContainer );
                    togglerMethod( $('.mh-search-button')   , $('.mh-search-form-container') );
                    togglerMethod( $('.mh-un-button')       , $('.mh-user-nav-container') );
                    togglerMethod( $('.mh-mn-social-button'), $('.mh-mn-social-items') );
                    mobileMenuBodyDisabler();

                    //enable or disable adjusment on sticky bar.
                   /* var disableStickyAdjust = false;
                    if(config.siteName == 'busch') {
                        disableStickyAdjust = true;
                    }
                    // set sticky menu behavior
                    stickyMenu(disableStickyAdjust);
					*/
					stickyMenu(disableStickyNav);
                    














    





















                })(jQuery);
            }); // ends jQuery document.ready
        }); // require ends

    </script>

    











  
  
