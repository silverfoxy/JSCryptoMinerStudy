    var roomId = "";

    if(window.location.hash && window.location.hash.match(/^#[a-z]{2}\-[a-z]+\-\d+$/))
    {
        var parts  = window.location.hash.split("-");
        var server = parts[0].substring(1);
        var room   = parts[1];
        var roomId = parseInt(parts[2]);

        var optionServer = $("#server option[data-alias=" + server + "]");
        if(optionServer.length)
        {
            $("#server").val(optionServer.val());
        }

        var optionRoom = $("#room option[data-alias=" + room + "]");
        if(optionRoom.length)
        {
            $("#room").val(optionRoom.val());
        }
    }

    var game = null;

    function start()
    {
        if(game === null)
        {
            $("#layout").fadeOut(1000);
            $("#game").fadeIn(1000, function()
            {
                $("#layout").remove();
                $("#labels").show();

                            });

            game = new Game
            ({
                canvas : document.getElementById("canvas"),
                server :
                {
                    protocol : "ws",
                    host     : $("#server").val(),
                    port     : $("#room").val(),
                    alias    : $("#server option:selected").data("alias"),
                    room     : $("#room option:selected").data("alias"),
                    roomId   : roomId
                },
                name : $("#nickname").val()
            });
        }

        game.start();
    }

    function hexToRGB(hex)
    {
        var result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
        return {
            x : parseInt(result[1], 16) / 255,
            y : parseInt(result[2], 16) / 255,
            z : parseInt(result[3], 16) / 255
        };
    }

    function realToHex(value)
    {
        return ("0" + parseInt(value * 255).toString(16)).substr(-2);
    }

    function RGBToHex(rgb)
    {
        return "#" + realToHex(rgb.x) +
                     realToHex(rgb.y) +
                     realToHex(rgb.z);
    }

    $(document).ready(function()
    {
        WebGLReport.post("/webgl", 1);

        var gl = document.getElementById("canvas").getContext("experimental-webgl") || document.getElementById("canvas").getContext("webgl");
        if(gl === null)
        {
            $(".no-webgl").toggle();
            gl = null;
            return;
        }

        Resources.preload(function()
        {
            $("#play").text($("#play").data("ready"));
            $("#play").prop("disabled", false);
        });

        $("#nickname").focus();

        $("#nickname").keypress(function(e)
        {
            if(e.which == 13)
            {
                Cookies.set("nickname", $("#nickname").val(), { expires : 1000 });
                start();
            }
        });

        $("#play").click(function()
        {
            Cookies.set("nickname", $("#nickname").val(), { expires : 1000 });

                            
                start();
                    });

        $("#playAgain").click(function()
        {
            $("#gameOver").fadeOut(300);
                            try
                {
                    // factorem.refreshAds([2], true);
                    aiptag.cmd.display.push(function() { aipDisplayTag.refresh("oceanar-io_300x250"); });
                }
                catch(e)
                {
                }
                        start();
        });

        $("#copyRoomURL").click(function(e)
        {
            $("#roomURL").show();

            document.getElementById("roomURL").select();
            document.execCommand("copy");

            $("#roomURL").hide();

            $("#copiedToClipboard").show();

            setTimeout(function()
            {
                $("#copiedToClipboard").fadeOut("slow");
            }, 2000);

            e.preventDefault();
        });
    });
