    <script type="text/javascript">
        $(document).ready(function()
        {
            var rulesIndex = 1;

            var rules = ["Feed your Queen fish to grow your school","Protect your Queen fish!","Feed your damaged creatures to restore their health","Avoid of jellyfish!","Crabs can join your school","Devour other player's Queen fish and grab her creatures!","Hold left mouse button or W key to direct your creatures","Hold right mouse button or Space key to merge equal creatures into stronger one!"];

            function rotateRules()
            {
                $("#rules").fadeOut("slow", function()
                {
                    $(this)
                        .text(rules[rulesIndex++ % rules.length])
                        .fadeIn("slow", function()
                        {
                            setTimeout(function()
                            {
                                rotateRules();
                            }, 3000);
                        });
                });
            }

            setTimeout(function()
            {
                rotateRules();
            }, 3000);
        });
    </script>

