    /* ******** */
	/* CATALOGO */
	/* ******** */

div#CATALOGO ul,
div#CATALOGO li{
    padding: 0;
    margin: 0;
    list-style: none;}
 
div#CATALOGO input{
    /*position: absolute;*/
    opacity: 0;
	width: 24px;
	height: 24px;
	cursor: pointer;}
 
div#CATALOGO{
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;}
 
div#CATALOGO a{
    color: #005baa;
    text-decoration: none;}
 
div#CATALOGO a:hover{
	color: #005baa;
    text-decoration: underline;}

div#CATALOGO input + label{
    font-weight: bold;}
 
div#CATALOGO input + label + ul{
    margin: 0 0 0 24px; /*identação*/}
 
div#CATALOGO input ~ ul{
    display: none;}
 
div#CATALOGO input:disabled + label{
    cursor: default;
    opacity: .6;}
 
div#CATALOGO input:checked:not(:disabled) ~ ul{
    display: block;}
 
div#CATALOGO label,
div#CATALOGO label::before,
div#CATALOGO a.IMG,
div#CATALOGO a.AUD,
div#CATALOGO a.VID,
div#CATALOGO a.MAT,
div#CATALOGO a.MNU,
div#CATALOGO a.DOC{
    background: url(//adcon.rn.gov.br/icons/icon_CATALOGO.png) no-repeat;}
 
div#CATALOGO label,
div#CATALOGO a,
div#CATALOGO label::before{
    display: inline-block;
    height: 24px;
    line-height: 24px;
    vertical-align: middle;}
 
div#CATALOGO label{
    background-position: 24px 0; /*icone + ou -*/
	margin: 0 !important}
 
div#CATALOGO label::before{
    content: "";
    width: 24px;
    margin: 0; /*padrão para exibir icone pasta 0 24px 0 0;*/
    vertical-align: middle;
    background-position: 0 -48px; /*icone +, se já estiver aberto*/}
 
div#CATALOGO input:checked + label::before{
    background-position: 0 -24px; /*icone -, mesmo que não tenha subpastas*/}

div#CATALOGO a.IMG,
div#CATALOGO a.AUD,
div#CATALOGO a.VID,
div#CATALOGO a.MAT,
div#CATALOGO a.MNU,
div#CATALOGO a.DOC{
	padding: 0 0 0 24px;}
	
div#CATALOGO a.IMG{
    background-position: 0 -72px;}

div#CATALOGO a.AUD{
    background-position: 0 -96px;}

div#CATALOGO a.VID{
    background-position: 0 -120px;}

div#CATALOGO a.MAT{
    background-position: 0 -144px;}

div#CATALOGO a.MNU{
    background-position: 0 -168px;}

div#CATALOGO a.DOC{
    background-position: 0 -192px;}
 
/* webkit adjacent element selector bugfix */
@media screen and (-webkit-min-device-pixel-ratio:0)
{
    div#CATALOGO{
        -webkit-animation: webkit-adjacent-element-selector-bugfix infinite 1s;}
 
    @-webkit-keyframes webkit-adjacent-element-selector-bugfix{
        from{ 
            padding: 0;} 
        to{ 
            padding: 0;}
    }
}



