    function updateValidation() {
        var hasDates = true;
        var entryDateString = $('#txtEntryDateCarPark').val();
        var entryHour = $('#ddlEntryTimeCarPark').val().substring(0, 2);
        var exitDateString = $('#txtExitDateCarPark').val();
        var exitHour = $('#ddlExitTimeCarPark').val().substring(0, 2);

