    function updateValidationBus() {
        var hasDates = true;
        var entryDateString = $('#txtBusTicketDepartingDate').val();
        var entryHour = $('#ddlEntryTimeBusTicket').val().substring(0, 2);

        var exitDateString = $('#txtBusTicketReturnDate').val();
        var exitHour = $('#ddlReturnTimeBusticket').val().substring(0, 2);

        var errEntryDate = $('#divErrorEntryDateBus');
        var errEntryTime = $('#divErrorEntryTimeBus');

        var errExitDate = $('#divErrorExitDateBus');
        var errExitTime = $('#divErrorExitTimeBus');


        var errDateInvalid = $('#divErrorDatesBus');
        var errEntryDatePast = $('#divErrorDepartDatePastBus');
        var errExitDatePast = $('#divErrorReturnDatePastBus');
        var errJourneyType = $('#divErrorJourneyType');
        var errDestination = $('#divErrorDestination');
        var errDepart = $('#divErrorDepartingFrom');

        if (errEntryDate.is(':visible')) {
            if (entryDateString == "") {
                $('#divErrorEntryDateBus').css("display", "block");
            } else {
                hasDates = false;
            }
        }
        if (errExitDate.is(':visible')) {
            if (exitDateString != "" && $("#ddlJourneyType").val() != "ACS") {
                errExitDate.css("display", "none");
            } else {
                hasDates = false;
            }
        }
        if (errJourneyType.is(':visible')) {
            if ($('#ddlJourneyType').val() != "0") {
                errJourneyType.css("display", "none");
            } else {
                hasDates = false;
            }
        }
        if (errDepart.is(':visible')) {
            if ($('#ddlBusDeparture').val() != "0") {
                errDepart.css("display", "none");
            } else {
                hasDates = false;
            }
        }
        if (errDestination.is(':visible')) {
            if ($('#ddlBusDestination').val() != "0") {
                errDestination.css("display", "none");
            } else {
                hasDates = false;
            }
        }
        if (errEntryTime.is(':visible')) {
            if ($('#ddlEntryTimeBusTicket').val() != "0") {
                errEntryTime.css("display", "none");
            } else {
                hasDates = false;
            }
        }
        if (errExitTime.is(':visible')) {
            if ($('#ddlReturnTimeBusticket').val() != "0" && $("#ddlJourneyType").val() != "ACS") {
                errExitTime.css("display", "none");
            } else {
                hasDates = false;
            }
        }



        if (hasDates) {
            var entrydate = new Date(entryDateString.replace(/(\d{2})[-/](\d{2})[-/](\d+)/, "$2/$1/$3"));

            entrydate.setHours(entryHour);

            var exitdate = new Date(exitDateString.replace(/(\d{2})[-/](\d{2})[-/](\d+)/, "$2/$1/$3"));
            exitdate.setHours(exitHour);

            var now = new Date();
            if (errEntryDatePast.is(':visible')) {
                if (entrydate >= now) {
                    errEntryDatePast.css("display", "none");
                }
            }
            if (errExitDatePast.is(':visible')) {
                if (exitdate >= now) {
                    errExitDatePast.css("display", "none");
                }
            }
            if (errDateInvalid.is(':visible')) {
                if ($("#ddlJourneyType").val() == "ACR") {
                    if (entrydate < exitdate) {
                        errDateInvalid.css("display", "none");
                    }
                }
            }
        }
    }

