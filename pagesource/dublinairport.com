        var errExitTime = $('#divErrorExitTimeCarPark');


        var errDateInvalid = $('#divErrorDatesCarPark');
        var errEntryDatePast = $('#divErrorEntryDatePastCarPark');
        var errExitDatePast = $('#divErrorExitDatePastCarPark');

        if (errEntryDate.is(':visible')) { // If entry date empty error already shown, update once changed
            if (entryDateString != "") {
                errEntryDate.css("display", "none");
            } else {

                hasDates = false;
            }
        }
        if (errExitDate.is(':visible')) { // If entry date empty error already shown, update once changed
