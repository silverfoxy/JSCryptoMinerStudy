                errEntryTime.css("display", "none");
            } else {
                hasDates = false;
            }
        }
        if (errExitTime.is(':visible')) { // If entry date empty error already shown, update once changed
            if ($('#ddlExitTimeCarPark').val() != "0") {
                errExitTime.css("display", "none");
            } else {
                hasDates = false;
            }
        }

        if (hasDates) {
            var entrydate = new Date(entryDateString.replace(/(\d{2})[-/](\d{2})[-/](\d+)/, "$2/$1/$3"));
            entrydate.setHours(entryHour);

            var exitdate = new Date(exitDateString.replace(/(\d{2})[-/](\d{2})[-/](\d+)/, "$2/$1/$3"));
            exitdate.setHours(exitHour);

            var now = new Date();
            if (errEntryDatePast.is(':visible')) {
                if (entrydate >= now) {
                    errEntryDatePast.css("display", "none");
                }
            }
            if (errExitDatePast.is(':visible')) {
                if (exitdate >= now) {
                    errExitDatePast.css("display", "none");
                }
            }
            if (errDateInvalid.is(':visible')) {
