    function setUpPopUp() {


        var entryDateString = $('#txtEntryDateCarPark').val();

        var entryHour = $('#ddlEntryTimeCarPark').val().substring(0, 2);

        var exitDateString = $('#txtExitDateCarPark').val();

        var exitHour = $('#ddlExitTimeCarPark').val().substring(0, 2);

        clearform();

        isValid = true;

        if (entryDateString == "") {
            $('#divErrorEntryDateCarPark').css("display", "block");
            isValid = false;

        }

        if (exitDateString == "") {
            $('#divErrorExitDateCarPark').css("display", "block");
            isValid = false;
        }

        if ($('#ddlEntryTimeCarPark').val() == "0") {
            $('#divErrorEntryTimeCarPark').css("display", "block");
            isValid = false;
        }

        if ($('#ddlExitTimeCarPark').val() == "0") {
            $('#divErrorExitTimeCarPark').css("display", "block");
            isValid = false;
        }

        if (isValid) {

            var entrydate = new Date(entryDateString.replace(/(\d{2})[-/](\d{2})[-/](\d+)/, "$2/$1/$3"));

            entrydate.setHours(entryHour);

            var exitdate = new Date(exitDateString.replace(/(\d{2})[-/](\d{2})[-/](\d+)/, "$2/$1/$3"));
            exitdate.setHours(exitHour);

            var now = new Date();
            if (entrydate < now) {

                $('#divErrorEntryDatePastCarPark').css("display", "block");
                isValid = false;
            }
            if (exitdate < now) {
                $('#divErrorExitDatePastCarPark').css("display", "block");
                isValid = false;
            }
            if (isValid) {
                if (entrydate >= exitdate) {
                    $('#divErrorDatesCarPark').css("display", "block");
                    isValid = false;
                }
            }

        }

        if (isValid) {
            window.document.forms[0].target = '_blank';
            setTimeout(function () { window.document.forms[0].target = ''; }, 500);
            return true;
        }
        else {
            return false;
        }

        return false;
