	<style type="text/css">
.ban_mask { width:100%; height:350px; overflow:hidden;}
ul.ban_images { position:relative; top:0px;list-style-type: none !important; margin-left: 0px; margin:0;}
ul.ban_images li { position:absolute; top:0px; left:0px; list-style-type: none !important;}

ul.triggers { position: relative; float: right; top: -30px; list-style-type: none;    margin-bottom: -20px;}
ul.triggers li { 
	float: left; 
	margin: 0 3px; 
	cursor: pointer;
	background-color: #ccc; color: #000; 
	padding: 4px;
	border-radius: 35px;
	border: 2px solid gray;
	list-style-type: none !important;
	font-size: 18px;

}
ul.triggers li.active ,ul.triggers li:hover{
	background-color: white;
	color: #FFF;
}

.control {position:relative; color:#fff; cursor:pointer;}

.ban_arrow{
	position: relative;
	z-index: 2;	
	width: 100%
}

.ban_arrow .prev, .ban_arrow .next{
	position: absolute;
	top: 150px;
	font-size: 50px;
	text-decoration: none;
	    color: #525252;
    background: rgba(255, 255, 255, 0.69);
	padding: 5px 3px 5px 3px;
	z-index:2;
    cursor: pointer;
}

.ban_arrow .prev{
	left :0;
}

.next{
	right:0;
}	</style>
<script>


$( "ul.ban_images li" ).each(function(index) {
  $( "ul.triggers" ).append( "<li></li>" );
});

var triggers = $('ul.triggers li');
var images = $('ul.ban_images li');
var lastElem = images.length-1;
var target;

$('ul.triggers li').first().addClass('active');
$('ul.ban_images li').hide().first().show();

function sliderResponse(target) {
    $('ul.ban_images li').fadeOut(300).eq(target).fadeIn(300);
    $('ul.triggers li').removeClass('active').eq(target).addClass('active');
}

$('ul.triggers li').click(function() {
	
    if ( !$(this).hasClass('active') ) {
        target = $(this).index();
        sliderResponse(target);
        resetTiming();
    }
});

$('.ban_arrow .next').click(function() { 
    target = $('ul.ban_images li:visible').index();
    target === lastElem ? target = 0 : target = target+1;
    sliderResponse(target);
    resetTiming();
});

$('.ban_arrow .prev').click(function() {	
    target = $('ul.ban_images li:visible').index();
    target === 0 ? target = lastElem : target = target-1;
    sliderResponse(target);
    resetTiming();
});

function sliderTiming( lastElem ) {
	if(lastElem == 0)
		return;
    target = $('ul.ban_images li:visible').index();
    target >= lastElem ? target = 0 : target = target+1;
    sliderResponse(target);
}

var timingRun = setInterval(function() { sliderTiming(lastElem); },8000);
function resetTiming() {
    clearInterval(timingRun);
    timingRun = setInterval(function() { sliderTiming(lastElem); },8000);
}
$(".ban_mask").mouseenter(function(){
clearInterval(timingRun);
timingRun = false;
});

$(".ban_mask").mouseleave(function(){
	resetTiming();
});

$("ul.ban_images li img").load(function(){
	var max = Math.max.apply(Math, $("ul.ban_images li").map(function() { return $(this).height(); }));
	$(".ban_mask").height(max);
	var adjust_arrow= max/2-20;
	$('.ban_arrow .next').css('top', adjust_arrow+'px');
	$('.ban_arrow .prev').css('top', adjust_arrow+'px');
});
</script></p>
