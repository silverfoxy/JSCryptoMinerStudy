function getTimeRemaining(endtime) {
	var t = Date.parse(endtime) - Date.parse(new Date());
	var seconds = Math.floor((t / 1000) % 60);
	var minutes = Math.floor((t / 1000 / 60) % 60);
	var hours = Math.floor((t / (1000 * 60 * 60) ) );
	var days = 0;
	if(window.include_day == true)
	{
		hours = Math.floor((t / (1000 * 60 * 60) % 24) );
		days = Math.floor((t / (1000 * 60 * 60 * 24)) );
	}
	
	return {
		'total': t,
		'days': days,
		'hours': hours,
		'minutes': minutes,
		'seconds': seconds
	};
}
function initializeClock(id, endtime) {
  var clock = document.getElementById(id);
  
if(clock){
	var daysSpan = clock.querySelector('.days') || [];
	var hoursSpan = clock.querySelector('.hours') || [];
	var minutesSpan = clock.querySelector('.minutes') || [];
	var secondsSpan = clock.querySelector('.seconds') || [];

  function updateClock() {
    var t = getTimeRemaining(endtime);
	daysSpan .innerHTML = ('0' + t.days).slice(-2);
    hoursSpan.innerHTML = ('0' + t.hours).slice(-2);
    minutesSpan.innerHTML = ('0' + t.minutes).slice(-2);
    secondsSpan.innerHTML = ('0' + t.seconds).slice(-2);


	daysSpan .innerHTML = daysSpan .innerHTML.substr(0,1)+" "+daysSpan .innerHTML.substr(1,2);
	hoursSpan .innerHTML = hoursSpan .innerHTML.substr(0,1)+" "+hoursSpan .innerHTML.substr(1,2);
	minutesSpan .innerHTML = minutesSpan .innerHTML.substr(0,1)+" "+minutesSpan .innerHTML.substr(1,2);
	secondsSpan .innerHTML = secondsSpan .innerHTML.substr(0,1)+" "+secondsSpan .innerHTML.substr(1,2);
	

    if (t.total <= 0) {
		clearInterval(timeinterval);
		daysSpan .innerHTML = "- -";
		hoursSpan.innerHTML = "- -";
		minutesSpan.innerHTML = "- -";
		secondsSpan.innerHTML = "- -";
    }
	
  }
  updateClock();
  var timeinterval = setInterval(updateClock, 1000);
}
}
var c_end = "2018-01-05 07:00:00";
var include_day = false;
var tmp = c_end.split(" ");
var dateArray = tmp[0].split("-");
var yy = dateArray[0];
var mm = dateArray[1];
var dd = dateArray[2];
dateArray = tmp[1].split(":");
var hr = dateArray[0];
var min = dateArray[1];
var sec = dateArray[2];
var expire = new Date();
expire.setUTCFullYear(yy,mm-1,dd); 
expire.setUTCHours(hr,min,sec);
var deadline = new Date(Date.parse(expire));

initializeClock('clockdiv', deadline);
</script></div>
