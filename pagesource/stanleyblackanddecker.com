<script>
	if (document.body.clientWidth < 785) {
		var $squareBlocks = document.getElementById('block-views-frontpage-squareblocks-block');
		var mc = new Hammer($squareBlocks);

		swipeTime = true; //Ability to swipe
		swipeInt = 700; //Interval between swipes


		var setSwipeTimeInterval = setInterval(setSwipeTime, swipeInt);


		mc.on('swipe', function(ev) {
			if (swipeTime) {
				//Swipe LtR
				if (ev.direction == 4) {
					swipeLeft();
					swipeTime = false;
					clearInterval(setSwipeTimeInterval);
					setSwipeTimeInterval = setInterval(setSwipeTime, swipeInt);
				}
				//Swipe RtL
				if (ev.direction == 2) {
					swipeRight();
					swipeTime = false;
					clearInterval(setSwipeTimeInterval);
					setSwipeTimeInterval = setInterval(setSwipeTime, swipeInt);
				}
			}
		});
	}

	function setSwipeTime() {
		swipeTime = true;
	}


	var $squareBlock = "";
	var current_block = 0;
	var total_blocks = 0;
	var z_index = 30;

	(function($) {
		$squareBlock = $("#block-views-frontpage-squareblocks-block .views-row");
		$squarePage = $(".slide-page div");
		total_blocks = $squareBlock.length - 1;

		$squarePage.eq(0).attr("style", "opacity:1");
	}(jQuery));



	function swipeRight() {
		console.log("right");
		(function($) {
			if (current_block < total_blocks) {
				current_block++;
			} else {
				current_block = 0;
			}

			$squareBlock.removeClass("active-square-r active-square-l");
			$squareBlock.eq(current_block).addClass("active-square-r");
			z_index++;
			$squareBlock.eq(current_block).attr("style", "z-index:" + z_index);

			$squarePage.attr("style", "");
			$squarePage.eq(current_block).attr("style", "opacity:1");
		}(jQuery));
	}

	function swipeLeft() {
		console.log("left");
		(function($) {
			if (current_block > 0) {
				current_block--;
			} else {
				current_block = total_blocks;
			}

			$squareBlock.removeClass("active-square-r active-square-l");
			$squareBlock.eq(current_block).addClass("active-square-l");
			z_index++;
			$squareBlock.eq(current_block).attr("style", "z-index:" + z_index);

			$squarePage.attr("style", "");
			$squarePage.eq(current_block).attr("style", "opacity:1");
		}(jQuery));
	}
</script>
