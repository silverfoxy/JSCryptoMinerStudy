		<script type="text/javascript">
			//Newsletter footer signup form
			jQuery(document).ready(function () {
			jQuery("#emailSignupFormFooter").submit(function(e) {

				var selectedValues = [];
				jQuery('#emailSignupFormFooter .newsletter-item input:checked').each(function(i, input) {
				 var value = jQuery(input).val().trim();
				if(value != "all"){
					selectedValues.push(value);
				}
				});
				selectedValues = selectedValues.join(',');
				var email = jQuery('footer .newsletters input[name=txtEmailAddressFooter]').val();
				var data = "txtAudienceGUID=" + selectedValues + "&txtEmailAddress=" + email;


				jQuery.ajax({
				crossDomain:true,
				data:data,
				type: "POST",
				cache: false,
				url: "http://newsletters.buffalonews.com/templates/addToAudience.php",
				error: function(xhr, status, error) {
						window.location.replace("//buffalonews.com/newsletter/?message=" + error);
				},
				success: function (msg){
					if (msg == '<p>Fail: Invalid Email Address.</p>') {
						window.location.replace("//buffalonews.com/newsletter/?message=Please enter a valid email address.");
					} else if (msg == '<p>Fail: Invalid AudienceGUID.</p>'){
						window.location.replace("//buffalonews.com/newsletter/?message=Please check the newsletters you'd like.");
					} else {
						 window.location.replace("//buffalonews.com/newsletter/?success");
					}
				}
				});

				e.preventDefault();
				return false;
				 });
			});
		</script>

