	var adSizeTopRectangleAd = [[[1180, 0], ["fluid", [300, 250], [300, 600]]], [[728, 0], [728, 90]], [[0, 0], ["fluid", [300, 250]]]];

	// set targeting string
	var targetingString = '' + '/4974125/Front-Page';

	googletag.cmd.push(function () {
		// *** CUSTOM AD SIZING : non-custom sizing is inline with googletag.defineSlots
					// *** END CUSTOM AD SIZING

		// *** DEFINE AD SLOTS
			// photogallery interstitial fully-responsive : 720x750=gallery interstitial
			
			// top leaderboard, billboard and large leaderboard: 728x90=leaderboard, 970x250=billboard, 970x90=large leaderboard
							var adSlot0_sizing = googletag.sizeMapping()
					.addSize([0, 0], [])
					.addSize([992, 0], [[970, 250], [728, 90], [970, 90]])
					.build();
				var adSlot0 = googletag.defineSlot(targetingString, [[728, 90], [970, 250], [970,90]], 'dfp-ad-leaderboard-1')
					.defineSizeMapping(adSlot0_sizing)
					.setTargeting('POS', ['ATF'])
					.addService(googletag.pubads());
			
			// custom ad slot : 1x1=wallpaper, 2x2=pagepeel  3x3=empty pixel ---> ***MOVE TOPHAT AT SOME POINT??!!( CAN WE ADD TOPHAT? - tophat is ALWAYS visible no matter what size the screen is. Where as wallpapaer and peel only show on desktop)
			var adSlot1_sizing = googletag.sizeMapping()
				.addSize([0, 0], [3, 3])
				.addSize([992, 0], [[1, 1], [2, 2], [3, 3]])
				.build();
			var adSlot1 = googletag.defineSlot(targetingString, [[1, 1], [2, 2], [3, 3]], "div-gpt-ad-custom")
				.defineSizeMapping(adSlot1_sizing)
				.setTargeting('custom', ['true'])
				.addService(googletag.pubads());

			// tophat fully-responsive : 0x0=fluid  3x3=empty pixel
			var adSlot2 = googletag.defineSlot(targetingString, ['fluid', [3, 3]], "div-dfp-ad-tophat")
				.setTargeting('tophat', ['true'])
				.addService(googletag.pubads());

			// bottom leaderboard : 728x90=leaderboard
			var adSlot3_sizing = googletag.sizeMapping()
				.addSize([0, 0], [300, 250])
				.addSize([750, 0], [728, 90])
				.build();
			var adSlot3 = googletag.defineSlot(targetingString, [728, 90], 'dfp-ad-leaderboard-2')
				.defineSizeMapping(adSlot3_sizing)
				.addService(googletag.pubads());

			// 1st and 2nd rectangle ads
								// 1st
					var adSlot4 = googletag.defineSlot(targetingString, ['fluid', [300, 250], [300, 600], [728, 90]], 'bnAdBlock-boxmedium-1')
						.setTargeting('POS', ['ATF'])
						.setTargeting('sponsored', ['fluid-1'])
						.defineSizeMapping(adSizeTopRectangleAd)
						.addService(googletag.pubads());
								// 2nd
				var adSlot5 = googletag.defineSlot(targetingString, [300, 250], "bnAdBlock-boxmedium-2")
				.addService(googletag.pubads());
			
			// 3rd rectangle ad
							var adSlot6 = googletag.defineSlot(targetingString, [300, 250], "bnAdBlock-boxmedium-3").addService(googletag.pubads());
			
			// in-content ad
					// *** END DEFINE AD SLOTS

		// *** TARGETING
			// logged in users
			jQuery(document).ready(function (jQuery) {
				if (isUserLoggedin()) {
					googletag.pubads().setTargeting('LoggedIn', 'Yes');
				} else {
					googletag.pubads().setTargeting('LoggedIn', 'No');
				}
			});

			// category
							googletag.pubads().setTargeting('Category', 'Home');
			
			// page id
			googletag.pubads().setTargeting('PAGE', '682586');

			// primary-front
							googletag.pubads().setTargeting('Section','Primary_Front');
					// *** END TARGETING

		// *** EVENT LISTENER - when AD IS RENDERED IN SLOT
			googletag.pubads().addEventListener('slotRenderEnded', function(event) {
				if (!event.isEmpty) {
					var adSize = event.size[0] + 'x' + event.size[1];
					if (event.slot.getSlotElementId() == "div-gpt-ad-custom") {
						if (adSize == '1x1') {
							if (jQuery(window).width() > 1199) {
								jQuery('#div-gpt-ad-custom').css({
									'position' : 'absolute',
									'top' : '0px',
									'right' : '0px',
									'z-index' : '999999',
									'display:' : 'block'
								});
							}
						} else if (adSize == '2x2') {
							jQuery('#div-gpt-ad-custom').css({
								'width' : 'auto',
								'height': 'auto'
							});
						} else if (adSize == '3x3') {  // 3x3 pixel - hide ad slot
							jQuery('#div-gpt-ad-custom').css({
								'display' : 'none'
							});
							console.log('3x3 pixel was loaded in custom ad position'); // remove after testing
						}
					}
					if (event.slot.getSlotElementId() == "bnAdBlock-boxmedium-1") {
						if (adSize == '0x0') {
							// for custom styling to force the fluid native sponsored ad to display responsively
							jQuery('body.single .right-rail, body.page .right-rail').addClass('fluid-sponsored');
						}
					}
					if (event.slot.getSlotElementId() == "div-dfp-ad-tophat") {
						if (adSize == '0x0') {
							jQuery('#headerSpacer').css({
								'position' : 'absolute'
							});
						}
						if (adSize == '3x3') { // 3x3 pixel - hide ad slot
							jQuery('#div-dfp-ad-tophat').css({
								'display' : 'none'
							});
							console.log('3x3 pixel was loaded in tophat position'); // remove after testing
						}
					}
					if (event.slot.getSlotElementId() == "bnAdBlock-photo-gallery") {
						if (adSize == '3x3') { // 3x3 pixel
							console.log('3x3 pixel was loaded in photo gallery interstitial position'); // remove after testing
						}
					}
					if (event.slot.getSlotElementId() == "article-content-ad") {
						jQuery( "#article-content-ad" ).prepend( "<p class='adStylingP'>Advertisement</p>" );
					}
				}
			});
		// *** END EVENT LISTENER - AD RENDERED IN SLOT

		googletag.pubads().enableSingleRequest();
		googletag.pubads().collapseEmptyDivs();
