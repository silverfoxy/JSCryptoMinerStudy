.general-box {
    background-color: #f5f5f5;
    border: 1px solid #999;
    padding: 0;
}
.general-box .box-title {
    color: #333;
    border-bottom: 1px solid #999;
}
.general-box.classifica .box-title {
	border-left: 1px solid #999;
}

.general-box ul li {
    background-color: #f5f5f5;
	color: #333;
	border-bottom: 1px solid #999;
	margin-bottom: 0;
	height: 31px;
}
.sliding-matches {
	background-color: #999;
}
.general-box ul {
    margin-top: 0;
	border-bottom: 1px solid #999;
}
.general-box ul li .result {
    background-color: transparent;
	border-left: 1px solid #999;
	border-right: 1px solid #999;
}
.general-box ul li .result a span{
    color: #333;
}
.general-box ul li.selected .points span, .general-box ul li.selected .result span, .general-box ul li.selected .data-table span{
    color: #fff !important;
}
.general-box ul li .data-table {
    background-color: transparent;
	color: #333;
	padding: 2px 5px;
	border-left: 1px solid #999;
	border-right: 1px solid #999;
}
.general-box ul li.selected, .general-box ul li.selected .place {
    color: #3c9fdc;
    background-color: #f5f5f5;
}
.giornata {
    color: #333 !important;
    background-color: #f5f5f5;
}
.sliding-matches {
    background-color: #f5f5f5;
	color: #333;
}
.match-box {
    margin-top: 0;
	padding: 10px;
	float: left;
	width: 100%;
}
.match-box .day-box{
	background-color: #f5f5f5;
}
.match-box .day-box *{
    background-color: #f5f5f5;
	color: #333 !important;
}
.match-box .teams {
	background-color: #f5f5f5;
	margin-top: 0;
}
.general-box.splash-matches .owl-controls {
    top: 0;
}
.match-box h3 {
    color: #333;
}
.general-box ul li .place {
    border-right: 1px solid #999;
    border-bottom: 1px solid #999;
    color: #333;
}
.general-box ul li .points, .par, .gf, .gs, .per, .vin {
    border-bottom: none;
    border-left: 1px solid #999;
}
.general-box ul li .points {
    background-color: #F5F5F5;
}
ul.edicola {
    padding: 10px 0;
	border-bottom: none;
}
ul.edicola li {
	height: auto;
    -webkit-transform: scale(1); 
    -ms-transform: scale(1);
    -moz-transform: scale(1);
    transform: scale(1);
	-webkit-transform-style: preserve-3d;
	-webkit-backface-visibility: hidden;
	-moz-backface-visibility:    hidden;
	-ms-backface-visibility:     hidden;
}
ul.edicola li:hover a {
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	-ms-transition: all 0.3s;
	-o-transition: all 0.3s;
	transition: all 0.3s;
	box-shadow: 0 0 4px rgba(0,0,0,0.4);
	opacity: 1 !important;
}
ul.edicola li:hover {
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	-ms-transition: all 0.3s;
	-o-transition: all 0.3s;
	transition: all 0.3s;
    -webkit-transform: scale(1.08); 
    -ms-transform: scale(1.08);
    -moz-transform: scale(1.08);
    transform: scale(1.08);
	opacity: 1 !important;
}
.general-box.edicola .box-title{
	background-color: #006baf;
	color: #fff;
	
}
div.tabbed {
    display: none;
    padding: 0;
    /* border-top: 1px solid #999; */
    position: relative;
    z-index: 2;
}
input.tabCheck {
  display: none;
}

label.tabs {
    display: inline-block;
    margin: 0 0 -1px;
    padding: 10px;
    text-align: center;
    color: #006baf;
	font-weight: bold;
    font-size: 14px;
    border: 1px solid transparent;
    position: relative;
    z-index: 2;
    border-bottom: 1px solid #f5f5f5;
}

label.tabs:before {
  font-family: fontawesome;
  font-weight: normal;
  margin-right: 10px;
}

label.tabs:hover {
  cursor: pointer;
}

input.tabCheck:checked + label.tabs {
    color: #555;
    border: 1px solid #006baf;
	background-color: #006baf;
	color: #fff;
    position: relative;
    z-index: 3;
}

#tab1:checked ~ #content1,
#tab2:checked ~ #content2,
#tab3:checked ~ #content3 
{
    display: block;

    -webkit-animation: fadeInFromNone 0.3s ease-out;
    -moz-animation: fadeInFromNone 0.3s ease-out;
    -o-animation: fadeInFromNone 0.3s ease-out;
    animation: fadeInFromNone 0.3s ease-out;
}

@-webkit-keyframes fadeInFromNone {
    0% {
        display: none;
        opacity: 0;
    }

    1% {
        display: block;
        opacity: 0;
    }

    100% {
        display: block;
        opacity: 1;
    }
}

@-moz-keyframes fadeInFromNone {
    0% {
        display: none;
        opacity: 0;
    }

    1% {
        display: block;
        opacity: 0;
    }

    100% {
        display: block;
        opacity: 1;
    }
}

@-o-keyframes fadeInFromNone {
    0% {
        display: none;
        opacity: 0;
    }

    1% {
        display: block;
        opacity: 0;
    }

    100% {
        display: block;
        opacity: 1;
    }
}

@keyframes fadeInFromNone {
    0% {
        display: none;
        opacity: 0;
    }

    1% {
        display: block;
        opacity: 0;
    }

    100% {
        display: block;
        opacity: 1;
    }
}
@media screen and (max-width: 650px) {
  label.tab {
    font-size: 0;
  }
  label.tab:before {
    margin: 0;
    font-size: 18px;
  }
}

@media screen and (max-width: 400px) {
  label.tab {
    padding: 15px;
  }
}
